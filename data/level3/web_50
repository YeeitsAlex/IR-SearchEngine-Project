   jQuery(document).ready(function($) {
	// interval = (# of seconds on each slide) * 1000, speed = (# of seconds for transition) * 1000
	$("#l_carousel_scroller").scrollable({
		circular: true, mousewheel: true, speed: 1000
	}).navigator({
		// ul#l_carousel_controls now functions as our navigator
		navi:'ul#l_carousel_controls li'
	}).autoscroll({
		interval: 8000
	});
});
  </script>
