   $(function() {
      $(".dropdown").hover(
          function(){ $(this).addClass('open') },
          function(){ $(this).removeClass('open') }
      );
  });

  $("img").error(function () { 
      $(this).hide();
      // or $(this).css({visibility:"hidden"}); 
