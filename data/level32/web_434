    .nr4ReviewLine3 {
        padding-bottom: 20px;
    }
    .reviewMoreLink {
        padding: 10px 0;
        background-color: #F8F8F8;
        text-align: center;       
    }
     .reviewMoreLink a{        
        text-align: center;       
        font: bold 12px "roboto", sans-serif;
        display: block;
        position: relative;
    }
    .sidebar .reviewBody, .right-rail .reviewBody {
        position: relative;
        clear: both;
        min-height: 400px;
