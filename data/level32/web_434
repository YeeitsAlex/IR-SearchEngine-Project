         <script type="text/javascript">
          $(document).ready(function () {

        var $list = $("#frcc-blog-posts-list-PageContent_SideContent_C041_masterBlogPostsFrontend_ctl00");
        var $switcher = $("#frcc-blog-posts-switcher-PageContent_SideContent_C041_masterBlogPostsFrontend_ctl00");
        $list.children().first(".item").addClass("active");

        $switcher.empty();
        $.each($list.children(), function (idx, val) {
            if (idx == 0) {
                $switcher.append("<li data-index-start=\"0\" class=\"active\"></li>");
            }
            else {
                $switcher.append("<li data-index-start=\"" + idx + "\"></li>");
            }
            $(this).attr('data-tab', idx);
        });
        $("#carousel-sidebar").keydown(function (event) {
            var $li = $switcher.children("li.active");
            var $tab = $list.children("div.item.active");
            var totalLi = $switcher.children("li").length - 1;
            var indexStart = $li.data("indexStart");
            switch (event.which) {
                case 37: //Arrow left
                    if (indexStart == 0) {
                        indexStart = totalLi;
                    }
                    else {
                        indexStart -= 1;
                    }
                    break;
                case 39: //Arrow Right
                    if (totalLi == indexStart) {
                        indexStart = 0;
                    }
                    else {
                        indexStart += 1;
                    }

                    break;
            }
            // hide visible items       
            var $tabItem = $("*[data-tab=" + indexStart + "]");
            $list.children("div.item").removeClass("active");
            $switcher.children("li").removeClass("active");
            $tab.fadeOut("fast", function () {
                // hidden done, show selected item
                $tabItem.fadeIn('slow');
                return false;
            });
            
            // set active class
            $tabItem.addClass("active");
            $("li[data-index-start='" + indexStart + "']").addClass("active");
        });
        $switcher.children("li").on("click", function (e) {
            var $li = $(this);
            var indexStart = $li.data("indexStart");

            // hide visible items
            $list.children("div.item:visible").fadeOut("fast", function () {
                // hidden done, show selected item
                $.each($list.children("div.item"), function (idx, item) {
                    if (idx == indexStart) {
                        $(item).fadeIn('slow');
                        return false;
                    }
                });
            });

            // set active class
            $switcher.children("li").removeClass("active");
            $li.addClass("active");
        });
    });
         </script>
