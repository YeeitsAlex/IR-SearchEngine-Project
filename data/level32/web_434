              <script type="text/javascript">
               try
    {
       if (nr4interval_token != null) {
        clearInterval(nr4interval_token);
      }
    } catch (err) { }
    var nr4interval_token = setInterval("nr4RunBlendImageDiv()", 6000 );
    var nr4countDiv = 0;
    var nr4countDivMax = 20;
    var nr4fadeInSequence = new Array(nr4countDivMax);
    var nr4fadeOutSequence = new Array(nr4countDivMax);
    var nr4fadeTime = 1200;
    var nr4fadeEvents = 9;

    function nr4Opacity(nr4countDiv, opacityStart, opacityEnd, millisec, transitionevents) {
        //speed for each frame, transition events is usually 2 - 100
      try {
        var fadeIn = true;
        if (opacityStart > opacityEnd) {
            fadeIn = false;
        }
        if (fadeIn) {
            nr4fadeInSequence[nr4countDiv] = 0;
        } else {
            nr4fadeOutSequence[nr4countDiv] = 0;
        }

        var speed = Math.round(millisec / transitionevents);
        var opacityValue = 0;
        var adjustedi = 0;
        var id = "nr4div" + nr4countDiv;

        for (var i = 1; i <= transitionevents; i++) {
            if (opacityStart < opacityEnd) {
                adjustedi = i;
            } else {
                adjustedi = transitionevents - i;
            }
            opacityValue = Math.round(100 * adjustedi / transitionevents);
            if (opacityValue > 99) { opacityValue = 100; }
            if (opacityValue < 1) { opacityValue = 0; }
            setTimeout("nr4ChangeOpacity(" + opacityValue + ", '" + id + "', " + nr4countDiv + ", " + i + ", " + fadeIn + ")", (i * speed));
        }
      } catch (err) { }
    }

    //change the opacity for different browsers
    function nr4ChangeOpacity(opacity, id, nr4countDiv, eventSequenceNumber, fadeIn)
    {
      try
      {
        var nr4continueIt = true;
        if (fadeIn) {
          if (nr4fadeInSequence[nr4countDiv] > eventSequenceNumber) {
            nr4continueIt = false;
          } else {
            nr4fadeInSequence[nr4countDiv] = eventSequenceNumber;
          }
        } else {
          if (nr4fadeOutSequence[nr4countDiv] > eventSequenceNumber) {
            nr4continueIt = false;
          } else {
            nr4fadeOutSequence[nr4countDiv] = eventSequenceNumber;
          }
        }
        if (nr4continueIt) {
          var object = document.getElementById(id).style;
          object.opacity = (opacity / 100);
          object.MozOpacity = (opacity / 100);
          object.KhtmlOpacity = (opacity / 100);
          object.filter = "alpha(opacity=" + opacity + ")";
        }
      } catch (err) { }
    }

    function nr4BlendImageDiv(nr4countDivFadeOut, nr4countDivFadeIn, millisec, transitionevents)
    {
        //Transitions between two divs.  Requires the <DIV>s be on top of each other.
        nr4fadeInSequence[nr4countDivFadeIn] = 0;
        nr4ChangeOpacity(0, "nr4div" + nr4countDivFadeIn, nr4countDivFadeIn, 0, true);
        nr4Opacity(nr4countDivFadeOut, 100, 0, millisec, transitionevents);
        nr4Opacity(nr4countDivFadeIn, 0, 100, millisec, transitionevents);
    }

    function nr4RunBlendImageDiv()
    {
        try
        {
            if (nr4countDivMax > 0)
            {
                var nr4countDivFadeOut = nr4countDiv;

                nr4countDiv++;
                if (nr4countDiv >= nr4countDivMax)
                {
                    nr4countDiv = 0;
                }
                var nr4countDivFadeIn = nr4countDiv;

                nr4BlendImageDiv(nr4countDivFadeOut, nr4countDivFadeIn, nr4fadeTime, nr4fadeEvents);
            }
        }
        catch (err) { }
    }
              </script>
             </div>
            </div>
