         <article about="/home" class="cu-landing-page full clearfix" role="article">
          <h1 class="sr-only">
           <div class="container">
            Home
           </div>
          </h1>
          <div class="content">
           <div class="field field--name-field-cu-landing-page-content field--type-entity-reference-revisions field--label-hidden landing-content">
            <div class="paragraph paragraph--type--cu-paragraph-hero paragraph--view-mode--default with-image">
             <div class="banner-image-and-text">
              <div class="banner-image">
               <div class="stellar-relative">
                <div class="paragraph paragraph--type--single-image paragraph--view-mode--default anchored" id="single_image-153">
                 <figure class="field field--name-field-cu-image field--type-image field--label-hidden image-with-borders image-wrapper cap-bot image-with-modal" id="this-is-a-hero-image-153">
                  <img alt="This is a hero image" class="img-responsive" height="300" src="/sites/default/files/styles/cu_crop/public/content/pics/HERO4.jpg?itok=cndU3hP3" typeof="foaf:Image" width="1400"/>
                 </figure>
                </div>
