   <script type="text/javascript">
    $(document).ready(function()
	{
		$('#searchform .field').live('focusin', function()
		{
			if( $(this).attr('value') == 'Search our site' )
			{
				$(this).attr('value', '');	
				$(this).addClass('searchFormSelected');
			}
		});

		$('#searchform .field').live('focusout', function()
		{
			if( $(this).attr('value') == '' )
			{
				$(this).attr('value', 'Search our site');	
				$(this).removeClass('searchFormSelected');
			}
		});
			
		if( $('#replaceMainHeading').length > 0 )
		{
			$('#titleBox .container h1').remove();
			$('#titleBox .container').append($('#replaceMainHeading'));
		}
		
		$('#playFeaturedVideo').on('click', function() {
			$('#featuredVideo, .closeFeaturedVideo').fadeIn();
			$('#featuredVideo').attr('src', $('#featuredVideo').data('source'));
		});
		
		$('.closeFeaturedVideo').on('click', function() {
			$('#featuredVideo, .closeFeaturedVideo').fadeOut();
			$('#featuredVideo').attr('src', '');
		});
		
		$('.toggleNav').on('click', function() {
			if ($('.slideMenuContainer').hasClass('on')) {
				$('.slideMenuContainer').removeClass('on');
			} else {
				$('.slideMenuContainer').addClass('on');
			}
		});
		
		$('.categoriesList li, .postBlockCategories ul li').hover(
		  function() {
			var classList = $(this).attr('class').split(/\s+/);
			$.each(classList, function(index, item) {
				if (item.indexOf("category_slug") != -1) {
					$('.postBlockStyle2').addClass('off');
					$('.'+item).addClass('on');
					$('.'+item).closest('.postBlockStyle2').removeClass('off');
				}
			});
		  }, function() {
			$('.categoriesList li, .postBlockCategories ul li').removeClass('on');
			$('.postBlockStyle2').removeClass('off');
		  }
		);
	});
   </script>
