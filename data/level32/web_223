function check_ie() {
	return !!(navigator.appName == "Microsoft Internet Explorer" && navigator.appVersion >= "4.0");
}
function clearall() {
	if (check_ie()) {
		document.getElementById("HumSub").style.display = "none";
		document.getElementById("AniCare").style.display = "none";
		document.getElementById("HIPAA").style.display = "none";
