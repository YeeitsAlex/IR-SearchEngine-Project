		document.getElementById("Helptab").className = "";
	}
}
function toggle(x) {
	if (check_ie()) {
		if (document.getElementById(x).style.display != "block") {
			clearall();
			document.getElementById(x).style.display = "block";
			document.getElementById("" + x + "tab").className = "on";
		}
	}
}
