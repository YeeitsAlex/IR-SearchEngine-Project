       <script type="application/ld+json">
        {"@context":"https://schema.org","@type":"WebSite","@id":"https://lamarcc.edu/#website","url":"https://lamarcc.edu/","name":"Lamar Community College","potentialAction":{"@type":"SearchAction","target":"https://lamarcc.edu/?s={search_term_string}","query-input":"required name=search_term_string"}}
