      <script>
       function fixJustified() {
	if($(window).width() < 1057) {
		$('#navbar-ul-justified').removeClass('nav-justified');
		$('#navbar-ul-justified').addClass('navbar-nav');

		$('.menu-second-dropdown').each(function() {
			$(this).addClass('menu-second-dropdown-css')
		});
	}
	else {
		$('#navbar-ul-justified').addClass('nav-justified');
		$('#navbar-ul-justified').removeClass('navbar-nav');
		
		$('.menu-second-dropdown').each(function() {
			$(this).removeClass('menu-second-dropdown-css')
		});
	}
}


$(function() {
	fixJustified();
	$(window).on('resize', function () {
		fixJustified();
	});
});



