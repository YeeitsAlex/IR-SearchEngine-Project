  <script type="text/javascript">
   var set_logo = function($) {
	var logo_container = $("#logo");
	var logo = $("#logo img");        
	if ($(window).width() < 1200) {
		logo.attr('src', '/sites/default/files/mobile-logo-retina.png');
		logo.width(146);
		logo.height(55);
		logo_container.addClass('compact');
	} else {
		logo.attr('src', '/sites/default/files/ucla-astronomy-logo-retina.png');
		logo.width(497);
		logo.height(47);
		logo_container.removeClass('compact');
	}
}
      jQuery(document).ready(function($){
        set_logo($); 
        $(window).resize(function() {
          set_logo($);
        });
      });
  </script>
