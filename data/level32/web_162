  <script>
   $(document).ready(function () {

            $(window).scroll(function () {
                if ($(this).scrollTop() > 100) {
                    $('.scrollToTop').fadeIn();
                } else {
                    $('.scrollToTop').fadeOut();
                }
            });

            //Click event to scroll to top
            $('.scrollToTop').click(function () {
                $('html, body').animate({ scrollTop: 0 }, 800);
                return false;
            });

            var path = location.pathname.split('/');
            if (path.length > 1) {
                var pathString = path[1];
                if (pathString.indexOf(".aspx") === -1) {
                    pathString += ".aspx";
                }
                $("[href='/" + pathString + "']").parent().addClass("active");
            }

            $(".hero-content-container").each(function () {
                var hero = $(this);
                var heroChild = $(this).children(".hero-content");
                if (hero.innerHeight() > 336 || heroChild.innerHeight() > 336) {
                    hero.removeClass("v-center-short-content");
                    heroChild.removeClass("v-center-short-content");
                }
            });
        });
  </script>
