    <script>
     $(document).ready(function () {
        // Menu functions
        $(".dropdown").hover(function () {
            var submenu = $(this).children(".dropdown-menu");
            $(submenu).show();
        });

        $(".dropdown").mouseleave(function () {
            var submenu = $(this).children(".dropdown-menu");
            $(submenu).hide();
        });

        //Starts Search functionality
        $("#search-btn").click(function () {
            var term = $(".banner-search-form input[type='search']").val();
            window.location.href = encodeURI('/search?q=' + term);
        });

        $("#desktop-search").keydown(function (event) {
            if (event.keyCode == 13) {
                event.preventDefault();
                $("#search-btn").click();
            }
        });

        // Activates sticky menu functionality
        //$(window).stuck();
    });
    </script>
