                      <!-- Illinois Tech University News -->
                      <style type="text/css">
                       #university-news-feed-container a {
    text-decoration: none;
    padding: 15px 0;
    display: block;
}

#university-news-feed-container a:hover {
    text-decoration: none;
    background-color: #f8f8f8;
}

#university-news-feed-container .university-news-entry-container {
    border-top: 1px dotted #444;
}

#university-news-feed-container .university-news-entry-container:first-child {
    border-top: none;
}

#university-news-feed-container .university-news-entry-contents h2 {
    color: #333;
    font-size: 19px;
    text-transform: none;
    letter-spacing:normal;
    font-family: 'oswald',sans-serif;
    font-weight: 400;
    border: 0;
    margin:0;
    line-height:1.25em;
}

#university-news-feed-container .university-news-entry-contents h3 {
    color: #111;
    font-size: 16px;
    text-transform: none;
    font-family: 'Open Sans Condensed',sans-serif;
    letter-spacing:normal;
    font-weight: 400;
    border: 0;
    margin:0;
    line-height:1.25em;
}
                      </style>
                      <div id="university-news-feed-container">
                      </div>
                      <div class="more-link">
                       <a href="https://news.iit.edu/" target="_blank">
                        View all ...
                       </a>
                      </div>
                      <script type="text/javascript">
                       (function(){
    var url = 'https://news.iit.edu/jsonapi/node/story?sort=-created&page[limit]=5';
    var monthArray = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];
    var container = document.getElementById('university-news-feed-container');

    function handleData(e) {
        var json = JSON.parse(this.responseText);
        console.log(json.data);

        for (var i = 0; i < json.data.length; i++) {
            var entry = json.data[i];

            var entryDiv = document.createElement('div');
            entryDiv.className = 'university-news-entry-container';

            var entryLink = document.createElement('a');
            entryLink.href = 'https://news.iit.edu' + entry.attributes.path.alias;
            entryLink.target = "_blank";
            entryDiv.appendChild(entryLink);

            var entryContents = document.createElement('div');
            entryContents.className = 'university-news-entry-contents';
            entryLink.appendChild(entryContents);

            var entryTitle = document.createElement('h2');
            entryTitle.innerHTML = entry.attributes.field_headline.processed;
            entryContents.appendChild(entryTitle);

            if (entry.attributes.field_subhead) {
                var entrySubHead = document.createElement('h3');
                entrySubHead.innerHTML = entry.attributes.field_subhead.processed;
                entryContents.appendChild(entrySubHead);
            }

            container.appendChild(entryDiv);
        }
    }

    var xhr = new XMLHttpRequest();
    xhr.onload = handleData;
    xhr.open('GET', url);
    xhr.setRequestHeader('Accept', 'application/vnd.api+json');
    xhr.send();
})();
                      </script>
                      <!-- end of Illinois Tech University News -->
                     </div>
                    </div>
                   </div>
