     <script type="text/javascript">
      google.load('search', '1');
        function inputFocus() {
        	document.getElementById('query-input').style['background'] = 'white';
        }
        function inputBlur() {
        	var queryInput = document.getElementById('query-input');
					if (!queryInput.value) {
						queryInput.style['background'] = 'white';
					// 'white url(http://www.google.com/coop/images/';
					// mdv8 20120321 removed the watermark + 'google_custom_search_watermark.gif) no-repeat 0% 50%';
					}
        }
        function init() {
					google.search.CustomSearchControl.attachAutoCompletion(
					'005440058277293590618:5_evqn2hsam',
					document.getElementById('query-input'),
					'two-page-form');
					inputBlur();
        }
        function submitQuery() {
					window.location = 'http://dea.human.cornell.edu/search/node/'
					+ encodeURIComponent(
					document.getElementById('query-input').value);
					return false;
        }
        google.setOnLoadCallback(init);
     </script>
     <div class="input-group pull-right" id="search-form" style="margin-top: 25px;">
