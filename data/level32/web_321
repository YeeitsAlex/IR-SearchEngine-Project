 (function($) {
    $(function() {
        $(".panel-link").css({cursor: "pointer"});
        $(".panel-link").click(function(event) {
            var link = $(this).find(".link-destination").attr("href");
            if (link) { location.href = link; }
        });
    });
})(jQuery);
</script>
