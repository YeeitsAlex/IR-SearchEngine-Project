      <style>
       .nav-search-btn {
visibility: hidden;
}
      </style>
      <script>
       $(document).ready(function() {
//scripts for main page only. move to inpoage-index later
$(document).scroll(function() {
  var y = $(this).scrollTop();
  if (y > 200) {
       $('#bodysearch').css({'display' : 'none'});
       $('.nav-search-btn').css({'visibility' : 'visible'});
  } else {
    $('#bodysearch').css({'display' : 'block'});
    $('.nav-search-btn').css({'visibility' : 'hidden'});
    $('#headersearch').css({'display' : 'none'});
  }
});
});
      </script>
