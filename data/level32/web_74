       <script type="text/javascript">
        // <![CDATA[
            (
            			
            			function() 
            			{ 
            			   
            				var f = document.getElementById('frame2'); 
            		
            				if (f && f.q) { 
            					var q = f.q; 
            					var n = navigator; 
            					var l = location; 
            					if (n.platform == 'Win32') { 
            						q.style.cssText = ''; 
            					}
            				
            					var f = function() { 
            		
            						if (q.value == 'Search CSU') { 
            							q.style.background = '#FFFFFF';					
            							q.value='';
            						}
									
								var b = function() { 
            						if ( q.value == '') { 
            							q.value = 'Search CSU';
            							q.style.background = '#FFFFFF';	
            							q.style.height='25px';
            							q.style.width='140px';
            							q.style.color="#000000";
            							q.style.display="inline";
										q.style.font-size='16px'; 
										q.style.font-family="Helvetica,Arial";
            
            							
            						} 
            						else
            						 {
            							q.style.background = '#FFFFFF';	
            							 q.style.height='25px';
            							 q.style.display="inline";
            						 }
            					};
            						
            					}; 
            					
            					q.onblur = b; 
            					q.onfocus = f;
            					
            					if (!/[&?]q=[^&]/.test(l.search)) { 
            						b(); 
            					} 
            				} 
            			} 
            		) ();
            // ]]>
       </script>
