  <script type="text/javascript">
   var ou_dir = "/";
			var ou_filename = "index.htm";
			


$(document).ready(function () {
    $('.side-link a').each(function(){
		var href = $(this).attr('href');
			if (href == ou_dir + ou_filename || href ==  ou_dir) {
        $(this).addClass('selected');
        e.preventDefault();
	}
    });
});
  </script>
