   $(document).ready(function() {

			if (0) {
				document.getElementById('special').style.display = "block";
			} else {
				$("#util-nav").bind("dblclick", function(){
				$("#special").fadeIn("slow", $("#util-nav").unbind());
				});
			}
	
			$("#special_close").bind("click", function(){
			$("#special").fadeOut("fast",$("#logo").bind("dblclick",
				function(){
				$("#special").fadeIn("slow", $("#logo").unbind());
				})
				);
			});

		});
