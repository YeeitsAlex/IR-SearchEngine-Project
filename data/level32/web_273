  <script>
   $(function () {
			var isMobile = navigator.userAgent.match(/Android|webOS|iPhone|BlackBerry|IEMobile|Opera Mini|Opera Mobile| Windows Phone/);
			if (isMobile) {
			
			if ($("#mission, #infographic, #legacybutton, #giftsbutton, #successesbutton, #eventsbutton, #futurestudentsbutton, #currentstudentsbutton, #alumniandfriendsbutton").hasClass("in")) {
			$("#mission, #infographic, #legacybutton, #giftsbutton, #successesbutton, #eventsbutton, #futurestudentsbutton, #currentstudentsbutton, #alumniandfriendsbutton").addClass("out");
			$("#mission, #infographic, #legacybutton, #giftsbutton, #successesbutton, #eventsbutton, #futurestudentsbutton, #currentstudentsbutton, #alumniandfriendsbutton").removeClass("in");
			}
			}
			});
  </script>
