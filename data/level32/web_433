      <script>
       var MainNav = $('.menu-main-navigation-container li'),
    WRAP_BY = 3;
for (var i = 0; i < MainNav.length; i += WRAP_BY) {
    //first loop, elements 0 : 15, next loop elements 15:30 and so on
    MainNav.slice(i, i + WRAP_BY).wrapAll('<ul/>');
}
