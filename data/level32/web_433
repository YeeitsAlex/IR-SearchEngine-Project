<script>
 $(document).ready(function(){
$(".footnav a").removeClass();
});
$(document).ready(function(){
$(".footnav a:nth-child(6)").addClass('right');
});
$(document).ready(function(){
$(".footnav a:nth-child(12)").addClass('right');
});

//MAIN NAVIGATION
var allElements = $('.project-container .project'),
    WRAP_BY = 4;
for (var i = 0; i < allElements.length; i += WRAP_BY) {
    //first loop, elements 0 : 15, next loop elements 15:30 and so on
    allElements.slice(i, i + WRAP_BY).wrapAll('<div class="project-row" />');
}
var ProjectStaff = $('.proj-staff li'),
    WRAP_BY = 5;
for (var i = 0; i < ProjectStaff.length; i += WRAP_BY) {
    //first loop, elements 0 : 15, next loop elements 15:30 and so on
    ProjectStaff.slice(i, i + WRAP_BY).wrapAll('<ul/>');
}
$(document).ready(function(){
$(".proj-staff:first ul:first").addClass('proj-long');
});
$(document).ready(function(){
$(".proj-staff ul:nth-child(3)").addClass('proj-long-2');
});
$(document).ready(function(){
$("#gform_wrapper_2 .gform_body #gform_fields_2 #field_2_1 #input_2_1_6_container").before('<br><br><label>Your Last Name<span class="gfield_required">*</span></label><br>');
});
$(document).ready(function(){
$("#gform_wrapper_2 .gform_body #gform_fields_2 #field_2_2").before('<br>');
});
$(document).ready(function(){
$("#gform_wrapper_2 .gform_body #gform_fields_2 #field_2_2").after('<br>');
});
$(document).ready(function(){
$("#gform_wrapper_2 input_2_1_6_container").before('Enter Your Last Name');
});
$(document).ready(function(){
$("#gform_wrapper_2 input").addClass('medium');
});
$(document).ready(function(){
$("#gform_wrapper_2 #gform_submit_button_2").removeClass('medium');
});
$(document).ready(function(){
	var first_name = 'Enter Your First Name';
$('.ginput_container #input_2_1_3_container input').attr('value', first_name).focus(function(){
	if ($(this).val() == first_name) {
	$(this).attr('value', '');
		}
	}).blur(function(){
		if ($(this).val() == '') {
		$(this).attr('value', first_name);
		}
	});
});
$(document).ready(function(){
$('.ginput_container #input_2_1_3_container input').addClass('namevalue');
});
$(document).ready(function(){
	var last_name = 'Enter Your Last Name';
$('.ginput_container #input_2_1_6_container input').attr('value', last_name).focus(function(){
	if ($(this).val() == last_name) {
	$(this).attr('value', '');
		}
	}).blur(function(){
		if ($(this).val() == '') {
		$(this).attr('value', last_name);
		}
	});
});
$(function(){
	var reversedSet = $(".caption-alt").get().reverse();
	$(reversedSet).each(function(){
		$(".wp-caption:last").addClass('caption-alt');
	});
});
$(document).ready(function(){ 
$("div.caption-alt").removeAttr("style");
});
$(document).ready(function(){
$('.ginput_container #input_2_1_6_container input').addClass('namevalue');
});
