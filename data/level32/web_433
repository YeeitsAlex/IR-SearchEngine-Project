   jQuery(document).ready(function() {
var $ = jQuery;
$(".project-row .project:nth-child(4)").addClass("project-last");

$("#events-id").addClass("current-cat");

$(".year-list").each(function() {
$(this).click(function() {
var Year = $(this).attr("id");
var theQuery = "action=do_wp_query&year="+Year;

$.ajax({
type: "POST",
url: 'http://www.bctr.cornell.edu/wp-admin/admin-ajax.php',
data: theQuery,
success: function( response ) {
$("article").html(response);
},
error: function(why, why2, why3 ) {
$("article").html("<p>"+why +" | "+why2+" | "+why3+"</p>");
}
});

});
});

});
  </script>
