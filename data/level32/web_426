function addEngine(name)
{
  // Opensearch plugin for IE 7 and Firefox 2.x
  if ((typeof window.external == "object") &&
      (typeof window.external.AddSearchProvider == "function"))
  {

window.external.AddSearchProvider("http://ads.harvard.edu/plugins/"+name+".xml");
  }
  // Sherlock plugin for Firefox 1.x
