  <script type="text/javascript">
   function checkRemapOnComplete(result, userData) {
            if(result != '') window.location.href = result;
        }

        function checkRemapOnError(result, userData) {
            alert('Error checking remap:\n' + result.get_message());
        }

        function checkRemap() {
            PageMethods.set_path('/index.aspx')
            PageMethods.GetRedirectTo(window.location.href, checkRemapOnComplete, checkRemapOnError);
        }

        function showRss() {
            var amOnChrome = false; 

            var isChromium = window.chrome;
            var winNav = window.navigator;
            var vendorName = winNav.vendor;
            var isOpera = typeof window.opr !== "undefined";
            var isIEedge = winNav.userAgent.indexOf("Edge") > -1;
            var isIOSChrome = winNav.userAgent.match("CriOS");

            if (isIOSChrome) {
                amOnChrome = true;
            } else if(
                isChromium !== null &&
                typeof isChromium !== "undefined" &&
                vendorName === "Google Inc." &&
                isOpera === false &&
                isIEedge === false
            ) {
                amOnChrome = true;
            } else { 
                amOnChrome = false;
            }
            
            var onChrome = $('div#rssfeeds span.onchrome');
            amOnChrome?(onChrome.show()):(onChrome.hide());
            $('div#rssfeeds').dialog({ modal: true, title: 'Our RSS Feeds', width: 400, height: (amOnChrome?240:220), resizable: false });
        }

        function setupInputs() {
            $("input[type=text]").focus(function () { $(this).select(); });
        }

        function closeDialogs() {
            $(".ui-dialog-content").dialog("close");
        }
  </script>
  <script type="text/javascript">
   var lastShownButton;
        function pageBanner(who) {
            var btn = $(who);
            var idx = btn.attr('index');
            setPageButton(idx);

            var oRotator = $find("rotator");
            oRotator.set_currentItemIndex(idx);

        }

        function OnClientItemShown(oRotator, args) {
            var idx = args.get_item().get_index();
            setPageButton(idx);
        }

        function setPageButton(idx) {
            $('.pages.ui-state-highlight').removeClass('ui-state-highlight');
            $('.pages[index='+new String(idx)+']').addClass('ui-state-highlight');
        }
  </script>
