        <script type="text/javascript">
         $(document).ready(function () {
        $('span.search').click(function () {
            var search_input = $(".banner-search-form input[type='search']");
            /*to make this flexible, I'm storing the current width in an attribute*/
            $(search_input).attr('data-default', $(this).width());
            $(search_input).animate({ width: 300 }, 'slow');
            $(search_input).focus();
        });

        $("#search").keyup(function (e) {
            if (e.keyCode == 13) {
                $(this).trigger("enterKey");
            }
        });

        $(".banner-search-form input[type='search']").blur(function () {
            /* lookup the original width */
            var w = $(this).attr('data-default');
            $(this).animate({ width: w }, 'slow');
        });

        $(".banner-search-form input[type='search']").bind("enterKey", function (e) {
            var term = $(this).val();
            window.location.href = encodeURI('/search?q=' + term);
        });

        $(window).keydown(function (e) {
            if (e.keyCode == 13) {
                e.preventDefault();
                return false;
            }
        });
    });
        </script>
        <div class="banner-search-form pull-right hidden-xs hidden-sm">
