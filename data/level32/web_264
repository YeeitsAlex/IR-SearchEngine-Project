   <script type="text/javascript">
    function validateEmail(email) {
        var re = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
        return re.test(email);
    }

    function validatePhone(value) {
        return value.match(/\d/g).length === 10;
    }

    function validate() {
        var isValid = true;
        var nameField = $("input[id*='field_38702258C5B24AB3BCB233DC4C7488A1']");
        var titleField = $("input[id*='field_3D3994BF833A47CB8AA499D22BBD3F06']");
        var organizationField = $("input[id*='field_02EB6882F86A4FE7B53967413BE2B9DE']");
        var emailField = $("input[id*='field_A5C9814D3D68404392EF82661B6E58CC']");
        var phoneField = $("input[id*='field_CAFC72F905F14B7AB853E878BC915FF6']");

        if (nameField.val().length == 0 || nameField.val().length > 256) {
            nameField.parent().addClass("has-error");
            $("span[id*='field_38702258C5B24AB3BCB233DC4C7488A16ADFFAE3DADB451AB530D89A2FD0307B_validator']").show();
            isValid = false;
        }
        else {
            $("span[id*='field_38702258C5B24AB3BCB233DC4C7488A16ADFFAE3DADB451AB530D89A2FD0307B_validator']").parent().removeClass("has-error");
            $("span[id*='field_38702258C5B24AB3BCB233DC4C7488A16ADFFAE3DADB451AB530D89A2FD0307B_validator']").hide();
        }

        if (titleField.val().length == 0 || titleField.val().length > 256) {
            titleField.parent().addClass("has-error");
            $("span[id*='field_3D3994BF833A47CB8AA499D22BBD3F066ADFFAE3DADB451AB530D89A2FD0307B_validator']").show();
            isValid = false;
        }
        else {
            $("span[id*='field_3D3994BF833A47CB8AA499D22BBD3F066ADFFAE3DADB451AB530D89A2FD0307B_validator']").parent().removeClass("has-error");
            $("span[id*='field_3D3994BF833A47CB8AA499D22BBD3F066ADFFAE3DADB451AB530D89A2FD0307B_validator']").hide();
        }

        if (organizationField.val().length == 0 || organizationField.val().length > 256) {
            organizationField.parent().addClass("has-error");
            $("span[id*='field_02EB6882F86A4FE7B53967413BE2B9DE6ADFFAE3DADB451AB530D89A2FD0307B_validator']").show();
            isValid = false;
        }
        else {
            $("span[id*='field_02EB6882F86A4FE7B53967413BE2B9DE6ADFFAE3DADB451AB530D89A2FD0307B_validator']").parent().removeClass("has-error");
            $("span[id*='field_02EB6882F86A4FE7B53967413BE2B9DE6ADFFAE3DADB451AB530D89A2FD0307B_validator']").hide();
        }

        if (emailField.val().length == 0 || !validateEmail(emailField.val())) {
            emailField.parent().addClass("has-error");
            $("span[id*='field_A5C9814D3D68404392EF82661B6E58CC5D10AF7533054C39908EB25E8CB4ABDC_validator']").show();
            isValid = false;
        } else {
            $("span[id*='field_A5C9814D3D68404392EF82661B6E58CC5D10AF7533054C39908EB25E8CB4ABDC_validator']").parent().removeClass("has-error");
            $("span[id*='field_A5C9814D3D68404392EF82661B6E58CC5D10AF7533054C39908EB25E8CB4ABDC_validator']").hide();
        }

        if (phoneField.val().length == 0 || !validatePhone(phoneField.val())) {
            phoneField.parent().addClass("has-error");
            $("span[id*='field_CAFC72F905F14B7AB853E878BC915FF6C3CC8A327EF14D138163826E11482E4D_validator']").show();
            isValid = false;
        } else {
            $("span[id*='field_CAFC72F905F14B7AB853E878BC915FF6C3CC8A327EF14D138163826E11482E4D_validator']").parent().removeClass("has-error");
            $("span[id*='field_CAFC72F905F14B7AB853E878BC915FF6C3CC8A327EF14D138163826E11482E4D_validator']").hide();
        }

        return isValid;
    }

    $(document).ready(function () {
        //Remove Default WebForm Style
        $("*[class^='sc']").removeAttr("class");

        $($("#openModal").find("label:contains('Brief description')")[0]).addClass('section-label');
        var coreCompSpan = $("#openModal").find("span:contains('Compentency')")[0];
        $(coreCompSpan).css("display", "block");
        $(coreCompSpan).addClass('section-label');

        if (location.hash.indexOf("openModal") !== -1) {
            $("#openModal").show();
        }

        $("input[type='text']").each(function () {
            $(this).parent().addClass("form-group");
            $(this).addClass("form-control");
        });

        $(".partner a").click(function () {
            $("#openModal").show();
        });

        $("a.closeModalDialog").click(function () {
            $("#openModal").hide();
        });
    });
   </script>
