  <script>
   $(document).ready(function () {
            // Menu functions
            $(".dropdown").hover(function () {
                var submenu = $(this).children(".dropdown-menu");
                $(submenu).show();
            });

            $(".dropdown").mouseleave(function () {
                var submenu = $(this).children(".dropdown-menu");
                $(submenu).hide();
            });

            var parts = window.location.href.split('/');
            if (parts[3] == "") {
                $(".darkblue").addClass("darkblue2").removeClass("darkblue");
            }

            $("input[type='text']").each(function () {
                $(this).parent().addClass("form-group");
                $(this).addClass("form-control");
            });

            $("input[value='Submit']").attr("onclick", "if(validate()){ " + $("input[value='Submit']").attr("onclick") + " } else{ return false; }");

            var path = location.pathname.split('/');
            if (path.length > 1) {
                var pathString = path[1].toLowerCase();

                if (pathString !== "") {
                    if (pathString.indexOf(".aspx") === -1) {
                        pathString += ".aspx";
                    }

                    $(".navbar-nav a").each(function () {
                        var re = new RegExp(pathString, "i");
                        if ($(this).attr('href').match(re)) {
                            $($(this).parent()).addClass('current');
                        }
                    });
                }
            }
        });
  </script>
