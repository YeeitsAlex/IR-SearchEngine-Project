   <script>
    $(document).ready(function(){
            var scrolled=0;
            var div_height = $("#touchScroller").height();
            var div_height_negative = (div_height - (div_height * 2)) + (60*2);
            $("#downClick").on("click" ,function(){
                //alert(scrolled+' ' + div_height + ' ' +div_height_negative);
                scrolled=scrolled-60;
                /*$("#touchScroller").css({top : scrolled+"px"});*/
                $("#touchScroller").animate({top : scrolled+"px"})
                if(scrolled == 0){
                    $("#upClick").fadeOut();
                }else{
                    $("#upClick").fadeIn();
                }
                if(scrolled <= div_height_negative){//reset
                    //scrolled=60;
                    //$("#touchScroller").css({top : scrolled+"px"});
                    $("#downClick").fadeOut();
                }else{
                    $("#downClick").fadeIn();
                }
            });
            $("#upClick").on("click" ,function(){
                scrolled=scrolled+60;
                $("#touchScroller").animate({top : scrolled+"px"});
                if(scrolled == 0){
                    $("#upClick").fadeOut();
                }else{
                    $("#upClick").fadeIn();
                }
                if(scrolled <= div_height_negative){//reset
                    //scrolled=60;
                    //$("#touchScroller").css({top : scrolled+"px"});
                    $("#downClick").fadeOut();
                }else{
                    $("#downClick").fadeIn();
                }
                /*$("#touchScroller").animate({
                 scrollTop:  scrolled
                 });*/
            });

        });
   </script>
