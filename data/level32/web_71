        <script type="text/javascript">
         $(document).ready(function(){
					$.ajax({
						url: "/_resources/wnl/spif-events-home-ajax.ashx",
						data: {
								//41361
								// Cristina: I have commented this link out because the issues we have had with the bedework calendar
								// This feed has been added programatically and is this file: /_resources/events/generate-events-xml.xml
								feed: encodeURI('/_resources/events/generate-events-xml.xml'),
								count: encodeURI('5'),
								link: encodeURI('http://oucalendar.cod.edu/cal')
						},
						success: function(data, status){

							if(status == "success"){
								$('#response').replaceWith(data).fadeIn(100);
								$('.slider').anythingSlider({ width: "600"});
							}
						}
					});
				});
