    <div id="bannertext">
     <h1 class="page-title">
      New Mexico State University
     </h1>
     <h2 id="edition-hero">
      2018-2019 Edition
     </h2>
    </div>
   </div>
   <div aria-label="Breadcrumb navigation" id="breadcrumb" role="navigation">
   </div>
   <div id="left-col">
    <a id="sidebar-nav-mobile">
     2018-2019 Edition
    </a>
    <div id="sidebar">
     <div class="sidebar-title">
      2018-2019 Edition
     </div>
     <!-- Begin Regular Nav -->
     <nav aria-label="Catalog navigation" id="cl-menu" role="navigation">
