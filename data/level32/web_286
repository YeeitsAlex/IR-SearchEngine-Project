 <!-- jQuery to change text of single button -->
 <script>
  var phn = "label:contains('xxx')";
			var pcode = "label:contains('Passcode')";
			var clickybits = "input:checked[name='IDToken1']";
			var checkval = $(clickybits).attr("value");
			var buttons = $("input.button[value='Continue']");
			var notifybutt = "SEND MY NOTIFICATION NOW";
			var passbutt = "ENTER MY PASSCODE NOW";
			var allradios = $("div.radios [name='IDToken1']")

			$(document).ready(function(){
				//set a default radio state on load
     			     	$(buttons).attr("value", notifybutt);
     			     	//how many radios are there?
     			     	if ($(allradios).length > 1)
     			     	{
					//there's more than one radio button
         				console.log("there\'s more than one radio");
             				//which radio is currently selected
             				if ($(clickybits).next(pcode).length > 0) {
                 			//if the checked button is next to pcode set the correct button text
                 			console.log("the page loaded with pcode selected");
                			$(buttons).attr("value", notifybutt);
             		     		};
         		     	}else{
					      //there is only one button, in which case it is a hardware token
         				      //except when someone only sets one phone
					      console.log("there\'s only one radio");
         				      if ( $(clickybits).next(phn).length > 0) {
         				      console.log("yo it\'s a phone");
         				      $(buttons).attr("value", notifybutt);
         				      }else{
         				      console.log("yo it\'s not a phone");
         				      $(buttons).attr("value", passbutt);
         				      }
         				      //set the correct text
         				      //$(buttons).attr("value", passbutt);
         			};
    		       });
    		       //change the button text when clicked, and only if a radio is clicked
    		       $("input[name='IDToken1'][type='radio']").click(function(){
				console.log("CLICKED!" + $(this).attr("value"));
    		       	    	//if the radio clicked is adjacent to a label
    		       	    	//does selected button have passcode in the label?
    		       	    	if ($(clickybits).next(pcode).length > 0) {
    		       	    	//yes, then change the button to passcode version
    		       	    	console.log('yup');
    		       	    	$(buttons).attr("value", passbutt);
    		       }else{
				//no, so change it to send a message to a device version
    		       	    	console.log('nope');
    		       	    	$(buttons).attr("value", notifybutt);
		       	    };
		       })
 </script>
