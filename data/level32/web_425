  <script>
   function switchTab(target) {
        $('#tab-content > div').addClass('hideFlex');
        $('#tabs a').removeClass('active').attr('aria-selected', 'false');
        $(target.attr('href')).removeClass('hideFlex');
        target.addClass('active').attr('aria-selected', 'true');
    }

    // tab clicks
    $('#tabs a[aria-controls]').click(function (e) {
        switchTab($(this));
        e.preventDefault();
    });
    
    // select first tab when page loads
    if ($('#tabs a[aria-controls]').length) {
    	switchTab($('#tab1'));
    }
  </script>
  <script>
   function contentReorder(fourTwenty) {

			if (fourTwenty.matches) {
				 $('.grid').before($('#social'));
			}
			else {
				$('.tab').before($('#social'));
				}
		}

       $(document).ready(function(){
            if( $( '#home-slides' ).attr( 'data-randomize' ) === 'true' )
			$( '#home-slides' ).children( 'div' ).shuffle( );
			
			$( '#home-slides' ).slick( {
				centerMode: true,
				centerPadding: '0px',
				slidesToShow: 1,
				speed: 750,
				autoplay: JSON.parse( $( '#home-slides' ).attr( 'data-autoplay' ) ),
				autoplaySpeed: $( '#home-slides' ).attr( 'data-autoplaySpeed' ) * 1,
				responsive: [
					{
						breakpoint: 700,
						settings: {
							swipe: true,
							dots: true,
						}
					}
				]
			});
			
			    if (matchMedia) {
					var fourTwenty= window.matchMedia("(max-width: 420px)");
					fourTwenty.addListener(contentReorder);
					contentReorder(fourTwenty);
				}




        });

 (function($){ 
     
        $.fn.shuffle = function() { 
     
            var allElems = this.get(),
                getRandom = function(max) {
                    return Math.floor(Math.random() * max);
                },
                shuffled = $.map(allElems, function(){
                    var random = getRandom(allElems.length),
                        randEl = $(allElems[random]).clone(true)[0];
                    allElems.splice(random, 1);
                    return randEl;
               });
     
            this.each(function(i){
                $(this).replaceWith($(shuffled[i]));
            });
     
            return $(shuffled);
     
        };
		
     
    })(jQuery);
  </script>
