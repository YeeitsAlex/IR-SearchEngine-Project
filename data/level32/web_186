     <div class="sidebar-column col-md-3">
      <aside class="widget_text widget widget_custom_html" id="custom_html-2">
       <h3 class="widget-title">
        Search
       </h3>
       <div class="textwidget custom-html-widget">
        <label>
         <span class="screen-reader-text">
          Search for:
         </span>
         <input class="search-field" id="search-box" name="s" onkeypress="return searchEnterHandler(event);" placeholder="Search …" style="
    padding: 5px;
    height: 37px;
" type="search" value=""/>
        </label>
        <div style="
    display: inline-block;
">
         <a class="roll-button more-button" href="/search" onclick="redirectToSearch(); return false;" style="
    display: inline;
    padding: 9px;
    margin: 5px;
">
          Go
         </a>
        </div>
        <script>
         function searchEnterHandler(event) {
        if (event.which == 13 || event.keyCode == 13) {
            redirectToSearch();
            return false;
        }
        return true;
    };
    function redirectToSearch() {
        window.location.href = "/?s=" + escape(jQuery("#search-box").val())
    }
        </script>
       </div>
      </aside>
