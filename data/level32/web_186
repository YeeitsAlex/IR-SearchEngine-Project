   var isGPSA = false;
   
   try {
     //top.location is not allowed in some browsers when in different domain
     //we also want to allow iframes for WP customizer and Live Editor
     isGPSA = (top.location.host == "gpsa.asu.edu");
   }
   catch(e) {}

   //Check if inside a frame
   if (self != top && !isGPSA) {
       //First show empty page
       var elem = document.body;
       elem.parentNode.removeChild(elem);
       
