  <!-- Insert custom twitter if needed -->
  <script type="text/javascript">
   jQuery('.twitter-block').on('DOMSubtreeModified propertychange',"#twitter-widget-0", function() {

    hideTweetMedia();

    });

     var hideTweetMedia = function() {

			jQuery('.twitter-block').find('.twitter-timeline').contents().find('.timeline-Tweet-media').css('display', 'none');
			jQuery('.twitter-block').find('.twitter-timeline').contents().find('.timeline-Tweet-author').css('display', 'none');
			jQuery(".twitter-block").find(".twitter-timeline").contents().find(".timeline-Tweet-text").attr("style", "font-family: 'Josefin Slab' !important; font-size: 24px !important; line-height: 28px !important; letter-spacing:1px;");
			jQuery(".twitter-block").find(".twitter-timeline").contents().find(".timeline-Tweet-brand").attr("style", "font-size: 40px !important; line-height: 40px !important;");
			jQuery('.twitter-block').css('height', '100%');
    jQuery('.twitter-block').find('.twitter-timeline').contents().find('.timeline-TweetList').bind('DOMSubtreeModified propertychange', function() {

    hideTweetMedia(this);

    });

    }
  </script>
