  <script type="text/javascript">
   jQuery(document).ready(function(){
		jQuery.history.init(pageload);
	});
	function pageload(hash) {
		if(hash) {
			var a = hash;
            

				if (document.getElementById(a.substring(0,a.indexOf('='))) && a.substring(a.indexOf('=')+1) != '')
				{
					confirmMessage(this,'');
					//document.getElementById('main').style.display = 'none';
					//document.getElementById('main').innerHTML = '<div style="text-align:center; padding-top:30px"><span id="loaderActivity" class="loader"><img src="/images/loader-big.gif"/><br/>Loading Activity</span></div>';
					if (a.indexOf('?ax=1') > 0 || a.indexOf('&ax=1') > 0)
						{
						getContentOver(a.substring(0,a.indexOf('=')),a.substring(a.indexOf('=')+1) + '&ts=31015323');
					}
					else
					{
					if (a.indexOf('?') > 0)
						{
						getContentOver(a.substring(0,a.indexOf('=')),a.substring(a.indexOf('=')+1) + '&ax=1&ts=31015323');
						}
						else
						{
						getContentOver(a.substring(0,a.indexOf('=')),a.substring(a.indexOf('=')+1) + '?ax=1&ts=31015323');
						}
					}
				}
				
		} else {
			// start page
            
		}
	}
  </script>
