   $(document).ready(function(){
	  $("#skip-to-content > a").click(function(e) {
		e.preventDefault();
		document.getElementById("searchDesktop").focus();
	  });
	  $("#skip-to-content > a").keypress(function(e) {
		e.preventDefault();
		document.getElementById("searchDesktop").focus();
	  });
