              color: #b4d5ff;
            }
            #carousel-component .carousel-control.play-pause {
              left: 80px;
            }
            #carousel-component .carousel-control .icon-prev,
            #carousel-component .carousel-control .icon-next {
              position: initial;
            }
            #carousel-component .carousel-control {
              position: absolute;
              top: 84%;
              left: 30px;
              height: 30px;
              z-index: 1000;
              opacity: 1;
              width: 30px;
            }
            #carousel-component .carousel-indicators > li:hover,
            #carousel-component .carousel-indicators > li:focus {
              opacity: .5;
            }
            #carousel-component .carousel-indicators {
              bottom: 14px;
              text-align: right;
              width: 47%;
              margin-left: 0;
            }
            @media (max-width: 991px) {
              #carousel-component .carousel-control {
                top: 83%;
              }
              #carousel-component .carousel-indicators {
                bottom: 5px;
              }
            }
       </style>
       <div aria-label="image carousel" class="carousel slide" id="carousel-component">
        <div class="carousel-inner" role="listbox">
         <div class="item active">
          <a href="student-focused/teachers-who-care" target="_parent">
           <img alt='A student working one on one with her teacher with a caption "Ready for Teachers who know me."' class="img-responsive w-100 hidden-xs" src="images/home_page/Ready%20Round%207/7-Sewing_WebAds-Teachers%20HOME.jpg"/>
           <img alt='A student working one on one with her teacher with a caption "Ready for Teachers who know me."' class="img-responsive w-100 hidden-sm hidden-md hidden-lg" src="images/home_page/Ready%20Round%207/7-Sewing_WebAds-Teachers.jpg"/>
          </a>
         </div>
        </div>
       </div>
       <script>
        $("#carousel-component > a").click(function(e) {
              e.preventDefault();
            })
            $("#carousel-component > a").keypress(function(e) {
              e.preventDefault();
            })
            function nextSlide(carouselID) {
              $('#' + carouselID).carousel('next');
            }
            function prevSlide(carouselID) {
              $('#' + carouselID).carousel('prev');
            }
            function startCarousel(carouselID) {
              $('#' + carouselID).carousel('cycle');
              $('#' + carouselID + ' > a.play-pause > span.icon-play').toggleClass('icon-play icon-pause');
              $('#' + carouselID + ' > a.play-pause > span.sr-only').text('image carousel: Pause');
              $('#' + carouselID + ' > a.play-pause').attr('onclick', 'stopCarousel("' + carouselID + '");');
              $('#' + carouselID + ' > a.play-pause').attr('onkeypress', 'stopCarousel("' + carouselID + '");');
            }
            function stopCarousel(carouselID) {
              $('#' + carouselID).carousel('pause');
              $('#' + carouselID + ' > a.play-pause > span.icon-pause').toggleClass('icon-play icon-pause');
              $('#' + carouselID + ' > a.play-pause > span.sr-only').text('image carousel: Play');
              $('#' + carouselID + ' > a.play-pause').attr('onclick', 'startCarousel("' + carouselID + '");');
              $('#' + carouselID + ' > a.play-pause').attr('onkeypress', 'startCarousel("' + carouselID + '");');
            }
       </script>
      </div>
