   <div class="container">
    <div id="feature">
     <article class="slide out">
      <p>
       One of the largest university-based facilities - working at the intersection of biology and engineering.
      </p>
     </article>
     <article class="slide out">
      <p>
       MNTL has 15 cleanrooms, 46 general purpose labs, and a large biosafety level-2 complex all under one roof.
      </p>
     </article>
     <article class="slide out">
      <p>
       Designing, building, and testing innovative nanoscale technologies, spanning sizes from atoms to entire systems.
      </p>
     </article>
     <article class="slide out">
      <p>
       MNTL: A jewel in the Illinois College of Engineering crown.
      </p>
     </article>
     <script>
      var intDelay = 6000;
	var intCurrentSlide = 0;
	var intMax = $('article.slide').length;
	var myTimer;

	document.write("<div id='pager'>");
	for (var i = 0; i < intMax; i++) {
		document.write("<a href='javascript:void(0)' onclick='SetSlide(" + i + "); myTimer = setTimeout(SwitchSlides, intDelay); return 0;' class='out'>" + i + "</a> ");
	}
	document.write("</div>");

	$("#feature article").first().removeClass("out").addClass("in");
	$("#feature #pager a").first().removeClass("out").addClass("active");

	var SwitchSlides = function() {
		intNextSlide = (intCurrentSlide + 1) % intMax;
		SetSlide(intNextSlide);
		myTimer = setTimeout(SwitchSlides, intDelay);
	}

	myTimer = setTimeout(SwitchSlides, intDelay);

	function NextSlide() {
		intNextSlide = (intCurrentSlide + 1) % intMax;
		SetSlide(intNextSlide);
		myTimer = setTimeout(SwitchSlides, intDelay);
		return false;
	}

	function PrevSlide() {
		intNextSlide = (intCurrentSlide - 1);
		if (intNextSlide < 0) { intNextSlide = intMax - 1; }
		SetSlide(intNextSlide);
		clearTimeout(myTimer);
		return false;
	}

	function SetSlide(n) {
		var intOldSlide;
		clearTimeout(myTimer);
		intOldSlide = intCurrentSlide;
		intCurrentSlide = n

		$('article.slide').each(function(index) {
			if (intOldSlide == index) {
				$(this).removeClass( "in" ).addClass( "leaving" );
			}
		});

		$('article.slide').each(function(index) {
			if (intCurrentSlide == index) {
				$(this).removeClass( "out" ).addClass( "in" );
			}
		});


		$('#pager a').each(function(index) {
			if (intCurrentSlide == index) {
				$(this).removeClass( "out" ).addClass( "active" );
			}
		});

		$('#pager a').each(function(index) {
			if (intOldSlide == index) {
				$(this).removeClass( "active" ).addClass( "out" );
			}
		});

		setTimeout(function() {
			$('article.slide').each(function(index) {
				if (intOldSlide == index) {
					$(this).removeClass( "leaving" ).addClass( "out" );
				}
			});
		}, 1500);

		return false;
	}
     </script>
