  <g2:fileinfo filename="ftp://gsft_database/fp_rollover_script" linenumber="2">
   <script>
    $j(document).ready(function() {
            //attach event handlers to the menu items
            $j('.menuitem.roleHover').on('mouseenter', roleMenuMouseOver);
            $j('.menuitem.megaHover').on('mouseenter', megaMenuMouseOver);
            $j('.menuitem.roleHover').on('mouseleave', fpRoleMenuOut);
            $j('.menuitem.megaHover').on('mouseleave', fpMenuMouseOut);
            $j('.menuitem.roleHover').on('click', roleMenuClick);
            $j('.menuitem.megaHover').on('click', megaMenuClick);
            //These handlers make sure the menu actually close
            $j('.menuitem.roleHover').children("div.sub").on('mouseleave', fpRoleMenuOut);
            $j('.menuitem.megaHover').children("div.mega").on('mouseleave', fpMenuMouseOut);
        });
        //global var available so everything in this scope has access to the timeout variable
        var timeoutIdMega = 0;
        var timeoutIdRole = 0;
        //wrapper function for the timeout during mouse over
        function megaMenuMouseOver(evt) {
            timeoutIdMega = window.setTimeout(fpMenuMouseOver, 1000, evt);
        }        
        function roleMenuMouseOver(evt) {
            timeoutIdRole = window.setTimeout(fpRoleMenuOver, 1000, evt);
        }

        //Wrappers for click functions
        function megaMenuClick(evt) {
            var element = event.target;
            //If the menu is already displayed, close it
            if ($j(element).siblings("div.mega").is(":visible")) {
                fpMenuMouseOut(evt);
            }
            else {
                fpMenuMouseOver(evt);
            }
        }
        function roleMenuClick(evt) {
            var element = event.target;
            //If the menu is already displayed, close it
            if ($j(element).siblings("div.sub").is(":visible")) {
                fpRoleMenuOut(evt);
            }
            else {
                fpRoleMenuOver(evt);
            }
        }
    
        function fpMenuMouseOver(evt) {
            var targ;
            if (!evt) {
                var evt = window.event;
            }
            if (evt.target) {
                targ = evt.target;
            } else if (evt.srcElement) {
                targ = evt.srcElement;
            }
            if (targ.nodeType == 3) { // defeat Safari bug
                targ = targ.parentNode;
            }
            
            if (evt.type == "mouseover" || evt.type == "click") {
                //grab UL object for positioning purposes
                targ = targ.parentNode;
                var ul = targ.parentNode;
    
                //grab mega child (mega menu)
                var megaChildren = targ.childNodes;
                var megaChild = null;
                var i = 0;
                for (i = 0; i != megaChildren.length; i++) {
                    if (megaChildren[i].className == 'mega') {
                        megaChild = megaChildren[i];
                        //positioning variable
    
                        var rightMargin = targ.parentNode.parentNode.offsetLeft + targ.parentNode.parentNode.offsetWidth;
                        var rightValue = targ.parentNode.parentNode.offsetLeft + targ.offsetLeft + 0;
                        //var rightValue = targ.parentNode.parentNode.offsetLeft + targ.offsetLeft + megaChild.offsetWidth;
                        //var rightValue = document.getElementsByClassName('center-fix')[0].offsetLeft + targ.offsetLeft + megaChild.offsetWidth;
    
                        var leftValue = '0';
    
                        //alert("First: " + targ.parentNode.parentNode.offsetLeft + "\nSecond: " + targ.offsetLeft + "\nThird: " + megaChild.offsetWidth);
    
                        if (rightValue > rightMargin) {
                            leftValue = -(targ.parentNode.parentNode.offsetLeft + targ.offsetLeft + 0 - rightMargin);
                            //leftValue = -(targ.parentNode.parentNode.offsetLeft + targ.offsetLeft + megaChild.offsetWidth - rightMargin);
                        }
    
                        megaChild.style.display = 'block';
                        megaChild.style.opacity = '1';
                        megaChild.style.left = leftValue + 'px';
                    }
                }
            }
        }
    
        function fpMenuMouseOut(evt) {
            //clear the timeout from the event if they mouse off before
            window.clearTimeout(timeoutIdMega);
            var targ;
            if (typeof $j(evt.target).parents(".menuitem.megaHover")[0] != "undefined") {
                targ = $j(evt.target).parents(".menuitem.megaHover")[0];
            }
            else {
                if (!evt) {
                    var evt = window.event;
                }
                if (evt.target) {
                    targ = evt.target;
                } else if (evt.srcElement) {
                    targ = evt.srcElement;
                }
                if (targ.nodeType == 3) { // defeat Safari bug
                    targ = targ.parentNode;
                }
                targ = targ.parentNode;    
            }
            
            //grab mega child (mega menu)
            var megaChildren = targ.childNodes;
            $j(megaChildren).find("div.mega").hide();
            var megaChild = null;
            var i = 0;
            for (i = 0; i != megaChildren.length; i++) {
                if (megaChildren[i].className == 'mega') {
                    megaChild = megaChildren[i];
                    megaChild.style.display = '';
                    megaChild.style.opacity = '';
                }
            }
        }
    
        function fpRoleMenuOver(evt) {
            var targ;
            if (!evt) {
                var evt = window.event;
            }
            if (evt.target) {
                targ = evt.target;
            } else if (evt.srcElement) {
                targ = evt.srcElement;
            }
            if (targ.nodeType == 3) { // defeat Safari bug
                targ = targ.parentNode;
            }
            if (evt.type == "mouseover" || evt.type == "click") {
                targ = targ.parentNode;
                var subChildren = targ.childNodes;
                var subChild = null;
                var i = 0;
                for (i = 0; i != subChildren.length; i++) {
                    if (subChildren[i].className == 'sub') {
                        subChild = subChildren[i];
                        //positioning variable
    
                        var rightMargin = targ.parentNode.parentNode.offsetLeft + targ.parentNode.parentNode.offsetWidth;
                        var rightValue = targ.parentNode.parentNode.offsetLeft + targ.offsetLeft + 0; //hard-coded width of the sub menus with 2 px extra for the border
                        //var rightValue = targ.parentNode.parentNode.offsetLeft + targ.offsetLeft + subChild.offsetWidth;
    
                        var leftValue = '0';
    
						// This code wasn't working in Fuji and when we upgraded to Geneva it was always triggered and moved the menu
						// for some options too far to the left and they weren't accessible dlb44 05/13/2016
		
                        //if (rightValue > rightMargin) {
                        //    leftValue = -(targ.parentNode.parentNode.offsetLeft + targ.offsetLeft + 0 - rightMargin); //hard-coded width of the sub menus with 2 px extra for the border
                            // leftValue = -(targ.parentNode.parentNode.offsetLeft + targ.offsetLeft + subChild.offsetWidth - rightMargin);
                        //}
    
                        subChild.style.display = 'block';
                        subChild.style.opacity = '1';
                        subChild.style.left = leftValue + 'px';
                    }
                }
            }
        }
    
        function fpRoleMenuOut(evt) {
            //Stop the timeout if they don't stay hovered over the menu.
            window.clearTimeout(timeoutIdRole);
            var targ;
            if (typeof $j(evt.target).parents(".menuitem.roleHover")[0] != "undefined") {
                targ = $j(evt.target).parents(".menuitem.roleHover")[0];
            }
            else {
                if (!evt) {
                    var evt = window.event;
                }
                if (evt.target) {
                    targ = evt.target;
                } else if (evt.srcElement) {
                    targ = evt.srcElement;
                }
                if (targ.nodeType == 3) { // defeat Safari bug
                    targ = targ.parentNode;
                }   
            }
            targ = targ.parentNode;
            var subChildren = targ.childNodes;
            $j(subChildren).find("div.sub").hide();
            var subChild = null;
            var i = 0;
            for (i = 0; i != subChildren.length; i++) {
                if (subChildren[i].className == 'sub') {
                    subChild = subChildren[i];
                    subChild.style.display = '';
                    subChild.style.opacity = '';
                }
            }
        }
   </script>
  </g2:fileinfo>
  <script language="javascript">
   // Escape frame
		if (window != top
				&& top.location != "https://itbuild.byu.edu/navpage.do"
				&& top.location != "https://itbuild.byu.edu/") {
			  try {
				window.stop();
				} catch(e) {
					try {
						document.execComment("Stop");
					} catch (e) {
						// do nothing
					}
				}
			top.location.replace(document.location.href);
		}
  </script>
