      <div id="login">
       <script language="javascript">
        function cas_login(chat) {
               var location = window.location.toString();
               // +'s are getting replaced by spaces in places not visible to our faces, so we're replacing them with spaces
               // This appears to be happening during CAS's processing of the authentication
               location = location.replace(/\+/g, " ");
	
               if (location.substring(location.length-1) == "?") {
                  location = location.substring(0, location.length-1);
               }
		// Strip out the domain name since it is added on with the cas_redirector page
		location = location.replace("https://it.byu.edu", "");
               var redirect_a = escape('https://it.byu.edu/cas_redirector.do?uri=');
			   var redirect_b = escape(escape(location));
			   //alert("Redirect: " + redirect_a + redirect_b);
               //redirect = redirect.replace(/\%26/g,"%26amp;");
               redirect_a = redirect_a.replace(/\%25/g,"%"); //added to fix the problem of escaping the % when something has already been escaped
               //var redirect = window.location;
			 
               window.location = "https://cas.byu.edu/cas/login?&method=POST&service=" + redirect_a + redirect_b;
            }
			 
			 function readCookie(name){
			 	var nameEQ = name + "=";		
	        	 var ca = document.cookie.split(';');		
	        	 alert(ca.length);
	             for(var i=0; i < ca.length; i++) {		
	                 var c = ca[i];		
	                 alert(c);
	                 while (c.charAt(0)==' ') c = c.substring(1,c.length);		
	                 if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);		
	             }		
	             return null;		
			 }
       </script>
