  <script type="text/javascript">
   //Google Analytics

$(document).ready(function(){	  
  
    console.log("EM:attaching listeners...");
    $(".btn-primary").click(function(){ 	
       console.log("EM:click detected...");
	var btnLable = $(this).text();
        var btnLink = $(this).attr("href");
      	var activeItem = document.getElementsByClassName("item active")[0];
      	var tabIndex = activeItem.getAttribute("aria-labelledby")
        var tabTitle = activeItem.getElementsByClassName("title")[0].innerHTML;
      var etLable = tabIndex + " : " + tabTitle;
      
       //console.log("EM:click detected on '" + tabIndex + "' ...");
      console.log("EM:click detected on '" + etLable);
      console.log("EM: ga:" + ga);
      var trackerName = ga.getAll()[0].get('name');
      console.log("EM: ga[0].name:" + trackerName);
      ga(trackerName + '.send', 'event', {
  		eventCategory: 'carousel-button-click',
  		eventAction: btnLink,
  		eventLabel: etLable,
                transport: 'beacon'
      });
      console.log("EM: event added.");
      //alert("Thank you for interested in our service! \n\n" + tabTitle);
    }); 
    console.log("EM:listeners attached...");  
  
});

$(document).ready(function(){ 
    console.log("EM:attaching listeners...");
    $(".program-wrapper").click(function(){           
       console.log("EM:click detected...");
                var serviceText = $(".title-text strong", this).text();
       //console.log("EM:click detected on '" + serviceText + "' ...");
      console.log("EM:click detected on '" + serviceText);
      console.log("EM: ga:" + ga);
      var trackerName = ga.getAll()[0].get('name');
      console.log("EM: ga[0].name:" + trackerName);
      ga(trackerName + '.send', 'event', {
                                eventCategory: 'Service Grid',
                                eventAction: 'Service click',
                                eventLabel: serviceText,
                transport: 'beacon'
      });
      console.log("EM: event added.");
    }); 
    console.log("EM:listeners attached...");
    return true;
});
  </script>
  <script type="text/javascript">
   //KA: Reverse the order in the carousel. Show most recent first

$(document).ready(function(){	    
$.fn.reverseChildren = function() {
  return this.each(function(){
    var $this = $(this);
    $this.children().each(function(){ $this.prepend(this) });
  });
};
$('.carousel-inner').reverseChildren();  

   $('.carousel-inner div:first').addClass('active'); 
  $('.carousel-inner div:last-child').removeClass('active'); 
  
});
  </script>
