       <script type="text/javascript">
        /* Keyboard navigation for carousel */
    (function ($) {
	        
        $(document).ready(function () {

            $(document).on({
                "keydown": function (e) {
                    console.log("document keydown: " + e.which);
                    var $carousel = $(e.target).closest(".carousel");
                    console.log("document keydown carousel: " + $carousel + ":" + $carousel.length);
                }
            });

            var $carousel = $(".carousel");
            $carousel.on({

                /* UI events */
		
		/* Intercept keydown events on Carousel */
                "keydown": function (e) {
                    var carousel = $(this);

                    var keys = {
                        backspace : 8,
                        tab : 9,
                        enter : 13,
                        pause : 19,
                        space : 32,
                        pageup : 33,
                        pagedown : 34,
                        end : 35,
                        home : 36,
                        leftArrow : 37,
                        upArrow : 38,
                        rightArrow : 39,
                        downArrow : 40,

                        key0 : 48,
                        key1 : 49,
                        key2 : 50,
                        key3 : 51,
                        key4 : 52,
                        key5 : 53,
                        key6 : 54,
                        key7 : 55,
                        key8 : 56,
                        key9 : 57,

                        numpad0 : 96,
                        numpad1 : 97,
                        numpad2 : 98,
                        numpad3 : 99,
                        numpad4 : 100,
                        numpad5 : 101,
                        numpad6 : 102,
                        numpad7 : 103,
                        numpad8 : 104,
                        numpad9 : 105
                    };

                    var nextKeys = [keys.rightArrow, keys.downArrow];
                    var prevKeys = [keys.leftArrow, keys.upArrow];
                    var numKeys = [keys.key0, keys.key1, keys.key2, keys.key3, keys.key4, keys.key5, keys.key6, keys.key7, keys.key8, keys.key9];

                    var key = event.which;
                    console.log("carousel keydown: " + e.which + "@" + carousel.prop("tagName"));
		    
		    /* Go to previous slide on "prevKeys" keydown*/
                    if (prevKeys.indexOf(key) > -1) {
                        console.log("carousel keydown prevKeys");
                        //carousel.trigger("carousel:previous", [carousel]);
                        return;
                    }

		    /* Go to next slide on "nextKeys" keydown */
                    if (nextKeys.indexOf(key) > -1) {
                        console.log("carousel keydown nextKeys");
                        //carousel.trigger("carousel:next", [carousel]);
                        return;
                    }

		    /* Go to specific slide on "numKeys" keydown */
                    if (numKeys.indexOf(key) > -1) {
                        console.log("carousel keydown numKeys");
                        var index = -1 + parseInt(String.fromCharCode(key));
                        carousel.trigger("carousel:goto", [carousel, index]);
                        return;
                    }

                    console.log("keydown no action");

                },

                "mouseup": function (event) {
                    var carousel = $(this);
                    console.log("carousel mouseup");
                    //carousel.trigger("carousel:next")
                    
                },

                /* custom events */
		
                "carousel:previous": function (e, source) {
                    var carousel = $(this);
                    console.log("carousel:previous");
                    carousel.carousel("prev");
                },

                "carousel:next": function (e, source) {
                    var carousel = $(this);
                    console.log("carousel:next");
                    carousel.carousel("next");
                },

                "carousel:pause": function (e, source) {
                    var carousel = $(this);
                    console.log("carousel:pause");
                    carousel.carousel("pause");
                },

                "carousel:resume": function (e, source) {
                    var carousel = $(this);
                    console.log("carousel:resume");
                    carousel.carousel("cycle");
                },

                "carousel:goto": function (e, source, index) {
                    var carousel = $(this);
                    console.log("carousel:goto " + index);
                    carousel.carousel(index);
                },

            });
        });
    }(jQuery));
       </script>
      </div>
