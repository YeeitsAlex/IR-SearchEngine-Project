                     <div id="ctl00_ctl56_g_1f5828c8_6899_4e57_b6e4_0514e7e380c3">
                      <script type="text/javascript">
                       function getCookie(cname) { var name = cname + '='; var ca = document.cookie.split(';'); for (var i = 0; i < ca.length; i++){ var c = ca[i]; while (c.charAt(0) == ' ') {  c = c.substring(1);  } if (c.indexOf(name) == 0) {return c.substring(name.length, c.length);} } return '';}
                      </script>
                      <div class="cstm-section cstm-events">
                       <h3>
                        EVENTS
                       </h3>
                       <div>
                        <h4>
                         <a href="https://www.olin.wustl.edu/EN-US/Events/Pages/Event.aspx?CID=1642%7cExecutive+Programs+Calendar&amp;Referrer=https%3a%2f%2folin.wustl.edu%2fEN-US%2fPages%2fdefault.aspx">
