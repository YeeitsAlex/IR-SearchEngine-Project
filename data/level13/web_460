   var hasSetModalCookie = typeof Cookies.get('stateSpecificModal') === 'undefined' ? false : true;

   if(!hasSetModalCookie) {
    iffy.track({
        stateSpecificModal: {
        value: 'mainpage', // the key we exemplify for this use case, this can be named anything
        arbitraryKey: 'Test arbitrary key', // add unlimited keys and values for templating
