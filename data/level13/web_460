console.log("Loaded in the iffy tracker"); 


var iffy = {
     track: function(key) {
          var cookieObj = Cookies.get("iffy") ? Cookies.get("iffy") : {};

          if(key ) {

             cookieObj[key] = true;
             
             Cookies.set('iffy', cookieObj, { expires: 1 });
          }
          
          console.log("Running the track function");
          console.log("The cookie so far is:");
          console.log(Cookies.get("iffy"));
     }
}


