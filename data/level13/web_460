                <script type="text/javascript">
                 <!--//--><![CDATA[// ><!--


// Trigger a targeted modal if they visited one of the state pages
jQuery(document).ready(function() {
      iffy.hasTracker('stateSpecificModal', function() {
       var stateSpecificModal = JSON.parse(Cookies.get('stateSpecificModal'));
       var statePageVisited =  stateSpecificModal.value;
       var hasShownModal = stateSpecificModal.hasShown;
       var modalToShow = '';


      switch(statePageVisited) {
          case 'mainpage':
               hasShownModal ? null :  jQuery('#MainPage_Modal').modal();
          break;
          case 'california':
               jQuery('#California_Modal').modal();
          break;
          case 'washington':
               jQuery('#Washington_Modal').modal();
          break;
          case 'hawaii':
               jQuery('#Hawaii_Modal').modal();
          break;
          case 'idaho':
               jQuery('#Idaho_Modal').modal();
          break;
          case 'arizona':
               jQuery('#Arizona_Modal').modal();
          break;
          case 'new mexico':
               jQuery('#NewMexico_Modal').modal();
          break;
          case 'colorado':
               jQuery('#Colorado_Modal').modal();
          break;
          case 'illinois':
               jQuery('#Illinois_Modal').modal();
          break;
          case 'minnesota':
               jQuery('#Minnesota_Modal').modal();
          break;
          case 'montana':
               jQuery('#Montana_Modal').modal();
          break;
          case 'nevada':
               jQuery('#Nevada_Modal').modal();
          break;
          case 'texas':
               jQuery('#Texas_Modal').modal();
          break;
          case 'utah':
               jQuery('#Utah_Modal').modal();
          break;
          default:
          break;
       }
      });
});



// Trigger a location redirect if they are from one of the associated regions


// Only trigger the location redirect once
iffy.hasGeolocationAccess().then(function(location) {
   // Running the hasGeolocationAccess will provide you with the 'location' variable in it's .then() function for use
  // So that you can do whatever you want with it, like redirect based on the state the user is in
  var redirectLocation = 'https://admissions.oregonstate.edu/recruitment-territory/';
  var redirectLocation2 = 'https://admissions.oregonstate.edu'; // Start by setting a base location so that you can concatenate based on the exact territory
  var shouldRedirect = true;


  // The requirement is that the location redirect should only happen once
  // So check if it has happened before using the 'Cookie' helper
  if(typeof Cookies.get('hasRedirectedLocation') === 'undefined') { // Lack of a placed cookie returns undefined
     // Set the cookie so that this only happens once
     Cookies.set('hasRedirectedLocation', true);

     // And then implement the redirect logic
        switch(location.address.state) {
            case 'Arkansas':
                redirectLocation = redirectLocation + 'arkansas'; // Append the appropriate link to the associated recruitment page
            break;
            case 'Oklahoma':
                redirectLocation = redirectLocation + 'oklahoma';
            case 'New Jersey':
                redirectLocation = redirectLocation + 'new-jersey';
            case 'Ohio':
                redirectLocation = redirectLocation + 'ohio';
            case 'New York':
                redirectLocation = redirectLocation + 'new-york';
            break;
            case 'West Virginia':
                redirectLocation = redirectLocation + 'west-virginia';
            break;
            case 'Pennsylvania':
                redirectLocation = redirectLocation + 'pennsylvania';
            case 'Virginia':
                redirectLocation = redirectLocation + 'virginia';
            break;
            case 'Vermont':
                redirectLocation = redirectLocation + 'vermont';
            break;
            case 'Utah':
                redirectLocation = redirectLocation + 'utah';
            break;
            case 'South Dakota':
                redirectLocation = redirectLocation + 'south-dakota';
            break;
            case 'South Carolina':
                redirectLocation = redirectLocation + 'south-carolina';
            break;
            case 'North Dakota':
                redirectLocation = redirectLocation + 'north-dakota';
            break;
            case 'North Carolina':
                redirectLocation = redirectLocation + 'north-carolina';
            break;
            case 'New Hampshire':
                redirectLocation = redirectLocation + 'new-hampshire';
            break;
            case 'Michigan':
                redirectLocation = redirectLocation + 'michigan';
            break;
            default:
            shouldRedirect = false; // If they don't match make sure we don't send them anywhere
            break;
        }

    if(shouldRedirect ) {
        // Now actually do the redirect
        window.location.href = redirectLocation;
    }
  }
});

//--><!]]>
                </script>
