   <header class="full-header has-video full-width-text center">
    <img alt="A student floats while examining an experiment during a zero-gravity test flight." class="attachment-full_header size-full_header" height="662" sizes="(max-width: 767px) calc(768px - ((768px - 100vw)/ 1.26)), (max-width: 991px) 992px, (max-width: 1500px) 100vw, 1500px" src="https://www.ncsu.edu/content/uploads/2019/02/zer-gravity-corehp-1500x662.jpg" srcset="https://www.ncsu.edu/content/uploads/2019/02/zer-gravity-corehp.jpg 1500w, https://www.ncsu.edu/content/uploads/2019/02/zer-gravity-corehp-300x132.jpg 300w, https://www.ncsu.edu/content/uploads/2019/02/zer-gravity-corehp-768x339.jpg 768w, https://www.ncsu.edu/content/uploads/2019/02/zer-gravity-corehp-1024x452.jpg 1024w, https://www.ncsu.edu/content/uploads/2019/02/zer-gravity-corehp-1200x530.jpg 1200w, https://www.ncsu.edu/content/uploads/2019/02/zer-gravity-corehp-950x419.jpg 950w, https://www.ncsu.edu/content/uploads/2019/02/zer-gravity-corehp-600x265.jpg 600w, https://www.ncsu.edu/content/uploads/2019/02/zer-gravity-corehp-460x203.jpg 460w, https://www.ncsu.edu/content/uploads/2019/02/zer-gravity-corehp-230x102.jpg 230w" width="1500"/>
    <video autoplay="" class="full-header_video" loop="" muted="" playsinline="" preload="auto">
     <source src="https://www.ncsu.edu/content/uploads/2019/02/zero-gravity-corehp-compressed.mp4" type="video/mp4"/>
     <source src="https://www.ncsu.edu/content/uploads/2019/02/zero-gravity-corehp-compressed.webmhd.webm" type="video/webm"/>
    </video>
    <div class="container full-header_container">
     <a class="full-header_box " data-ua-action="Top Feature" data-ua-cat="Homepage Interactions" data-ua-label="https://sciences.ncsu.edu/news/a-zero-gravity-laboratory/?utm_campaign=zero-gravity-lab&amp;utm_medium=referral&amp;utm_source=ncsu.edu&amp;utm_content=hp-hero" href="https://sciences.ncsu.edu/news/a-zero-gravity-laboratory/?utm_campaign=zero-gravity-lab&amp;utm_medium=referral&amp;utm_source=ncsu.edu&amp;utm_content=hp-hero" id="header_select_0_cta_url">
      <h1 class="full-header_heading">
       A Zero-Gravity Laboratory
      </h1>
      <div class="full-header_cta">
       <span class="full-header_cta-txt">
        Board a unique research
        <span class="nowrap">
         flight
         <span class="glyphicon glyphicon-bold-arrow">
         </span>
        </span>
       </span>
      </div>
     </a>
    </div>
    <div class="full-header_video-controls">
     <a data-ua-action="Play/Stop Video" data-ua-cat="Homepage Interactions" data-ua-label="https://sciences.ncsu.edu/news/a-zero-gravity-laboratory/?utm_campaign=zero-gravity-lab&amp;utm_medium=referral&amp;utm_source=ncsu.edu&amp;utm_content=hp-hero" href="#" id="header_select_0_cta_url">
      Stop Video
     </a>
    </div>
   </header>
   <script type="text/javascript">
    var header = document.querySelector('.full-header.has-video'),
        video = document.querySelector('.full-header_video'),
        video_toggle = document.querySelector('#full-header_video-toggle'),
        cookie_id = getCookieId(video),
        set_to_paused = getCookie(cookie_id);

    if (set_to_paused) {
        handleVideoToggle();
    }

    // Pause button
    video_toggle.addEventListener("click", function(e){
        e.preventDefault();
        handleVideoToggle();
    });

    function getCookieId(video) {
        var src = video.firstElementChild.getAttribute('src');

        return src.substring(src.lastIndexOf('/') + 1, src.lastIndexOf('.'));
    }

    function getCookie(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for(var i = 0; i <ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    function handleVideoToggle(){
        header.classList.toggle('hide-video');

        var expires = new Date(),
            current_text = video_toggle.innerText || video_toggle.textContent;

        if(current_text === "Stop Video") {
            video.pause();
            expires.setTime(expires.getTime()+(365*24*60*60*1000));
            document.cookie = cookie_id + "=1; expires=" + expires.toUTCString();
            video_toggle.innerText = "Play Video";
        } else {
            video.play();
            expires.setTime(expires.getTime()-(1000));
            document.cookie = cookie_id + "=0; expires=" + expires.toUTCString();
            video_toggle.innerText = "Stop Video";
        }
    };
   </script>
