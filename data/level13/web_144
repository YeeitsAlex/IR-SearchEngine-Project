<script type="text/javascript">
 jQuery(function($) {
var mediaBox = $("#mediaBox");
var mediaInner = mediaBox.find("#mediaInner");
var charsColoredBox = mediaBox.find("#charsColoredBox");
var shouldResizeColoredBox = true;
var winWidth = $(window).width();

function onMouseMove(e) {
	if(winWidth > 1024) {
		//var parentOffset = $(this).parent().offset(); 
		var parentOffset = mediaInner.offset(); 
		//or $(this).offset(); if you really just want the current element's offset
		var relX = e.pageX - parentOffset.left;
		var relY = e.pageY - parentOffset.top;
		var widthPerc = relX / mediaInner.width();
		//console.log("relX: ", relX);
		//console.log("widthPerc: ", widthPerc);
		charsColoredBox.width(relX)
	}
}

if(mediaBox.length > 0) {
mediaInner.on("mousemove", onMouseMove);
$(window).resize(function() {
	winWidth = $(window).width();	
});

}




});
</script>

