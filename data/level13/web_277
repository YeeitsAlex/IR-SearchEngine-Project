    <div data-content-field="navigation-folderNav" id="folderNav">
     <div class="folder-nav-toggle">
     </div>
     <nav class="folder-nav" role="navigation">
      <ul>
       <li class="nav-section-label">
        The Project
       </li>
       <li class="page-collection active-link">
        <a href="/">
         Overview
        </a>
       </li>
       <li class="page-collection">
        <a href="/how-it-works">
         Vision
        </a>
       </li>
      </ul>
     </nav>
