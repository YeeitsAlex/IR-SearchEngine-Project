      <script charset="utf-8" type="text/javascript">
       function showLoading() {
            $("#loadingDiv").show();
            $("#loadingDiv").expose({ closeOnEsc: false, closeOnClick: false });
        }

        function hideLoading() {
            $("#loadingDiv").hide();
            $.mask.close();
        }

        function showLocalLoading(divId) {
            var loadingImg = "<img src='/images/ajax-loader.gif' alt='Loading....' id='" + divId + "_Loading' style='width: 30px; height: 30px; margin: 0 auto;' />";
            $('#' + divId).append(loadingImg);
        }

        function hideLocalLoading(divId) {
            $('#' + divId + '_Loading').remove();
        }

        function LoadActionResult(actionUrl, divToLoad, loadingSymbolElement, ajaxCallback) {

            //$('#' + divToLoad).html("");
            if (!loadingSymbolElement) {
                loadingSymbolElement = divToLoad;
            }
            showLocalLoading(loadingSymbolElement);
            $.ajax({
                url: actionUrl,
                cache: false,
                success: function (html) {
                    $('#' + divToLoad).html(html);

                },
                error: function () {
                    hideLocalLoading(loadingSymbolElement);
                    $("#" + loadingSymbolElement).find(".card-body").append("<p>There was a problem loading this content. Please check back later.</p>");
                },
                complete: function () {
                    hideLocalLoading(loadingSymbolElement);
                    if (ajaxCallback && typeof ajaxCallback === "function") {
                        ajaxCallback();
                    }
                }

            });

        }

        $(function () {
            $(".zeroDefault").each(function () {
                $("option[value='']", this).val("0");
            });

            $(".input-validation-error").addClass("is-invalid");
            $(".field-validation-error-holder").addClass("invalid-feedback");

        });



        // trim on change of trimmable fields and disabling of initial fields classed as .ocem-disabled
        $(function () {

            $(document).on("change", ".ocem-trimmable", function () {
                $(this).val(
                    $.trim($(this).val())
                );
            });

            $(".ocem-disabled").prop("disabled", true);

        });
      </script>
