    .da-slide h2,
    #bootstrap-slider .carousel-caption h2 {
        font-size: 100%;
        letter-spacing: 1px;
    }
    #slide_holder .featured-title a {
        font-size: 80%;
        letter-spacing: 1px;
    }
    .da-slide p,
    #slide_holder p,
