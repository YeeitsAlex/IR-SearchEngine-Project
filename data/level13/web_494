   $(document).ready(function(){
            $(".tabs").accessibleTabs({
                tabhead:'h2',
                fx:"show",
                fxspeed:null,
            });
	var nth = 3;
	var rnd = Math.floor(Math.random() * nth) + 1;
	if (rnd == nth && $.cookie('webcomm_survey') != '1') {
		$.cookie('webcomm_survey', '1', { expires: 30, path: '/' });
