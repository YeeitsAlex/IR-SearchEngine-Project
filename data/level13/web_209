     <div class="search-results" ng-show="searching">
      <div class="search-results-header" ng-show="results || waitingForResults">
       <div class="container">
        <div class="row">
         <div class="col-xs-12">
          <hgroup>
           <h1 class="results-title">
            Search Results
            <span class="separator">
             //
            </span>
            <span class="search-term" ng-show="waitingForResults">
             "{{ term }}"
            </span>
            <span class="search-term" ng-hide="waitingForResults">
             "{{ results.meta.query }}"
            </span>
           </h1>
           <h4 class="result-count" ng-show="results &amp;&amp; results.success &amp;&amp; results.posts.length != 1">
            {{ results.meta.result_count }} results
           </h4>
           <h4 class="result-count" ng-show="results &amp;&amp; results.success &amp;&amp; results.posts.length == 1">
            1 result
           </h4>
           <h4 class="result-count" ng-show="results &amp;&amp; results.success === false">
            no results
           </h4>
