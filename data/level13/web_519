     <div class="region region-home-status">
      <div class="block block-block ews-status first odd" id="block-block-5">
       <h2 class="block__title block-title">
        EWS Lab Usage
       </h2>
       <script type="text/javascript">
        (function($) {
	$(document).ready(function(){
	
		$.getJSON("https://my.engr.illinois.edu/labtrack/util_data_json.asp?callback=?",
			function(json_data) {
				var items = [];
  				$.each(json_data.data, function(key, val) {
    				items.push('<div id="' + val.strlabname.replace(/ /g,"_") + '" class="lab">' + 
    				'<div class="name">' + val.strlabname + '</div>' +
    				'<div class="graph" style="width: ' + Math.round(((val.inusecount / val.machinecount) * 100) / 2) + 'px; margin-right: ' + 
    				Math.round((100 - Math.round(((val.inusecount / val.machinecount) * 100)))/2) + 'px;">&nbsp;</div>' + 
    				'<div class="count">' + val.inusecount + ' / ' + val.machinecount + '</div><div class="clear">&nbsp;</div>' + 
    				'</div>');
  				});

  			$('#workstations #data').html(items.join(''));
  			
  			});
		});
    })(jQuery);
       </script>
       <style type="text/css">
        #data {
width: 468px;
height: 220px;
}

.lab {
   border-bottom: 1px solid #ddd;
   margin-bottom: 5px;
width: 50%;
float: left;
}

.clear {
		height: 1px;
		clear: both;
	}
	
	#workstations .name,
	#workstations .graph,
	#workstations .count {
		float: left;
	}
	
	#workstations .name {
		margin-right: 10px;
		width: 85px;
	}
	
	#workstations .count {
		font-size: 0.8em;
		margin-left: 10px;
	}
	
	#workstations .graph {
		background: #1e5799; /* Old browsers */
		background: -moz-linear-gradient(left, #1e5799 0px, #2989d8 25px, #7db9e8 50px); /* FF3.6+ */
		background: -webkit-gradient(linear, left top, right top, color-stop(0px,#1e5799), color-stop(25px,#2989d8), color-stop(50px,#7db9e8)); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(left, #1e5799 0px,#2989d8 25px,#7db9e8 50px); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(left, #1e5799 0px,#2989d8 25px,#7db9e8 50px); /* Opera 11.10+ */
		background: -ms-linear-gradient(left, #1e5799 0px,#2989d8 25px,#7db9e8 50px); /* IE10+ */
		background: linear-gradient(to right, #1e5799 0px,#2989d8 25px,#7db9e8 50px); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1e5799', endColorstr='#7db9e8',GradientType=1 ); /* IE6-9 */	
	}
       </style>
       <br/>
       <br/>
       <div id="workstations">
        <div id="data">
        </div>
       </div>
