jQuery(document).ready(function($) {
  
  // Make links out of obfuscated email address 
  // use this instead of 'recaptcha mailhide'
  // email addresses must be in the format 'username*domain*tld'
  $('.add-email-link').each(function(){
    var addressParts = $(this).text().split('*');
    var email = addressParts[0]+'@'+addressParts[1]+'.'+addressParts[2];
    var subject = encodeURIComponent($(this).attr('title'));
    $(this).html('<a href="mailto:'+email+'?subject='+subject+'" target="popup" title="Send an email to '+email+'">'+email+'');
  });
  
  // add target to popup links
  $('a[href*="?target=popup"],a[href*="&target=popup"]').attr('target','popup');
  popupWindow ='height=800,width=650,top=20,left=20,scrollbars=yes,resizable=yes,menubar=yes,directories=yes,toolbar=yes,location=yes';
  $('a[target="popup"]').click(function(e){
      var url = $(this).attr('href');
      newwindow = window.open(url,'popup',popupWindow);
      newwindow.focus();
      return false;
  });
  
  // add tooltips to labels that have title attribute
 $('abbr,.label[title],.badge[title],.btn[title],.show-tooltip *[title],.show-tooltip[title]').tooltip({
 	placement:'bottom',
 }).click(function(e) {
    e.preventDefault();
    $(this).tooltip('toggle');
 });

 });

