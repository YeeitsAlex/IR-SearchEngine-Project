    <script>
     (function($) {
        $(document).ready(function() {
            $('.follow').click(function () {
                $('#social-bar').toggle();
            })

            $('body').click(function (e) {
                var menu = $('.mobile-menu');

                if ('mobile-menu' == e.target.class || 'pull' == e.target.id) {
                    e.stopPropagation();
                }

                else {
                    if (menu.hasClass('is-open')) {
                        menu.removeClass('is-open');
                    }
                }
            });
        });
    }(jQuery));
    </script>
