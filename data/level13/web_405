                 <div aria-controls="views_slideshow_cycle_div_explore_carnegie-block_1_1_3" class="views-slideshow-pager-field-item views_slideshow_pager_field_item views-row-even views-row-last" id="views_slideshow_pager_field_item_top_explore_carnegie-block_1_1_3">
                  <div class="views-field-title">
                   <div class="views-content-title">
                    <a href="/events/lectures/age-data-visualizing-revolution">
                     The Age of Data: Visualizing the Revolution
                    </a>
                   </div>
                  </div>
                  <div class="views-field-field-date">
                   <div class="views-content-field-date">
                    <a href="#slideshow-3">
                     <span class="date-display-single" content="2019-04-02T18:30:00-04:00" datatype="xsd:dateTime" property="dc:date">
                      April 2, 2019
                     </span>
                    </a>
                   </div>
