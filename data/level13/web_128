               <script type="text/javascript">
                $(document).ready(function () {
        var slideOneViewCount = 1;

        $("#wu-slideshow .pages").carouFredSel({
            circular: false,
            pagination: {
                container: ".pager",
                anchorBuilder: function (nr, item) {
                    var tab = item.find(".tab").text();
                    var acss = item.find(".acss").text();
                    var licss = item.find(".licss").text();
                    return "<li class='" + licss + "'><a href='#' class='" + acss + "'>" + tab + "</a></li>";
                },
                onAfter: function () {
                    $("#wu-slideshow .pages").trigger("stop");
                }
            },
            auto: {
                play: true,
                delay: 5000,
                pauseDuration: 5000
            },
            scroll: {
                fx: "none",
                onAfter: function (oldItems, newItems, newSizes, duration) {
                    var pageID = newItems.attr("id");

                    if (pageID.indexOf("page_0") > -1) {
                        //The first slide is being viewed, add to view count
                        slideOneViewCount++;
                    }

                    if (slideOneViewCount > 1) {
                        //The first slide has been viewed twice or more, stop cycle
                        $("#wu-slideshow .pages").trigger("stop");
                    }
                }
            }
        });
    });
               </script>
