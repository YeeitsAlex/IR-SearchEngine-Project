			  top: 170
			, bottom: function () {
				return (this.bottom = jQuery('#copyrights').outerHeight(true))
			  }
			}
		});	

		
		jQuery("#topbar .language").click(function () {
			jQuery('#block-locale-language').slideToggle( "slow" );
		});
  </script>
  <!-- Add titles from alt text -->
  <script>
   jQuery(document).ready(function($) {
	$("img:not([title])").each(function() {
			if($(this).attr("alt") != '') $(this).parent().attr("title", $(this).attr("alt"))
			else $(this).parent().attr("title", $(this).attr("src"));
	});
});
  </script>
  <!-- Google fix -->
  <script>
   jQuery(window).load(function() {
document.querySelector('img.gsc-branding-img').alt = "Google";
document.querySelector('input.gsc-search-button').alt = "Google Custom Search Button";
});
  </script>
