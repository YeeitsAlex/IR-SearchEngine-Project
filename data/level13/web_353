    </script>
    <div class="beige--block">
     <form action="https://search.library.brandeis.edu/primo_library/libweb/action/dlSearch.do" id="searchForm" name="searchForm" onsubmit="searchPrimo();">
      <div>
       <input id="query" name="query" type="hidden"/>
       <input id="primoQuery" name="primoQuery" placeholder="Enter search terms..." type="text"/>
       <label class="hidden" for="primoQuery">
        Enter search term
       </label>
       <input name="institution" type="hidden" value="01BRAND"/>
       <input name="vid" type="hidden" value="BRAND"/>
       <div class="form__select">
        <div class="form__select__arrow">
         <span class="svgstore svgpad">
