           <use aria-hidden="true" xlink:href="//www.brandeis.edu/_files/svg/svgstore.svg#chevron-down">
           </use>
          </svg>
         </span>
        </div>
        <div class="form__select__text" id="selecttext">
         Library Catalog
        </div>
        <select id="tab" name="tab" onblur="$('#selecttext').removeClass('focusring');" onchange="selectChange()" onfocus="$('#selecttext').addClass('focusring');">
         <option class="dropdown__item" selected="selected" value="everything">
          All OneSearch
         </option>
         <option class="dropdown__item" value="alma">
          Library Catalog
         </option>
         <option class="dropdown__item" value="pci">
