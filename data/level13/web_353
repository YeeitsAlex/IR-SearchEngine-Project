    <div class="widget">
     <div class="widget__heading">
      Today’s Hours
     </div>
     <div id="todayshours">
     </div>
     <script type="text/javascript">
      // <![CDATA[
$(document).ready(function() {
  var libcalHours = "https://api3.libcal.com/api_hours_today.php?iid=3115&lid=6858&format=json&callback=?";
  $.getJSON(libcalHours, function(data){
    $.each(data.locations, function(i,hours){
      $("#todayshours").append('<p><strong>' + hours.name + '</strong><br/>' + hours.rendered + '</p>');
    });
  });
});
// ]]>
     </script>
