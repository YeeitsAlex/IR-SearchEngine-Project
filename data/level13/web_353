    <script type="text/javascript">
     // <![CDATA[
$( document ).ready(function() { selectChange(); }); 

function selectChange() { 
    $("#selecttext").text($("#tab option:selected").text()); 
}

      function searchPrimo() {
             document.getElementById("query").value = "any,contains," + document.getElementById("primoQuery").value;
             document.forms["searchForm"].submit();
      }
      
      function searchKeyPress(e) {
            if (typeof e == 'undefined' && window.event) {
                  e = window.event;
            }
            if (e.keyCode == 13) {
                  document.getElementById('go').click();
            }
      }
// ]]>
    </script>
    <div class="feed-group" id="listFeed1">
     <div class="feed">
      <img alt="Four students sitting at a table in the library" class="align-right align-small align-zero" src="_images/students-at-table.jpg"/>
      <div class="feed__text feed__text--alt">
       <div class="h6">
        <a href="research/index.html">
         Start Your Research
        </a>
       </div>
       <div id="libchat_8695fcdfdfdf1ec2b50aabde6d1afd61">
       </div>
       <p>
        The Brandeis Library supports you throughout all stages of your research. Learn more about research help and find scholarly resources below.
       </p>
       <p>
        <a class="button" href="https://search.library.brandeis.edu/">
         OneSearch
        </a>
