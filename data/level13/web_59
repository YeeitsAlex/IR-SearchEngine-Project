   <!-- BEGIN content area -->
   <section class="bluebox-content-wrap" role="main">
    <!-- BEGIN page heading wrap -->
    <header class="page-heading-wrap">
     <div class="bluebox-container clearfix">
      <h1>
       Email &amp; Calendaring
      </h1>
     </div>
    </header>
    <!-- END page heading  wrap-->
    <div class="bluebox-container">
     <div class="row-fluid">
      <div class="main-content span8" id="main-content">
       <div class="bluebox-builder-row last-row">
        <div class="row-fluid bluebox-container">
         <div class="gumm-layout-element page-content-layout-element span12" id="page-content-element-5c84b4549aa2c">
          <div class="row-fluid ">
           <div class="pf-content">
            <p>
             The Email &amp; Calendaring service, powered by Office 365 provides reliable and cost-effective collaboration services. It is useful for email communication, calendaring, IM/video conferencing and access to Office applications, like Word, Excel, PowerPoint and OneNote.
            </p>
            <hr/>
            <h4>
             <strong>
              Requirements &amp; Considerations
             </strong>
            </h4>
            <p>
             Access requires a faculty, staff, student, or alumni role and an activated WUSTL Key.
            </p>
           </div>
          </div>
         </div>
        </div>
       </div>
      </div>
      <!-- .main-content end -->
      <div class="sidebar span3 right-sidebar">
       <div class="widget_text widget-wrap">
        <div class="textwidget custom-html-widget">
         <a class="button type-1-3 small " href="https://outlook.office.com/owa/?realm=wustl.edu">
          Login to Office 365
          <span class="icon-chevron-right">
          </span>
         </a>
        </div>
       </div>
       <div class="widget-wrap">
        <div class="menu-email-calendaring-container">
         <ul class="menu" id="menu-email-calendaring">
          <li class="menu-item menu-item-type-post_type menu-item-object-page current-menu-item page_item page-item-8365 current_page_item menu-item-has-children menu-item-8374" id="menu-item-8374">
           <a href="https://it.wustl.edu/services/email-calendaring/">
            Email &amp; Calendaring
           </a>
           <ul class="sub-menu">
            <li class="menu-item menu-item-type-post_type menu-item-object-page menu-item-8375" id="menu-item-8375">
             <a href="https://it.wustl.edu/services/email-calendaring/features-options/">
              Features &amp; Options
             </a>
            </li>
            <li class="menu-item menu-item-type-post_type menu-item-object-page menu-item-8376" id="menu-item-8376">
             <a href="https://it.wustl.edu/services/email-calendaring/service-level-expectations/">
              Service Level Expectations (SLE)
             </a>
            </li>
            <li class="menu-item menu-item-type-post_type menu-item-object-page menu-item-8377" id="menu-item-8377">
             <a href="https://it.wustl.edu/services/email-calendaring/rates/">
              Rates
             </a>
            </li>
            <li class="menu-item menu-item-type-post_type menu-item-object-page menu-item-8792" id="menu-item-8792">
             <a href="https://it.wustl.edu/services/email-calendaring/training-how-to/">
              Training &amp; How-To
             </a>
            </li>
            <li class="menu-item menu-item-type-post_type menu-item-object-page menu-item-9211" id="menu-item-9211">
             <a href="https://it.wustl.edu/services/email-calendaring/service-status/">
              Service Status
             </a>
            </li>
           </ul>
          </li>
         </ul>
        </div>
       </div>
