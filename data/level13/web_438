<script>
 jQuery(document).ready(function() {
        // init
        var controller = new ScrollMagic.Controller();

        // ====================================
        // detect if on a touch or mouse device

        // is there touch?
        window.addEventListener('touchstart', function onFirstTouch() {

            //if so, run this!
            //alert('touch!');
            window.touch = true;

            // also, stop checking
            window.removeEventListener('touchstart', onFirstTouch, false);

        }, false);

        // =========================
        // set up ScrollMagic scenes

        var debug_scrollmagic = false;

        var scene_stickynav = new ScrollMagic.Scene({
            triggerElement: "#intro",
            triggerHook: "onLeave",
            offset: jQuery('#intro').height()  //offset so animation happens after the intro is scrolled out
        })
            .setClassToggle("#stickynav", "fixed");
        if(debug_scrollmagic) {
            scene_stickynav.addIndicators() // add indicators (requires plugin)
        }
        scene_stickynav.addTo(controller);

        jQuery('.feature').each(function(index, element) {
            var scene = new ScrollMagic.Scene({
                triggerElement: '#' + jQuery(element).attr('id'),
                triggerHook: "onLeave",
                duration: "91%",
                offset: "-69"
            })
                .setClassToggle("#nav" + (index + 1), "active");

            if(debug_scrollmagic) {
                scene.addIndicators() // add indicators (requires plugin)
            }

            scene.addTo(controller);
        });
    });
</script>

