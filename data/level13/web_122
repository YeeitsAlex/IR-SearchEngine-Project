                </td>
               </tr>
              </tbody>
             </table>
            </div>
           </div>
          </div>
         </div>
         <div class="large-8 medium-7 small-12 columns calendar-column">
         </div>
        </div>
       </div>
      </div>
     </div>
     <script>
      // this is a mini calendar javaScript used to show in a panel
// the click feature has been disabled and mouseover is used


  // add a full calendar link to the bottom of the mini calendar
  $('.calendar-calendar').append("<div class='full-calendar'><a href='/calendar'>See full calendar</a></div>");
  // add the class first to the first event to make it visible later
  $('.today .mini-day-on a').next('.calendar_tooltips').addClass('today');

  // for each day with an event move the event information into the right column
  $('.mini-day-on a').each(function(){
    var calendarId = $(this).closest('td').attr("id");
    $(this).next('.calendar_tooltips').appendTo('.calendar-column').addClass(calendarId);
    $('.calendar_tooltips.today').addClass('active');
  });

  // on click make the calendar day appear but disable the links on the calendar
  $('.mini-day-on').on('click mouseover', function(event){
    event.preventDefault(); //this may get deprecated in jquery 2.0 and changed to defaultPrevented
    var calendarDate = $(this).closest('td').attr('id');
    $('.calendar-calendar').find('.has-events').removeClass('active');
    $('#'+calendarDate).addClass('active');
    $('.calendar_tooltips').removeClass('active');
    $('.calendar_tooltips'+'.'+calendarDate).addClass('active');
    $('.calendar-column').addClass('mobile');
  });

  $('.calendar-column').append("<div class='back-button'><a href='#'>&#10006;</a></div>");

  
    // disable the month from being clicked
  $('.date-heading h3 a').on('click', function(event){
    event.preventDefault(); //this may get deprecated in jquery 2.0 and changed to defaultPrevented
  });

  // Strip out the current month and year to use later 
  var monthLink = $('.date-heading h3 a').attr('href').split('/');
  var yearMonth = monthLink[5].split('-');
  var month = yearMonth[1];
  var year = yearMonth[0];

 
  // Find out current day of month
  //var currentDay = [];
  var currentDaySingleNum = parseInt($('.calendar-calendar .today').text().trim());
  var currentDayHasEvent =  parseInt($('.calendar-calendar .today.has-events').text().trim());
  var startDayHasEvent = parseInt($('.calendar-calendar .has-events').first().text().trim());

  if (currentDay  < 10) {
    var currentDay = "0" + currentDaySingleNum;
  } else {
    var currentDay = currentDaySingleNum;
  }

  if (startDayHasEvent  < 10) {
    var startDay = "0" + startDayHasEvent;
  } else {
    var startDay = startDayHasEvent;
  }


  // Find all days with events and add to array and count the array
  var hasEvents = [];
  var count = 0;
  $('.calendar-calendar td.has-events').each(function(n){
    hasEvents[n] = parseInt($(this).text().trim());
    count = count + 1;
  });

  // Find how many events occur after today's date and count them
  var count2 = 0;
  $.each(hasEvents, function(n){
    if (hasEvents[n] > currentDay){
      count2 = count2 + 1;
    }
  });

  // Find the difference in counts and use the count to find the next day
  var nextDayCount = count - count2;
  var nextDay = hasEvents[nextDayCount];
  // if (nextDayCount == 0){
  //   var nextDay = hasEvents[0];
  // }

  // Use the variables to determine which day to display by default 

  // If current day (.today) does not exist and there is no start day 
  //(no dates on the month) display no calendar items message
  if (isNaN(currentDay)) {
    if (isNaN(startDay)){
      $('div.calendar-column').append("<div class='calendar_tooltips no-calendar-items active'>There are no calendar items at this time. Please check the full calendar.<br/><br/><a href='/calendar'>See full calendar</a></div>");
    }
    // If current day (.today) does not exist and there is a start day
    // (actual events on the month) then display the first day with an event
    else {
      $('.event_detail_calendar-'+year+'-'+month+'-'+startDay).addClass('active');
      $('#event_detail_calendar-'+year+'-'+month+'-'+startDay).addClass('active');
    }
  }
  // If the current day exists (.today) meaning we are in the current month
  // and (.today) does not have an event, move to the next day with an event and 
  // display it
  else {
    if (isNaN(currentDayHasEvent) && (typeof(nextDay) != "undefined")) {
      $('.event_detail_calendar-'+year+'-'+month+'-'+nextDay).addClass('active');
      $('#event_detail_calendar-'+year+'-'+month+'-'+nextDay).addClass('active');
    }
    else {
      if(typeof(nextDay) === "undefined"){
        $('div.calendar-column').append("<div class='calendar_tooltips no-calendar-items active'>There are no more calendar items this month. Please check the full calendar.<br/><br/><a href='/calendar'>See full calendar</a></div>");
      }
      else {
        $('.calendar-calendar .today').addClass('active');
        $('#event_detail_calendar-'+year+'-'+month+'-'+currentDay).addClass('active');
      }
    }
  }
  $('.calendar-column .back-button').on('click', function(event){
    event.preventDefault();
    $('.calendar-column').removeClass('mobile');
  });
     </script>
    </div>
   </div>
