$(document).ready(function(){
	$("#expand").click(function (){
		$("#subnav").slideToggle("slow");
		$('#expand').hide();
		$('#hide').show();
	});
});

$(document).ready(function(){
	$("#hide").click(function () {
		$("#subnav").slideToggle("slow");
		$('#expand').show();
		$('#hide').hide();
	});
});
