    $('.toggle div').hide();
    $('.toggle h3').hover(

    );
    $('.toggle h3').click(function() {
        var o = $(this).next('div');
        if (o.is(":visible")) {
            $(this).addClass('closed');
            $(this).removeClass('open');
        } else {
            $(this).addClass('open');
            $(this).removeClass('closed');
        }
        $(this).next('div').toggle({
            'effect':'slideDown',
            'duration' : 'fast'

        });
    });
});
