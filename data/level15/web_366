  <script>
   $(document).ready(function() {
  $('[data-toggle="tooltip"]').tooltip({'placement': 'top','html':'true'});
  $('.custom-popover').popover({
    placement: 'top',
    trigger: 'click',
    html: 'true'
  });
  $('.custom-popover').on('keydown', function (event) {
    if (event.which != 13) {
      return;
    }
    $(this).popover("toggle");
  });
  $(document).on('click', '.custom-popover', function (e) {
    e.stopPropagation();
    $('.custom-popover').not($(this)).popover('hide');
  });
  $(document).on('click', function (e) {
    if (($('.popover').has(e.target).length == 0) || $(e.target).is('.close')) {
      $('.custom-popover').popover('hide');
    }
  });
})
  </script>
  <script>
   //<![CDATA[

    var pusher = new Pusher('4f4cd8f18c63ab37b699');
    var channel = pusher.subscribe('status_page_msuservicestatus');
    channel.bind("status_updated", function(data) {
      var spread = parseInt(data.spread);
      var waitUntilReload = Math.floor(Math.random() * spread + 1);
      setTimeout(function(){window.location.reload()}, waitUntilReload * 1000);
    });

//]]>
