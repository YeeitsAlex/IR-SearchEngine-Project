  <script>
   function urlVars()
        {
            var vars = {};
            var parts = window.location.href.replace(/[?#&]+([^=&]+)=([^&#]*)/gi, function (m, key, value) {
                vars[key] = value;
            });
            return vars;
        }
        $(function(){
            $('#demo-bar-footer').show();
            if(urlVars()['powertour'] == '1'){

                $.powerTour({
                    tours: [
                        {
                            trigger:"",
                            startWith:1,
                            easyCancel:false,
                            escKeyCancel:true,
                            scrollHorizontal:false,
                            keyboardNavigation:true,
                            loopTour:false,
                            highlightEndSpeed: 100,
                            onStartTour:function(ui){
                                //mckay.log("Starting tour...");
                                $('#demo-bar-footer').animate({bottom: 0},1000);
                            },
                            onEndTour:function(ui){
                                //mckay.log("Ending tour...");
                                // animate back to the top
                                $('html, body').animate({scrollTop:0}, 1000, 'swing');
                                //$('html, body').animate({scrollLt:0}, 1000, 'swing');

                                // progress meter hide and reset
                                $('#progressmeter').children('#progressmeter-bar').width(0).next().text();

                                // hide bottom bar
                                $('#demo-bar-footer').animate({bottom: '-70px'},1000);
                            },
                            onProgress:function(ui){
                                var i = ui.stepIndex;
                                var total   = ui.totalSteps;
                                var barSize = 100 / total * i+'%';

                                // progress meter
                                $('#progressmeter-text').html('<span>'+i+'</span> / '+total+'').prev('#progressmeter-bar').animate({width: barSize},400);
                            },
                            steps: [
                                {
                                    hookTo:"body",
                                    content:"#virtual-library-tour-welcome",
                                    width:400,
                                    position:"tm",
                                    offsetY:-400,
                                    offsetX:0,
                                    fxIn:"fadeIn",
                                    fxOut:"fadeOut",
                                    showStepDelay:0,
                                    center:"step",
                                    scrollSpeed:400,
                                    scrollEasing:"swing",
                                    scrollDelay:0,
                                    timer:"00:10",
                                    highlight:true,
                                    keepHighlighted:true,
                                    keepVisible:false,
                                    onShowStep:function(ui){
                                        $('#controls2').delay(500).fadeIn(500);
                                        $('#virtual-library-tour-welcome').show();
                                    },
                                    onHideStep:function(ui){
                                        $('#highlightstep, #anycontent, #highlighthook, #controls2').fadeOut(500);
                                        $('#virtual-library-tour-welcome').hide();
                                    }
                                },
                                {
                                    hookTo:".virtual-library-tour-main-search",
                                    content:"#virtual-library-tour-main-search",
                                    width:400,
                                    position:"bm",
                                    offsetY:20,
                                    offsetX:0,
                                    fxIn:"fadeIn",
                                    fxOut:"fadeOut",
                                    showStepDelay:0,
                                    center:"step",
                                    scrollSpeed:400,
                                    scrollEasing:"swing",
                                    scrollDelay:0,
                                    timer:"00:10",
                                    highlight:true,
                                    keepHighlighted:true,
                                    keepVisible:false,
                                    onShowStep:function(ui){
                                        $('#virtual-library-tour-main-search').show();
                                    },
                                    onHideStep:function(ui){
                                        $('#virtual-library-tour-main-search').hide();
                                    }
                                },
                                {
                                    hookTo:".virtual-library-tour-chat",
                                    content:"#virtual-library-tour-chat",
                                    width:400,
                                    position:"rm",
                                    offsetY:20,
