  <script>
   $(function(){

            if (typeof(Storage) !== "undefined" && sessionStorage['toast'] != 'false' && !mckay.isMobile()) {
                setTimeout(function(){
                    $('.toast').toast({autohide: false}).toast('show');
                }, 1000);
            }else{
                $('.toast').remove();
            }

            $(document).on('click', '.toast .close', function(){
                ga('send', 'event', 'Toast', 'close', 'Toast Closed');
                if (typeof(Storage) !== "undefined") {                    sessionStorage['toast'] = 'false';
                } else {
                    // Sorry! No Web Storage support..
                }
            });

            $(document).on('click', '.toast a', function(){
                ga('send', 'event', 'Toast', 'click', 'Toast Link Clicked');
            });
        });
  </script>
