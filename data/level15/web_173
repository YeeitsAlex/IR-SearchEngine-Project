     <script type="text/javascript">
      // <![CDATA[
	function submitForm()
{
    var urlParam = "query="+document.getElementById("query").value;
    var URL = "http://www.fresnostate.edu/search";
    var encordedUrl = URL+"?"+encodeURI( urlParam )+"&applied_filters=&col=8&page=1&pagesize=10&sort=relevance&sortdir=desc&tune.0=100";
    document.location.href = encordedUrl;
}
// ]]>
     </script>
     <input aria-label="Search" class="searchbox" id="query" name="query" placeholder="Search" type="text"/>
     <input alt="submit" class="searchicon" id="searchsubmit" src="http://www.fresnostate.edu/omniresources/responsive/images/search_icon.png" type="image">
     </input>
    </form>
