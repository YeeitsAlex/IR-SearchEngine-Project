    <script>
     var bgcount = 24;
        function changebg() {
        var num =  Math.ceil( Math.random() * bgcount );
        //document.body.background = 'images/random-'+num+'.jpg';
        document.getElementById('randomize-banner-images').style = 'background: url("/main_page/images/home-banner-random-'+num+'.jpg");background-repeat:no-repeat;';
        }

        
            $(document).on('click', '#nextButton',function( event ) {
                event.preventDefault();
                window.location.href = $(this).attr('href') +'&swoof=1' + '&pa_degree-level='+$('select[name="pa_degree-level"] option:selected').val() + '&product_cat='+$('select[name="product_cat"] option:selected').val();

                if ($('#degree_level option:selected').val() == "doctoral"){
                    window.location.href = $(this).attr('href')+'&swoof=1' + '&pa_degree-level='+$('select[name="pa_degree-level"] option:selected').val() + '&product_cat='+$('select[name="product_cat2"] option:selected').val();

                }
                else if ($('#degree_level option:selected').val() == "graduate-certificates"){
                    window.location.href = $(this).attr('href')+'&swoof=1'  + '&pa_degree-level='+$('select[name="pa_degree-level"] option:selected').val() + '&product_cat='+$('select[name="product_cat3"] option:selected').val();

                }
                else if ($('#degree_level option:selected').val() == "graduate-educator-certifications"){
                    window.location.href = $(this).attr('href')+'&swoof=1'  + '&pa_degree-level='+$('select[name="pa_degree-level"] option:selected').val() + '&product_cat='+$('select[name="product_cat4"] option:selected').val();

                }
            });

            $("#category2").hide();
            $("#category3").hide();
            $("#category4").hide();
           
            $("#degree_level").change(function () {
                
                if ($('#degree_level option:selected').val() == ""){
                    $("#category").show();

                }
                else {
                    $("#category").hide();
                    $("#category2").hide();
                    $("#category3").hide();
                    $("#category4").hide();
                }
            });
            
            $("#degree_level").change(function () {
                
                if ($('#degree_level option:selected').val() == "doctoral"){
                    $("#category2").show();

                }
                else if ($('#degree_level option:selected').val() == "masters" || $('#degree_level option:selected').val() == "bachelors"){
                    $("#category").show();
                    $("#category2").hide();
                    $("#category3").hide();
                    $("#category4").hide();
                }
                
            });

            $("#degree_level").change(function () {

                if ($('#degree_level option:selected').val() == "graduate-certificates"){
                    $("#category3").show();
                    
                }
                else if ($('#degree_level option:selected').val() == "masters" || $('#degree_level option:selected').val() == "bachelors"){
                    $("#category").show();
                    $("#category2").hide();
                    $("#category3").hide();
                    $("#category4").hide();
                }
                
            });
            $("#degree_level").change(function () {
                
                if ($('#degree_level option:selected').val() == "graduate-educator-certifications"){
                    $("#category4").show();
                }
                else{
                    $("#category4").hide();
                }
            });        
            window.addEventListener( "pageshow", function ( event ) {
                var historyTraversal = event.persisted || ( typeof window.performance != "undefined" && window.performance.navigation.type === 2 );
                if ( historyTraversal ) {
                    // Handle page restore.
                    window.location.reload();
                    document.getElementById('category').selectedIndex = 0;
                    document.getElementById('degree_level').selectedIndex = 0;
                }
            });
    </script>
    <script type="text/javascript">
     changebg();
    </script>
   </div>
  </section>
