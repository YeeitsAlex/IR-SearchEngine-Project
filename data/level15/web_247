  <!--progressBar-Ash-Feb13-2019-->
  <style type="text/css">
   .progress-wrap-container{
  width: 100%;
        position: relative;
}
.progress-bar{
    margin: 0 auto;
    width: 100%;
    z-index: 1;
    background: transparent;
}
#progress-bar-graph {
  width: 100%;
    display: inline-block;
    cursor: pointer;
    height: 6px;
    background: #041d37;
    border: none;
    vertical-align: middle;
    z-index: 1;
}

.header{
    border-bottom: 2px solid #eeb211 !important;
}

#progress-bar-graph::-webkit-progress-bar {
  /*Chrome-Safari BG*/
    background-color: transparent;
        border-top-right-radius: 5px;
}

#progress-bar-graph::-webkit-progress-value {
  /*Chrome-Safari value*/
  background-color: #eeb211;
  /*  background: linear-gradient(90deg, #0095c2 0, #bc2d0c);*/
      border-top-right-radius: 5px;
}

#progress-bar-graph::-moz-progress-bar {
  /*Firefox value*/
  background-color: #eeb211;
      border-top-right-radius: 5px;
}

#progress-bar-graph::-ms-fill {
  /*IE-MS value*/
  background-color: #eeb211;
   /* background: linear-gradient(90deg, #0095c2 0, #bc2d0c);*/
}
  </style>
  <div class="progress-wrap" id="progress-wrap">
   <div class="progress-wrap-container">
    <div class="progress-bar">
     <progress id="progress-bar-graph" value="">
     </progress>
    </div>
   </div>
  </div>
  <script type="text/javascript">
   $(document).ready(function(){

$('#progress-wrap').insertAfter($('.b-header__box'));

    document.getElementById('progress-bar-graph').addEventListener('click', function (e) {
        var maxValue = $(document).height() - $(window).height();
            var pos = e.pageX - $(this).offset().left; //Position cursor
            var s = pos * this.max / this.offsetWidth; 
            var a = Math.floor(s);
            $('body,html').animate({scrollTop: a }, 1e3);           
    });
    
    });

$(window).scroll(function(event){
    var scrollHeight = $(window).scrollTop();
        $('#progress-bar-graph').attr('value', scrollHeight);
});
  </script>
  <!--Call content-->
  <!--Program_Selector_dec13-->
  <style>
   @media only screen and (min-width:451px) and (max-width:700px) {
    
    .filter_program {
        text-align: center;
        width: 325px !important;
        padding-left: 20px !important;
        padding: 20px;
        padding-right: auto;
        padding-top: 290px !important;
        margin-left: auto !important;
        margin-right: auto !important;
    }

    .filterMain {
