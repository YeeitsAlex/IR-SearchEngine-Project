$( ".coverimages, #bookcover" ).each(function() {
  if ($(this).find('img').length) {
	var oldSource = $(this).find('img').attr("src");
  var newSource = oldSource.replace("zoom=5" , "zoom=1");
    $(this).find('img').attr("src" , newSource);
    $(".google-books-preview").hide();
  }
});
