});

// CHANGE "CLEAR ALL" TO "UNCHECK ALL" FOR BOOKBAG
$("#basket #CheckNone").text("Uncheck All");



$("#userupdatecontainer label[for='phone']").html("Primary Phone :");
$("#userupdatecontainer label[for='city']").html("City [Skip if using WU or BJC dept address]:");
