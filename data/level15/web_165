// Begin Aeon Integration Code

// Aeon URL to Monograph request form. If we had to deal with multiple types of requests, the URL could be broken down further.
var aeonBase = "https://becker-aeon.wustl.edu/logon/?Action=10&Form=20&Value=GenericRequestMonograph";

// Base item information retrieved from the upper portion of the catalog records
var itemTitle     = $("#catalogue_detail_biblio h1.title").text();
var itemAuthor    = $("#catalogue_detail_biblio h5.author span[property='author']").text();

// Accomodate non-RDA catalog records where publisher information is in th 260 field.
if ($("#catalogue_detail_biblio span.publisher").length > 0) {
  console.log("this is a 260 field record.");
  var publisherInfo = [];
  // publisherInfo["place"]     = $(this).find("span[property='location']").text();
  // publisherInfo["publisher"] = $(this).find("span[property='name']").text();
  // publisherInfo["year"]      = $(this).find("span[property='datePublished']").text();
  publisherInfo["place"]     = $(this + "span[property='location']").text();
  publisherInfo["publisher"] = $(this + "span[property='name']").text();
  publisherInfo["year"]      = $(this + "span[property='datePublished']").text();
}
// If the tags that indicate the publisher data was in the 260 field, treat as 264.
else {
  console.log("264 record. will attempt to parse");
  var publisherInfo = $("#catalogue_detail_biblio span[class='results_summary']:first").text();

  // Send the publisher information to the parsing function. Will return an associative array.
  publisherInfo = pubParse(publisherInfo);
}
console.log(publisherInfo);
// The showAeonColumn boolean will be used to control the visiblity of the "Request Item" column of the holdings table.
var showAeonColumn = false;

// The following arrays contain lists of various item types, collections, and locations for which we want to generate Aeon request links.
var validItemTypes   = ["ARB Reference", "Archives and Manuscripts", "Book Rare"];
var validCollections = ["Archival Serials", "Rare Medical Periodicals"];
var validLocations   = ["Archives and Rare Books Microform - Level 7"];

// The aeonItems array will hold the list of items[] for each row of the holdings table.
var aeonItems = [];

// store the holdings table data (for the items) in an object.
// pass this data to a function to do the following:
  // iterate through the table object and add table row item data to an item array - along with an index key.
  // At this point there are a couple of options. One would be to write to the dom directly from the function, the other would be to target the specific row with the aeonItems
  // index key.

var holdingsTableRows = $("table#holdingst tbody tr");
var tr = $(holdingsTableRows).eq(0).text();
console.log(holdingsTableRows);
$("table#holdingst thead tr").append('<th class="aeonlink">Schedule a Viewing</th>');
$("table#holdingst tbody tr").each(function(j) {
  var item = [];

  item["itemtype"]   = $(this).find("td.itype").text().trim();
  item["Location"]   = $(this).find("td.location").text().replace(/(Becker Medical Library)|\n/g, '').trim();
  item["CallNumber"] = ($(this).find("td.call_no").length > 0) ? $(this).find("td.call_no").text().replace(/\(Browse shelf\)|\n/g, '').trim() : "";
  item["ItemIssue"]  = ($(this).find("td.copynumber").length > 0) ? $(this).find("td.copynumber").text() : "";
  item["ItemVolume"] = ($(this).find("td.vol_info").length > 0) ? $(this).find("td.vol_info").text() : "";
  item["ItemNumber"] = ($(this).find("td.barcode").length > 0) ? $(this).find("td.barcode").text() : "";
  item["collection"] = $(this).find("td.collection").text();

  var aeonRequestLink = aeonBase + "&ItemNumber=" + encodeURIComponent(item["ItemNumber"]) + "&ItemTitle=" + encodeURIComponent(itemTitle) + "&ItemAuthor=" + encodeURIComponent(itemAuthor) + "&ItemPlace=" + encodeURIComponent(publisherInfo["place"]) + "&ItemPublisher=" + encodeURIComponent(publisherInfo["publisher"]) + "&ItemDate=" + encodeURIComponent(publisherInfo["year"]) + "&Location=" + encodeURIComponent(item["Location"]) + "&CallNumber=" + encodeURIComponent(item["CallNumber"]) + "&ItemVolume=" + encodeURIComponent(item["ItemVolume"]) + "&ItemIssue=" + encodeURIComponent(item["ItemIssue"]);

  console.log(aeonRequestLink);

  aeonItems[j] = item;
  if ((validItemTypes.indexOf(item["itemtype"]) > -1) || (validCollections.indexOf(item["collection"]) > -1) || (validLocations.indexOf(item["Location"]) > -1)) {
    console.log("Item is valid aeon type.");
    showAeonColumn = true;
    $(this).append('<td class="aeonlink"><a href="' + aeonRequestLink + '" target="_blank">Request this Item</td>');
  }
  else {
    console.log("Item is NOT a valid aeon type.");
    $(this).append('<td class="aeonlink"></td>');
  }

})
console.log(aeonItems);
console.log(showAeonColumn);
if (showAeonColumn == false) {
  $(".aeonlink").css("display","none");
}


// A function to parse the publisher information from the catalog. If the element is empty it will be handled by the default condition.
function pubParse(publine) {
  // New array to hold information about the pub data that we will return at the end of the function.
  // Defaults are coded and overwritten as needed.
  var pubData = [];
  pubData["place"] = "";
  pubData["publisher"] = "No publisher information provided in catalog.";
  pubData["year"] = "";

  // Split the publine at colons.
  var pubInfo = publine.split(":");

  // if the length of pubInfo == 3, we can proceed to the next step. Otherwise, we need to do some alternative handling.
  if (pubInfo.length == 3) {
    var publisherAndYear = pubInfo[2].split(",");
    // If the length of publisherAndYear == 2, we can be somewhat confident that the publisher and year can be accurately split.
    // Otherwise, leave pubInfo[2] intact and send pubData["publisher"].
    if (publisherAndYear.length == 2) {
      pubData["place"] = pubInfo[1].trim();
      pubData["publisher"] = publisherAndYear[0].trim();
      pubData["year"] = publisherAndYear[1].trim();
    }
    else {
      pubData["place"] = pubInfo[1].trim();
      pubData["publisher"] = pubInfo[2].trim();
    }
  }

  // If the length of the pubInfo == 2, we know publisher information exists that we cannot accurately parse. Just pass to pubData["publisher"].
  else if (pubInfo.length == 2) {
    pubData["publisher"] = pubInfo[1].trim();
    return pubData;
  }

  // In the unlikely case that the length pubInfo > 3, we know publisher information exists. Combine everything and pass to pubData["publisher"].
  else if (pubInfo.length > 3) {
    pubInfo.shift();
    pubData["publisher"] = pubInfo.join().trim();
    return pubData;
  }

  // This is the return for cases when all publisher information can be parsed completely or when it does not exist at all.
  return pubData;
}


// Encode request components and assemble into query string.
// This is non-functional, but could implemented in the future.
function buildQueryString(rcomponent) {

  var query = [];

  for (var i in rcomponent) {
    if (rcomponent[i] && (rcomponent[i] !== '')) {
      query.push(i + '=' + encodeURIComponent(rcomponent[i]));
    }
  }

  var queryString = query.join('&');

  return queryString;

}

// End Aeon Integration Code

});

