		  highlightPreTag: '',
		  highlightPostTag: ''
		}), /* /source */
		templates: {
		  footer: function () {
			return wp.template('gsd-footer')({
			  label: _.escape('All Results')
			}); /* template */
		  }, /* /footer */
		  suggestion: wp.template(config['tmpl_suggestion'])
		} /* /templates */
	  }); /* /sources.push */

	});  /* /algolia.autocomplete.sources */

//MAY 18 This is where we left off!
	/* Setup dropdown menus */
	jQuery(algolia.autocomplete.input_selector).each(function (i) {
		
	  var $searchInput = jQuery(this);
	  var config = {
		debug: algolia.debug,
		hint: false,
		openOnFocus: true,
		appendTo: 'body',
		templates: {
		  empty: wp.template('autocomplete-empty')
		}
	  };
	  if (algolia.powered_by_enabled) {
		config.templates.footer = wp.template('autocomplete-footer');
	  }

	  /* Instantiate autocomplete.js */
	  var autocomplete = algoliaAutocomplete($searchInput[0], config, sources)
	  .on('autocomplete:selected', function (e, suggestion, datasetName) {
		/* Redirect the user when we detect a suggestion selection. */
		window.location.href = suggestion.permalink;
	  }).on('autocomplete:opened', function() {
	  	//Inline z-index style from plugin was hiding the dropdown menu
	  	jQuery('.algolia-autocomplete').css('z-index', 9999)
	  });

		});

	/* This ensures that when the dropdown overflows the window, Thether can reposition it. */
	jQuery('body').css('overflow-x', 'hidden');



	}); // /jQuery(function() ..)
   </script>
   <!-- /copied from algolia's library -->
  </meta>
 </head>
