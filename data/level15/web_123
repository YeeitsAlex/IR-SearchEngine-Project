                        });
                        wpPostsCarousel875496869.on("mousewheel", ".owl-stage", function(e) {
                if (e.deltaY > 0) {
                    wpPostsCarousel875496869.trigger("next.owl");
                } else {
                    wpPostsCarousel875496869.trigger("prev.owl");
                }
                e.preventDefault();
            });
