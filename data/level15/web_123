	jQuery("body").bind("DOMNodeInserted",function(event){
			var textNode = jQuery(event.target);
			var id = jQuery(textNode).attr('id');
			var atrname = jQuery(textNode).attr('name');

			if(id == 'fb_xdm_frame_http'){
				console.log("removing Presentational Attributes for: " + atrname );
				jQuery(textNode).removeAttr("frameborder");
				jQuery(textNode).removeAttr("allowtransparency");
				jQuery(textNode).removeAttr("allowfullscreen");
				jQuery(textNode).removeAttr("scrolling");
				jQuery(textNode).removeAttr("tabindex");
				jQuery(textNode).removeAttr("width");
