   <script>
    jQuery('.fusion-menu > .menu-item > a').focus(function(){
		  	jQuery('.menu-item-has-children > a').removeClass('hover');
		  	jQuery(this).addClass('hover');
	  });

	  jQuery('.fusion-menu > .menu-item > a').keydown(function(e){

	  		var toFocus = false;
			var link = jQuery(this);

			switch(e.keyCode) {
				case 39: // left arrow
					// Make sure to stop event bubbling
                e.preventDefault();
                e.stopPropagation();

                // This is the first item in the top level mega menu list
                if(link.parent('li').prevAll('li').filter(':visible').first().length == 0) {
                    // Focus on the last item in the top level
                    link.parent('li').nextAll('li').filter(':visible').last().find('a').first().focus();
                } else {
                    // Focus on the previous item in the top level
                    link.parent('li').prevAll('li').filter(':visible').first().find('a').first().focus();
                }
                break;

				case 38: /// up arrow
                // Find the nested element that acts as the menu
                var dropdown = link.parent('li').find('.menu, .sub-menu');

                // If there is a UL available, place focus on the first focusable element within
                if(dropdown.length > 0){
                    e.preventDefault();
                    e.stopPropagation();

                    dropdown.find('a').filter(':visible').first().focus();
                }

                break;

				case 37: // right arrow
                // Make sure to stop event bubbling
                e.preventDefault();
                e.stopPropagation();

                // This is the last item
                if(link.parent('li').nextAll('li').filter(':visible').first().length == 0) {
                    // Focus on the first item in the top level
                    link.parent('li').prevAll('li').filter(':visible').last().find('a').first().focus();
                } else {
                    // Focus on the next item in the top level
                    link.parent('li').nextAll('li').filter(':visible').first().find('a').first().focus();
                }
                break;

				case 40: // down arrow
                // Find the nested element that acts as the menu

				/*e.preventDefault();
			   	jQuery(this).addClass('hover');
			   	var sibling = jQuery(this).siblings('.fusion-megamenu-wrapper');
			   	var child = jQuery(sibling[0]).find('a').first();
			   	jQuery(child).focus();*/
				jQuery(this).addClass('hover');
				var dropdown = link.parent('li').find('.menu, .sub-menu');

                // If there is a UL available, place focus on the first focusable element within
                if(dropdown.length > 0){
                    // Make sure to stop event bubbling
                    e.preventDefault();
                    e.stopPropagation();

                    dropdown.first().find('a').first().focus();
                }
                break;

				case 9:
					jQuery(this).removeClass('hover');
					if(!e.shiftKey){
						var next = link.parent('li').next();
						if(next.length > 0){
							e.preventDefault();
							e.stopPropagation();

							next.find('a').first().focus();
						}
					}
				break;
			}


			/*if ( e.which == 40 ) {
			   e.preventDefault();
			   jQuery(this).addClass('hover');
			   var sibling = jQuery(this).siblings('.fusion-megamenu-wrapper');
			   var child = jQuery(sibling[0]).find('a').first();
			   jQuery(child).focus();
			}

			if ( event.which == 9 ) {
			   event.preventDefault();
			   jQuery(this).parent().next().find('a').focus();
			}*/
	  });

	  jQuery('.fusion-menu .menu > .menu-item > a, .sub-menu  > .menu-item > a').keydown(function(e){

	  		var toFocus = false;
			var link = jQuery(this);

			switch(e.keyCode) {
				case 39: // left arrow
					// Make sure to stop event bubbling
                e.preventDefault();
                e.stopPropagation();

				var dropdown = link.parent('li').next().find('a');

				if(dropdown.length > 0)
					dropdown.focus();
				

				else{
					var sibling = link.closest('li.fusion-megamenu-submenu').next();
					console.log(sibling);
					if(sibling)
						sibling.find('.menu > .menu-item > a').first().focus();

					//link.parent('li.fusion-megamenu-submenu').next().find('a').first().focus();
				}
                break;

				case 38: /// up arrow

                e.preventDefault();
                e.stopPropagation();

				var dropdown = link.parent('li').prev().find('a');
				
				if(dropdown.length > 0)
					dropdown.focus();	
				else if(link.closest('ul').hasClass('sub-menu')){
					var submenu = link.closest('ul.sub-menu');
					submenu.parent('li').find('a').first().focus();	
						
				}

				else{
					var sibling = link.closest('li.fusion-megamenu-submenu').prev();
					console.log(sibling);
					if(sibling)
						sibling.find('.menu > .menu-item > a').last().focus();

					//link.parent('li.fusion-megamenu-submenu').next().find('a').first().focus();
				}

                break;

				case 37: // right arrow
                // Make sure to stop event bubbling
                e.preventDefault();
                e.stopPropagation();

				var dropdown = link.parent('li').prev().find('a');

				if(dropdown.length > 0)
					dropdown.focus();

				else{
					var sibling = link.closest('li.fusion-megamenu-submenu').prev();
					console.log(sibling);
					if(sibling)
						sibling.find('.menu > .menu-item > a').last().focus();

					//link.parent('li.fusion-megamenu-submenu').next().find('a').first().focus();
				}
				
				
                break;

				case 40: // down arrow
                // Find the nested element that acts as the menu

				/*e.preventDefault();
			   	jQuery(this).addClass('hover');
			   	var sibling = jQuery(this).siblings('.fusion-megamenu-wrapper');
			   	var child = jQuery(sibling[0]).find('a').first();
			   	jQuery(child).focus();*/

				e.preventDefault();
                e.stopPropagation();
				
				if(link.parent('li').find('ul').first().hasClass('sub-menu')){
					var submenu = link.parent('li').find('ul').first();
					submenu.find('a').first().focus();	
						
				}
				else{

					var dropdown = link.parent('li').next().find('a');
	
					if(dropdown.length > 0)
						dropdown.focus();
						
					else if(link.closest('ul').hasClass('sub-menu')){
						var submenu = link.closest('ul.sub-menu');
						submenu.parent('li').next().find('a').first().focus();	
							
					}
					
					else{
						var sibling = link.closest('li.fusion-megamenu-submenu').next();
						console.log(sibling);
						if(sibling)
							sibling.find('.menu > .menu-item > a').first().focus();
	
						//link.parent('li.fusion-megamenu-submenu').next().find('a').first().focus();
					}
				}

                // If there is a UL available, place focus on the first focusable element within
                /*if(dropdown.length > 0){
                    // Make sure to stop event bubbling
                    e.preventDefault();
                    e.stopPropagation();

                    dropdown.first().find('a').first().focus();
                }*/
                break;

				case 9:
					e.preventDefault();
                    e.stopPropagation();

					var selected = jQuery(".hover");
					jQuery(selected).removeClass(".hover");

					selected.parent('li').next().find('a').first().focus();

				break;
			}


			/*if ( e.which == 40 ) {
			   e.preventDefault();
			   jQuery(this).addClass('hover');
			   var sibling = jQuery(this).siblings('.fusion-megamenu-wrapper');
			   var child = jQuery(sibling[0]).find('a').first();
			   jQuery(child).focus();
			}

			if ( event.which == 9 ) {
			   event.preventDefault();
			   jQuery(this).parent().next().find('a').focus();
			}*/
	  });

	  jQuery('.fusion-megamenu-wrapper a').keydown(function(event){
			if ( event.which == 27 ) {
				event.preventDefault();
				jQuery(".hover").removeClass('hover');
			}
	  });
	  

	  jQuery('.fusion-menu > .menu-item > a').hover(function(){
		  	jQuery('.menu-item-has-children > a').removeClass('hover');
	  });

	  /*jQuery('.menu-item-has-children > a').blur(function(){
		  	var sibling = jQuery(this).siblings('.fusion-megamenu-wrapper');
			jQuery(sibling).children('a').first().focus();
	  });*/
   </script>
  </link>
