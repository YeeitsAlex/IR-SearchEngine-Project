             <script language="JavaScript">
              function openRadioWin() {
  radioWin = window.open ("http:///www.gwradio.com", "radioWindow","status=0,toolbar=0,width=720,height=600,location=0,menubar=0");
  radioWin.focus();
  return false;
}
             </script>
