	}
	function openBuildingInfo(bldg_code) {
		newWin = window.open('/mod/calendar/map_it.cfm?BLDG_CODE='+bldg_code, 'bldg_info', 'width=300,height=400,toolbar=no,menubar=no,location=no,scrollbars=yes,resizable=yes');
		newWin.focus();
		return false;
	}
