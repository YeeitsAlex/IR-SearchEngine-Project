    <script>
     //
  $("a.category-item-section").click(function(event){
    event.preventDefault();
    $("a.category-item-section").removeClass('active').addClass('inactive');
    $(this).addClass('active').removeClass('inactive');
    $(".category-people-wrapper").removeClass("active").addClass("inactive");
    var showDescription = $(this).attr("href");
    $(".category-item-header-title").removeClass("active").addClass("inactive");
    $(".category-item-header-title." + showDescription).removeClass("inactive").addClass("active");

    var disciplineClass = '';

    if ($(this).hasClass("animal")) {
      var species = $(this).children('.category-item-title').html().trim();
      $.ajax({url: "https://ws.engr.illinois.edu/directory/areas.asp?unit=1538&haspeople=1&template=307&areaname=" + species, success: function(result) {
        result = result.replace(/species/g, species);

        var pos = 0;
        var i = -1;
        var originalText = '';
        var encodedText = '';

        while (pos != -1) {
          pos = result.indexOf('area/', i + 1);
          if (pos != -1) {
            originalText = result.substring(pos + 5, result.indexOf('"', pos + 5));
            encodedText = encodeURIComponent(originalText);
            encodedText = encodedText.replace('%2F', '/');
            result = result.replace(originalText, encodedText);
            i = pos;
          }
        }

        // For mobile, use the links to go to a new page
        $('#p-m-' + showDescription).html(result);

        // For desktop, show the people in place
        pos = 0;
        originalText = '';
        var newText = '';
        var url = [];

        while (pos != -1) {
          pos = result.indexOf('href="/');
          if (pos != -1) {
            originalText = result.substring(pos + 6, result.indexOf('"', pos + 6));
            url = originalText.split('/');
            disciplineClass = url[4].replace(/\%20/g, '');
            disciplineClass = disciplineClass.replace(/\%2C/g, '');
            newText = 'javascript:showPeople(' + "'" + url[3] + "', '" + url[4] + "')" + ';" class="category-people-link inactive ' + disciplineClass + '"';
            result = result.replace(originalText, newText);
          }
        }

        $('#p-' + showDescription).html(result);
        window.location.hash = "category-content-one";
      }});
    }
    else if ($(this).hasClass("discipline")) {
      var discipline = $(this).children('.category-item-title').html().trim();
      $.ajax({url: "https://ws.engr.illinois.edu/directory/list.asp?unit=1538&haspeople=1&template=308&areaname=" + discipline, success: function(result) {
        result = result.replace(/document.write\(\'/g, '');
        result = result.replace(/\'\);/g, '');
        result = result.replace(/\\/g, '');
        result = result.replace('class="row"', '');
        result = result.substring(0, result.indexOf('function ShowTab('));
        console.log(result);
        $(".category-people-discipline-wrapper").html(result);
        $(".category-people-discipline-wrapper").addClass("inactive").removeClass("active");
      }});

      window.location.hash = "category-content-two";
    }

    $(".category-item-description, .category-item-list").addClass("inactive").removeClass("active");
    $(".category-item-description."+showDescription).addClass('active').removeClass('inactive');
    $(".category-item-list."+showDescription).addClass('active').removeClass('inactive');
  });

  $("div.button-group button").click(function(){

    window.location.hash = "category-list";

    $("div.button-group button").removeClass('active').addClass('inactive');
    $(this).addClass("active").removeClass("inactive");
    var buttonValue = $(this).text();
    if (buttonValue === "Animals") {
      $(".category-one-section").addClass("active").removeClass("inactive");
      $(".category-two-section").addClass("inactive").removeClass("active");
      $(".category-people-discipline-wrapper").html('');
      $(".category-people-discipline-wrapper").addClass("inactive").removeClass("active");
    }
    else {
      $(".category-two-section").addClass("active").removeClass("inactive");
      $(".category-one-section").addClass("inactive").removeClass("active");
      $(".category-people-discipline-wrapper").addClass("inactive").removeClass("active");
    }

  });

  $(".discipline-button").click(function(event){
    $(".category-people-discipline-wrapper").addClass("active").removeClass("inactive");
  });

  $(".m-discipline-button").click(function(event){
    $(".category-people-discipline-wrapper").addClass("active").removeClass("inactive");
  });

  function showPeople(species, area) {
    $.ajax({url: "https://ws.engr.illinois.edu/directory/list.asp?unit=1538&id=$path[1]&mainareaname=" + species + "&areaname=" + area + "&template=308", success: function(result) {
      result = result.replace(/document.write\(\'/g, '');
      result = result.replace(/\'\);/g, '');
      result = result.replace(/\\/g, '');
      result = result.replace('class="row"', '');
      result = result.substring(0, result.indexOf('function ShowTab('));

      $(".category-people-wrapper").html(result);
      $(".category-people-wrapper").addClass("active").removeClass("inactive");
      $(".category-people-link").addClass("inactive").removeClass("active");
      disciplineClass = area.replace(/\s/g, '');
      disciplineClass = disciplineClass.replace(/,/g, '');
      $(".category-people-link." + disciplineClass).addClass("active").removeClass("inactive");
    }});
  };


  // trigger by event
  $('.small-12 .category-description-wrapper a.reveal-link').trigger('click');
  $('.small-12 .category-description-wrapper a.close-reveal-modal').trigger('click');
    </script>
   </div>
  </div>
