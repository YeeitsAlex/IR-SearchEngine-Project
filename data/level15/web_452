       $(document).ready(function() {

                    $('.news').empty();
                    $.get('/scripts/press/customfeedJSON.asp?home=1', function(data) {
                    
                    $.each(data, function(i, item){
                        if (i == 0){
                            $('.news').append('<div class="col photo-caption-box aside-box cu12-mobile cu6-mini cu4-tablet dark-gray"><div class="no-icons"><a href="' + item.link + '"><img src="/_graphics/slices/transNews.gif" alt="' + item.ImgAlt + '" data-original="' + item.thumb + '" /></a></div><div class="margin-block"><h2 class="news-article-title"><a href="' + item.link + '">' + item.title + '</a></h2></div></div>');
                            }
                        else{
                            $('.news').append('<div class="col photo-caption-box aside-box cu12-mobile cu6-mini cu4-tablet dark-gray"><div class="no-icons"><a href="' + item.link + '"><img src="/_graphics/slices/transNews.gif" alt="' + item.ImgAlt + '" data-original="' + item.thumb + '" /></a></div><div class="margin-block"><h2 class="news-article-title"><a href="' + item.link + '">' + item.title + '</a></h2></div></div>');
                            }

                })
                    }).done(function(){

                    $('.news').sliderific({
                        forceHeight : true,
                        webkitFix : true,
                    inside: 1
                        } );
                    $(".news-panel [data-original]").show().lazyload({
                        effect : "fadeIn"
                        });
                    });
                });
      </script>
