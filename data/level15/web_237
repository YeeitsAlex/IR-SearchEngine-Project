         var system = {"name":"LibGuides"};
                    springSpace.publicObj = new springSpace.public.Public({
                        constant: {
                            PROCESSING: {
                                
                            },
                            CONTENT: {
                            }
                        },
                        system_name: system.name,
                        customer: {"name":"Michigan State University Libraries"}
                    });
					springSpace.Session.CONST.ACTION_AUTH_SET_LG_SESSION_COOKIE = 750;
					jQuery(function() {
						springSpace.Session.getLGSessionStatus({
							url: "https://msu.libapps.com/libapps/login_process.php",
							data: {
								action: 5,
								app_id: 1,
								domain: "libguides.lib.msu.edu",
								site_id: 1474
							}
						});
					});
				jQuery(function() {
					setInterval(function() {
						springSpace.Session.getLGSessionStatus({
							url: "https://msu.libapps.com/libapps/login_process.php",
							data: {
								action: 5,
								app_id: 1,
