        <div class="block-inner clearfix">
         <div class="block-content content no-title">
          <p>
           <img alt="The Great Dome winter image" src="/sites/default/files/images/Winter_Dome_large_web.jpg"/>
          </p>
          <div id="st-safari-extension-is-installed">
          </div>
          <div id="st-safari-extension-is-installed">
          </div>
          <div id="st-safari-extension-is-installed">
          </div>
         </div>
        </div>
       </div>
      </div>
     </div>
    </div>
    <div id="footer-wrapper">
     <div class="container clearfix">
      <footer class="clearfix" role="contentinfo">
       <div class="region region-footer">
        <div class="block block-menu-block block-quicklinks-nav" id="block-menu-block-13" role="navigation">
         <div class="block-inner clearfix">
          <div class="block-content content no-title">
           <div class="menu-block-wrapper menu-block-13 menu-name-menu-ie-qfoot parent-mlid-0 menu-level-1">
            <ul class="menu clearfix">
             <li class="first leaf menu-mlid-695 menu-depth-1 menu-item-695">
              <a href="http://web.mit.edu/admissions/" title="">
               <span>
                Admissions
               </span>
              </a>
             </li>
             <li class="leaf menu-mlid-696 menu-depth-1 menu-item-696">
              <a href="http://events.mit.edu/" title="">
               <span>
                Events Calendar
               </span>
              </a>
             </li>
             <li class="leaf menu-mlid-697 menu-depth-1 menu-item-697">
              <a href="http://web.mit.edu/facilities/transportation/parking/visitors/public_parking.html" title="">
               <span>
                Parking
               </span>
              </a>
             </li>
             <li class="last leaf menu-mlid-698 menu-depth-1 menu-item-698">
              <a href="http://commencement.mit.edu/" title="">
               <span>
                Commencement
               </span>
              </a>
             </li>
            </ul>
           </div>
          </div>
         </div>
        </div>
        <div class="block block-block block-social" id="block-block-22">
         <div class="block-inner clearfix">
          <div class="block-content content no-title">
           <ul>
            <li class="facebook">
             Facebook:
             <a href="https://www.facebook.com/MITnews">
              facebook.com/MITnews
             </a>
            </li>
            <li class="twitter">
             Twitter:
             <a href="https://twitter.com/MIT">
              @MIT
             </a>
             ,
             <a href="https://twitter.com/MITevents">
              @MITevents
             </a>
            </li>
            <li class="instagram">
             Instagram:
             <a href="https://instagram.com/MITpics">
              @MITpics
             </a>
            </li>
           </ul>
           <p class="storify">
            <a href="privacystatement">
             <span style="color:#FFFFFF;">
              Institute Events Privacy Statement
             </span>
            </a>
           </p>
          </div>
         </div>
        </div>
        <div class="block block-block block-mitlogo" id="block-block-23">
         <div class="block-inner clearfix">
          <div class="block-content content no-title">
           <p id="divmitlogoh">
            <span class="screenonly">
             <a href="http://web.mit.edu/" title="MIT home">
              <img alt="Massachusetts Institute of Technology" class="nonretina" src="/sites/default/files/css_injector_images_image/mitlogo_232x30.png"/>
              <img alt="Massachusetts Institute of Technology" class="retina" src="/sites/default/files/css_injector_images_image/mitlogo_464x60.png"/>
             </a>
            </span>
            <span class="printonly">
             <img alt="Massachusetts Institute of Technology" src="/sites/default/files/css_injector_images_image/mitlogo_232x30_print.png"/>
            </span>
           </p>
          </div>
         </div>
        </div>
        <div class="block block-block block-javascript" id="block-block-24">
         <div class="block-inner clearfix">
          <div class="block-content content no-title">
           <script type="text/javascript">
            <!--//--><![CDATA[//><!--
/*
 * MIT Institute Events website javascript
 * rjw 8/15
 *
 * Requires:
 *   lightGallery.js              http://sachinchoolur.github.io/lightGallery/
 *   chosen.jquery.js             https://harvesthq.github.io/chosen/
 *   jquery.onfontresize.js       http://www.tomdeater.com/jquery/onfontresize/
 *
 */


jQuery(document).ready(function() {

  var $ = jQuery;

  var sitedomain = window.location.hostname;



  ////
  //// move the sidebar content from the center column to the sidebar
  ////
  //// (note that the field may not exist if it was left empty)
  //// (note that there may be more than one field item)
  ////
  $('.field-name-field-sidetext').each(function() {
    // at least one field item...

    // get sidetext placeholder in sidebar
    //
    $sidetextplaceholder = $('.block-sidetext-placeholder');

    if ($sidetextplaceholder.length) {
      // iterate through sidetext field items
      //
      $(this).find('.field-item').each(function() {

        // get content of field item
        //
        content = $(this).html();

        // clone sidetext placehoder block and change class
        //
        $newsideblock = $sidetextplaceholder.clone();
        $newsideblock.removeClass('block-sidetext-placeholder').addClass('block-sidetext');

        // insert content into new block
        //
        $newsideblock.find('.block-content').html(content);

        // insert new block into sidebar
        //
        $newsideblock.insertBefore($sidetextplaceholder);
      });
    }

    // remove sidetext field from center column
    //
    $(this).remove();

  });

  // remove original sidetext placeholder
  //
  $('.block-sidetext-placeholder').remove();



  ////
  //// remove all other placeholder blocks
  ////
  $('.block-placeholder').remove();



  ////
  //// move base photo block to a new region (this is used on the home page)
  ////
  $basephotoblock = $('.block-basephoto');
  if ($basephotoblock.length > 0) {
    $basephotoregion = $('<div id="base-photo-wrapper"><div class="container clearfix"><div class="region region-base-photo"></div></div></div>');
    $basephotoregion.find('.region').append($basephotoblock);
    $basephotoregion.insertBefore('#tertiary-content-wrapper');
  }



  ////
  //// copy sidebar alert to the features panel
  //// (may not exist, or may be more than one)
  ////
  $('.region-sidebar-second .block-alert').each(function() {
    $(this).clone().appendTo('.region-secondary-content');
    if (!($('body').hasClass('block-sidealert'))) {
      $('body').addClass('block-sidealert');
    }
  });



  ////
  //// photo page HTML rewrites
  ////
  if ($('body').hasClass('page-type-photo')) {

    // prepend text to title
    //
    $('.field-name-field-tax-event .field-item').prepend('Photo: ');

    // prepend text to sequence number
    //
    $('.field-name-field-seqnum .field-item').prepend('Photo ');
  }



  ////
  //// video page HTML rewrites
  ////
  if ($('body').hasClass('page-type-video')) {

    $('.field-name-field-ref-event .field-item a').eq(0).each(function() {

      // get link from reference field and wrap around the event taxonomy term (= page title), and prepend text
      //
      href = $(this).attr('href');
      $('.field-name-field-tax-event .field-item').wrapInner($('<a href="'+href+'"/>')).prepend('Video: ');

    });
  }



  ////
  //// gallery page HTML rewrites
  ////
  if ($('body').hasClass('page-type-gallery')) {

    $('.field-name-field-ref-event .field-item a').eq(0).each(function() {

      // get link from reference field and wrap around the event taxonomy term (= page title), and prepend text
      //
      href = $(this).attr('href');
      $('.field-name-field-tax-event .field-item').wrapInner($('<a href="'+href+'"/>')).prepend('Gallery: ');

    });

    // subheadings over photos and videos
    //
    $('.field-name-field-ref-photos').prepend($('<h2>Photos</h2>'));
    $('.field-name-field-ref-videos').prepend($('<h2>Video'+($('.field-name-field-ref-videos > .field-items > .field-item').length > 1 ? 's' : '')+'</h2>'));
  }



  ////
  //// event page HTML rewrites
  ////
  if ($('body').hasClass('page-type-event')) {

    // subheadings over photos and videos
    //
    $('.field-name-field-ref-photos').prepend($('<h2>Photos</h2>'));
    $('.field-name-field-ref-videos').prepend($('<h2>Video'+($('.field-name-field-ref-videos > .field-items > .field-item').length > 1 ? 's' : '')+'</h2>'));
  }



  ////
  //// eventlist view HTML rewrites
  ////
  if ($('.view-eventlist').length > 0) {

    // if external URL supplied, reroute links to event page
    //
    $('.views-field-field-xurl a').each(function() {

      // get external URL
      //
      var href = $(this).attr('href');

      // apply to link around thumbnail and around event title
      //
      $(this).parents('.views-row').find('.views-field-field-thumbnail a, .views-field-field-tax-event a').attr('href', href);

    });
  }



  ////
  //// basic page HTML rewrites
  ////
  if ($('body').hasClass('page-type-page')) {

    // subheadings over photos and videos
    //
    $('.field-name-field-ref-photos').prepend($('<h2>Photos</h2>'));
    $('.field-name-field-ref-videos').prepend($('<h2>Video'+($('.field-name-field-ref-videos > .field-items > .field-item').length > 1 ? 's' : '')+'</h2>'));
  }



  ////
  //// photo gallery with lightGallery plugin
  //// (assumes just one photo gallery on the page)
  ////

  if ($('.field-name-field-ref-photos').length == 1) {

    // prepare images and captions for lightGallery
    //
    $('.field-name-field-ref-photos > .field-items > .field-item').each(function(i) {

      // place data-src and data-sub-html attributes on the link around the thumbnail
      //
      var $link = $(this).find('.field-name-field-photo-image figure a');
      var href = $link.attr('href');
      $link.attr('data-src', href);
      $link.attr('data-sub-html', '#subhtml'+i);

      // place id on the corresponding caption, and wrap contents in lightGallery-friendly div
      //
      var $caption = $(this).find('.field-name-field-maintext .field-item');
      $caption.attr('id', 'subhtml'+i);
      $caption.wrapInner('<div class="custom-html"><div class="custom-html-inner"></div></div>');

    });

    // attach lightGallery to the photos
    //
    $('.field-name-field-ref-photos').lightGallery( {
                                        selector: '.field-name-field-photo-image figure a', // path down to the link around the thumbnail
                                        mode: 'fade',              // slide or fade
                                        loop: true,                // loop from last image to first
                                        auto: false,               // slideshow
                                        pause: 6000,               // pause on each slide, in ms
                                        counter: true,             // show counter of images
                                        addClass: 'photos',        // add class for theming
                                        xxx: false
                                     });
  }



  ////
  //// gallerylist photo gallery with lightGallery plugin
  //// (each element in view contains its own gallery, so they are dynamically created as needed)
  ////

  if ($('.view-gallerylist').length == 1) {

    // attach click behavior to all rows of view
    //
    $('.view-gallerylist .views-row').click(function() {

      // find the associated set of photos, which are contained in article tags in a photo reference field
      //
      $photoset = $(this).find('.views-field-field-ref-photos article');

      // create array of objects for lightGallery
      //
      var lgobjects = Array();
      $photoset.each(function() {

        // get full size image URL and caption
        //
        var src = $(this).find('.field-name-field-photo-image a').attr('href') || '';
        var thumbsrc = $(this).find('.field-name-field-photo-image img').attr('src') || '';
        var subhtml = $(this).find('.field-name-field-maintext .field-item').html() || '';

        // add to array, only if large image was found
        //
        if (src && thumbsrc) {
          subhtml = '<div class="custom-html"><div class="custom-html-inner">'+subhtml+'</div></div>';
          lgobjects[lgobjects.length] = { 'src': src, 'thumb': thumbsrc, 'sub-html': subhtml };
        }

      });


      // proceed only if associated gallery contains photos
      //
      if (lgobjects.length > 0) {
        $(this).lightGallery({

                  dynamic: true,             // load elements from array
                  dynamicEl: lgobjects,      // array of elements
                  html: true,                // (not sure what this does, if anything)
                  mode: 'fade',              // slide or fade
                  loop: true,                // loop from last image to first
                  auto: false,               // slideshow
                  pause: 6000,               // pause on each slide, in ms
                  counter: true,             // show counter of images
                  addClass: 'photos',        // add class for theming
                  xxx: false

                             });
      }

      return false;  // do not follow link
    });

  }



  ////
  //// videolist video with lightGallery plugin
  //// (each element in view contains its own video, so lightbox is dynamically created as needed)
  ////

  if ($('.view-videolist').length == 1) {

    // attach click behavior to all rows of view
    //
    $('.view-videolist .views-row').click(function() {

      // find the associated iframe
      //
      $iframe = $(this).find('.views-field-field-video-embed iframe');

      // proceed only if iframe found
      //
      if ($iframe.length == 1) {

        // get iframe src
        //
        var src = $iframe.attr('src') || '';

        // if techtv, add autoplay flag
        //
        if (src.indexOf('techtv') != -1) {
          src = src.replace('?','?autoplay=true&');
        }

        var lgobjects = Array({ 'src': src, 'iframe': 'true'  }); // NOTE 'true' as string not as boolean

        $(this).lightGallery({

                  dynamic: true,             // load elements from array
                  dynamicEl: lgobjects,      // array of elements
                  videoMaxWidth: '75%',      // maximum video width
                  videoAutoplay: true,       // autoplay (works only on youtube and vimeo)
                  thumbnail: false,          // show thumbnails button
                  addClass: 'ievideo',       // add class for theming
                  xxx: false

                             });
      }

      return false;  // do not follow link
    });

  }



  ////
  //// eventlist, gallerylist, videolist exposed filters
  ////
  if ($('.view-filters').length > 0) {

    // rearrange year filter
    //
    $('#edit-start-date-value-year, #edit-year-value-year').each(function() {
      var $select = $(this);
      var $allopt;
      $select.children().each(function(i) {
        // the first option is the 'all' selector - it needs to be fixed up a little, and reinserted at the beginning
        if (i == 0) {
          $allopt = $(this).text('all years');
        }
        // reverse all the rest, so the current year is at the top
        else {
          $(this).prependTo($select);
        }
      });
      $allopt.prependTo($select);
    });

    // pull form elements into a manageable layout table
    //
    var $table = $('<table class="layouttable"><tr><td class="query"></td><td class="filter"></td><td class="submit"></td></tr></table>');
    $table.find('.query').prepend($('#edit-keyword'));
    $table.find('.filter').prepend($('#edit-start-date-value-year, #edit-year-value-year'));
    $table.find('.submit').prepend($('.views-submit-button input'));
    $('.view-filters form').empty().prepend($table);

    // transform the select
    //
    //$table.find('.filter select').transformSelect();
    $table.find('.filter select').chosen({   // attach chosen plugin
                     disable_search: true,
                     width: "8em"
    });

  }


  ////
  //// adjust active and active-trail classes in menus
  ////

  // remove all active and active-trail from office (header) and audience (left sidebar) menus
  //
  $('.block-office-nav li, .block-office-nav a, .block-audience-nav li, .block-audience-nav a').removeClass('active active-trail');

  // make sure only one active menu item in secondary nav and in mobile menu, and that all parents are marked active-trail
  //
  $('.block-secondary-nav a.active, .block-sitemap-nav a.active').parent('li').removeClass('active').addClass('active-trail').parents('li').addClass('active-trail').children('a').removeClass('active').addClass('active-trail');



  ////
  //// mobile menu
  ////

  // create container, toggle button, and menu container
  //
 $mobilemenu = $('<div id="divmobmenu"><div id="divmobtoggle"><a href="#" id="mobtogglelink">Menu</a></div><div id="divmoblinks"><div id="divmobmainlinks"></div><div id="divmobquicklinks"></div></div></div>');

  // get main menu (= sitemap menu below content)
  //
  var $mainmenu = $('#tertiary-content-wrapper .block-sitemap-nav .menu-block-wrapper > ul').clone();
  $mobilemenu.find('#divmobmainlinks').append($mainmenu);

  // get quick links menu (from footer)
  //
  var $quicklinksmenu = $('#footer-wrapper .block-quicklinks-nav .menu-block-wrapper > ul').clone();
  $mobilemenu.find('#divmobquicklinks').append($quicklinksmenu);

  // set all li with a child ul to openable but closed
  //
  $mobilemenu.find('#divmoblinks li').children('ul').parent().addClass('openable closed');

  // ...but then open up the active trail
  //
  $mobilemenu.find('#divmoblinks a.active, #divmoblinks a.active-trail').parent().removeClass('closed').addClass('open');

  // create opener/closer buttons
  //
  $mobilemenu.find('#divmoblinks li.openable > a').append('<span class="opener">+</span><span class="closer">&ndash;</span>');


  // toggle mobile menu on/off
  //
  $mobilemenu.find('#divmobtoggle a').on('click', function(e) {
    e.preventDefault();
    $('#divmoblinks').slideToggle();
    $(this).toggleClass('active');
  });

  // toggle submenus on/off
  //
  $mobilemenu.find('span.opener').on('click', function(e) {
    e.preventDefault();
    $(this).parent('a').parent('li').removeClass('closed').addClass('open');
  });
  $mobilemenu.find('span.closer').on('click', function(e) {
    e.preventDefault();
    $(this).parent('a').parent('li').removeClass('open').addClass('closed');
  });

  // insert mobile menu into page
  //
  $('body').prepend($mobilemenu);



  ////
  //// set link classes and open links in new windows
  ////

  // links starting with "http" (but not to this site)
  //
  $('a[href^="http"]').not('[href*="'+sitedomain+'"]').attr('target','_blank').addClass('external');

  // links starting with "mailto"
  //
  $('a[href^="mailto"]').addClass('mailto');

  // links ending with ".pdf"
  //
  $('a[href$=".pdf"]').attr('target','_blank').addClass('pdf');

  // links ending with ".ppt"
  //
  $('a[href$=".ppt"]').attr('target','_blank').addClass('ppt');

  // links ending with ".doc"
  //
  $('a[href$=".doc"]').attr('target','_blank').addClass('doc');



  ////
  //// insert prompt into search box, then
  //// clear search on initial activation
  ////
  var searchtext = 'SEARCH';

  /* fill if empty */
  $('#top-panels-wrapper .block-search .form-text').each( function() {
    if (!this.searchstarted) {
      if (this.value == '') {  // only fill if blank
        this.value = searchtext;
      }
      if (this.value.toLowerCase() != searchtext.toLowerCase()) {  // other value inherited on page reload
        $(this).addClass('active');
        this.searchstarted = true;
      }
    }
  });

  /* check on focus */
  $('#top-panels-wrapper .block-search .form-text').focus( function() {
    if (!this.searchstarted) {
      if (this.value.toLowerCase() == searchtext.toLowerCase()) {  // only clear if it still has original text
        this.value = '';
        $(this).addClass('active');
      }
      this.searchstarted = true;
    }
  });

  ////
  //// change text of search button to "go" (obsolete as of 6/21/18)
  ////
  //$('#block-google-appliance-ga-block-search-form .form-submit').attr('value','Go');



  ////
  //// rewrite links to http to links to https for logged-in users
  ////
  if ($('body').hasClass('logged-in')) {
    $('a[href^="http://'+sitedomain+'"]').each(function() {
      var href = $(this).attr('href');
      href = href.replace('http', 'https');
      $(this).attr('href', href);
    });
  }



  ////
  //// openable content
  ////
  //// Should be set up with links in the form:
  ////   <a class="opener" href="#local">Local hotels</a>
  //// and openable content in the form:
  ////   <div class="openable" id="local">....</div>
  //// (the div may be a span, or any block tag (such as a table) -
  ////  spans are opened to display inline, all others to display block)
  ////
  //// Additional class may be added to the "openable" class:
  ////  <div class="openable addcloser"...
  //// This causes a "close" link to be appended to the openable content.
  //// If the openable content is a span, the closer is also a span;
  ////  otherwise it is a div. So note that this will not work well if the
  ////  openable tag is a table.
  ////
  if ($('a.opener').length) {

    // process each link tagged with 'opener'
    //
    $('a.opener').each(function() {

      // initially closed
      //
      $(this).addClass('closed');

      // append + and - indicators to link text
      //
      $(this).append('<span class="closed"> (+)</span><span class="open"> (&ndash;)</span>');

      // when link clicked, toggle "open" class of both this link and the target content
      //
      $(this).click(function() {
        targetid = $(this).attr('href');
        $(targetid).toggleClass('open');
        $(this).toggleClass('open');
        return false;
      });
    });

    // process each openable element
    //
    $('.openable').each(function() {

      // initially closed
      //
      $(this).addClass('closed');

      // possibly append a "close" link at the end of the element
      //
      var id = $(this).attr('id');
      if ($(this).hasClass('addcloser')) {
        if (this.nodeName == 'SPAN') {
          $(this).wrap('<span class="openable" id="'+id+'" />').removeClass('openable').removeAttr('id').parent().append('<span class="closer"><a href="#'+id+'">Close (&ndash;)</a></span>');
        }
        else {
          $(this).wrap('<div class="openable" id="'+id+'" />').removeClass('openable').removeAttr('id').parent().append('<div class="closer"><a href="#'+id+'">Close (&ndash;)</a></div>');
        }
      }
    });

    // act when the close links we just added are clicked
    //
    $('.closer a').click(function() {

      // get the href, which is also the selector for the content
      //
      targetid = $(this).attr('href')

      // toggle "open" class of element and of the corresponding opener
      //
      $(targetid).toggleClass('open');
      $('a[href="'+targetid+'"].opener').toggleClass('open');

      return false;
    });
  }



  ////
  //// responsive embedded iframes for videos, google maps, etc.
  //// adapted from http://css-tricks.com/NetMag/FluidWidthVideo/demo.php
  ////

  // on this site, include all iframes embedded into content area, unless explicitly requested not to
  var $allVideos = $('.content-style iframe, .region-sidebar-second iframe').not('.noresize');

  // initialize
  //
  $allVideos.each(function() {
    $(this)
      // jQuery .data does not work on object/embed elements
      .attr('data-aspectRatio', this.height / this.width)
      .removeAttr('height')
      .removeAttr('width')
      .wrap('<div class="videowrapper"></div>');

    // attempt to give a helpful style sheet to TechTV videos so they will act responsively
    //
    if ($(this).is('iframe[src*="techtv.mit.edu"]')) {
      var src = $(this).attr('src');
      src = src.replace('http:','');                    // remove http: so that video works through https
      src = src.replace(/size=\w*/,'size=responsive');  // change size to responsive
      src = src.replace('external_stylesheet=','external_stylesheet=//'+sitedomain+'/sites/default/files/css_injector_images_image/techtvresponsive.css');
      $(this).attr('src', src);
    }
  });

  // react to window resize event
  //
  function _videoresize() {
    $allVideos.each(function() {
      var $el = $(this);
      var newWidth = $el.parent().width();
      $el
        .width(newWidth)
        .height(newWidth * $el.attr('data-aspectRatio'));
    });
  }
  $(window).resize(_videoresize);

  // execute initial video sizing
  //
  //_videoresize();  // will be triggered by window resize below



  ////
  //// Monitor for font resize and call window resize when found
  //// Requires jquery.onfontresize.min.js
  ////
  $(document).bind('fontresize', function () {
    $(window).resize();
  });

  $(window).resize(); // kick off resize-triggered events when page loaded

});
//--><!]]>
           </script>
          </div>
         </div>
        </div>
       </div>
      </footer>
     </div>
    </div>
   </div>
  </div>
