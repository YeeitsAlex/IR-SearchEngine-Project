  <script>
   jQuery( document ).ready( function($) {
	$('.bawpvc-ajax-counter').each( function( i ) {
		var $id = $(this).data('id');
		var t = this;
		var n = 0;
		$.get('https://policy.tennessee.edu/wp-admin/admin-ajax.php?action=bawpvc-ajax-counter&p='+$id+'&n='+n, function( html ) {
			$(t).html( html );
		})
	});
});
  </script>
  <script type="text/javascript">
   jQuery(document).ready(function($){

		$('#utk_seek').on('submit', function(event) {
			var search_type = $('select[name="qtype"]').val();
			if (search_type == 'policy') {
				$('#utk_seek').attr('action', 'https://policy.tennessee.edu/policy-search');
			}
		});
	});
  </script>
