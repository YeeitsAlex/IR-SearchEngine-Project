    <script type="text/javascript">
     var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-27049557-1']);
  _gaq.push(['_setDomainName', 'illinois.edu']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = 'https://www.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();
    </script>
    <!--UA-25434923-1 old code-->
    <!--emergency alert code-->
    <script type="text/javascript">
     //messy Safari Hack. Without this hack it does not load the alert system consistently
	if (/WebKit/i.test(navigator.userAgent)) { // sniff
	var _timer = setInterval(function() {
			if (/loaded|complete/.test(document.readyState)) {
				clearInterval(_timer);
				attachWebAlert(); // call the onload handler
			}
		}, 10);
	}

	(function($) {
	  $(document).ready(function() {
		  // Attach Emergency Web Alert System
		  attachWebAlert();
	  });
	})(jQuery);

	function attachWebAlert(){
		var head = document.getElementsByTagName('head')[0];
		var el = document.createElement('script');
		el.setAttribute('type', 'text/javascript');
		//el.setAttribute('src', 'https://emergency.webservices.illinois.edu/test/alert.js'); //testing
		el.setAttribute('src', 'https://emergency.webservices.illinois.edu/illinois.js'); //live
		document.body.appendChild(el);
}
    </script>
