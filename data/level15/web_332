   <!--Begin Script for Random Background based off Admissions-->
   <script type="text/javascript">
    (function( $ ) {
    
    var images = ['front1.png', 'front1.png'];//Name of image files used for the randomization.
    
    $('.BackTAMU').css({'background-image': 'url(/App_Themes/ISS/img/front/' + 
                        images[Math.floor(Math.random() * images.length)] + ')'}); //Inject into the class BackTAMU to select as background image a random image based off our var images.
  })(jQuery);
   </script>
