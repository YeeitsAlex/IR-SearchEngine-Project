    jQuery("#vision-features").on("mouseover", "li", function() {
		var feature = jQuery(this);
		timer = setTimeout(function() {
			feature.find(".vision-feature-intro").show();
		}, 500)
    });
    jQuery("#vision-features").on("mouseleave", "li", function() {
		clearTimeout(timer);
