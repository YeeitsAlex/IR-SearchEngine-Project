            <h2 class="pane-title">
            </h2>
            <div class="pane-content">
             <div class="view view-gw-sidebars-no-article-type view-id-gw_sidebars_no_article_type view-display-id-panel_pane_3 view-dom-id-c7fb1eabc3555966f42218a3996416c3">
              <div class="view-content">
               <div class="views-row views-row-1 views-row-odd views-row-first views-row-last">
                <div about="/review-requirements-holds-academic-progress-cost-attendance-award-history-work-study-information" class="all-purpose-sidebar node node-promotion" id="node-194" typeof="sioc:Item foaf:Document">
                 <div class="image">
                  <a href="https://banweb.gwu.edu/PRODCartridge/twbkwbis.P_WWWLogin" target="_blank">
                   <div class="image-wrapper" style="width:220px;">
                    <img alt="Log into GWeb" height="140" src="https://financialaid.gwu.edu/sites/g/files/zaxdzs2151/f/styles/gw_all_purpose_sidebar_large/public/image/STAC_1314_2%20StudentAccounts_promos_220x140_2b.jpg?itok=-NI4f2sr" typeof="foaf:Image" width="220"/>
                    <span class="play">
                     <span>
                     </span>
                    </span>
                    <div class="clearfix">
                    </div>
                   </div>
                  </a>
                 </div>
                 <h3>
                  <a href="https://banweb.gwu.edu/PRODCartridge/twbkwbis.P_WWWLogin" target="_blank">
                   Review requirements, holds, academic progress, cost of attendance, award history, &amp; work study information.
                  </a>
                 </h3>
                 <!--<p></p>-->
                 <p>
                 </p>
