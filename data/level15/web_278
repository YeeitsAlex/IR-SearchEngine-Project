function randPix(){
  var yy = Math.round(Math.random()*(arrPic.length-1));
	document.write('<img src="'+arrPic[yy]+'"border="0"/>');
	document.write('<br/><br/>');
	document.write(arrText[yy]);
	document.write('<p class="colltext">'+arrText2[yy]+'</p>');
}
