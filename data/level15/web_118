     $(function() {
			$('#formHelp').tooltipster({
				content: $('<p>This form allows you to search the TAMHSC Faculty and Staff Directory.</p><p>The directory searches as you type, so there is no need to click the Search button.</p><p>The directory will only display 100 entries at a time.</p>')
			});
			$('#nameHelp').tooltipster({
				content: $('<p>Searching by name returns anyone who&#39;s name starts with your search value <em>except</em>,<br/>when the search value is less than 3 letters long you will receive exact matches only.</p><p>If you need an explicit wildcard (for example, in the middle of a name), use *.</p>')
			});
			$('#deptHelp').tooltipster({
				content: $('<p>To pick a department, first pick an HSC Unit.</p>')
			});
		});
