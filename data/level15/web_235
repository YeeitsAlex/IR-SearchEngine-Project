<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js" type="text/javascript">
</script>
<script src="/sites/all/themes/ahedu2012/a/j/jquery.masonry.min.js">
</script>
<script src="/sites/all/themes/ahedu2012/a/j/jquery.smartmenus.js">
</script>
<script>
 $(document).ready(function() {
		if ( $('.main .view-list-of-events-masonry-display .hub-tiles').length > 0 ) {
			var $container = $('.hub-tiles');

			$container.imagesLoaded(function(){
				$container.masonry({
					itemSelector: '.masonry-item',
					columnWidth: 235,
					isAnimated: 1,
						animationOptions: {
							duration: 500
						},
						isResizable: 1,
						isFitWidth: 0,
						gutterWidth: 0,
						isRTL: 0
				});
			});
		}
		else if ( $('.hub-tiles').length > 0 ) {
			var $container = $('.hub-tiles');
			
			$container.imagesLoaded(function(){
				$container.masonry({
					itemSelector: '.masonry-item',
					columnWidth: 245,
					isAnimated: 1,
						animationOptions: {
							duration: 500
						},
						isResizable: 1,
						isFitWidth: 0,
						gutterWidth: 0,
						isRTL: 0
				});
			});
		}
    $('#main-menu span.sub-arrow').attr('aria-hidden', true);
    //SmartMenus jQuery init
    $('#main-menu').smartmenus({
      mainMenuSubOffsetY: -1,
      subMenusSubOffsetX: 0,
      subMenusSubOffsetY: -1,
      subMenusMinWidth: '180px',
      subMenusMaxWidth: '30em',
      showTimeout: 50,
      hideTimeout: 10
    });
    $('#search-menu').smartmenus({
      subMenusSubOffsetX: 1,
      subMenusSubOffsetY: -8,
      mainMenuSubOffsetY: -1,
      keepHighlighted: true,
      rightToLeftSubMenus: true,
      subMenusMinWidth: '364px',
      showTimeout: 50,
      hideTimeout: 10
    });

    var ACTIVATING = 'activating';
    var defaultMenuUrl = '/';
    $('#context-menu').smartmenus({
      hideOnClick: false
    });

    $('#context-menu').on('activateItem', function (event, url) {
      // trim query string off url
      if ( url.indexOf("stories")>0 || url.indexOf("clubs-sigs")>0) {
        url = url.split("?")[0];
      }
      var $menuItem = $(this).find("a[href='" + url + "']");
      var menuItemList = $menuItem.parentsUntil('#context-menu').filter('li').children('a').get().reverse();

      menuItemList.forEach(function (el) {
        var $innerMenuItem = $(el);
        $innerMenuItem.parent().dataSM(ACTIVATING, true);
        $('#context-menu').smartmenus('itemActivate', $innerMenuItem.eq(-1));
        $innerMenuItem.parent().removeDataSM(ACTIVATING);
      })

      $menuItem.addClass('current');
    })

    // don't show the sub menus in collapsible mode unless the sub arrow is clicked
    // See: https://www.smartmenus.org/forums/topic/expand-on-icon-go-to-link-from-the-first-click-on-name/
    // for more info
    $('#context-menu').on('click', 'span.sub-arrow', function (e) {
      var obj = $('#context-menu').data('smartmenus');
      if (obj.isCollapsible()) {
        var $item = $(this).parent(),
          $sub = $item.parent().dataSM('sub');
        $sub.dataSM('arrowClicked', true);
      }
    }).bind({
      'beforeshow.smapi': function (e, menu) {
        var obj = $('#context-menu').data('smartmenus');
        if (obj.isCollapsible()) {
          var $menu = $(menu);
          if (!$menu.dataSM('arrowClicked') && !$menu.parent().dataSM(ACTIVATING)) {
            return false;
          }
          $menu.removeDataSM('arrowClicked');
        }
        return true;
      }
    });

    // Set up fly-out menu for keyboard control
    $('#context-menu span.sub-arrow').attr('tabindex', 0);
    $('#context-menu span.sub-arrow').attr('role', 'button')
    $('#context-menu span.sub-arrow').keypress(function (e) {
      if (e.keyCode === 0 || e.keyCode === 32  || e.keyCode === 13) {
        $(e.currentTarget).click();
      }
    });

    if (typeof defaultMenuUrl != 'undefined') {
      $('#context-menu').trigger('activateItem', defaultMenuUrl);
    }

    $('#mobile-menu').smartmenus({hideOnClick: false});

    if (typeof defaultMenuUrl != 'undefined') {
      var t_url;
      if (defaultMenuUrl.indexOf("stories")>0 || defaultMenuUrl.indexOf("clubs-sigs")>0) {
        t_url = defaultMenuUrl.split("?")[0];
      } else {
        t_url = defaultMenuUrl;
      }
      var el = $('#mobile-menu').find("a[href='" + t_url + "']");
      $('#mobile-menu').smartmenus('itemActivate', el);
      el.addClass('current');
    }

    // don't show the sub menus in collapsible mode unless the sub arrow is clicked
    // See: https://www.smartmenus.org/forums/topic/expand-on-icon-go-to-link-from-the-first-click-on-name/
    // for more info
    
    $('#mobile-menu').on('click', 'span.sub-arrow', function (e) {
      var obj = $('#mobile-menu').data('smartmenus');
      if (obj.isCollapsible()) {
        var $item = $(this).parent(),
          $sub = $item.parent().dataSM('sub');
        $sub.dataSM('arrowClicked', true);
      }
    }).bind({
      'beforeshow.smapi': function (e, menu) {
        var obj = $('#mobile-menu').data('smartmenus');
        if (obj.isCollapsible()) {
          var $menu = $(menu);
          if (!$menu.dataSM('arrowClicked') && !$menu.parent().dataSM(ACTIVATING)) {
            return false;
          }
          $menu.removeDataSM('arrowClicked');
        }
        return true;
      }
    });

    
    // Set up fly-out menu for keyboard control
    $('#mobile-menu span.sub-arrow').attr('tabindex', 0);
    $('#mobile-menu span.sub-arrow').attr('role', 'button')
    $('#mobile-menu span.sub-arrow').keypress(function (e) {
      if (e.keyCode === 0 || e.keyCode === 32  || e.keyCode === 13) {
        $(e.currentTarget).click();
      }
    });

 	// use the whole parent item as sub menu toggle button
    $('#search-menu').bind('click.smapi', function(e, item) {
        var obj = $(this).data('smartmenus');
            var $sub = $(item).dataSM('sub');
            if ($sub && $sub.is(':visible')) {
                obj.menuHideAll();
                return false;
            }
    });

	});
</script>
