  <script>
   //  ONE STOP QUEUE INTEGRATION
          $(document).ready(function ($) {
          //OnLoad make initial call to get the Queue.
            $.ajax({
                url: 'https://webapps.utk.edu/OneStop/WaitList/api/WaitList/',
                type: 'GET',
                dataType: 'jsonp',
                cache: 'false',
                success: function (data) {
                    // On success, 'data' contains waitlist.
                    if (data != '') {
                    // If we have data then remove messaging.
                    $('p.queue-off').css('display' , 'none');
                    $.each(data, function (key, val) {
                        // Format the text to display.
                        var str = '<tr class="qrow"><td>' + val.QueuePosition + '</td><td>' + val.DisplayName + '</td><td>' + val.CounselorStation + '</td></tr>';
                        // Add a table row for the wait list entry.
                        $('#waittable').append(str);
                    });
                  } else {
                  	// If the list is empty show message and hide table column labels.
				  	$('p.queue-off').css('display' , 'block');
                  }
                }
            });
            
            // Wait two minutes and then request the Queue again.
            zweiminuten = self.setInterval(function(){getQueue()},120000);
            
            function getQueue() {
            // Remove the Queue from two minutes ago.
            $('#waittable tr.qrow').remove();
            $.ajax({
                url: 'https://webapps.utk.edu/OneStop/WaitList/api/WaitList/',
                type: 'GET',
                dataType: 'jsonp',
                cache: 'false',
                success: function (data) {
                    // On success, 'data' contains waitlist.
                    if (data != '') {
                     // If we have data then remove messaging and display the column labels.
                    $('p.queue-off').css('display' , 'none');
         
                    $.each(data, function (key, val) {
                        // Format the text to display.
                        var newStr = '<tr class="qrow"><td>' + val.QueuePosition + '</td><td>' + val.DisplayName + '</td><td>' + val.CounselorStation + '</td></tr>';
                        // Add a table row for the wait list entry.
                        $('#waittable').append(newStr);
                    });
					} else {
					// If the list is empty show message and hide table column labels.
					 $('p.queue-off').css('display' , 'block');
                  }
                }
            });
            
           
          }

            
 });
  </script>
