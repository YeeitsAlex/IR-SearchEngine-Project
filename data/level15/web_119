@media only screen and (min-width: 959px) {
  #tamhsc #vital-record-post-board .gdlr-blog-title a {
    font-size: .9rem;
    line-height: 1.1rem;
  }
  .gdlr-blog-thumbnail img {
    width: 100%;
  }
  
  .gdlr-isotope .columns:nth-child(3n) .gdlr-blog-grid {
    margin: 0 0 30px 10px;
  }
  
  .gdlr-isotope .columns:nth-child(3n-1) .gdlr-blog-grid {
    margin: 0 10px 30px 0;
  }
  
  #content-section-2 {
    padding-right: 20px;
  }
}

