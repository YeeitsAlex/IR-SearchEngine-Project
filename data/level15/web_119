  vertical-align: middle;
  height: 72px;
}

.gdlr-isotope .gdlr-blog-grid .gdlr-blog-thumbnail {
  margin-bottom: 0;
  border: 1px solid #e5e5e5;
}

.body-wrapper #content-section-2 .gdlr-item,
.body-wrapper #content-section-2 .blog-item-wrapper {
  background-color: transparent;
}


