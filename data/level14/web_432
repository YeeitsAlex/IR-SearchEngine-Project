   $.get('https://api.library.tamu.edu/status-system-service/notifications/notification/active?location=main', function (data) {
            if (!$.isEmptyObject(data)) {
                $("#tamulib-notices-msg").addClass("alert-box warning");
                $('#tamulib-notices-msg').append('<div class="row"><div class="msg"></div><a class="close" href="#" onclick="closeAlert();">×</a></div>');
            }
            $('#tamulib-notices-msg .msg').append(data);
        });
        
        

        
        
        
				jQuery('#tamulib_notices').cycle({
				    delay: 0,
				    timeout: 8000,
				    speed: 1000,
				    pause: 1,
				    random: 1,
				    cleartype: 1
				});
  </script>
 </body>
</html>
