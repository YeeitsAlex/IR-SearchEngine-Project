  <script type="text/javascript">
   $(document).ready(function () {
                    $.get('//library.tamu.edu/lifetype/urgent_html.php?notice=cm', function(data){ 
                        if (!$.isEmptyObject(data)) {
                            $("#codemaroon").addClass("emergency");
                            $('#codemaroon').append("<div class='container'><div class='col-lg-12'><h3 class='codemaroon-title'><span class='heavy'>CODE</span>MAROON ALERTS IN PROGRESS</h3><div class='msg'></div><p><a href='http://codemaroon.tamu.edu'>Link to CodeMaroon</a></p></div></div></div>");
                        } 
                        $('#codemaroon .msg').append(data);
                    });
                });
  </script>
