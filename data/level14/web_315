       <script>
        (function($) {
$(function() {

$('[data-scrollable="true"]').each(function() {
  var slider = $(this).children('.slider');
  var controls = $(this).children('.navi');
  var circular = $(this).data('circular');
  var keyboard = $(this).data('keyboard');
  var mouseWheel = $(this).data('mouseWheel');
  var touch = $(this).data('touch');
  var autoscroll = $(this).data('autoscroll');
  var delay = $(this).data('delay');
  var vertical = $(this).data('vertical');

if ($(this).find('.item').length) {

  var api = slider.scrollable({ circular: circular, touch: touch, mousewheel: mouseWheel, keyboard: keyboard, vertical: vertical }).data('scrollable');

  if ((autoscroll && circular && $(this).find('.item').length > 3) || (autoscroll && !circular && $(this).find('.item').length > 1)) {
    slider.autoscroll(delay);
  }

  if (controls.length) {
    slider.navigator();
  }
 
}  
  
});

});
})(jQuery);
       </script>
