  <script type="text/javascript">
   var waypoint = new Waypoint.Inview({
    element: $('#timeline-sam'),
    enter: function(direction) {
        $('#timeline-sam ul li').each(function(index) {
            var isMobile = window.matchMedia("only screen and (min-width: 992px)");
            if(isMobile.matches) {
                if (index % 2 === 0) {
                    $(this).css({ "left" : index*100+18*index + "px" });
                } else {
                    $(this).css({ "left" : (index-1)*100+18*index-18+20 + "px", "top" : "220px" });
                }
            } else {

            }
            $(this).delay(400 * index).fadeIn();
            //$(this).show();
            //alert(index);
        });
    }
});

$('.scrollleft').click(function () {
    console.log('ok')
    $('#timeline-sam').animate({
        scrollLeft: '-=300'
    }, 1000);
});
$('.scrollright').click(function () {
    console.log('ok')
    $('#timeline-sam').animate({
        scrollLeft: '+=300'
    }, 1000);
});
  </script>
