	'ItemClick':function (s, e) { ASPx.CBLBItemMouseUp('cboCollege', e); }
});
dxo.InitializeProperties({
	'decorationStyles':[
		{'key':'F','className':'dxeFocused','cssText':''}
	]
});
dxo.AfterCreate();

//-->
            </script>
           </div>
          </div>
         </div>
        </div>
