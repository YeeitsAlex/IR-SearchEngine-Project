   function radMenuOnClick(sender, args) {

                var state = args.get_item().get_attributes().getAttribute("ExpandOnClick");
                args.get_item().get_attributes().setAttribute("ExpandOnClick", "true")
                args.get_item().open();
            }

            function radMenuOnOpening(sender, args) {
                var state = args.get_item().get_attributes().getAttribute("ExpandOnClick");
                if (state != "true")
                    args.set_cancel(true);
                args.get_item().get_attributes().setAttribute("ExpandOnClick", "false")
            }

            (function () {
                function loadHandler() {
                    var hf = $get('ctl14_TSSM');
                    if (!hf._RSSM_init) { hf._RSSM_init = true; hf.value = ''; }
                    hf.value += ';Telerik.Sitefinity.Resources, Version=6.0.4200.0, Culture=neutral, PublicKeyToken=null:en:b2c21a93-8894-4381-b940-3e622e747c39:7a90d6a;Telerik.Web.UI, Version=2012.3.1308.40, Culture=neutral, PublicKeyToken=121fae78165ba3d4:en:ed0b2505-4ecf-45ea-bd67-f51c9ccd1855:ed2942d4:580b2269:8cee9284;Telerik.Web.UI.Skins, Version=2012.3.1308.40, Culture=neutral, PublicKeyToken=121fae78165ba3d4:en:b27f2eb3-f44f-4ee1-b58e-cc4d9a7d6432:36964293:6c8ef648:7a57e389';
                    Sys.Application.remove_load(loadHandler);
                };
                Sys.Application.add_load(loadHandler);
            })

