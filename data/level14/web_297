    <!-- Add functionality to "Back to top" button. 1) Button fades in when have scrolled down page x amount. 2) Clicking button scrolls back to the top. -->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.2/jquery.min.js">
    </script>
    <script>
     $(document).ready(function () {
       $(window).scroll(function () {
           if ($(this).scrollTop() > 500) { 
               $('.scrollup').fadeIn();
           } else {
               $('.scrollup').fadeOut();
           }
       });
       $('.scrollup').click(function () {
           $("html, body").animate({
               scrollTop: 0
           }, 600);
           return false;
       });
   });
    </script>
