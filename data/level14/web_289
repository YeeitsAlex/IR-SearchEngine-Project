             <script language="javascript">
              function cas_login() {
					var location = window.location.toString();
					
					var redirect = "https://usu.service-now.com/cas_redirector.do?uri=" + location;

					var fullRedirect = "https://login.usu.edu";
                                        fullRedirect += "/cas/login?service=" + redirect;

                                       window.location = fullRedirect;

				}
				
				var path = window.location.pathname;
				console.log('current path: ' + path);
				if(path != '/usu/' && path != '/usu/home.do' && path != '/usu/welcome.do' && path.indexOf('/usu/kb_viewer') != 0 && path.indexOf('/usu/knowledge') != 0 && path.indexOf('/usu/search_results') != 0 && path.indexOf('/usu/side_door') != 0 && path.indexOf('/usu/password_reset_request') != 0 && path.indexOf('/usu/self_disclosure') != 0 && path.indexOf('/usu/driver_training_certification') != 0 && path.indexOf('/usu/kb_find') != 0){
				console.log("path equality: " + path == "/usu/");
				cas_login();
				}
             </script>
