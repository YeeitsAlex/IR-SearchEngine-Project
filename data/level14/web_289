           <script>
            function getCustomCmsScrollTop() {
  var offset;
	var body = $$('body')[0];
	if (body.hasClassName('msie10') || body.hasClassName('msie9')){
		offset = (window.scrollY || window.pageYOffset);
	} else {
		offset = Prototype.Browser.Gecko ? window.pageYOffset : document.body.scrollTop;
	}
	return offset;
}
           </script>
