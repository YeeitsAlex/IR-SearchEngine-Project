         <script>
          var menuParent;
		 var menuId;
         var superMenuBlocks = new Array();
         var superMenuBlocksShowing = new Array();
         function showSuperMenuBlock(menuID, sectionID) {
            if (superMenuBlocksShowing[menuID] != null)
               hideSuperMenuBlock(menuID, true);
            var id = menuID + "_" + sectionID;
			menuId = menuID;
            var div = $("content_" + id);
            superMenuBlocks[menuID] = "keep";
            superMenuBlocksShowing[menuID] = sectionID;
            var tab = gel("tab_" + id);
            if (div) { // menu sections that are just links won't have a div
               var align = div.getAttribute("alignWithTab");
               if (align == "true") {
                  var tabX = grabOffsetLeft(tab);
                  div.style.left = tabX + "px";
               }
               showObject(div);
            }
            menuParent = jQuery("#content_"+id).parents('div.cms_header_top_menu');
			jQuery(menuParent).on('focusout', tabLeftSuperMenuBlock);
            tab.className = "cms_menu_super_menu_bar_item_selected";
            setTimeout("hideSuperMenuBlock('" + menuID + "')", 650);
         }
		  
		 function tabLeftSuperMenuBlock(event) {
			  if (menuParent){
				  if (menuParent.has(event.relatedTarget).length == 0) {
					  leftSuperMenuBlock(menuId);
					  jQuery(menuParent).off('focusout');
				  }
			  }
		  }
         function leftSuperMenuBlock(menuID) {
            superMenuBlocks[menuID] = "keep";
            superMenuBlocks[menuID] = "discard";
            setTimeout("hideSuperMenuBlock('" + menuID + "')", 650);
         }
         function enterSuperMenuBlock(menuID, sectionID) {
            superMenuBlocks[menuID] = "keep";
         }
         function hideSuperMenuBlock(menuID, override) {
            if (typeof override == "undefined")
               override = false;
            if (override || superMenuBlocks[menuID] != "keep") {
               superMenuBlocks[menuID] = "gone";
               var showingID = superMenuBlocksShowing[menuID];
               if (showingID == null)
                  return;

               var id = menuID + "_" + showingID;
               var div = $("content_" + id);
               superMenuBlocksShowing[menuID] = null;
               if (div) { // menu sections that are just links won't have a div
                  if (override)
                     hideObject(div);
                  else
                     div.fadeOut("slow");
               }
               var tab = gel("tab_" + id);
               tab.className = "cms_menu_super_menu_bar_item";
            }
         }
         </script>
        </div>
