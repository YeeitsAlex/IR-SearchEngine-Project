  <script>
   jQuery(document).ready(function($) {
  /* Add wrench icon to View title */
  if ($("#block-views-sidebar_node_title-block_2").hasClass("block-views")) {
    $("#block-views-sidebar_node_title-block_2").find("h1").prepend('<p><i class="fa fa-wrench fa-4x"></i></p> ');
  } else if ($("#block-views-sidebar_node_title-block").hasClass("block-views")) {
    $("#block-views-sidebar_node_title-block").find("h1").prepend('<p><i class="fa fa-wrench fa-4x"></i></p> ');
    /* Add share-alt icon to View title */
    if ($("#block-views-related_diys-block").hasClass("block-views")) {
      $("#block-views-related_diys-block").find("h2:contains('Related DIYs')").html('<i class="fa fa-share-alt"></i> Related DIYs');
    }
  }
});
  </script>
