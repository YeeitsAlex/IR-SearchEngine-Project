            if (img.attr("alt") == 'pause') {
                $(rotatorData).cycle('pause');
                img.attr("alt", "play");
                img.attr("src", "./images/RotatingOffers/play.jpg");
                //$(this).text('Play');
            } else {
                $(rotatorData).cycle('resume');
                img.attr("alt", "pause");
                img.attr("src", "./images/RotatingOffers/pause.jpg");
                //$(this).text('Pause');
            }
        });
    }
    
    function FetchOffersHtml_OnSuccess(msg, status, opts) {
        var delay  = opts.delay;
        //var rotatorId = "#rotator";
        var html = $(msg.d).find("#offers").html();  //extract offers element from returned data;
        setTimeout( function() {DisplayOffers("#rotator", html);}, delay);
    }
    
    function FetchOffersHtml() {

        var initialDelay = 20; 
        var url = "webServices/OfferService.asmx/GetOffers";
        var data = "{xmlKey:\"RotatingOffersXml\",xslKey:\"RotatingOffersXslLime\",filter:\"\"}";

        jQuery.ajax({
            type: "POST",
            contentType: "application/json; charset=utf-8",
            url: url,
            data: data,
            dataType: "json",
            cache: false,
            success: function(msg, status) { FetchOffersHtml_OnSuccess(msg, status, { delay: initialDelay  }); }
            //complete: function(request, status) {
                //alert(request);
            //}
        });


