     <div class="l-rw regions container pr-featured__rw arc--1 hr--1" data-at-regions="">
      <div class="l-r region pr-featured__featured" data-at-region="region-featured">
       <div class="views-element-container block block-config-provider--views block-plugin-id--views-blockfront-page-content-block-1" id="block-views-block-front-page-content-block-1">
        <div class="block__inner">
         <div class="block__content">
          <div>
           <div class="view view-front-page-content view-id-front_page_content view-display-id-block_1 js-view-dom-id-1ee2b9d1975d99ba16f892b3af626b691191ffcd4fb3801193246470dc2d2164">
