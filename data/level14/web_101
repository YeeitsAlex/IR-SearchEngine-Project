        jQuery(document).ready(function(){
  jQuery(jQuery('.e-menu-main a')[1]).attr('href','http://jobs.illinois.edu/academic-job-board/?start=1&per=5000');

  if(window.location.href == 'http://jobs.illinois.edu/faculty-positions/' || window.location.href == 'https://jobs.illinois.edu/faculty-positions/')
{ 
  window.location.href = 'https://jobs.illinois.edu/faculty-positions/?start=1&per=5000';
}
  
  if(window.location.href == 'http://jobs.illinois.edu/academic-job-board/' || 
  window.location.href == 'https://jobs.illinois.edu/academic-job-board/' || 
