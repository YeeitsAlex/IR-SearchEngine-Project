 <script type="text/javascript">
  var wordpressDefault = ' ';
var cx              = '006549799505564222509:-8lddip9q2g';
var cof             = 'FORID:11';

//leave skinId blank to use the campus default
var skinId              = '';

function sendSearch(){
	 if(document.searchForm.searchType_people.checked)
	 {
		document.searchForm.cx.value = '';
		document.searchForm.cof.value = '';
		if(document.searchForm.s.value == 'Search')
		{
		   document.searchForm.search.value = '';
		}
		else
		{
		   document.searchForm.search.value = document.searchForm.s.value;  
		}
		document.searchForm.skinId.value = skinId;
		document.searchForm.action = "http://illinois.edu/ds/search";
	}
	else
	{
		document.searchForm.cx.value = cx;
		document.searchForm.cof.value = cof;
		document.searchForm.id = cx;
		document.searchForm.action = wordpressDefault;
	}
}
function checkSearchValue(){
    if(document.searchForm.s.value == 'Search'){document.searchForm.s.value ='';}
}
 </script>
 <script>
  $(document).ready(function(){



		function openSidepage() { 

		    	$('#nav-icon').addClass('open');

			    $('.wrapper-nav').animate({  
			    	width: '200px'  
			    }, 400, 'easeOutQuint');

			    $('.wrapper-page').animate({  
			    	padding: '0 0 0 200px'  
			    }, 400, 'easeOutQuint');

			    $('.site-navigation').css("visibility", "visible");

		    }  
		      
		    function closeSidepage(){  

			    $("#nav-icon").removeClass("open");  

			    $('.wrapper-nav').animate({  
			    	width: '60px'  
			    }, 400, 'easeOutQuint');   

			    $('.wrapper-page').animate({  
			    	padding: '0 0 0 60px'  
			    }, 400, 'easeOutQuint');

			    $('.site-navigation').css("visibility", "hidden");

		    }



		if( $(window).width() < 680) {
			closeSidepage();
		}

		$(window).resize(function() {

		  if ($(this).width() < 680) {

		   	closeSidepage();

		  } else {

		    openSidepage();

		    }

		});

		

		$("#nav-icon").on("click", function(e){
			
			e.preventDefault();
			var distance = $('.wrapper-nav').css('width');


			
			if(distance != "200px") {
				$(this).addClass("open");
				openSidepage();
			} 

			else {
				closeSidepage();
			}

		}); 


		      


	});
 </script>
 <script type="text/javascript">
  $(document).ready(function() {
	 
	    //Default Action
	    $(".tab-content").hide(); //Hide all content
	    $("ul.tabs li:first").addClass("active").show(); //Activate first tab
	    $(".tab-content:first").show(); //Show first tab content
	     
	    //On Click Event
	    $("ul.tabs li").click(function() {
	        $("ul.tabs li").removeClass("active"); //Remove any "active" class
	        $(this).addClass("active"); //Add "active" class to selected tab
	        $(".tab-content").hide(); //Hide all tab content
	        var activeTab = $(this).find("a").attr("href"); //Find the rel attribute value to identify the active tab + content
	        $(activeTab).fadeIn(); //Fade in the active content
	        return false;
	    });
	 
	});
 </script>
 <script>
  // Major Tab
$(function() {

	var newSelection = "";
	
	$(".all-major").hide();

	$("#majors-list a").click(function(){

	    $(".all-major").hide();
	
		$("#majors-list a").removeClass("current");
		$(this).addClass("current");
		
		newSelection = $(this).attr("rel");

		var majorName = $(this).text();

		$("span.selected-major").html(majorName);
		
		$(".major").not("."+newSelection).hide();
		$("."+newSelection).show();
		


	    $(".all-major").show();

	    $('html, body').animate({
            scrollTop: $("#widget-connect-tool").offset().top
        }, 200);
		
	});
	
});

// Discipline Tab
$(function() {

	var newSelection = "";
	
	$(".all-discipline").hide();

	$("#discipline-list a").click(function(){

	    $(".all-discipline").hide();
	
		$("#discipline-list a").removeClass("current");
		$(this).addClass("current");
		
		newSelection = $(this).attr("rel");

		var majorName = $(this).text();

		$("span.selected-discipline").html(majorName);
		
		$(".discipline").not("."+newSelection).hide();
		$("."+newSelection).show();
		


	    $(".all-discipline").show();

	    $('html, body').animate({
            scrollTop: $("#widget-connect-tool").offset().top
        }, 200);
		
	});
	
});
 </script>
 <script>
  // Show all majors button
$(document).ready(function () {
    $('.every-major').hide();
    $('.open-close').click(function (event) {
        event.preventDefault();

        $('.every-major').slideToggle(600, function () {
            $('.open-close').html($(this).is(':visible') ? '<strong>CLOSE</strong>' : 'VIEW <strong>ALL</strong> MAJORS');
        });

    });
});
 </script>
