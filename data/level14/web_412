         <div class="module_content" id="7005-content">
          <script type="text/javascript">
           function start_slideshow_7005(frame, start_frame, end_frame, delay) {
	fadeTimer = setTimeout(switch_slides_7005(frame, start_frame, end_frame, delay), delay);
}
function switch_slides_7005(frame, start_frame, end_frame, delay) {
	return (function() {
		Effect.Fade('slide_7005_' + cSlide7005, { duration: 1 });
		if (cSlide7005 == end_frame) { cSlide7005 = start_frame; } else { cSlide7005 = cSlide7005 + 1; }
		clearTimeout(fadeTimer);
		Effect.Appear('slide_7005_' + cSlide7005, { duration: 1 });
		fadeTimer = setTimeout(switch_slides_7005(cSlide7005, start_frame, end_frame, delay), delay);
	})
}
cSlide7005 = 0;
          </script>
          <div class="slideshow_7005" id="main_feature">
           <script type="text/javascript">
            start_slideshow_7005(0, 0, -1, 6500);
           </script>
