  <script>
   window.onload = function(){
		var hrURL = "hr.msu.edu";
		var locURL = location.href;
		var cINTVL = 5000;			//default to 5 sec interval
		if (locURL.includes(hrURL)) {
			cINTVL = 4000;			// URL contains 'hr.msu.edu' so rotate on this interval
		} else {
			cINTVL = false;			// should be in OU staging ... no rotation (easier to edit)
		}
		$('#carousel-01').carousel({
			interval: cINTVL,
			pause: "hover",
			wrap: true,
			keyboard: true
		})
	};
  </script>
