    <section aria-label="In The Spotlight" class="content-blocks background-white" role="contentinfo">
     <div class="section-header-content tan-header">
      <div class="section-header-title">
       <h2 class="up title-bg">
        <span>
         In The Spotlight
        </span>
       </h2>
      </div>
     </div>
     <div class="content-layout">
      <div class="med-12 columns">
       <ul class="image-list">
        <li>
         <figure class="image-container">
          <img alt="TAMUG Sailboat photo with Moody Gardens in the background" height="520" src="assets/images/aggies-by-the-sea.jpg" width="455"/>
