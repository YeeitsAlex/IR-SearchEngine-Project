             <script>
              <!--//--><![CDATA[// ><!--

(function ($) {
  Drupal.behaviors.vimeoVideo = {
    attach: function (context, settings) {
      var button = $('#splash--pause', context);
      var video = $('#splash-video', context).get(0);
      
      $(button).once('video-fixed', function (i, button) {
        $(button).click(function () {

          if (video.paused) {
            $(button).addClass('fa-pause-circle');
            $(button).removeClass('fa-play-circle');
            $(button).attr('aria-label', Drupal.t('Pause the video'));
            $(button).find('.visually-hidden').html(Drupal.t('Pause the video'));

            video.play();
          }
          else {
            $(button).removeClass('fa-pause-circle');
            $(button).addClass('fa-play-circle');
            $(button).attr('aria-label', Drupal.t('Play the video'));
            $(button).find('.visually-hidden').html(Drupal.t('Play the video'));

            video.pause();
          }

        })
      });

      $(video).on('ended', function () {
        $(button).removeClass('fa-pause-circle');
        $(button).addClass('fa-play-circle');
        $(button).attr('aria-label', Drupal.t('Play the video'));
        $(button).find('.visually-hidden').html(Drupal.t('Play the video'));
      });
    }
  }
})(jQuery);


//--><!]]>
             </script>
