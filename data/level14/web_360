  <script>
   $(function() {
			var pull 		= $('#pull');
				menu 		= $('nav#primary ul');  //this slides up the page items menu in the phone version
				menuHeight	= menu.height();

			$(pull).on('click', function(e) {
				e.preventDefault();
				menu.slideToggle();
			});

			$(window).resize(function(){
        		var w = $(window).width();
        		if(w > 320 && menu.is(':hidden')) {
        			menu.removeAttr('style');
        		}
    		});
		});
  </script>
  <script>
   $(function() {
			var pulltop 		= $('#pulltop');
				topmenu 		= $('nav#topmenu ul');  //this slides down the site pages menu in the phone version
				topmenuHeight	= topmenu.height();

			$(pulltop).on('click', function(e) {
				e.preventDefault();
				topmenu.slideToggle();
			});

			$(window).resize(function(){
        		var w = $(window).width();
        		if(w > 320 && topmenu.is(':hidden')) {
        			topmenu.removeAttr('style');
        		}
    		});
		});
  </script>
