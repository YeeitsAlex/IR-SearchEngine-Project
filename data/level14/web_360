        <script>
         function go-to-top() {
window.scroll(0,0);
    return false;
  }
function keyHandler(event)
  {
    switch (event.which) {
      case KEY_SPACE: {
        event.stopPropagation;
        return go-to-top();
        break;
      }
    } //end switch
    return true;
  }
        </script>
