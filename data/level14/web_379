   function DelPeopleSoft()
{
   var exp = new Date();
   exp.setTime (exp.getTime() - 1);
   document.cookie = "PS_TOKEN = ''; path=/; domain=acs.ncsu.edu; expires=" + exp.toGMTString();
   document.cookie = "PS_TOKEN = ''; path=/; domain=ncsu.edu; expires=" + exp.toGMTString();

   document.cookie = "UAC = ''; path=/; domain=acs.ncsu.edu; expires=" + exp.toGMTString();
   document.cookie = "UAC = ''; path=/; domain=ncsu.edu; expires=" + exp.toGMTString();
   document.cookie = "TAC = ''; path=/; domain=acs.ncsu.edu; expires=" + exp.toGMTString();
   document.cookie = "TAC = ''; path=/; domain=ncsu.edu; expires=" + exp.toGMTString();

   document.cookie = "SignOnDefault = ''; path=/; domain=acs.ncsu.edu; expires=" + exp.toGMTString();
   document.cookie = "ExpirePage = ''; path=/; domain=acs.ncsu.edu; expires=" + exp.toGMTString();
   document.cookie = "PS_LOGINLIST = ''; path=/; domain=acs.ncsu.edu; expires=" + exp.toGMTString();
   document.cookie = "PS_TOKENEXPIRE = ''; path=/; domain=acs.ncsu.edu; expires=" + exp.toGMTString();
   document.cookie = "PS_TOKENEXPIRE = ''; path=/; domain=ncsu.edu; expires=" + exp.toGMTString();

   document.cookie = "MP91PRD-PORTAL-PSJSESSIONID = ''; path=/; domain=acs.ncsu.edu; expires=" + exp.toGMTString();
   document.cookie = "MP91PRD-PORTAL-PSJSESSIONID = ''; path=/; domain=ncsu.edu; expires=" + exp.toGMTString();
}

function ReDirectWin()
{
   var loc= location.search;
   var redir= "none";
   redir= "https://portalsp.acs.ncsu.edu/psp/MP91PRD/EMPLOYEE/EMPL/c/NUI_FRAMEWORK.PT_LANDINGPAGE.GBL";

   parent.parent.location= redir;
}

