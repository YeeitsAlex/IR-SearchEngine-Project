  </script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.15/jquery.mask.min.js">
  </script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jSignature/2.1.2/jSignature.min.js">
  </script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.8.0/js/bootstrap-datepicker.min.js">
  </script>
  <script src="/lib/autogrow.min.js">
  </script>
  <!--<script  src="/lib/signature_pad.js"></script>-->
  <script src="https://www.googletagmanager.com/gtag/js?id=UA-105571103-2">
  </script>
  <script type="text/javascript">
   $(document).ready(function() {
		$("#pleasewait").hide(1);
		$("#pleaseshow").show(1);
		$('.sign-textarea').autogrow({ onInitialize: true });
		$('.sign-date').datepicker({
			format: "dd M yyyy",
			daysOfWeekHighlighted: "0,6",
			assumeNearbyYear: true,
			autoclose: true,
			todayHighlight: true
		});
		
		$("#signature-pad").jSignature({
			lineWidth: 2,
			width: "100%",
			color: "#0033cc",
		});
		$("#btn-clear").click(function() {
			$("#signature-pad").jSignature('reset');
		});

		$.validate({
			form: '#validate-form',
			onSuccess: function($form) {
				// test if signature pad exists
				if( $("#signature-pad").length ) {
					// test if signature was drawn
					if( $("#signature-pad").jSignature('isModified') ) {
						$("#signature-pad").popover('hide');
						$('input[name="signature"]').val( $("#signature-pad").jSignature('getData') );
						$('#to').hide();
						$('#btn-sign').hide();
						$('#btn-wait').show();
						return true;
					} else {
						$("#signature-pad").popover('show');
						return false;
					}
				}
				
			},
			
		});  // validate()
		// Google Analytics code
		window.dataLayer = window.dataLayer || [];
		function gtag(){dataLayer.push(arguments);}
		gtag('js', new Date());
		gtag('config', 'UA-105571103-2');
	});  // ready()
  </script>
