$('.camera-location').click( function() {
    center = currentCenter.replace(/'/g, '"');
    map.setZoom(18);
    map.setMapTypeId(google.maps.MapTypeId.HYBRID);
    map.setCenter(JSON.parse(center));
    $('#location-dialog').parent().css('visibility', 'visible');
    $('#location-dialog').dialog('open');
  });
}
google.maps.event.addDomListener(window, 'load', initialize);

function setWrapperSize(src) {
    var newImg = new Image();
    newImg.onload = function() {
      var height = newImg.height;
      var width = newImg.width;
      var screen = $(window).width() - 20;
      $('.title').css("margin-left", "0px");
      $('.stream').css("height", height);
      if (screen < width) {
          height = (screen / width) * height;
          width = screen;
          $('.stream').css("height", height);
          $('.title').css("margin-left", "15px");
      }
      $('.stream-wrapper').css("width", width);
      $('.options').css("width", width);
    }

    newImg.src = src;
}

$( "#about-dialog" ).dialog({
    autoOpen: false,
    width: 600,
    show: {
      effect: "clip",
      duration: 150
    },
    hide: {
      effect: "clip",
      duration: 150
    },
    modal: true,
    open: function() {
      $('.ui-widget-overlay').bind('click', function() {
      $('#about-dialog').dialog('close');
    })
   }
  });

$( "#lapse-dialog" ).dialog({
    autoOpen: false,
    width: 600,
    show: {
      effect: "clip",
      duration: 150
    },
    hide: {
      effect: "clip",
      duration: 150
    },
    modal: true,
    open: function() {
      $('.ui-widget-overlay').bind('click', function() {
      $('#lapse-dialog').dialog('close');
    })
   }
  });

var screenWidth = $(window).height() * .6;
$( "#location-dialog" ).dialog({
    width: '60%',
    height: screenWidth,
    show: {
      effect: "clip",
      duration: 150
    },
    hide: {
      effect: "clip",
      duration: 150
    },
    modal: true,
    open: function() {
      $('.ui-widget-overlay').bind('click', function() {
      $('#location-dialog').dialog('close');
    })
   }
  });

//Hide the dialog while we init google maps
$('#location-dialog').parent().css('visibility', 'hidden');

$('.about').click( function() {
  $( "#about-dialog" ).dialog( "open" );
});

$('.daily-lapse').click( function() {
  $( "#lapse-dialog" ).dialog( "open" );
});

$('#slider #aboutdata').each( function() {
    var about = $(this).html();
    var stream = $(this).data('stream');
    var center = $(this).data('center');
    var title = $(this).data('location');
    var name = $(this).prev().data('partial');
    var path = "url(/cam/" + name + "/live/live.jpg?1552200869)";
    $(this).parent().click(function() {
        window.location.href= '/' + name;
    });
  });

$('.play').click( function() {
  $(this).css('display', 'none');
  testImage(currentStream);
});

function testImage(url) {
    var tester = new Image();
    tester.onload = imageFound;
    tester.onerror = errorFound;
    timer = setTimeout(loading, 1000)
    tester.src = url;
}

function errorFound() {
  $('.loadingError').html('This camera is currently down.');
}

function loading() {
  $('.loadingError').css('display', 'block');
  $('.loadingError').html('This stream is taking longer than expected to load, The camera may be down.');
}

function imageFound() {
  clearTimeout(timer);
  $('.loadingError').css('display', 'none');
  path = "url(" + currentStream + ")"
  $('.stream').css('background', path);
  $('.cameraStatus').css('display', 'block');
  setWrapperSize(currentStream);
}

//We cannot init the google map unless the dialog is open, once finished we close it
$(window).load(function () {
  $('#location-dialog').dialog('close');
});

function fetchLapse(path) {
  $.ajax({
    type:"GET",
    data:{
      'path' : path
    },
    url: '/fetchLapse.php',
    success: function(data) {
      if (data != false) {
        $('#timelapse').html(data);
      } else {
        $('#timelapse').html("No existing timelapses for this camera.");
      }
    }
  });
}
 </script>
</html>

