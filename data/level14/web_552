        <script>
         jQuery(document).ready(function() {
	"use strict";
	jQuery('.widget-slider .flexslider').flexslider({
		animation: "slide",
		slideshow: "true", 
		slideshowSpeed: 5000,
		animationSpeed: 500, 
		easing: "swing", 
