  <script>
   // The function actually applying the offset
        $(document).ready(function () {

            function offsetAnchor() {
                if (location.hash.length !== 0) {
                    window.scrollTo(window.scrollX, window.scrollY - 80);
                }
            }

            // Captures click events of all <a> elements with href starting with #
            $(document).on('click', 'a[href^="#"]', function (event) {
                // Click events are captured before hashchanges. Timeout
                // causes offsetAnchor to be called after the page jump.
                window.setTimeout(function () {
                    offsetAnchor();
                }, 100);
            });

            // Set the offset when entering page with hash present in the url
            window.setTimeout(offsetAnchor, 0);
        });
  </script>
  <style>
   .container {
        display: flex;
        background-color: #f6f6f6;
        border-radius: 25px;
        flex-flow: row wrap;
        vertical-align: middle;
        padding: 0;
    }
  </style>
  <script src="shortcut.js" type="text/javascript">
  </script>
  <script src="index.js">
  </script>
  <script>
   /*loads PHP data for the SSC feed and plays in a loop */
    $(document).ready(function() {

        $.ajax({
            type: "POST",
            url: "ssc_feed.php",
            success: function(data){
               var quotes = data;
                let i = 1;

                if (quotes != null) {
                    $("#sysname").html(quotes[0].sysname);
                    $("#sysimage").html(quotes[0].sysimage);
                    $("#sysmessage").html(quotes[0].sysmessage);
                }
                else {
                    $("#sysname").html("Error: no status data found.");
                }

                if (quotes.length > 1) {
                    setInterval(function () {
                        $("#sysname").html(quotes[i].sysname);
                        $("#sysimage").html(quotes[i].sysimage);
                        if (quotes[i].sysmessage.length > 350) {
                            var trunc = quotes[i].sysmessage.substring(0, 250);
                            trunc += "... <br>For more details visit the <a href='https://help.utk.edu/status/' target='_blank'>IT System Status page</a>.";
                        }
                        else {
                            trunc = quotes[i].sysmessage;
                        }
                        $("#sysmessage").html(trunc);
                        if (i == (quotes.length - 1)) {
                            i = 0;
                        }
                        else {
                            i++;
                        }
                    }, 10 * 1000);
                }
            },
            dataType: "JSON",
            error: function(req,status,err){

                alert("problem: "+ status + err);
            }
        });

    });

    $(document).ready(function () {
        $.ajax({
            type: "POST",
            url: "get_status.php",
            success: function(data){

                var info = JSON.parse(data);
                $("#statusbox").html(info[0] + " " + info[1]);


            },
            error: function(req,status,err){
                alert(status +" " + err);
            },
            dataType: "text"
        });
    });
  </script>
