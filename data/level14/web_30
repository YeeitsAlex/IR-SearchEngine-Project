function getUrlVars() {
    var vars = {};
    var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
        vars[key] = value;
    });
    return vars;
}
//http://stackoverflow.com/questions/3514784/best-way-to-detect-handheld-device-in-jquery
if ((/Android|webOS|iPhone|iPod|BlackBerry/i.test(navigator.userAgent)) && !(getUrlVars()["full"] == "true")){
	window.location = "http://windtunnel.caltech.edu/index_m.html";
}
  </script>
