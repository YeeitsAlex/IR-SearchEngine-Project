        /*******************************************************************
Show/hide announcement details
********************************************************************/
$('.show_hide').click(function(){
		var $this = $(this);
		$this.toggleClass('show_hide');
		if($this.hasClass('show_hide')){
			$this.text('See details');
		} else {
			$this.text('Hide details');
		}
	});
       </script>
