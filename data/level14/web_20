       <script type="text/javascript">
        $(function() {
	
	//MANUAL settings---------------------------------------------------------- 
	//To use the auto settings set both variables to 0
	
	//control the fao_upload button link: 1=on, 0=off
	var active_button = 0;
	//control the alert message: 1=on, 0=off
	var alert_message = 0;
		
	//AUTO settings------------------------------------------------------------
    //Enter the start and stop times
   
	//Set START time
	//var startTime = "Tue Nov 28 2017 07:00:00 GMT-0700 (Pacific Daylight Time)"
	var startTime = "Thu Nov 29 2018 08:00:00 GMT-0800 (Pacific Standard Time)"
	var msStartTime = Date.parse(startTime);

	//Set STOP time
	//var stopTime = "Wed Nov 29 2017 07:00:00 GMT-0700 (Pacific Daylight Time)"
	var stopTime = "Thu Nov 29 2018 13:28:00 GMT-0800 (Pacific Standard Time)"

	var msStopTime = Date.parse(stopTime);
	
	//Gets the current datetime in millisecond format
	var msCurrentTime = $.now();	
		
	//Hides the fao_upload login button and shows the alert message
	if ((msCurrentTime>=msStartTime && msCurrentTime<msStopTime) || (active_button==0 && alert_message==1)) {
		$('#fao_upload_enabled').hide();
		$('#fao_upload_disabled').show();
		$('#fao_upload_alert').show();
		$('#fao_upload_normal_message').hide();
		$('.fao_upload_inlinelink_enabled').hide();
		$('.fao_upload_inlinelink_disabled').show();
	}
	//Shows the fao_upload login button and the alert message
	else if ((msCurrentTime<msStartTime) || (active_button==1 && alert_message==1)) {
		$('#fao_upload_enabled').show();
		$('#fao_upload_disabled').hide();
		$('#fao_upload_alert').show();
		$('#fao_upload_normal_message').hide();
		$('.fao_upload_inlinelink_enabled').show();
		$('.fao_upload_inlinelink_disabled').hide();
	}
	//Shows the fao_upload login button and hides the alert message
	else if (active_button==1 && alert_message==0) {
		$('#fao_upload_enabled').show();
		$('#fao_upload_disabled').hide();
		$('#fao_upload_alert').hide();
		$('#fao_upload_normal_message').show();
		$('.fao_upload_inlinelink_enabled').show();
		$('.fao_upload_inlinelink_disabled').hide();
	}
	//Default settings
	else {
		$('#fao_upload_enabled').show();
		$('#fao_upload_disabled').hide();
		$('#fao_upload_alert').hide();
		$('#fao_upload_normal_message').show();
		$('.fao_upload_inlinelink_enabled').show();
		$('.fao_upload_inlinelink_disabled').hide();
	}
	
//Use this for testing
//document.write("<span>" + msStopTime +"</span>");
	
});
       </script>
