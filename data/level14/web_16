             if(!document.getElementById( "builder-styles" ) && document.getElementById( "builder-styles-css" ) ) document.getElementById( "builder-styles-css" ).insertAdjacentHTML( "beforebegin", "<link id='builder-styles' rel='stylesheet' href='http://endrape.msu.edu/wp-content/plugins/themify-builder/css/themify-builder-style.min.css?ver=4.1.4' type='text/css' />" );
