       <script type="text/javascript">
        (function( $ ) {
    activate();
  
  function activate() {
	var pageUrl = "/";
	$(".MainCMSListMenuLinkHighlighted[href='" + pageUrl + "']").addClass("MainCMSListMenuLinkActive");
  
    $("#mobile-menu-label").click(function(){toggleMenu()});
    if($(window).width() < 768 && $("#mobile-menu-toggle").is(':checked') ) {
      $("#mobile-menu-label").click();
    }
    $(window).resize(function() {
      updateMenu();
    })
  }
  
  function updateMenu() {
    if($(window).width() > 767 && $("#mobile-menu-toggle").is(':checked')  ) {
      $("#mobile-menu-label").click();
    }
  }
      
    
  function toggleMenu(){
    if($("#mobile-menu-toggle").is(':checked')){
      $("#menuElem").slideDown();
    }
    else{
      $("#menuElem").slideUp();
    }
  }
  })(jQuery);
       </script>
