       <script type="text/javascript">
        google.load("feeds", "1");
  function initialize() {
    var feed = new google.feeds.Feed("http://www.getrave.com/rss/gwu/channel2?prevent_cache" + Math.floor(Date.now()));
    feed.load(function(result) {
      if (!result.error && result.feed.entries.length != 0) {
        var noalert = document.getElementById("nogwalerts");
        noalert.innerHTML = "";
        var container = document.getElementById("feed");
        var s = "";
        for (var i = 0; i < result.feed.entries.length; i++) {
          var entry = result.feed.entries[i];
          var div = document.createElement("div");
          var idstr = 'alertItem' + i;
          div.setAttribute("id", idstr);
          div.setAttribute("class", "itemblock");
          var date = entry.publishedDate;
          var myDate = new Date(date) 
          s += '<span class=title>' + entry.title + '<\/span>';
          //s += '<span class=link><a href=' + entry.link + '>'  + entry.link + ' <\/a><\/span>';
          s += '<span class=date>' + myDate + '<\/span>';
          s += '<span class=alertcontent>' + entry.content.replace(/\n+/g, '<\/br />'); + '<\/span>';
            div.appendChild(document.createTextNode(''));
            div.innerHTML = s;
            s = "";
          container.appendChild(div);
        }
      }
    });
  }
       </script>
