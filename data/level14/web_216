      <div class="panel-pane pane-custom pane-3">
       <script type="text/javascript">
        function rightNowM() {
    var dm = new Date();
    var nt = dm.toString();
    jQuery('#timeNowMetro').html(nt);
  };

  function formatJsonDate(jsonDate) {
    return (new Date(parseInt(jsonDate.substr(6))));
  }

  function getIncidentsLine(indata) {
    var line = [];
    jQuery.each(indata, function() {
      jQuery.each(this, function(p) {
        searchline = this.LinesAffected;
        if (searchline.search("BL") >= 0 || searchline.search("OR") >= 0) {
          line.push(p);
        };
      });
    });
    return line;
  };

  function jsonToHtml(eldata) {
    var item = [];
    if (eldata.ElevatorIncidents.length > 0) {
      jQuery('#noEldata').hide();
      jQuery.each(eldata, function() {
        jQuery.each(this, function(i) {
          item.push('<ul class="Ellist">' + '<li class="lab">Date Updated</li><li class="items">' + formatJsonDate(this.DateUpdated) + '</li>' + '<li class="lab">Equipment Type</li><li class="items">' + this.UnitType + '</li>' + '<li class="lab">Location Description</li><li class="items">' + this.LocationDescription + '</li>' + '<li class="lab">Reason</li><li class="items">' + this.SymptomDescription + '</li><\/ul>');
        });
      });
      jQuery('#yesEldata').html(item.join(''));
    }
  }

  function getElevatorData() {
    jQuery.ajax({
      url: 'https://api.wmata.com/Incidents.svc/json/ElevatorIncidents?StationCode=C04&api_key=nssnqudys4g27vhcjgrkda4u',
      dataType: 'jsonp',
      jsonpCallback: 'jsonToHtml',
      data: 'eldata',
      success: function() {},
      error: function() {}
    });
  }

  function jsonToHtmlIncident(indata) {
    var tstation = [];
    var tline = [];
    var it = getIncidentsLine(indata);
    if (it.length > 0) {
      for (var x = 0; x < it.length; x++) {
        p = it[x];
        if ((indata.Incidents[p].IncidentType == "Station Incident") && (indata.Incidents[p].StartLocationFullName == "Foggy Bottom-GWU" || indata.Incidents[p].EndLocationFullName == "Foggy Bottom-GWU")) {
          jQuery('#noIncidentStationData').hide();
          tstation.push('<ul class="INlist">');
          tstation.push('<li class="lab">Date Updated</li><li class="items">' + formatJsonDate(indata.Incidents[p].DateUpdated) + '</li>' + '<li class="lab">Description</li><li class="items">' + indata.Incidents[p].Description + '</li>');
          if (indata.Incidents[p].EmergencyText != null) {
            tstation.push('<li class="lab">EmergencyText</li><li class="items">' + indata.Incidents[p].EmergencyText + '</li>');
          }
          tstation.push('<\/ul>');
        } else {
          jQuery('#noIncidentLineData').hide();
          tline.push('<ul class="INlist">' + '<li class="lab">Date Updated</li><li class="items">' + formatJsonDate(indata.Incidents[p].DateUpdated) + '</li>' + '<li class="lab">Incident Type</li><li class="items">' + indata.Incidents[p].IncidentType + '</li>');
          if (indata.Incidents[p].DelaySeverity != null) {
            tline.push('<li class="lab">Delay Severity</li><li class="items">' + indata.Incidents[p].DelaySeverity + '</li>');
          }
          if (indata.Incidents[p].PassengerDelay > 0) {
            tline.push('<li class="lab">Passenger Delay</li><li class="items">' + indata.Incidents[p].PassengerDelay + '</li>');
          }
          tline.push('<li class="lab">Description</li><li class="items">' + indata.Incidents[p].Description + '</li>');
          if (indata.Incidents[p].EmergencyText != null) {
            tline.push('<li class="lab">EmergencyText</li><li class="items">' + indata.Incidents[p].EmergencyText + '</li>');
          }
          if (indata.Incidents[p].StartLocationFullName != null) {
            tline.push('<li class="lab">Start Location</li><li class="items">' + indata.Incidents[p].StartLocationFullName + '</li>');
          }
          if (indata.Incidents[p].EndLocationFullName != null) {
            tline.push('<li class="lab">End Location</li><li class="items">' + indata.Incidents[p].EndLocationFullName + '</li>');
          }
          tline.push('<\/ul>');
        }
      }
      jQuery('#yesIncidentStationData').html(tstation.join(''));
      jQuery('#yesIncidentLineData').html(tline.join(''));
    }
  }

  function getIncidentData() {
    jQuery.ajax({
      url: 'https://api.wmata.com/Incidents.svc/json/Incidents?api_key=nssnqudys4g27vhcjgrkda4u',
      dataType: 'jsonp',
      jsonpCallback: 'jsonToHtmlIncident',
      data: 'indata',
      success: function() {},
      error: function() {}
    });
  }
       </script>
       <!--
<div id="metro"><h3>Metro Status:</h3><div class="wrap"><h4 style="margin-top:1em;">Foggy Bottom Metro Station</h4><div id="noIncidentStationData"><h6>No station incidents at this time.</h6></div><div id="yesIncidentStationData">&nbsp;</div><span class="stationdetails"><a href="https://www.wmata.com/rail/station_detail.cfm?station_id=40" target="_blank">Station Details</a></span></div><div class="wrap"><h4 style="margin-top:1em;">Blue/Orange/Silver Lines</h4><div id="noIncidentLineData"><h6>There are no line incidents reported at this time.</h6></div><div id="yesIncidentLineData">&nbsp;</div></div><br /><div class="asof">This Information is current as of:</div><div id="timeNowMetro">&nbsp;</div><div class="asof">Updated at 10 min interval.</div></div>
-->
