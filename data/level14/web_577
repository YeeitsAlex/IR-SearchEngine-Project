  <script>
   function changeToggle() {
    var toggles = document.getElementsByClassName('trigger');
    toggles[2].click();
    if(document.getElementsByClassName('toggle_container')[3].style.display !== "block")
        toggles[3].click();
}

window.onload = function() {
	// Scholarships page, change toggles when automatic grants clicked. Feel free to extend this to any toggle ;)
    if(document.getElementById('toggleJump') != null) {
        document.getElementById('toggleJump').onclick = changeToggle;
    }

	// cost and funding page compute code
	if(jQuery(".add1").is("*")) {
		jQuery(".add1").html("<div style='width: 185px;'><span style='float: left;'>$</span><input type='text' class='cost-box' value='0.00'></div>");
		jQuery(".sub1").html("<div style='width: 185px;'><span style='float: left;'>$</span><input type='text' class='cost-box' value='0.00'></div>");

		jQuery(".add1 input, .sub1 input").on("click", function() {
			jQuery(this).select();
		});

		jQuery(".add1, .sub1").keyup(function() {
			var total = 0.0;
			jQuery(".add1 input").each(function(i, e) {

				var val = 0.0; val = parseFloat(jQuery(e).val().replace(",", "")); 
				if(isNaN(val)) {
					val = 0.0; 
				}
				total += val;
			});

			jQuery(".sub1 input").each(function(i, e) {

				var val = 0.0; val= parseFloat(jQuery(e).val().replace(",", "")); 
				if(isNaN(val)) {
					val = 0.0;
				}
				total -= val;
			});
	                     jQuery(".eq1").html("<strong style='display: block; width: 185px;'>$" + total + "</strong>");
	            
			
		});

		jQuery(".eq1").html("<strong style='display: block; width: 185px;'>$0</strong>");
	}



	try {
		if(document.getElementsByClassName("see-toggle-closed").length != 0) {
			document.getElementsByClassName("see-toggle-closed")[0].innerHTML += '<div class="see-toggle"><div id="c-toggle" class="see-toggle-button">See More</div></div>';
			var e = document.getElementById("c-toggle");
			e.onclick = function() {
				var e = document.getElementById("c-toggle");
				if(e.innerText == "See More") {
					e.parentElement.parentElement.setAttribute("class", "left-my-table scaleble");
					e.innerText = "See Less";
				}
				else {
					e.parentElement.parentElement.setAttribute("class", "left-my-table scaleble see-toggle-closed");
					e.innerText = "See More";
				}
			}
		}

		if(document.getElementsByClassName("t-main").length != 0) {
			var content = document.getElementsByClassName("t-main")[0].innerHTML;
			document.getElementsByClassName("t-main")[0].innerHTML = '<div class="t-content closed">' + content + '</div><div class="t-show">See More</div>';
			
			document.getElementsByClassName("t-show")[0].onclick = function() {
				var e = document.getElementsByClassName("t-show");
				var content = document.getElementsByClassName("t-content");
				if(e[0].innerText == "See More") {
					content[0].setAttribute("class", "t-content");
					e[0].innerText = "See Less";
				}
				else {
					content[0].setAttribute("class", "t-content closed");
					e[0].innerText = "See More";
				}
			}
		}

	} catch(e) {}

	// Setting countdown clock on main page
	if(document.getElementById("count-down") != null) {
		var c = document.getElementById("count-down");
		c.class = "";
		c.innerHTML = '<div id="count-down"><div class="block"><div class="number" id="day">00</div><label>Days</label></div><div class="block"><div class="number" id="hour">00</div><label>Hours</label></div><div class="block"><div class="number" id="minute">00</div><label>Minutes</label></div><div class="block"><div class="number" id="second">00</div><label>Seconds</label></div><div style="clear: both"></div></div>';
		window.setInterval(function() {
			var offset = new Date("March 15 2019 16:30:00") - new Date();
			day = Math.floor(offset / 86400000) + "";
			offset -= day * 86400000;
			hour = Math.floor(offset / 3600000) + "";
			offset -= hour * 3600000;
			minute = Math.floor(offset / 60000) + "";
			offset -= minute * 60000;
			second = Math.floor(offset / 1000) + "";

			document.getElementById("day").innerHTML = day.length == 1 ? "0" + day : day;
			document.getElementById("hour").innerHTML = hour.length == 1 ? "0" + hour : hour;
			document.getElementById("minute").innerHTML = minute.length == 1 ? "0" + minute : minute;
			document.getElementById("second").innerHTML = second.length == 1 ? "0" + second : second;
		}, 1000);
	}

	// Donation form 
	if(document.getElementById("drop-donate-form") != null) {
		document.getElementById("drop-donate-form").innerHTML = "<form method='POST' action='https://ps.uif.uillinois.edu/Gifts/Payment.aspx' target='_blank'>  <input type='hidden' name='FUND1' value='33335311/Study Abroad Accessible Frontiers Fund'>  <input type='hidden' name='FUND2' value='33342579/Nina T. Shepherd Scholarship Fund'>  <input type='hidden' name='FUND3' value='33774003/Ikenberry Award SAUIC'>  <input type='hidden' name='FUND4' value='33774008/Curry Endowed Award SAUIC'>  <input type='hidden' name='DEPARTMENT' value='The Study Abroad Office'>  <input type='hidden' name='DEPARTMENT_EMAIL' value='sao@uic.edu'>  <input type='hidden' name='TRACKING_CODE' value='5W123'>  <div class='su-table' style='text-align: left; max-width: 590px; margin: 0 auto;'>   <div style='padding-bottom: 15px'>    <div style='display: inline-block; width: 72%'><div style='font-weight: bold'>Study Abroad Accessible Frontiers Fund</div>  <div style='font-style: italic;'>Scholarships to support the diversity of participation among students from traditionally underrepresented groups.</div></div>    <div style='display: inline-block; width: 25%; line-height: 30px'>$ <input type='text' style='display: inline-block; width: 100px' name='gift_amount1' size='10'></div>   </div>   <div style='padding-bottom: 5px'>    <div style='display: inline-block; width: 72%'><div style='font-weight: bold'>Nina T. Shepherd Scholarship Fund</div>  <div style='font-style: italic;'>Honors the memory of the founding director of the UIC Study Abroad Office.</div></div>    <div style='display: inline-block; width: 25%; line-height: 30px'>$ <input type='text' style='display: inline-block; width: 100px' name='gift_amount2' size='10'></div>   </div>   <div style='padding-bottom: 5px'>    <div style='display: inline-block; width: 72%'><div style='font-weight: bold'>Stanley Ikenberry Award</div>  <div style='font-style: italic;'>A need-based award named for a former president of the University of Illinois.</div></div>    <div style='display: inline-block; width: 25%; line-height: 30px'>$ <input type='text' style='display: inline-block; width: 100px' name='gift_amount3' size='10'></div>   </div>   <div style='padding-bottom: 15px'>    <div style='display: inline-block; width: 72%'><div style='font-weight: bold'>Judy Curry Award</div>  <div style='font-style: italic;'>This award supports students studying abroad in Asia, with a preference for China, and is named for a former UIC professor of Communications.</div></div>    <div style='display: inline-block; width: 25%; line-height: 30px'>$ <input type='text' style='display: inline-block; width: 100px' name='gift_amount4' size='10'></div>   </div>  <input type='submit' value='Submit'>  </div>  </form>";
	}
};
  </script>
