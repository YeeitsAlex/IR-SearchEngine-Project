  <script type="text/javascript">
   if ( undefined !== window.jQuery ) {
    // script dependent on jQuery
	
	$('#same_as').click(function() {
	  	var checked = $(this).prop('checked');
	  	var first_name = $('#billing_first_name').val();
	  	var last_name = $('#billing_last_name').val();
		var student_email = $('#billing_email').val();
	  	var student_phone = $('#billing_phone').val();
		if ($(this).is(":checked")) {
		  $('#student_first_name').val(first_name);
		  $('#student_last_name').val(last_name);
		  $('#student_email').val(student_email);
		  $('#student_phone').val(student_phone);
		} else {
		  $('#student_first_name').val('');
		  $('#student_last_name').val('');
		  $('#student_email').val('');
		  $('#student_phone').val('');
		}
		
	});
  }
  </script>
