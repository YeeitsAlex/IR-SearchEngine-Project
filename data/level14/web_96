   <script type="text/javascript">
    jQuery(
function ($) {
    $('.feature-story-img').hide();
$(document).ready(function () {
    $('.feature-story-img').show();

$('#feature-stories').coinslider({ 
width: 870, // width of slider panel
height: 240, // height of slider pane
spw: 3, // squares per width
sph: 1, // squares per height
delay: 10000, // delay between images in ms
sDelay: 30, // delay beetwen squares in ms
opacity: 0.7, // opacity of title and navigation
titleSpeed: 5000, // speed of title appereance in ms
effect: 'straight', // random, swirl, rain, straight
navigation: true, // prev next and buttons
links : true, // show images as links
hoverPause: true // pause on hover
});

});
//$('.feature-story-img').show();

    $('.toggle div').hide();
    $('.toggle h3').hover(

    );
    $('.toggle h3').click(function() {
        var o = $(this).next('div');
        if (o.is(":visible")) {
            $(this).addClass('closed');
            $(this).removeClass('open');
        } else {
            $(this).addClass('open');
            $(this).removeClass('closed');
        }
        $(this).next('div').toggle({
            'effect':'slideDown',
            'duration' : 'fast'

        });
    });
});
   </script>
