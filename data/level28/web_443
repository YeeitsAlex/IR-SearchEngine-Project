    <div class="content-area grid col-940" id="content-full">
     <main class="site-main full-width" id="main" role="main">
      <article class="post-3364 page type-page status-publish hentry" id="post-3364">
       <div class="post-entry">
        <div class="vc_row wpb_row vc_row-fluid None vc_row-has-fill martech-row-background martech-hero-header-hide-bg-image martech-row-background--dark" style="None">
         <div class="wpb_column vc_column_container vc_col-sm-12">
          <div class="vc_column-inner">
           <div class="wpb_wrapper">
            <div class="ult-spacer spacer-5c84b8ef926fa" data-height="150" data-height-mobile="0" data-height-mobile-landscape="0" data-height-tab="" data-height-tab-portrait="" data-id="5c84b8ef926fa" style="clear:both;display:block;">
            </div>
            <div class="martech-header-hero martech-header-hero-row martech-header-hero--light martech-header-hero--short">
             <div class="martech-header-hero-copy-container">
              <div class="martech-header-hero-background-image">
               <img alt="" class="attachment-story-thumb size-story-thumb" height="230" sizes="(max-width: 600px) 100vw, 600px" src="https://executivemba.wharton.upenn.edu/wp-content/uploads/2019/03/Swati-Chopra-head-2-600x230.png" srcset="https://executivemba.wharton.upenn.edu/wp-content/uploads/2019/03/Swati-Chopra-head-2-600x230.png 600w, https://executivemba.wharton.upenn.edu/wp-content/uploads/2019/03/Swati-Chopra-head-2-300x115.png 300w" width="600"/>
