<script type="text/javascript">
 function hubspotFormAjax(){
        (function($){
                //put in the loader
                var loader = 'https://executivemba.wharton.upenn.edu/wp-content/plugins/martech-chupacabra/includes/martech-shortcodes/../images/load.gif';
                $('#formWrapper').append('<div class="loader"><img src="'+loader+'" /></div>');
                //convert php to js variables if i need em like so
                var postID = 3364;
                //bind to the hubspot form submit
                var postData = $('form.hubspot-form').serializeArray();
                //console.log(postData);
                //get the form variables
                //do the ajaxness
                $.ajax({
                        url: "https://executivemba.wharton.upenn.edu/wp-admin/admin-ajax.php",
                        type:'POST',
                        data: {action:'martech_hubspot_form_submit', formInput:postData, postInfo:postID},
                        success: function(response){
                            console.log('Got this from the server: ' + response);
                            openEventFrame();
                        }
                });


        }(jQuery));
    }
    function openEventFrame() {
        //do the safari check
        //disable safari check for testing of new workflow
        var isSafari = false;//;/Safari/.test(navigator.userAgent) && /Apple Computer/.test(navigator.vendor);
        var isIOS = false;
        if (window.navigator.userAgent.match(/iPad/i) || window.navigator.userAgent.match(/iPhone/i)) {
            // iPad or iPhone
            var isIOS = true;
        }

        //Assign the IDs that are set by the form assembly interface
        var eventService = document.getElementById("eventLink").dataset.service;
        console.log(eventService);
        if(eventService === 'targetx'){
            var firstnameField = 'tfa_3';
            var lastnameField = 'tfa_2';
            var emailField = 'tfa_4';
        } else {
            var firstnameField = 'firstName';
            var lastnameField = 'lastName';
            var emailField = 'email';
        }

        //dynamically change the href of the submit button.
        var myURLString = "&"+firstnameField+ "=" + document.getElementById("hubspot-firstname").value;
        myURLString = myURLString + "&"+lastnameField+ "=" + document.getElementById("hubspot-lastname").value;
        myURLString = myURLString + "&"+emailField+ "=" + document.getElementById("hubspot-email").value;

        //Update 10/20/2016 always open new window
        // alert(document.getElementById("eventLink").href + myURLString);
        window.location.href = document.getElementById("eventLink").href + myURLString;
    }
</script>
