  <script type="text/javascript">
   var duke = {
                useragent: navigator.userAgent.toLowerCase(),
                platform: navigator.platform,
                appVersion: navigator.appVersion,
                calc: function() {
                    this.device = this.useragent.match(/android|webos|iphone|ipad|ipod|blackberry|iemobile|opera/i) ? this.useragent.match(/android|webos|iphone|ipad|ipod|blackberry|iemobile|opera/i)[0] : false;
                    this.screenWidth = (window.innerWidth > 0) ? window.innerWidth : screen.width;
                    if (screen.width > 0 && screen.width < window.innerWidth) {
                        this.screenWidth = screen.width;
                    }

                    this.isMobile = this.screenWidth < 768;
                    this.isTablet = !this.isMobile && (this.device === 'android' || this.device === 'ipad');
                    this.isDesktop = !this.isMobile && !this.isTablet;
                }
            };
            duke.calc();

            if (duke.isMobile) {
                document.write('<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1, user-scalable=0">');
                document.write('<meta name="format-detection" content="telephone=no">');
            }
            else {
                document.write('<meta name="viewport" content="width=1040">');
            }
  </script>
