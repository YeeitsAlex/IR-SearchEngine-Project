   <script type="text/javascript">
    // <![CDATA[
$.validate({
        form: '#newsletter',
        scrollToTopOnError : false,
        onSuccess: function() {

            //get input field values
            var email= $('input[name=newsletterEmail]').val();

            //data to be sent to server
            post_data = {
                'email': email
            };

            $('.newsletter-results').remove();
            $('.newsletter-results-error').remove();

            //Ajax post data to server
            $.ajax({
                type: "POST",
                url: "/files/php/subscribe.php",
                data: post_data
            }).done(function(data, textStatus, jqXHR) {
                $('#newsletter').prepend('<div class="newsletter-results"><div class="alert alert-success col-md-12" role="alert"><strong>Success! You&#39;ve been added. </strong>'+data+'</div></div>');
                $('input[name=newsletterEmail]').val("");

            }).fail(function(data, textStatus, jqXHR) {
                $('#newsletter').prepend('<div class="newsletter-results-error"><div class="alert alert-danger col-md-12" role="alert"><strong>Oh no! </strong>'+textStatus+'</div></div>');
            });

            return false;

        }
    });
// ]]>
   </script>
