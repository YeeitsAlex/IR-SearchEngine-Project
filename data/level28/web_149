  <script type="text/javascript">
   // 
	$(window).on("scroll", function(){
		var scroll = $(window).scrollTop();
		scroll_target = 1850;
 
		if(scroll > scroll_target)
		{
			$(".dean-section").removeClass("animated fadeOut");
			$(".dean-section").addClass("animated fadeIn");

            $(".thedean").removeClass("animated slideOutLeft");
            $(".thedean").addClass("animated slideInLeft");
			
   		}
		else
		{
			$(".dean-section").removeClass("animated fadeIn");
			$(".dean-section").addClass("animated fadeOut");


            $(".thedean").removeClass("animated slideInLeft");
            $(".thedean").addClass("animated slideOutLeft");

		}
		console.log("Scroll Position: " + scroll);
}); //
  </script>
