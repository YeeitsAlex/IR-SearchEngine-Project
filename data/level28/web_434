       <script>
        var system = {"name":"Lamson Library"};
                    springSpace.publicObj = new springSpace.public.Public({
                        constant: {
                            PROCESSING: {
                                
                            },
                            CONTENT: {
                            }
                        },
                        system_name: system.name,
                        customer: {"name":"Plymouth State University"}
                    });
