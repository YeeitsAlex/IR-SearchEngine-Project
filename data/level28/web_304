     <main id="content" role="main">
      <div class="sub-middle-section" id="stl-main-content">
       <!-- homepage banner -->
       <article about="/impact" class="node-7509 node node-page view-mode-full" typeof="foaf:Document">
        <div class="header-image header-featured">
         <div class="inner">
          <a href="meet-our-donors">
           <h1 class="page-title">
            Leave a legacy with lasting impact
           </h1>
          </a>
         </div>
        </div>
       </article>
       <!-- end -->
       <!-- journey/get started -->
       <div class="block block-views block-section even" id="stl-journey-getting-started-container">
        <div class="stl-journey stl-clearfix" id="stl-journey-container">
         <div class="stl-journey-paths journey-paths adventure clearfix">
          <h3 class="hidden-xs visible-md visible-lg">
           You Decide How You Give. Start Here.
          </h3>
          <p>
           Take a look at the different options that you think may work best.
          </p>
          <div class="journey-main stl-colwrap" style="">
           <div class="stl-col stl-w-04">
            <div class="stl-info-slider">
             <img alt="" src="images/Stelter/master-site/ph-196x170.gif"/>
             <h4>
              Giving Amount
             </h4>
             <script>
              function setSessionValue(key, value) {
            $.post('../../../Action/setSession.aspx?key=' + key + '&value=' + value);
        }
             </script>
             <ul>
              <li>
               <a class="stl-popup" data-target="#157" data-toggle="modal" href="#157" onclick="setSessionValue('Giving Amount', 'Under+%2425%2c000');">
                Under $25,000
               </a>
              </li>
              <li>
               <a class="stl-popup" data-target="#155" data-toggle="modal" href="#155" onclick="setSessionValue('Giving Amount', '%2425%2c000+to+%2499%2c999');">
                $25,000 to $99,999
               </a>
