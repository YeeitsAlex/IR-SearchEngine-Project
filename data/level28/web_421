     <script>
      $(function () {
      	awardsnews('http://news.uark.edu/rssfeeds/feed?Type=RSS&FbyS=1&sch=14');

            
    });
	
	
 
    function awardsnews(url) {
       $.ajax({
           url: document.location.protocol + '//ajax.googleapis.com/ajax/services/feed/load?v=1.0&num=5&callback=?&q=' + encodeURIComponent(url),
           dataType: 'json',
           success: function (data) {
           var newsandevents = '';
           $.each(data.responseData.feed.entries, function () {
           		var headline = this.title;
		   		headline = headline.replace("â€˜","'").replace("â€™","'").replace("â€œ","\"").replace("â€","\"");
                var link =  this.link; 
                newsandevents += '<li><a href="' + link + '">' + headline + '</a></li>';
           	})
           $(newsandevents).appendTo("#newsandevents");
           }
        });
          
     }
     </script>
