  <script>
   jQuery(document).ready(function() {
		var isMobileKEO = ( function(a) { return /Mobile|iP(hone|od|ad)|Android|BlackBerry|IEMobile|Kindle|NetFront|Silk-Accelerated|(hpw|web)OS|Fennec|Minimo|Opera M(obi|ini)|Blazer|Dolfin|Dolphin|Skyfire|Zune/i.test(a) }) ( navigator.userAgent || navigator.vendor || window.opera);
	if (!isMobileKEO) {
		jQuery('#canvas').css({'background-image':'url("/wp-content/themes/keough/images/globe-rotation0001.png")','background-size':'cover'});
	
		var animationplayed = false;
	
		var waypoint = new Waypoint({
		  element: document.getElementById('canvas'),
		  handler: function() {
		    if (animationplayed == false) {
		    	init();
		    	animationplayed = true;
		    }
		  },
		  offset: '86%'
		});
	
		var canvas, stage, exportRoot;
	
		
	} else {
		//jQuery('#canvas').css({'background-image':'url("/wp-content/uploads/2015/05/globe.jpg")','background-size':'cover'});
		jQuery('.canvas-wrapper').remove();
		jQuery('.canvas-wrapper-static').removeClass('visible-xs');
		jQuery('.canvas-wrapper-static').removeClass('visible-sm');
	}
	
	function init() {
			canvas = document.getElementById("canvas");
			images = images||{};
	
			var loader = new createjs.LoadQueue(false);
			loader.addEventListener("fileload", handleFileLoad);
			loader.addEventListener("complete", handleComplete);
			loader.loadManifest(lib.properties.manifest);
		}
	
		function handleFileLoad(evt) {
			if (evt.item.type == "image") { images[evt.item.id] = evt.result; }
		}
	
		function handleComplete() {
			exportRoot = new lib.globerotation();
	
			stage = new createjs.Stage(canvas);
			stage.addChild(exportRoot);
			stage.update();
	
			createjs.Ticker.setFPS(lib.properties.fps);
			createjs.Ticker.addEventListener("tick", stage);
		}
	
	
	
	
		
	
	jQuery('#vtest').vide({
		mp4: "https://keough.nd.edu/wp-content/uploads/2015/05/Keou-home-vid_1-368Mbps-2.mp4",		webm: "https://keough.nd.edu/wp-content/uploads/2015/05/Keou-home-vid_1-368Mbps-2.webmsd.webm",		ogv: "https://keough.nd.edu/wp-content/uploads/2015/05/Keou-home-vid_1-368Mbps-2.oggtheora.ogv",		poster: "https://keough.nd.edu/wp-content/uploads/2015/05/Keough_Still_Frame_2_optim.jpg"
	}, {
		autoplay: true,
		loop: true,
		muted: true,
		position: "50% 0%",
		posterType: 'jpg'
	});
	
	// Get instance of the plugin
	var instance = jQuery('#vtest').data('vide');
	
	if(instance != null){
	
	// Get video element of the background. Do what you want.
	var video = instance.getVideoObject();
	
	// Pause video when it is off screen, unpause when on screen
	var waypoint = new Waypoint({
		element: video,
		handler: function(direction) {
		if (direction === 'down') {
			video.pause();
		}
		if (direction === 'up') {
			video.play();
		}
		},
		offset: '-100%'
	})
	
	}
	
	
	
	
	
	
	
	

	
	
	
	
	
	
	
	});
  </script>
  <script>
   // Add IDs to content links
    jQuery('#primary a').each( function(){
		var url = jQuery(this).attr('href');
		
		if (typeof url !== typeof undefined && url !== false) {
			if(url.substr(url.length - 1) == '/') {
		        var url = url.substr(0, url.length - 1);
		    }
	    
			var parts = url.split('/');
			var lastPart = parts.pop(); // == '' ? parts[parts.length - 1] : parts.pop(); // The last part of the url
			
			if( !jQuery(this).attr('id') ) { // If the link doesn't already have an id
				jQuery(this).attr('id', 'Content_' + lastPart);
			}
		}
	});
	
	jQuery('.site-footer a').each( function(){
		var url = jQuery(this).attr('href');
		
		if (typeof url !== typeof undefined && url !== false) {
			if(url.substr(url.length - 1) == '/') {
		        var url = url.substr(0, url.length - 1);
		    }
	    
			var parts = url.split('/');
			var lastPart = parts.pop(); // == '' ? parts[parts.length - 1] : parts.pop(); // The last part of the url
			
			if( !jQuery(this).attr('id') ) { // If the link doesn't already have an id
				jQuery(this).attr('id', 'Footer_' + lastPart);
			}
		}
	});
  </script>
