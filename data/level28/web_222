  <script type="text/javascript">
   // <![CDATA[
$(window).load(function(){	

	if ( $.fn.waypoint ) {
		$( '.gc-waypoint' ).waypoint( {
			offset: '75%',
			handler: function() {
				$(this).addClass( 'gc-animated' );
			}
		} );
	}

	$.scrollUp({
		scrollName: 'scrollUp',
		topDistance: '300',
		topSpeed: 300,
		animation: 'fade',
		animationInSpeed: 200,
		animationOutSpeed: 200,
		scrollText: '',
		activeOverlay: false,
	});
	
	// ACCORDION chevron rotation
	$('.collapse.in').prev('.panel-heading').addClass('active');
	$('.panel-group')
		.on('show.bs.collapse', function(a) {
		$(a.target).prev('.panel-heading').addClass('active');
	})
	.on('hide.bs.collapse', function(a) {
		$(a.target).prev('.panel-heading').removeClass('active');
	});
});		

$('.collapse').collapse()


// Stop Youtube video on modal hide
$('.modal').bind('hidden.bs.modal', function (e) {
	var vid = $('.modal iframe[src*="youtube"]');
	if ( vid.length > 0 ){
		var src = null;
		src = vid.attr('src').replace("autoplay=1", "autoplay=0");
		vid.attr('src', '');
		vid.attr('src', src);
	}
});
// ]]>
  </script>
