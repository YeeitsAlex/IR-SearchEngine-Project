   <section aria-label="global campus news items" role="region">
    <div class="container">
     <div class="row">
      <div class="col-md-12">
       <h2 class="top" id="global-campus-headlines">
        GLOBAL CAMPUS
        <strong>
         HEADLINES
        </strong>
       </h2>
      </div>
      <script type="text/javascript">
       // 
					$(function () {
						$.ajax({
							url: '//campusdata.uark.edu/apiv2/articles?$orderby=publishDate+desc&$top=3&$filter=((Schools/any(s4:+s4/id+eq+4)))',
							data: {},
							dataType: 'jsonp',
		//jsonpCallback: 'Articles',
							cache: 'true',
							success: function (articles) {
								dataDisplay = "";
								$.each(articles, function (key, a) {
									var content = "", image = "";
									var date = new Date(a.publishDate);
									date = date.toDateString();
									if (a.brief != null && a.brief != "") { content = a.brief; }
									var briefText = content.replace(/(<p>|<\/p>)/g, "");
									if (a.image != null && a.image != "") {
										image = a.image;
									} else { image = "PMUA_Logo-web.jpg"; }
									dataDisplay = "<div class='col-sm-4 news-item'>" +
											"<div class='row'>" +
												"<div class='col-md-4'>" +
													"<img class='img-responsive img-thumbnail' src='//campusdata.uark.edu/resources/images/articles/" + image + "?w=150' alt='" + a.headline + "' /></a>" +
												"</div>" +

												"<div class='col-md-8'>" +
													"<p class='headline'><strong><a href='/news.php?id=" + a.id + "'>" + a.headline + "</a></strong></p>" +
													//"<i>" + date + "</i>" +
													"<p>" + briefText + "</p>" +
												"</div>" +
											"</div>" +
										"</div>";
									$(".newsfeed").append(dataDisplay);
								});


							}
						});

					});
					//
      </script>
      <div class="newsfeed">
      </div>
     </div>
    </div>
   </section>
