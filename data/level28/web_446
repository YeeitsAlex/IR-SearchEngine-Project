<script>
 function slideHeight(){
			return $(".slide").height();
		}
		$(document).ready(function(){
			if($(".slide").length > 0){
			
				  $("#slides").slidesjs({
					height: slideHeight(),
					play: {
						auto: true,
						interval: 5000
					}, 
					callback: {
						loaded: function(number){
							$(".slidesjs-container").css("height", slideHeight());
						}
					}
				  });
			}  
			$("#tabs").tabs();
		});
</script>
