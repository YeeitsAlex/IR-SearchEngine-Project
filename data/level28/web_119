        streamOpts['s-smart-p'] = "0";
        streamOpts['s-tablet-l'] = "0";
        streamOpts['s-tablet-p'] = "0";
    }
    else if ( streamOpts.layout == 'list' ) {  /*the same with list, we only need news feed style*/
        streamOpts['layout'] = 'masonry';
