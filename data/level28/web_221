   <section class="main">
    <!-- full width section-->
    <div class="gcb-twocol-front">
     <section class="front-top-1">
      <div class="container">
       <div class="row">
        <div class="panel-pane pane-custom pane-1 block-title whats-new">
         <div class="pane-content">
          <h2>
           What's New
          </h2>
         </div>
        </div>
       </div>
      </div>
     </section>
     <section class="front-middle-1">
      <div class="container">
       <div class="row">
        <div class="col-md-8">
         <div class="">
          <div class="panel-pane pane-views-panes pane-news-view-front-news-pane news-front-block" id="news-front">
           <div class="pane-content">
            <div class="view view-news-view view-id-news_view view-display-id-front_news_pane view-dom-id-41ae4a2dbcc6988ed99b44506fe672aa view-front view-front">
             <div class="view-content">
              <div class="views-gcbbootstrap-grid-plugin-style test" id="views-bootstrap-grid-2">
               <div class="row news_teaser_row news_teaser_front_page">
                <div class="col news_teaser_wrapper col-xs-12 col-sm-6 col-md-6">
                 <article class="news_teaser">
                  <div class="image-date news-image-date views-fieldset" data-module="views_fieldsets">
                   <div class="news-image">
                    <a href="/news/fri-03082019-1445/get-focused">
                     <img alt="two students in lab" class="img-responsive" height="191" src="https://genome.duke.edu/sites/genome.duke.edu/files/styles/gcb_480x191/public/focus.jpg?itok=IjS0vmTs" typeof="foaf:Image" width="480"/>
                    </a>
                   </div>
                  </div>
