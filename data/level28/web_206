      <!-- <script>
$(function ()
{
parseAnnouncements('https://registrar.uark.edu/announcements/registrar.rss');
});

function parseAnnouncements(url)
{
$.ajax({
url: document.location.protocol + '//ajax.googleapis.com/ajax/services/feed/load?v=1.0&amp;num=6&amp;callback=?&amp;q=' + encodeURIComponent(url),
dataType: 'json',
cache: 'true',
success: function (data)
{
var announcements = '';
$.each(data.responseData.feed.entries, function ()
{
var headline = this.title;
var link = this.link;
var content = this.content;
console.log (link.substr(-3));
if (link == 'https://registrar.uark.edu/index.php')  // If no link was specified, don't include "Learn more"
{announcements += '<h3>' + headline + '</h3><p>' + content + '</p>';}
else if ((link.substr(-3) == "pdf") || (link.substr(-3) == "doc")  || (link.substr(-4) == "docx"))  // The link is to a PDF or Word doc, so change the "Learn More" to "Download"
{announcements += '<h3>' + headline + '</h3><p>' + content + ' <a href="' + link  + '">Download</a></p>';}
else {
announcements += '<h3>' + headline + '</h3><p>' + content + ' <a href="' + link  + '">Learn&nbsp;more&hellip;</a></p>';
}
})

$(announcements).appendTo("#announcements_list");
}
});

}
</script> -->
