<script type="text/javascript">
 jQuery(function($){

    // Check if alert has been closed
    if( $.cookie('alert-survey') === 'closed' ){
        $('.alert-survey').hide();
    }

    $('.alert-close').click(function( e ){
        //e.preventDefault();
        $.cookie('alert-survey', 'closed', { expires: 7, path: '/' });
				jQuery(this).parent().fadeOut('fast', function(c){
				});
    });

	});
</script>

