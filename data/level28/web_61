         <div class="landing-col span4">
          <section aria-label="twitter" class="section-twitter">
           <h2 class="section-header">
            Twitter
           </h2>
           <div class="twitter-feed list" data-handle="/social-stream twitterStream copy" data-module="twitter-feed" data-view="list">
            <div class="feed">
             <div class="clearfix">
              <script id="twitter-template" type="text/template">
               <div class="head">
                        <h1>Tweets</h1>
                    </div>
                                <% _.each( statuses, function( item ){ %>
                    <article class="tweet clearfix">
                        <div class="info">
                            <div class="userinfo">
                                <a href="https://www.twitter.com/<%- item.user.screen_name %>" target="_blank">
                                    <span class="icon gicon-twitter"></span>
                                    <span class="logo"><img src="<%- item.user.profile_image_url_https %>" alt=""></span>                                
                                    <span class="title"><%- item.user.name %></span>
                                    <span class="user"><%- item.user.screen_name %></span>
                                </a>
                            </div>
                            <div class="tweet-info">
                                                                    <div class="text"><%= item.linkedText %></div>
                                                                <% if (item.media_entities.length) { %>
									<div class="thumb" data-src="<%- item.media_entities[0].media_url_https %>" data-alt="<%- item.media_entities[0].alt_text %>"></div>
                                <% } %>
                                                            </div>
                            <div class="links">
                                <a href="https://twitter.com/intent/tweet?in_reply_to=<%- item.id_str %>" class="icon gicon-reply action-button" role="button" aria-label="reply this tweet created by <%- item.user.screen_name %> on <%- item.date %> <%- item.time %>" target="blank"><span class="text">Reply</span></a>
                                <a href="https://twitter.com/intent/retweet?tweet_id=<%- item.id_str %>" class="icon gicon-retweet action-button" role="button" target="blank" aria-label="retweet this tweet created by <%- item.user.screen_name %> on <%- item.date %> <%- item.time %>"><span class="text">Retweet</span></a>
                                <a href="https://twitter.com/intent/favorite?tweet_id=<%- item.id_str %>" class="icon gicon-favorite action-button" role="button" target="blank" aria-label="favorite this tweet created by <%- item.user.screen_name %> on <%- item.date %> <%- item.time %>"><span class="text">Favorite</span></a>
                                <span class="date"><%- item.date %></span>
                            </div>                        
                        </div>
                    </article>
                <% }); %>
              </script>
             </div>
            </div>
           </div>
          </section>
