  <script>
   jQuery(document).ready(function ($) {
    var images = [{
        url: "images/carousel/1.jpg",
        alt: "outreach pics"
    }, {
        url: "images/carousel/2.jpg",
        alt: "outreach pics"
    }, {
        url: "images/carousel/3.jpg",
        alt: "outreach pics"
    }, {
        url: "images/carousel/4.jpg",
        alt: "outreach pics"
    }, {
        url: "images/carousel/5.jpg",
        alt: "outreach pics"
    }, {
        url: "images/carousel/6.jpg",
        alt: "outreach pics"
    }, {
        url: "images/carousel/7.jpg",
        alt: "outreach pics"
    }, {
        url: "images/carousel/8.jpg",
        alt: "outreach pics"
    }, {
        url: "images/carousel/9.jpg",
        alt: "outreach pics"
    }, {
        url: "images/carousel/10.jpg",
        alt: "outreach pics"
    }, {
        url: "images/carousel/11.jpg",
        alt: "outreach pics"
    }, {
        url: "images/carousel/12.jpg",
        alt: "outreach pics"
    }, {
        url: "images/carousel/13.jpg",
        alt: "outreach pics"
    }, {
        url: "images/carousel/14.jpg",
        alt: "outreach pics"
    }];

    // http://css-tricks.com/snippets/javascript/shuffle-array/
    images.sort(function () { return 0.5 - Math.random(); });

    $.each(images, function (index, image) {

        var element = $('<div class="item"><a href="#"><img src="' + image.url + '" alt="' + image.alt + '" /></a></div>')

        if (index === 0) {
            element.addClass("active");   
        }

        element.appendTo("div.carousel-inner");
    });

    $('.carousel').carousel({
      interval: 4000,
    })
});
  </script>
