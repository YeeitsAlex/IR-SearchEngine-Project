    <figure>
     <a href="/cdm/landingpage/collection/p17212coll6">
      <img alt="" src=""/>
     </a>
     <figcaption>
      <a href="/cdm/landingpage/collection/p17212coll6">
       A Belated Gift - U.S. Copyright and the Public Domain
      </a>
     </figcaption>
    </figure>
    <p class="text">
    </p>
    <p>
     <a class="btn btn-default uaRed" href="/cdm/landingpage/collection/p17212coll6">
      Browse this Collection
     </a>
    </p>
   </div>
   <script>
    $.ajax({
  		url: '/utils/getcollectionhomepagetext/collname/' + encodeURIComponent("A Belated Gift - U.S. Copyright and the Public Domain") + '/collection/p17212coll6', // fetch collection thumbnails and text.
  		success: function (col) {
   			$('#collection-alpha > .text').append($(col.html).find('#digcollhometext').text());	
   			$('#collection-alpha').find('img').attr('src',$(col.html).find('img').last().attr('src'));
   			
   			console.log( $(col.html).find('img').last().attr('src') );
  		}
});
   </script>
   <script>
    function fetch_collection(title, slug) {

$.ajax({
  		url: '/utils/getcollectionhomepagetext/collname/' + encodeURIComponent(title) + '/collection/' + slug, // fetch collection thumbnails and text.
  		success: function (col) {
   			$('[data-key="' + slug +'"] > .text').append($(col.html).find('#digcollhometext').text()); 
    		var background_thumb = $(col.html).find('img').last().attr('src');
    		if (background_thumb && background_thumb.indexOf('loading_100.gif') == -1) {
        		$('[data-key="' + slug +'"] > a').css('background-image','url(' + background_thumb + ')');
    		}
  		}
	});

}
   </script>
