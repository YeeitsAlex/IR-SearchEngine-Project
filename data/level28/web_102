  setupSideNav();
  addImgResponsiveClass();
  activeMainNav();
  //directedit();
});//end ready
// set active nav link , nest child navs
function setupSideNav(){
    // append child navs, mark selected items
  var current_link  = $('.sidenav a[href="'+current_path+'"]');
  current_link.addClass('current');
  $('.sidenav ul:not([data-nav-path="/index.php"])').each(function(){
   var data_nav_path = $(this).attr('data-nav-path');
   var parent_link = $('.sidenav a[href="'+data_nav_path+'"]');
   $(this).appendTo(parent_link.parent());
   parent_link.addClass('current').parent().addClass('active');
  });//end each ul, append nav tree
}
function addImgResponsiveClass()
{
	//  iterate through any image found within main-container div
	$("#main-container img").each(function () { 
		$('img:not(.img-responsive, .no-img-responsive, .media)').addClass("img-responsive");
	});
}
function activeMainNav() 
{
	var current_path = window.location.pathname;
	// iterate through ul with id=mainnav and all child li's 
	$("ul#mainnav li").each(function () { 
		// if child a href is equal to current page URL path, add class current
		$(this).children('a[href="'+current_path+'"]' ).addClass('current');
		// match any URLs whereby index.php isn't specified, but href ends with directory/index.php
		if (current_path.substr(-1) === "/") {
			$(this).children('a[href="'+current_path+'index.php"]' ).addClass('current');
		}
		// match any URLs whereby index.php is specified, but href only ends with respective directory
		if (/index.php/i.test(current_path)){
			var res = current_path.split(/\bindex.php\b/ig); 
			$(this).children('a[href="'+res[0]+'"]' ).addClass('current');
		}
	});
}

