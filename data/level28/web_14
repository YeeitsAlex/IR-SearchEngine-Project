   <section class="section section-content" id="section-content">
    <div class="zone-wrapper zone-content-wrapper clearfix" id="zone-content-wrapper">
     <div class="zone zone-content clearfix container-12" id="zone-content">
      <div class="grid-12 region region-content region-weight_2" id="region-content">
       <div class="region-inner region-content-inner equal-height-container">
        <a id="main-content">
        </a>
        <div class="block block-system block-main block-system-main odd block-without-title equal-height-element" id="block-system-main">
         <div class="block-inner clearfix">
          <div class="content clearfix">
           <div class="panel-display mygranite-grid mygranite-12-twocol-8-split-4-stacked" id="homepage">
            <div class="left-container grid-8">
             <div class="panel-panel grid-8 lefttop">
              <div class="inside">
               <div class="panel-pane pane-views pane-slideshow">
                <div class="pane-content">
                 <div class="view view-slideshow view-id-slideshow view-display-id-dept_image_slideshow_block slideshow view-dom-id-da605c4800d03e90f3f18e9b628d399a">
                  <div class="view-content">
                   <div class="skin-default">
                    <div class="flexslider_views_slideshow_main views_slideshow_main" id="flexslider_views_slideshow_main_slideshow-dept_image_slideshow_block_1">
                     <div class="flex-nav-container">
                      <div class="flexslider">
                       <ul class="flexslider-views-slideshow-main-frame slides" id="flexslider_views_slideshow_slideshow-dept_image_slideshow_block_1">
                        <li class="flexslider-views-slideshow-main-frame-row flexslider_views_slideshow_slide views-row-1 views-row-odd">
                         <div class="views-field views-field-nothing">
                          <span class="field-content">
                           <a href="https://casprod.granite.edu/cas/login?service=https%3A//gscssomgr.unh.edu%3A443/ssomanager/c/SSB" target="_blank">
                            <img alt="" height="255" src="https://my.granite.edu/sites/default/files/styles/homepage/public/field/image/mygranite_hero_reg-open_11.png?itok=2eHoePwx" typeof="foaf:Image" width="620"/>
                           </a>
                           <div class="caption">
