        jQuery(document).ready(function () {
	jQuery('a[href*="https://uwlax.photoshelter.com/gallery/"]').each(function () {
		if (jQuery(this).text().toLowerCase().indexOf("gallery: ") == 0) {
			var photoAlbum = jQuery(this).attr("href").match(/\/G00(.*)\//g).toString().replace(/\//g, "");

			jQuery(this).after('<h3>' + jQuery(this).text() + '</h3><div class="photo-album-embed" data-ps-embed-type="slideshow" data-ps-embed-gid="' + photoAlbum + '"></div>');
			jQuery(this).remove();
		}
	});
