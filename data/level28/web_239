     <script>
      $( document ).ready(function() {

	$('#tabs').find('.dropdown-menu > li > a').click(function() {
		$('#tabs').find('.dropdown-menu > li.active').removeClass('active');
		$('#search-content').find('.searchchoice.active').removeClass('active');
  		$(this).parents('li').addClass('active');

		$('#' + $(this).attr('data-search-target')).addClass('active');
	});


