      <script>
       $( document ).ready(function() {
    $('#pageURL').val(window.location.href);
});

$( '#email-this-form' ).submit(function( event ) {
  event.preventDefault();
  
	$( '#email-this-form' ).prepend('<div class="throbber"><i class="fa fa-spinner fa-pulse"></i></div>');
	$( '#email-this-form' ).css('opacity',0.4);
	$('#email-this-form > button' ).prop('disabled', true);
  
  var $form = $( this );
  title = $form.find( 'input[name="title"]' ).val(),
    pageURL = $form.find( 'input[name="pageURL"]' ).val(),
    SENDTO = $form.find( 'input[name="SENDTOEMAIL"]' ).val(),
    recipient = $form.find( 'input[name="recipient"]' ).val(),
    post_to = $form.attr( 'action' );
 
  var posting = $.post( $form.attr( 'action' ), { title: $form.find( 'input[name="title"]' ).val(), pageURL: $form.find( 'input[name="pageURL"]' ).val(), SENDTO: SENDTO, recipient: recipient } );
 
  posting.done(function( data ) {
  
  console.log(data);
      if(data == 'success'){
      	$('#email-this').modal('hide');
      } else {
      $( '#email-this-form' ).css('opacity',1);
      		$('#email-this-form').empty().html('<div class="well"><p>There was a problem sending your email. Please notify <a href="mailto:dshurd@uark.edu">Web Services</a>.</p></div>');

      }
  });
});
      </script>
