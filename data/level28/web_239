     <script>
      $.ajax({
    type: 'GET',
    url: '//calendars.uark.edu/RSSSyndicator.aspx?type=N&department=4',
    data: {},
    cache: 'true',
    success: function (data) {
        var list_data = '';
        var $xml = $(data).find('item:lt(5)');
        $xml.each(function () { var $this = $(this);
        var headline = $this.find('title').text();
        var link = $this.find('link').text();
        list_data += '<li><a href="' + link + '">' + headline + '</a></li>';
    });
    $(list_data).appendTo("#upcoming");
    if ($(list_data).length === 0) {$('#upcoming-container').hide()};
    }
    });
     </script>
