      <script>
       $(function(){
    $('#discovery').find('.dropdown-menu li a').click(function(){
        $('.dropdown-toggle .selected').text($(this).text()); // replaces text in dropdown-toggle button (visible only on mobile) with current selection
        $('#tabs').find('.dropdown-menu > li.active').removeClass('active');
		$('#search-content').find('.searchchoice.active').removeClass('in').removeClass('active');
  		$(this).parents('li').addClass('active'); // makes clicked tab active
		$('#' + $(this).attr('data-search-target')).addClass('in').addClass('active'); // makes corresponding search box visible
   });

});
      </script>
