       <div class="post-entry">
        <div class="vc_row wpb_row vc_row-fluid main vc_custom_1502208170211 martech-row-background martech-row-background--dark None vc_row-has-fill" style="None">
         <div class="HPdoctxt wpb_column vc_column_container vc_col-sm-12">
          <div class="vc_column-inner">
           <div class="wpb_wrapper">
            <div class="martech-header-hero martech-header-hero-row martech-header-hero--light martech-header-hero--short">
             <div class="martech-header-hero-copy-container">
              <div class="martech-header-hero-background-image">
               <img alt="" class="attachment-story-thumb size-story-thumb" height="230" sizes="(max-width: 600px) 100vw, 600px" src="https://undergrad.wharton.upenn.edu/wp-content/uploads/UGRAD_Home_1900x580-600x230.jpg" srcset="https://undergrad.wharton.upenn.edu/wp-content/uploads/UGRAD_Home_1900x580-600x230.jpg 600w, https://undergrad.wharton.upenn.edu/wp-content/uploads/UGRAD_Home_1900x580-300x115.jpg 300w" width="600"/>
              </div>
              <h1 style="text-align: center;">
               <span style="color: #ffffff;">
                The best of both worlds: Business and More
               </span>
              </h1>
             </div>
            </div>
           </div>
          </div>
         </div>
        </div>
        <!-- Row Backgrounds -->
        <div class="upb_bg_img" data-animation-repeat="repeat" data-bg-animation="left-animation" data-bg-animation-type="h" data-bg-img-position="" data-bg-img-repeat="no-repeat" data-bg-img-size="cover" data-bg-override="ex-full" data-bg_img_attach="scroll" data-custom-vc-row="" data-fadeout="" data-fadeout-percentage="30" data-image-id="id^4271|url^https://undergrad.wharton.upenn.edu/wp-content/uploads/UGRAD_Home_1900x580.jpg|caption^null|alt^null|title^UGRAD_Home_1900x580|description^null" data-img-parallax-mobile-disable="true" data-is_old_vc="" data-overlay="true" data-overlay-color="rgba(0,0,0,0.23)" data-overlay-pattern="" data-overlay-pattern-attachment="scroll" data-overlay-pattern-opacity="0.8" data-overlay-pattern-size="" data-parallax-content="" data-parallax-content-sense="30" data-parallx_sense="30" data-row-effect-mobile-disable="true" data-rtl="false" data-theme-support="" data-ultimate-bg="url(https://undergrad.wharton.upenn.edu/wp-content/uploads/UGRAD_Home_1900x580.jpg)" data-ultimate-bg-style="vcpb-default" data-upb-bg-animation="" data-upb-overlay-color="rgba(0,0,0,0.23)" data-vc="5.7">
        </div>
        <!-- Row Backgrounds -->
        <div class="upb_bg_img" data-animation-repeat="repeat" data-bg-animation="left-animation" data-bg-animation-type="h" data-bg-img-position="" data-bg-img-repeat="repeat" data-bg-img-size="cover" data-bg-override="ex-full" data-bg_img_attach="scroll" data-custom-vc-row="" data-fadeout="" data-fadeout-percentage="30" data-image-id="id^4271|url^https://undergrad.wharton.upenn.edu/wp-content/uploads/UGRAD_Home_1900x580.jpg|caption^null|alt^null|title^UGRAD_Home_1900x580|description^null" data-img-parallax-mobile-disable="true" data-is_old_vc="" data-overlay="false" data-overlay-color="" data-overlay-pattern="" data-overlay-pattern-opacity="" data-overlay-pattern-size="" data-parallax-content="" data-parallax-content-sense="30" data-parallx_sense="30" data-row-effect-mobile-disable="true" data-rtl="false" data-theme-support="" data-ultimate-bg="url(https://undergrad.wharton.upenn.edu/wp-content/uploads/UGRAD_Home_1900x580.jpg)" data-ultimate-bg-style="vcpb-default" data-upb-bg-animation="" data-upb-overlay-color="" data-vc="5.7">
