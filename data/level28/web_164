     <script>
      if ('' == 'true') {
        $('#div-nav').hide();
        $(document).ready(function () {
            adjustDivNav();
            $(window).resize(adjustDivNav);
            $('#menu-link').click(function () {
                $('#div-nav').toggle();
                $('#div-nav > ul').css({'margin-left': '-7px', 'margin-top': '0'});
                $('#div-nav > ul > li').css('min-width', '100%');
            });
        });

        function adjustDivNav() {
            var siteNavCount = 0;
            if (siteNavCount != 0){
                $('#div-nav').show();
            }
            else{
                $('.content-container').css('margin-top', '10px');
            }

            $('#div-nav > ul > li').css('width', 100 / siteNavCount + '%');

            if ($("#streaming_wrapper").length && $('#streaming_wrapper').css('display') != 'none') {
                $('#div-nav').css('margin-top', '-30px');
                $("#streaming_wrapper").prepend($('#div-nav'));
            }
            else if ($("#local_crop").length && $('#local_crop').css('display') != 'none') {
                $("#local_crop").prepend($('#div-nav'));
            }
            else if ($('.carousel').length) {
                $('.carousel').prepend($('#div-nav'));
            }
            else {
                $('#div-nav').hide();
                $('#div-nav').insertBefore('.header-media:first');
            }
        }
    }
     </script>
