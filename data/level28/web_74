       <div id="signup">
        <header>
         <h2>
          Duke Engineering Newsletter
         </h2>
         <a href="/about/news/duke-engineering-now">
          View archives
         </a>
        </header>
        <p>
         <em>
          Duke Engineering Now
         </em>
         is an e-newsletter sent to alumni and friends of the Pratt School of Engineering at Duke University.
        </p>
        <form accept-charset="UTF-8" action="https://app.icontact.com/icp/signup.php" id="icpsignup2016" method="post" name="icpsignup" onsubmit="return verifyRequired2016();">
         <input name="redirect" type="hidden" value="http://www.icontact.com/www/signup/thanks.html"/>
         <input name="errorredirect" type="hidden" value="http://www.icontact.com/www/signup/error.html"/>
         <fieldset>
          <label for="fields_email">
           Email Address
          </label>
          <input id="fields_email" name="fields_email" placeholder="name@company.com" type="text">
          </input>
         </fieldset>
         <input name="listid" type="hidden" value="31803"/>
         <input name="specialid:31803" type="hidden" value="WQ4G"/>
         <input name="clientid" type="hidden" value="1411105"/>
         <input name="formid" type="hidden" value="2016"/>
         <input name="reallistid" type="hidden" value="1"/>
         <input name="doubleopt" type="hidden" value="0"/>
         <button>
          Subscribe
         </button>
        </form>
        <script type="text/javascript">
         var icpForm2016 = document.getElementById('icpsignup2016');

if (document.location.protocol === "https:")

	icpForm2016.action = "https://app.icontact.com/icp/signup.php";

function verifyRequired2016() {
	if (icpForm2016["fields_email"].value == "") {
		icpForm2016["fields_email"].focus();
		alert("The Email field is required.");
		return false;
	}

	if (icpForm2016["fields_email"].value && icpForm2016["fields_email"].value !== "") {
		icpForm2016["fields_email"].value = icpForm2016["fields_email"].value.trim();
	}



	return true;
}

function validDate2016(value) {

	if (value == "") {
		return true;
	}

	var dateSections = value.split('/');
	if (dateSections.length != 3) {
		return false;
	}

	var month = dateSections[0];
	if (month && (month.length > 2 || month < 1 || month > 12)) {
		return false;
	}

	var day = dateSections[1];
	if (day && (day.length > 2 || day < 1 || day > 31)) {
		return false;
	}

	var year = dateSections[2];
	if (!year || (year && year.length != 4)) {
		return false;
	}

	var dateValue = new Date(value);
	if (month != (dateValue.getMonth()+1) || day != dateValue.getDate() || year != dateValue.getFullYear()) {
		return false;
	}

	return true;
}
        </script>
