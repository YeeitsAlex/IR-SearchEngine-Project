	s.src = !("IntersectionObserver" in w) ? "https://summer.wisc.edu/wp-content/plugins/wp-rocket/inc/front/js/lazyload-8.15.2.min.js" : "https://summer.wisc.edu/wp-content/plugins/wp-rocket/inc/front/js/lazyload-10.17.min.js";
