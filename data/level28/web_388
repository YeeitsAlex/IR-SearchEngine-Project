  <script type="text/javascript">
   //--------------------------------------------
//Alert Slide Down Panel
//--------------------------------------------
jQuery(document).ready(function() {
	jQuery("div.alert_button").click(function(){
		var newHeight = pullHeightInfo();
		jQuery("div#activeAlertsHeader").animate({
			height: newHeight
		})
		.animate({
			height: newHeight
		}, "fast");
		jQuery("div.alert_button").toggle();
		jQuery(".ybar").css("display", "none");
	});
	jQuery("div#alertHideButton").click(function(){
		jQuery("div#activeAlertsHeader").animate({
			height: "40px"
		}, "fast");
		/*jQuery("div#activeAlertsHeader").css("overflow", "1px solid #663333");*/
		/*jQuery(".ybar").css("display", "block");
		jQuery(".ybar").css("display", "none");*/
	});

	checkNoAlerts();
}); 
	
function checkNoAlerts(){
	var weHappy = doAlertsExist();
	if(weHappy == 1)
	{
		jQuery("#activeAlertsHeader").css("background-color", "#edffec");
		jQuery("#activeAlertsHeader").css("border", "1px solid #24891a");
		jQuery("#alertPrimer").css("display", "none");
		jQuery("div.nab").toggle();
	}
}
function pullHeightInfo(){
	return jQuery("#ahbox").val();
}

function doAlertsExist(){
	return jQuery("#nahbox").val();
}
  </script>
