	Liferay.provide(
		Liferay.Util,
		'openKaleoDesignerPortlet',
		function(config) {
			var A = AUI();

			var namespacePrefix = Liferay.Util.getPortletNamespace('2_WAR_kaleodesignerportlet');

			var params = [];

			A.Object.each(
				{
					availableFields: config.availableFields,
					availablePropertyModels: config.availablePropertyModels,
					ddmStructureId: config.ddmStructureId,
					draftVersion: config.draftVersion,
					kaleoProcessId: config.kaleoProcessId,
					name: config.name,
					openerWindowName: config.openerWindowName,
					portletResourceNamespace: config.portletResourceNamespace,
					propertiesSaveCallback: config.propertiesSaveCallback,
					refreshOpenerOnClose: config.refreshOpenerOnClose,
					saveCallback: config.saveCallback,
					uiScope: config.uiScope,
					version: config.version
				},
				function(item, index, collection) {
					if (item) {
						params.push(namespacePrefix + encodeURIComponent(index) + '=' + encodeURIComponent(item));
					}
				}
			);

			config.uri = Liferay.Util.addParams(params.join('&'), config.baseKaleoDesignerURL);

			var dialogConfig = config.dialog;

			if (!dialogConfig) {
				var region = A.one(Liferay.Util.getOpener()).get('region');

				dialogConfig = {
					modal: true,
					title: config.name,
					width: region.width * 0.85
				};

				config.dialog = dialogConfig;
			}

			if (!('align' in dialogConfig)) {
				dialogConfig.align = Liferay.Util.Window.ALIGN_CENTER;
			}

			var dialogIframeConfig = config.dialogIframe;

			if (!dialogIframeConfig) {
				dialogIframeConfig = {
					closeOnEscape: false
				};

				config.dialogIframe = dialogIframeConfig;
			}

			Liferay.Util.openWindow(config);
		},
		['liferay-portlet-url']
	);

