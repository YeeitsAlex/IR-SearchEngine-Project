      $(function(){
    $('#shell').addClass('carousel');
    
                    var effect = "None";
                    if ( effect.indexOf(' ') != -1) {
                        effect = effect.split(' ');
                        effect = effect[0].toLowerCase() + effect[1];
                    }
                    else {
                        effect = effect.toLowerCase();
                    }
                    $('#slideshow-28').cycle({
                        fx: effect, delay: 0000, timeout:20000
                    });
                    $('#controls-28 > button').click( function() {
                        $('#slideshow-28').cycle($(this).attr('class'));
                    });
                
});
     </script>
