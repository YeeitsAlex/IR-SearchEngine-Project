      <div class="section-wrapper slider-wrapper" id="slider-wrapper">
       <div class="clearfix element-max-width-padding" id="slider">
        <div class="region region-slider clearfix">
         <div class="block block-bean block-bean-type-hero-unit block-bean-hero-unit-homepage-" id="block-bean-hero-unit-homepage-">
          <div class="block-inner-wrapper block-inner clearfix">
           <div class="content">
            <div class="hero-unit hero-unit-text hero-size-large hero-align-left-half hero-text-white hero-background-black hero-links-blue" id="hero-3">
             <div class="hero-unit-content-wrapper element-max-width">
              <div class="hero-unit-content">
               <div class="hero-unit-content-inner">
                <h2>
                 In case of emergency, make sure you get the message.
                </h2>
               </div>
              </div>
             </div>
            </div>
            <style>
             #hero-3 {
      background-image:url(https://alerts.colorado.edu/sites/default/files/styles/medium/public/hero/alert-hero2.jpg?itok=nnf48bPh);
    }
    @media all and (min-width: 480px) and (max-width: 959px) {
      #hero-3 {
        background-image:url(https://alerts.colorado.edu/sites/default/files/styles/large/public/hero/alert-hero2.jpg?itok=DSW6BVPJ);
      }
    }
    @media all and (min-width: 960px) {
      #hero-3 {
        background-image:url(https://alerts.colorado.edu/sites/default/files/styles/hero/public/hero/alert-hero2.jpg?itok=ACV57Ubc);
      }
    }
    @media all and (min-width: 1600px) {
      #hero-3 {
        background-image:url(https://alerts.colorado.edu/sites/default/files/styles/xlarge/public/hero/alert-hero2.jpg?itok=_IHIkOYc);
      }
    }
            </style>
