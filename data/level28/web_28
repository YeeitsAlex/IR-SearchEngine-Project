                <script type="text/javascript">
                 $(document).ready(function () {

        $('.bttn').hover(function () {
            $(this).toggleClass('ui-state-default');
            $(this).toggleClass('ui-state-hover');
        });

        $('.dateheading').css('margin-bottom', '5px');

    });

    window.onload = function () {

        var svg = $("#svgObject").getSVG();

        svg.find('path').each(function () {
            id = $(this).attr('alt');
            $(this).attr('onclick', 'window.top.location.href="/CurrentMap/StateDroughtMonitor.aspx?' + id.replace(" ","_") + '"')
            $(this).attr('cursor', 'pointer');
            //$(this).append('<title>' + id + '</title>');
        });

        svg.find('path').mouseenter(function () {
            var reg = $(this).attr('alt');
            svg.find('[alt="' + reg + '"]').each(function () {
                $(this).attr('fill', '#00ff00');
                $(this).attr('fill-opacity', '0.25');
                $(this).attr('stroke', '#00ff00')
            });

        });

        svg.find('path').mouseleave(function () {
            var reg = $(this).attr('alt');
            svg.find('[alt="' + reg + '"]').each(function () {
                $(this).attr('fill', '#ffffff');
                $(this).attr('fill-opacity', '0');
                $(this).attr('stroke', '#666')
            });
        });

    };
                </script>
