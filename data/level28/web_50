        <section class="l-dws-panels-berlin__region l-dws-panels-berlin__region--tertiary--first">
         <div class="panel-pane pane-views-panes pane-calendar-panel-pane-3">
          <h2 class="pane-title">
           Upcoming Events
          </h2>
          <div class="view view-calendar view-id-calendar view-display-id-panel_pane_3 view-dom-id-7e1f9986f0e23cb047e6e0b7916bde21">
           <div class="view-content">
            <div class="item-list">
             <ul>
              <li>
               <article about="/student-life/events/discover-career-medical-science-liaison-fannie-mitchell-expert-residence-heather" class="node node--grad--event node--promoted node--micro-teaser node--grad--event--micro-teaser" role="article" typeof="sioc:Item foaf:Document">
                <header>
                 <h2 class="node__title">
                  <a href="/student-life/events/discover-career-medical-science-liaison-fannie-mitchell-expert-residence-heather" rel="bookmark">
                  </a>
                 </h2>
                 <span class="rdf-meta element-hidden" content="Discover a Career as a Medical Science Liaison with Fannie Mitchell Expert-in-Residence Heather Vita, Ph.D ’04" property="dc:title">
                 </span>
                </header>
                <div class="node__content">
                 <div class="field field--name-grad-event--event-date field--type-date field--label-hidden">
                  <span class="date-display-single" content="2019-03-13T00:00:00-04:00" datatype="xsd:dateTime" property="dc:date">
                   Wednesday, March 13, 2019
                  </span>
                 </div>
                 <span class="field field--name-grad-event--event-times field--type-date field--label-hidden">
                  <span class="date-display-single" content="0000-01-01T10:30:00-05:00" datatype="xsd:dateTime" property="dc:date">
                   10:30 am
                  </span>
                 </span>
                 <h2>
                  <a href="/student-life/events/discover-career-medical-science-liaison-fannie-mitchell-expert-residence-heather">
                   Discover a Career as a Medical Science Liaison with Fannie Mitchell Expert-in-Residence Heather Vita, Ph.D ’04
                  </a>
                 </h2>
