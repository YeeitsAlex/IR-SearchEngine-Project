       <script type="text/javascript">
        // 
$(function () {
$.ajax({
url: 'https://campusdata.uark.edu/apiv2/articles?$orderby=publishDate+desc&$top=1&$filter=((Schools/any(s12:+s12/id+eq+12)))',
data: {},
dataType: 'jsonp',
cache: 'true',
success: function (articles) {
dataDisplay = "";
$.each(articles, function (key, a) {
var briefText = "", image = "";
var date = new Date(a.publishDate);
date = date.toDateString();
if (a.brief != null && a.brief != "") { briefText = a.brief;  briefText = briefText.replace(/(<p>|<\/p>)/g, "");}
if (a.image != null && a.image != "") {
image = a.image;
} else { image = "PMUA_Logo-web.jpg"; }
dataDisplay = "<li class='media'><div class='media-left'>" +
"<a class='thumb' href='//campusdata.uark.edu/resources/images/articles/" + image + "'>" +
"<img class='media-object' src='//campusdata.uark.edu/resources/images/articles/" + image + "?w=80' alt='' /></a>" +
"</div>" +
"<div class='media-body'>" +
"<p class='media-heading'><a href='https://news.uark.edu/articles/" + a.id + "'>" + a.headline + "</a></p>" +
"<p><i>" + date + "</i></p>" +
"<p>" + briefText + "</p>" +
"</div></li>";
$("#newsArchive").append(dataDisplay);
});
}
});
});
//
       </script>
       <p class="pull-right">
