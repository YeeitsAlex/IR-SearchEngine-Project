                          <input aria-label="Books and Media Search" autocomplete="off" class="span2 homeSearchBox" data-autosuggest-enabled="true" data-autosuggest-path="https://find.library.duke.edu/suggest" id="catalogqueryinput" name="q" placeholder="Find books, music, movies &amp; more" type="text"/>
                          <button class="btn btn-primary" type="submit">
                           search
                          </button>
                         </div>
                         <ul class="inline small">
                          <li>
                           <a href="https://find.library.duke.edu/advanced?utm_campaign=dul&amp;utm_content=home_advanced_link&amp;utm_source=library.duke.edu&amp;utm_medium=referral" onclick="ga('send', 'event', { eventCategory: 'dulHome', eventAction: 'Discovery', eventLabel: 'Catalog Advanced'});">
                            Advanced Search
                           </a>
