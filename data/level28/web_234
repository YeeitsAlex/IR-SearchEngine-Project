       Today's Hours
       <span class="today">
        12pm - 5pm
       </span>
       <script>
        // Ensure Today's Hours updates with accurate info after page load
$(function() {
  var today = new Date().getDay();
  var sunday = "12pm - 5pm";
  var monday = "Closed";
  var tuesday = "10am - 5pm";
  var wednesday = "10am - 5pm";
  var thursday = "10am - 9pm";
  var friday = "10am - 5pm";
  var saturday = "10am - 5pm";

  // console.log(today);

  if (today == 0) {
    $('span.today').html(sunday);
  }
  else if (today == 1) {
    $('span.today').html(monday);
  }
  else  if (today == 2) {
    $('span.today').html(tuesday);
  }
  else  if (today == 3) {
    $('span.today').html(wednesday);
  }
  else if (today == 4) {
    $('span.today').html(thursday);
  }
  else if (today == 5) {
    $('span.today').html(friday);
  }
  else if (today == 6) {
    $('span.today').html(saturday);
  }
});
// @TODO Ensure it's not a holiday
       </script>
      </span>
