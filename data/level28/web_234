                      <source media="(max-width: 800px) and (min-width: 601px)" srcset="https://artmuseum.princeton.edu/files/styles/puam_micro_683/public/non-collections/james_rockyarcade_8x5.jpg?itok=rV3ya3jB&amp;timestamp=1489085001 1x, https://artmuseum.princeton.edu/files/styles/puam_micro_683/public/non-collections/james_rockyarcade_8x5.jpg?itok=rV3ya3jB&amp;timestamp=1489085001 2x"/>
                      <source media="(max-width: 1024px ) and (min-width: 801px) " srcset="https://artmuseum.princeton.edu/files/styles/puam_micro_683/public/non-collections/james_rockyarcade_8x5.jpg?itok=rV3ya3jB&amp;timestamp=1489085001 1x, https://artmuseum.princeton.edu/files/styles/puam_micro_683/public/non-collections/james_rockyarcade_8x5.jpg?itok=rV3ya3jB&amp;timestamp=1489085001 2x"/>
                      <source media="(min-width: 1025px)" srcset="https://artmuseum.princeton.edu/files/styles/puam_micro_683/public/non-collections/james_rockyarcade_8x5.jpg?itok=rV3ya3jB&amp;timestamp=1489085001 1x"/>
                      <!--[if IE 9]></video><![endif]-->
                      <img alt="" src="https://artmuseum.princeton.edu/files/styles/puam_micro_683/public/non-collections/james_rockyarcade_8x5.jpg?itok=rV3ya3jB&amp;timestamp=1489085001" title=""/>
                     </picture>
                     <div class="container">
                      <h3>
                       The Modern Museum: Between Preservation and Action | Director's Letter Winter 2018
                      </h3>
                      <p>
                       <span style="color:black">
                        James Steward discusses
                       </span>
                       ways in which the modern museum’s origins in the European Enlightenment continue to inform the core missions and strategies of most museums today, and the potential for museums to act as change agents that might foster critical dialogue and shape future discourse
                       <span style="color:black">
                       </span>
                      </p>
                     </div>
                    </div>
