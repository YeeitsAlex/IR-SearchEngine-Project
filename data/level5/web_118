    <script type="text/javascript">
     function makeshare() {
			$('#shareid').toggle();
		}
		function hideshare() {
			$('#shareid').hide();			
		}
		function makeemail() {
			$('#emailid').toggle();
		}
		function hideemail(){
			$('#emailid').hide();
			$('#emailsendid').hide();	
		}
		function closeall() {
			hideshare();
			hideemail();
		}
		$(function(){
			//clicks on the share/email links should open popups!
			$('.sharelink').bind('click',function(event){
				makeshare();
				event.stopPropagation();
				event.preventDefault();
			})
			$('.emaillink').bind('click',function(event){
				makeemail();
				event.stopPropagation();
				event.preventDefault();				
			});

			//clicks outside the active popup should close the popup	
			$('body, .emaillink').bind('click',function(){
				hideshare();
			});
			$('body, .sharelink').bind('click',function(){
				hideemail();
			});
			
			//clicks on the popup X should close each popup
			$('.closeshare > a').bind('click',function(event){
				closeall();
				event.preventDefault();
			});
			
			//Prevent clicks in the share/email popups themselves from closing the popups
			$('#shareid, #emailid').bind('click',function(event){
				event.stopPropagation(); //this prevents the click event from bubbling up to the body, also preventing the second set of listeners from firing
			});

		});
    </script>
