       <!-- Pre-content -->
       <!-- Main Content -->
       <div class="tabs">
       </div>
       <div id="content" role="main">
        <div class="region region-content">
         <div class="block block-system" id="block-system-main">
          <div class="content">
           <div about="/seafood-network" class="node node-page clearfix" id="node-82281" typeof="foaf:Document">
            <span class="rdf-meta element-hidden" content="Seafood Network Information Center" property="dc:title">
            </span>
            <div class="content">
             <div class="paragraphs-items paragraphs-items-field-paragraph paragraphs-items-field-paragraph-full paragraphs-items-full">
              <div class="field field-name-field-paragraph field-type-paragraphs field-label-hidden">
               <div class="field-items">
                <div class="field-item even">
                 <div about="" class="entity entity-paragraphs-item paragraphs-item-paragraph-1-col p-right paragraphs-item-5251" typeof="">
                  <div class="content">
                   <div class="field field-name-field-p-1-col-main field-type-text-long field-label-hidden">
                    <div class="field-items">
                     <div class="field-item even">
                      <p>
                       <em>
                        <strong>
                         SeafoodNIC
                        </strong>
                       </em>
                       is designed to assist the seafood industry and regulators implement seafood HACCP procedures.
                      </p>
                     </div>
                    </div>
                   </div>
                  </div>
                 </div>
                </div>
                <div class="field-item odd">
                 <div class="paragraph-menu entity entity-paragraphs-item paragraphs-item-paragraph-menu menu-orange paragraphs-item-4911 four-items">
                  <ul>
                   <li>
                    <a href="/seafood-network-information-center/news-publications/seafood-haccp-discussion-electronic-mailing">
                     <i class="icon-envelope-alt">
                     </i>
                     HACCP ListServ
                    </a>
                   </li>
                   <li>
                    <a href="http://osuseafoodlab.oregonstate.edu/">
                     <i class="icon-beaker">
                     </i>
                     OSU Seafood Lab
                    </a>
                   </li>
                   <li>
                    <a href="">
                    </a>
                   </li>
