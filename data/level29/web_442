      <script>
       /* TOP NAV - defines function for allowing users to access submenus using keyboard tabbing*/
    var openmenu_href = 'default';
    var current_self = '';
    function tabbable_topnav(current_obj) {
        /*store the href of the submenu a child link is in, as well as the href of the currently selected item*/
        var current_parent = $(current_obj).closest('.dropdown-menu').siblings('.dropdown-toggle').attr('href');
        var current_self = $(current_obj).attr('href');

        /*if neither of the two stored hrefs match the href of the currently open menu, close the currently open menu*/
        if (openmenu_href != current_parent && openmenu_href != current_self){
            $('.dropdown-toggle[href="' + openmenu_href + '"]').siblings('.dropdown-menu').removeClass("visible");
        }
        /*if this is a nav menu...*/
        if($(current_obj).closest("#s-lg-tabs-container").length > 0){
            /*check to see if it's a dropdown-toggle, and if it is, toggle the dropdown and record the href of the current dropdown*/
            if ($(current_obj).hasClass('dropdown-toggle')) {
                $(current_obj).siblings('.dropdown-menu').addClass("visible");
                openmenu_href = $(current_obj).attr('href');
            }
        }
    }

    function focuslink_sidenav(current_obj) {
        if ($(current_obj).closest('ul').hasClass("s-lg-boxnav")) {
            current_self = $(current_obj).attr('href');
            $(current_self).css("outline", "5px solid #A8CBE0");
            $(current_obj).focusout(function() {
                $(current_self).css("outline", "none");
            });
            $(current_self).css("outline", "5px solid #A8CBE0");
        }
    }

    function mouselink_sidenav(current_obj) {
        if ($(current_obj).closest('ul').hasClass("s-lg-boxnav")) {
            current_self = $(current_obj).attr('href');
            $(current_self).css("outline", "5px solid #A8CBE0").animate({outlineWidth: 0}, 700);
        }
    }


  $(document).ready(function(){

      /*fixes issue where top of the page isn't visible due to responsive flex layout*/
      if ($('.navbar-fixed-top').length){
          var height = $('.navbar-fixed-top').height();
          $('#s-lg-guide-header').css("margin-top", height+"px");
      }
      /*adds tab indexing to guide boxes so that if you link straight to a box (as with the foucslink_sidenav function), tabbing will start from that box instead of from the top of the screen*/
      $('.s-lib-box').attr('tabindex', '-1');
      /*whenever something gets focus...*/
      $("*").focus(function(){
          /*makes dropdown menus able to be tabbed through with the keyboard*/
          tabbable_topnav(this);
          /*highlights in-page boxes when you tab over the link to the box*/
          focuslink_sidenav(this);
      });
      /*whenever something is moused over*/
      $("*").mouseover(function(){
          /*flashes boxes when you mouseover in-page links*/
          mouselink_sidenav(this);
      });

    /*Sets tab index on the skiplink to keep it from being selected while tabbing through pages*/
    /*$("#s-lg-public-skiplink").attr("tabindex", "-1");*/
    /*Removes custom font color and font-family from inline styles. This can be overridden by adding custom classes*/
    $('.s-lib-box-content p').css("color", "").css("font-family", "").css("font-size", "");
    $('.s-lib-box-content span').css("color", "").css("font-family", "").css("font-size", "");
    /*removes placeholder text and labels from search boxes*/
      var searchform = $("form[role='search']");
      searchform.find("input[type='text']").removeAttr('placeholder');
      searchform.children("span").remove();
    /*Renames Profile box to 'Library Contact'*/
    $('div[id^="s-lg-box-profile_box"]').find("h2").text("Library Contact");
    /*Changes the "Email Me" button under Library contact to be "address@umich.edu"*/
   $('div[id^="s-lib-profile-email"]').find("a").each(function(){
        var email = $(this).prop("title");
        $(this).prop("text", email);
    }).css("font-size", ".9em");
    /*removes the greyout class*/
    $(".s-lg-text-greyout").removeClass("s-lg-text-greyout");
    /*wraps semantic 'section' tags around each lib box, to improve accessibility*/
    $(".s-lib-box").wrap("<section></section>");
    /*listens for changes to the DOM*/
    $(document).on("DOMNodeInserted", function(){
        /*Disables radio buttons we don't want people using in the Add item lightboxes*/
        $('.radio label:contains("Hover over ")').addClass("s-lg-text-disabled").find("input").attr("disabled", "true");
        /*Removes the ability to make gallery boxes*/
        $('#box_type option:contains("Gallery")').remove();
    });
    console.log("everything worked!");
});

/* FIXED CHAT WIDGET + CATPCHA */

window.onload = function() {

 var $recaptcha = document.querySelector('#g-recaptcha-response');

    if($recaptcha) {
        $recaptcha.setAttribute("required", "required");
    }

  var fixedChat = document.querySelector(".chat-btn");

  fixedChat.onclick = function() {
    var x = document.getElementById("iframe-content");
    var svgClosed = document.getElementsByClassName("closed-icon")[0];
    var svgExpanded = document.getElementsByClassName("expanded-icon")[0];

    if (x.style.display != "block") {
      x.style.display = "block";
      svgExpanded.style.display = "none";
      svgClosed.style.display = "inline-block";
    } else {
      x.style.display = "none";
      svgClosed.style.display = "none";
      svgExpanded.style.display = "inline-block";
    }
  };
};

/*Shows chat if online + available. Hides if offline, busy, unavailable*/
var lh3CheckPresence = function() {
  var url = "https://libraryh3lp.com/presence/jid/umlibraryaskalibrarian/chat.libraryh3lp.com/js",
    script = document.createElement("script");
  script.src = url + "?cb=lh3UpdatePresence";
  document.getElementsByTagName("head")[0].appendChild(script);
};
var lh3UpdatePresence = function() {
  for (var i = 0; i < jabber_resources.length; ++i) {
    var resource = jabber_resources[i];
    if (resource.show === "available" || resource.show === "chat") {
      document.getElementsByClassNameById("fixed-chat")[0].style.display = "";
    } else if (resource.show === "away") {
      document.getElementsByClassName("chat-btn")[0].style.display = "none";
    } else if (resource.show === "unavailable" || resource.show === "xa") {
      document.getElementsByClassName("chat-btn")[0].style.display = "none";
    }
  }
};

lh3CheckPresence();
setInterval(lh3CheckPresence, 10000);
      </script>
