             <div class="field-items">
              <div class="field-item even">
               <div about="/amy-gutmann-pull-quote" class="l-pull-quote-boundary node node-pull-quote-cta clearfix" id="node-345" typeof="sioc:Item foaf:Document">
                <div class="l-pull-quote-stage content">
                 <div class="l-circle-image">
                  <div class="field field-name-field-quote-image field-type-image field-label-hidden">
                   <div class="field-items">
                    <div class="field-item even">
                     <img alt="" src="https://www.nettercenter.upenn.edu/sites/default/files/styles/thumbnail__144x144_/public/penn-shield-yellow-circle.jpg?itok=vnuG5jO8&amp;c=9a7d6935768c2aa197f6e6a794bcb5f2" typeof="foaf:Image"/>
                    </div>
                   </div>
                  </div>
                  <div class="l-quote-penn-shield">
                   <img src="/sites/all/themes/netter-center/images/svg/shield-circle.svg"/>
