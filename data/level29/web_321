   <script src="/_resources/js/owl.carousel.min.js">
   </script>
   <script type="text/javascript">
    $(document).ready(function() {
			
				function random(owlSelector){
					owlSelector.children().sort(function(){
						return Math.round(Math.random()) - 0.5;
					}).each(function(){
						$(this).appendTo(owlSelector);
					});
				}

				var $owl = $('#client-list');
				$owl.on('initialize.owl.carousel', function(event){
					var selector = $('#client-list');
					random(selector);
				});
				$owl.owlCarousel({
					loop: true,
					margin: 100,
					center: true,
					autoWidth: false,
					autoplay: true,
					autoplayTimeout: 3000,
					autoHeight: true,
					animateOut: 'fadeOut',
					responsive:{
						0:{
							items:1,
							dots:false
						}
					}
				});

			});
   </script>
