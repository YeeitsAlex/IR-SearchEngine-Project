               <script type="text/javascript">
                //When the component is rendered, (on page load or eventlist component dialog update), set the event request parameters on the component's DOM object
    (function()
    {
		var objEventsRequestParameters = {};
        var strUniqueComponentId = "";
        var aryEventsRequestCULCategories = [];
        var aryEventsRequestLocations = [];
        var aryEventsRequestAudiences = [];
        var aryEventsRequestTypes = [];
        var aryEventsRequestTypesExclude = [];
        var strEventsRequestStartDate = "";
        var strEventsRequestEndDate = "";
        var intMaxEventsToDisplay = 0;
        var strReturnLinkURL = "";
        var strReturnLinkText = "";
        var strFormat = "";

        strUniqueComponentId = "eventslist_v1";
        aryEventsRequestCULCategories[0] = "org/cu_libraries"
        aryEventsRequestTypesExclude[0] = "Workshop"
        aryEventsRequestTypesExclude[1] = "Training"

    strEventsRequestStartDate = "";
    strEventsRequestEndDate = "";
    intMaxEventsToDisplay = 7;
    strReturnLinkURL = "/content/libraryweb/index";
    strReturnLinkText = "Libraries Home";
    strFormat = "homepage";

    objEventsRequestParameters["componentId"] = strUniqueComponentId;
    objEventsRequestParameters["culcategories"] = aryEventsRequestCULCategories;
    objEventsRequestParameters["locations"] = aryEventsRequestLocations;
    objEventsRequestParameters["audiences"] = aryEventsRequestAudiences;
    objEventsRequestParameters["types"] = aryEventsRequestTypes;
    objEventsRequestParameters["typesExclude"] = aryEventsRequestTypesExclude;
    objEventsRequestParameters["startDate"] = strEventsRequestStartDate;
    objEventsRequestParameters["endDate"] = strEventsRequestEndDate;
    objEventsRequestParameters["maxEventsToDisplay"] = intMaxEventsToDisplay;
    objEventsRequestParameters["returnLinkURL"] = strReturnLinkURL;
    objEventsRequestParameters["returnLinkText"] = strReturnLinkText;
    objEventsRequestParameters["format"] = strFormat;

    document.getElementById("eventslist_v1").initParams = objEventsRequestParameters;

    }());//End anonymous function to set eventlist params



    //If the component is being rendered after the $(document).ready() event has already fired (as in when the component dialog has just been updated)...
    if (typeof AEM != 'undefined')
    {
		//...we need to call the function directly to reload the list.
        AEM.populateEventsListComponents();
    }
               </script>
              </div>
