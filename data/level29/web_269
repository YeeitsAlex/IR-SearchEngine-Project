<!-- script contents: begin -->
<script>
 (function (OPR, $, undefined) {
    OPR.searchSubmit = function (f) {
        var form = $('#cse-search-box');
        var pu = $('input[name=searchtype]')[1].checked;
        var text = $('#q').val();
        if (pu) {
            var url = 'http://www.princeton.edu/main/tools/search/?q=';
            document.location.href = url + text;
            return false;
        }
        form.submit();
        return true;
    }
} (window.OPR = window.OPR || {}, jQuery));
</script>
<!-- script contents: end-->
