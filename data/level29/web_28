  <script>
   $(document).ready(function () {
    setTimeout(checkVideo, 100)
    function checkVideo() {
		var video = $(".bgvid")
		var i = 0;
		$.each(video, function( index, value ) {
		  //alert( index + ": " + value );
		  
		   isPlaying = video[i].currentTime > 0 && !video[i].paused && !video[i].ended 
				  && video[i].readyState > 2;
			  if (!isPlaying) {
				video[i].play();
			  }
		  i= i+1;
		});
    }
  })
  </script>
