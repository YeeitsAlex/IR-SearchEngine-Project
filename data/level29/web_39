        <script type="text/javascript">
         var icpForm2009 = document.getElementById('icpsignup2009');

if (document.location.protocol === "https:")

	icpForm2009.action = "https://app.icontact.com/icp/signup.php";

function verifyRequired2009() {
	if (icpForm2009["fields_email"].value == "") {
		icpForm2009["fields_email"].focus();
		alert("The Email field is required.");
		return false;
	}

	if (icpForm2009["fields_email"].value && icpForm2009["fields_email"].value !== "") {
		icpForm2009["fields_email"].value = icpForm2009["fields_email"].value.trim();
	}



	return true;
}

function validDate2009(value) {

	if (value == "") {
		return true;
	}

	var dateSections = value.split('/');
	if (dateSections.length != 3) {
		return false;
	}

	var month = dateSections[0];
	if (month && (month.length > 2 || month < 1 || month > 12)) {
		return false;
	}

	var day = dateSections[1];
	if (day && (day.length > 2 || day < 1 || day > 31)) {
		return false;
	}

	var year = dateSections[2];
	if (!year || (year && year.length != 4)) {
		return false;
	}

	var dateValue = new Date(value);
	if (month != (dateValue.getMonth()+1) || day != dateValue.getDate() || year != dateValue.getFullYear()) {
		return false;
	}

	return true;
}
        </script>
