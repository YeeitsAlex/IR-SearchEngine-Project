									}
									else
									{
									document.write('<li><label for="radioLabela"><input id="radioLabela" class="searchRadio" type="radio" name="sitesearch" value="http://' + hostname + '" checked /><i class="fa fa-lg fa-dot-circle-o"></i> Search this site</label></li>');
									document.write('<li><label for="radioLabelb"><input id="radioLabelb" class="searchRadio" type="radio" name="sitesearch" value="http://uark.edu" /><i class="fa fa-lg fa-dot-circle-o"></i> Search all uark.edu</label></li>');
