.co-branding-slider.magee-slider .carousel-control:hover span, .co-branding-slider.magee-slider .carousel-control:focus span, .co-branding-slider.magee-slider .carousel-control:target span, .co-branding-slider.magee-slider .carousel-control:active span {
    background-color: #0577B1;
}

.co-branding-slider .carousel-caption {
    text-shadow: none;
    color: #666666;
}

.co-branding-slider .carousel-control.left {
    text-align: left;
}

.co-branding-slider .carousel-control.right {
  text-align: right;
}

.co-branding-slider.magee-slider .carousel-control.right span {
    left: 2.25em;
}
   </style>
  </meta>
