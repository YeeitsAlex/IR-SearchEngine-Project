   jQuery(document).ready(function($) {
			setTimeout(function() {
				$('.flex-control-nav').attr('aria-hidden', 'true');
				$('.flex-direction-nav').attr('aria-hidden', 'true');
				$('<a class="mlslider-screen-reader-text skip-slider" href="#">Skip Passed Slider</a>').insertBefore('.slides');
				$('<div tabindex="-1" class="mlslider-screen-reader-text skip-slider-location">End of slider. Content continues below.</div>').insertAfter('.flex-direction-nav');
				$('.skip-slider-location').each(function(e) {
					$(this).addClass('skip-slider-location-' + e);
				});
				$('.skip-slider').each(function(e) {
					$(this).addClass('skip-slider-' + e);
					$('.skip-slider-' + e).click(function() {
						$('.skip-slider-location-' + e).focus();
						return false;
					});
				});
			}, 500);
		});
  </script>
