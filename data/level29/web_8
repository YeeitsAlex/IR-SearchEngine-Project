        <div class="views-row views-row-2 views-row-even">
         <div class="views-field views-field-field-primary-image">
          <div class="field-content">
           <a href="http://museum.cornell.edu/exhibitions">
            <img height="144" src="http://museum.cornell.edu/sites/default/files/styles/thumbnail/public/TradeTextiles-thumb.jpg?itok=CNR7YaLf" typeof="foaf:Image" width="300"/>
