$(this).parent().children('a.expand').hide();
$(this).parent().children('p.ab_text').show();
$(this).parent().children('a.close').show();
return false;
});
$('.ab a.close').click(function() {
$(this).parent().children('a.expand').show();
$(this).parent().children('p.ab_text').hide();
$(this).parent().children('a.close').hide();
return false;
});
});
