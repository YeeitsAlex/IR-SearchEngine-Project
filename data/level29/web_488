                            },
                            CONTENT: {
                            }
                        },
                        system_name: system.name,
                        customer: {"name":"Granite State College"}
                    });
					springSpace.Session.CONST.ACTION_AUTH_SET_LG_SESSION_COOKIE = 750;
					jQuery(function() {
						springSpace.Session.getLGSessionStatus({
							url: "https://granite.libapps.com/libapps/login_process.php",
							data: {
								action: 5,
								app_id: 1,
								domain: "library.granite.edu",
								site_id: 6607
							}
						});
					});
				jQuery(function() {
					setInterval(function() {
						springSpace.Session.getLGSessionStatus({
							url: "https://granite.libapps.com/libapps/login_process.php",
							data: {
								action: 5,
