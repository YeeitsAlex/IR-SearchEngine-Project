         <div class="skin-default">
          <div class="views_slideshow_cycle_main views_slideshow_main" id="views_slideshow_cycle_main_front_page_features-page_2_1">
           <div class="views-slideshow-cycle-main-frame views_slideshow_cycle_teaser_section" id="views_slideshow_cycle_teaser_section_front_page_features-page_2_1">
            <div aria-labelledby="views_slideshow_pager_field_item_bottom_front_page_features-page_2_1_0" class="views-slideshow-cycle-main-frame-row views_slideshow_cycle_slide views_slideshow_slide views-row-1 views-row-first views-row-odd" id="views_slideshow_cycle_div_front_page_features-page_2_1_0">
             <div class="views-slideshow-cycle-main-frame-row-item views-row views-row-0 views-row-odd views-row-first dac-slideshow-pane">
              <div class="views-field views-field-nodefield">
               <span class="field-content">
                <article about="/content/duke-study-we-are-recruiting-children-1-7-years-age-autism-adhd-or-typical-development" class="node node--front-page-feature node--promoted node--sticky node--full node--front-page-feature--full" role="article" typeof="sioc:Item foaf:Document">
                 <div class="node__content">
                  <div class="dac-featured-pane dac-left-container" style="background-image:url('https://autismcenter.duke.edu/sites/autismcenter.duke.edu/files/styles/hd_slideshow/public/award2.jpg?itok=3k4PXxML');">
                   <div class="l-region dac-container">
                    <div class="slide-align dac-left">
                     <div class="dac-featured-box">
                      <div <h3="" class="dac-featured-box-title">
                       Duke A+ Study: We are recruiting children 1-7 years of age with autism, ADHD, or typical development!
                      </div>
                     </div>
                    </div>
                   </div>
                  </div>
                 </div>
                </article>
               </span>
              </div>
             </div>
            </div>
