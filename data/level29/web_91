     <script>
      (function equalizeHeights(global) {
                function heightDoer(selection) {
                    els = document.querySelectorAll(selection) || 'undefined';
                    if (els) {
                        var h = 0;
                        els.forEach(function(el, idx) {
                           
//                            console.log(el);
//                            console.log(el.offsetHeight);

                            if (el.offsetHeight > h) {
                                h = el.offsetHeight;  
                            }
                            /*console.log(h);*/
                        });
                        if (h > 0) {
                            els.forEach(function(el) {
                                el.style.height = h+'px';
                            });
                        }
                    } 
                    
                }
                heightDoer('.at-a-glance-img');
//                window.onresize = function() {
//                    console.log('resize');
//                    heightDoer('.at-a-glance-img');
//                }
                
            }) (window);
     </script>
