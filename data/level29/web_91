  <script>
   function CustomOpenLHNChat(){
if (typeof lhnWin !== 'undefined' && lhnWin){

if (!lhnWin.closed){

lhnWin.blur();

setTimeout(lhnWin.focus(), 10);

return false;

}

}


var wleft = (screen.width - 580-32) / 2; 
var wtop = (screen.height - 420 - 96) / 2; 
lhnWin=window.open('https://www.livehelpnow.net/lhn/livechatvisitor.aspx?lhnid=29468', 'lhnchat', 'left=' + wleft + ',top=' + wtop +',width=580,height=435,toolbar=no,location=no,directories=no,status=yes,menubar=no,scrollbars=no,copyhistory=no,resizable=yes'); 
}
  </script>
