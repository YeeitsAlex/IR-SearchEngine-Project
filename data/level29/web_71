  <script type="text/javascript">
   // <![CDATA[
function getUTMCookie(cname) {
	var name = cname + "=";
	var decodedCookie = decodeURIComponent(document.cookie);
	var ca = decodedCookie.split(';');
	for(var i = 0; i < ca.length; i++) {
		var c = ca[i];
		while (c.charAt(0) == ' ') {
				c = c.substring(1);
			}
			if (c.indexOf(name) == 0) {
				return c.substring(name.length, c.length);
			}
		}
		return "";
	}

	getUTMSource = getUTMCookie("_uc_utm_source");
	getUTMMedium = getUTMCookie("_uc_utm_medium");
	getUTMCampaign = getUTMCookie("_uc_utm_campaign");
// ]]>
  </script>
