<script>
 //taken from underscore.js
  function debounce(func, wait, immediate) {
    var timeout;
    return function() {
      var context = this, args = arguments;
      var later = function() {
        timeout = null;
        if (!immediate) func.apply(context, args);
      };
      var callNow = immediate && !timeout;
      clearTimeout(timeout);
      timeout = setTimeout(later, wait);
      if (callNow) func.apply(context, args);
    };
  };
   
  
	  $('#alerts').linkify();

    // Scroll Depth


    function trackScrollDepth(elements){
      jQuery.scrollDepth({
        minHeight: 0,
        elements: elements,
        percentage: true,
        userTiming: false,
        pixelDepth: false,
        nonInteraction: false
      });
    }


    $(document).ready(function(){
      // GET ELEMENTS
      var scrollElements = new Array();
      $('.cu-info-main .cu-title').each(function(){
        var id = $(this).html().replace("&amp;","and").split(' ').join('_').toLowerCase();
        $(this).attr('id',id);
        scrollElements.push( '#'+id );
      });
      
      // Trigger Scroll Depth
      trackScrollDepth(scrollElements);
    });
</script>
