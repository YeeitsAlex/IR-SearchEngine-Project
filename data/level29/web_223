     <div class="col-md-7 col-sm-12">
      <p>
       <b>
        SINGLE TICKETS AVAILABLE NOW.  CLICK
        <a href="https://sa1.seatadvisor.com/sabo/servlets/EventSearch?presenter=UNIARKMUSIC">
         HERE
        </a>
        TO PURCHASE YOURS!
       </b>
      </p>
      <p>
       <span>
        Nestled near the stunning Ozark Mountains on the beautiful University of Arkansas
                     campus, the Jim &amp; Joyce Faulkner Performing Arts Center is a 500+ seat state of the
                     art venue dedicated to providing a community-centered approach to the performing arts.
                     In addition to hosting acclaimed artists and performers, the Faulkner Center is also
                     home to the University of Arkansas music department's large ensembles. With an impressive
                     offering of renowned performers and culturally relevant events, the Faulkner Center
                     excels in its mission of contributing to education and excellence, helping build a
                     new generation of artists and patrons.
       </span>
      </p>
      <hr style="margin: 5px 0;"/>
      <div id="upcoming-events-title" style="font-size: 22px;">
       UPCOMING EVENTS
      </div>
      <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.1.0/moment.min.js" type="text/javascript">
      </script>
      <style>
       .fa { padding-right: 5px;}
	.media-body {
   		width: initial;
	}
      </style>
      <script async="" type="text/javascript">
       // 	

				var itemsList1 = [];
				var itemsList2 = [];
				var itemsList3 = [];
				var list_data3 = '';
				var icon = '';

				

				// r25 events
				var a1 = 	$.ajax({
								type: 'GET',
								url: '/_resources/php/proxy.php',
								data: { requrl: "http://25livepub.collegenet.com/calendars/FPACMSTR.rss?days=365" },
								cache: 'true',
								success: function (data, textStatus, jqXHR)
								{
								   var months = ["JAN", "FEB", "MAR", "APR", "MAY", "JUN", "JULY", "AUG", "SEPT", "OCT", "NOV", "DEC"]; 
								   var days = ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"];
								   var list_data2 = '';

								   var $xml = $(data).find('item:lt(3)'); 
								   $xml.each(function () { 
										var $this = $(this); 
										var headline = $this.find('title').text(); 
										var link = $this.find('link').text(); 
										var publishedDate = $this.find('pubDate').text();
										var d = new Date();
										var date = new Date(publishedDate).getDate();
										var time = moment(publishedDate).format("h:mm A");
										var month = months[new Date(publishedDate).getMonth()];
										var day = days[new Date(publishedDate).getDay()];

										var year = new Date(publishedDate).getFullYear();  


										// create object
										var item1 = {};


										// push values to object properties
										item1 = {
											'headline': headline,
											'link': link,
											'publishedDate': publishedDate,
											'date': date,
											'time': time,
											'month': month,
											'day': day,
											'year': year
										};

										// push object to array
										itemsList1.push(item1);


									});

								}
							});

				// r25 events
				var a2 = 	$.ajax({
								type: 'GET',
								url: '/_resources/php/proxy.php',
								data: { requrl: "http://25livepub.collegenet.com/calendars/fpac-other.rss?days=365" },
								cache: 'true',
								success: function (data, textStatus, jqXHR)
								{
								   var months = ["JAN", "FEB", "MAR", "APR", "MAY", "JUN", "JULY", "AUG", "SEPT", "OCT", "NOV", "DEC"]; 
								   var days = ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"];
								   var list_data2 = '';

								   var $xml = $(data).find('item:lt(3)'); 
								   $xml.each(function () { 
										var $this = $(this); 
										var headline = $this.find('title').text(); 
										var link = $this.find('link').text(); 
										var publishedDate = $this.find('pubDate').text();
										var d = new Date();
										var date = new Date(publishedDate).getDate();
										var time = moment(publishedDate).format("h:mm A");
										var month = months[new Date(publishedDate).getMonth()];
										var day = days[new Date(publishedDate).getDay()];

										var year = new Date(publishedDate).getFullYear();  


										// create object
										var item2 = {};


										// push values to object properties
										item2 = {
											'headline': headline,
											'link': link,
											'publishedDate': publishedDate,
											'date': date,
											'time': time,
											'month': month,
											'day': day,
											'year': year
										};

										// push object to array
										itemsList2.push(item2);


									});

								}
							});
							
				// r25 events
				var a3 = 	$.ajax({
								type: 'GET',
								url: '/_resources/php/proxy.php',
								data: { requrl: "http://25livepub.collegenet.com/calendars/fpac-presents.rss?days=365" },
								cache: 'true',
								success: function (data, textStatus, jqXHR)
								{
								   var months = ["JAN", "FEB", "MAR", "APR", "MAY", "JUN", "JULY", "AUG", "SEPT", "OCT", "NOV", "DEC"]; 
								   var days = ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"];
								   var list_data3 = '';

								   var $xml = $(data).find('item:lt(3)'); 
								   $xml.each(function () { 
										var $this = $(this); 
										var headline = $this.find('title').text(); 
										var link = $this.find('link').text(); 
										var publishedDate = $this.find('pubDate').text();
										var d = new Date();
										var date = new Date(publishedDate).getDate();
										var time = moment(publishedDate).format("h:mm A");
										var month = months[new Date(publishedDate).getMonth()];
										var day = days[new Date(publishedDate).getDay()];

										var year = new Date(publishedDate).getFullYear();  


										// create object
										var item3 = {};


										// push values to object properties
										item3 = {
											'headline': headline,
											'link': link,
											'publishedDate': publishedDate,
											'date': date,
											'time': time,
											'month': month,
											'day': day,
											'year': year
										};

										// push object to array
										itemsList3.push(item3);


									});

								}
							});

					$.when(a1, a2, a3).done(function(r1, r2, r3) { 
						 // Each returned resolve has the following structure: 
						 // [data, textStatus, jqXHR] 
						 // e.g. To access returned data, access the array at index 0 
						 //console.log(r1[0]); 
						 //console.log(r2[0]); 
						 //console.log(r3[0]); 


						 // merge both arrays into a new one
							var finalArray = itemsList1.concat(itemsList2).concat(itemsList3);

							// sort ascending
							finalArray.sort(function(a,b){
							  // Turn your strings into dates, and then subtract them
							  // to get a value that is either negative, positive, or zero.
							  return new Date(a.publishedDate) - new Date(b.publishedDate);
							});

							// iterate over array and output
							//for (var i=0; i<finalArray.length; i++) {
							for (var i=0; i<3; i++) { // just get first 3

								// check URL and serve appropriate icon
								if (/fpac-presents/i.test(finalArray[i].link)){
									icon = '<i class="fa fa-music media-icon" aria-hidden="true" title="Music Event" alt="Music Event"><!--need comment here--></i>';
								} else if (/fpac-other/i.test(finalArray[i].link)){
									icon = '<i class="fa fa-calendar media-icon" aria-hidden="true" title="FPAC Other Event" alt="FPAC Other Event"><!--need comment here--></i>';
								}  else {
									icon = '<i class="fa fa-university media-icon" aria-hidden="true" title="FPAC Event" alt="FPAC Event"><!--need comment here--></i>';
								}

								list_data3 += '<div class="media"><div class="media-left media-left-date"><h4>' + finalArray[i].month + ' ' + finalArray[i].date + '</h4></div><div class="media-body media-body-icon"><p>' + icon + '</p></div><div class="media-body"><p><a href="'+ finalArray[i].link +'">' + finalArray[i].headline + '</a><br/>' + finalArray[i].time + '</p></div></div>';

							}

							if(finalArray.length==0){
								list_data3 = '<div class="row"><p>There are no upcoming events scheduled right now.  Check back soon!</p></div>';
							}

							$(list_data3).appendTo("#music-events-calendar");
					 }); 

				
			//
      </script>
      <div id="music-events-calendar">
