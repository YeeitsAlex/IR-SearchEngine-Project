    function Randomize() {
				var images = new Array("1.jpg","2.jpg","3.jpg","4.jpg","5.jpg","Fotolia_1709322_Subscription_L.jpg");
				var imageNum = Math.floor(Math.random() * images.length);
				document.getElementById("primaryContainer").style.backgroundImage = "url('/wp-content/themes/ctl/images/" + images[imageNum] + "')";
			}
			window.onload = Randomize;
   </script>
   <!--Inline filter for article elements-->
   <script type="text/javascript">
    jQuery(document).ready(function($){
				$('#subpagecontent article').each(function(){
					$(this).attr('data-search-term', $(this).text().toLowerCase());
				});
				$('.live-search-box').on('keyup', function(){
				var searchTerm = $(this).val().toLowerCase();
				    $('#subpagecontent article').each(function(){
				        if ($(this).filter('[data-search-term *= ' + searchTerm + ']').length > 0 || searchTerm.length < 1) {
				            $(this).show();
				        } else {
				            $(this).hide();
				        }
				    });
				});
			});
   </script>
