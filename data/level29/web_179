         <script type="text/javascript">
          $(document).ready(function(){

  var events_json = "https://ical.cps.edu/KeyEvents?callback=?";

  $.getJSON(events_json, function(data){
      var dis = "";
      $.each(data, function(i, value){
        if(i < 4) {
          dis += '<div class="col-sm-6 col-md-12">';
          dis += '<div class="row event">';
          dis += '<a href="#" data-target="#event' + i + '" data-toggle="modal">';
          dis += '<div class="col-xs-5 col-sm-4 col-md-5 col-lg-4">';
          dis += '<div class="event-calendar">';
          dis += '<div class="calendar-day">';
          dis += '<p>'+ value.eventStartDateDayOfWeekFullName +'</p>';
          dis += '</div>';
          dis += '<div class="calendar-date">';
          dis += '<p>' + value.eventStartDateMonthAbbreviatedName + ' ' + value.eventStartDateDayNumeric +'</p>';
          dis += '</div>';
          dis += '</div>';
          dis += '</div>';
          dis += '</a>';
          dis += '<div class="col-xs-7 col-sm-8 col-md-7 col-lg-8">';
          dis += '<div class="event-title">';
          dis += '<a href="#" data-target="#event' + i + '" data-toggle="modal"><strong>' + value.eventTitle +'</strong></a>';
          dis += '<p>' + value.timeRange + '</p>';
          dis += '</div>';
          dis += '</div>';
          dis += '</div><!--end event -->';
          dis += '</div>';

          dis += '<div class="modal fade" id="event' + i + '" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">';
          dis += '<div class="modal-dialog" role="document">';
          dis += '<div class="modal-content">';
          dis += '<div class="modal-header">';
          dis += '<button type="button" class="close" data-dismiss="modal" aria-label="Close">';
          dis += '<span aria-hidden="true">&times;</span>';
          dis += '</button>';
          dis += '<h4 class="modal-title">' + value.eventTitle + '</h4>';
          dis += '</div><!-- modal-header -->';
          dis += '<div class="modal-body">';
          dis += '<p class="event-detail"><strong>When:</strong></p>';
          dis += '<p>' + value.eventStartDateDayOfWeekFullName + ', '
                       + value.eventStartDateMonthFullName + ' '
                       + value.eventStartDateDayNumeric + ', '
                       + value.eventStartDateYearNumeric + ' '
                       + value.eventStartDateStartTime + ' to '
                       + value.eventEndDateEndTime +
                 '</p>';
          dis += '<p class="event-detail"><strong>Where:</strong></p>';
          dis += '<p>' + value.eventLocation + '</p>';
          dis += '<p class="event-detail"><strong>Description:</strong></p>';
          dis += '<p>' + value.eventDescription + '</p>';
          dis += '<p class="event-detail"><strong>Department:</strong></p>';
          dis += '<p>' + value.departmentName + '</p>';
          dis += '</div><!-- modal-body -->';
          dis += '<div class="modal-footer">';
          dis += '<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>';
          dis += '</div>';
          dis += '</div><!-- modal-content -->';
          dis += '</div><!-- modal-dialog -->';
          dis += '</div>';
        }
      });
    $("#calendarRow").html(dis);
  })
  .error(function(){
    $("#calendarRow").html("<p class='json-error'>To view upcoming events please visit our <a href='/Pages/DistrictCalendar.aspx'>calendar page</a>.</p>");
  });

});
         </script>
