  <!-- show DukeAlert and DukeMessage -->
  <script type="text/javascript">
   let todayDate = new Date();
	let endAlertDate = new Date(EXPDATE_ALERT);
	let endMessageDate = new Date(EXPDATE_MESSAGE);

	if( DukeAlertBody!="" ) {
		if( todayDate<endAlertDate )  showDukeAlert();
	}

	if( DukeMessageBody!="" ) {
		if( todayDate<endMessageDate ) showDukeMessage();
	}
  </script>
