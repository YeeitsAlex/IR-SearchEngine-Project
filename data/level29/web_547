   <script>
    $(document).ready(function(){
	// global vars
	var urlString = "";

	// window listener functions
	$(window).scroll(function(){
		scrollPosition = $(this).scrollTop();
		if(scrollPosition >= 200){
			$('.navbar-default').css('background-color','#272F38');
		} else {
			if(urlString != 'about'){
				$('.navbar-default').css('background-color','rgba(40,47,57,0.3)');
			}
		}
	});


// end database scripts
});

// modal activity
$(document).ready(function(){
	var mapsUrl = "https://risingabove.cast.uark.edu/maps";
	$('.maps-modal').click(function(){
		window.location.href=mapsUrl;
	});
	var threeUrl = "https://risingabove.cast.uark.edu/visualization";
	$('.3d-modal').click(function(){
		window.location.href=threeUrl;
	});
  var connectUrl = "https://risingabove.cast.uark.edu/connections";
  $('.connections-modal').click(function(){
    window.location.href=connectUrl;
  });
})
