                  <div class="panel-pane pane-views-panes pane-asu-news-new-panel-pane-4 add-margin-12-above ">
                   <h2 class="pane-title">
                    News
                   </h2>
                   <div class="pane-content">
                    <div class="view view-asu-news-new view-id-asu_news_new view-display-id-panel_pane_4 view-dom-id-b19091aa472f2f189f751a0b5e4310a0">
                     <div class="view-content">
                      <div class="views-view-grid grid-3">
                       <div class="row-1 row-last row">
                        <div class="gridCol gridborder col-1 col-md-4">
                         <div class="grid-item">
                          <div class="views-field views-field-field-asunews-image">
                           <div class="field-content">
                            <a href="/human-impacts-erode-behavioral-diversity-chimpanzees">
                             <img class="asunews-thumbnail" height="200" src="https://socialscience.asu.edu/sites/default/files/styles/asunews_thumbnail/public/images_asunews/abrams_-_adult_male_0.jpg?itok=wU9-LDO3" width="360"/>
                            </a>
                           </div>
