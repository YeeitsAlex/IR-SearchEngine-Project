      <h1 class="widget-title">
       Our Location
      </h1>
      <div id="sidebar-map" style="width: 270px; height: 260px;">
      </div>
      <script src="//maps.googleapis.com/maps/api/js?key=AIzaSyAYbR7nTJfkAcXTbt7EWa-0Pvu0vERl-A4&amp;sensor=false" type="text/javascript">
      </script>
      <script type="text/javascript">
       function initialize() {
							var mapOptions = {
							  zoomControl: true,
							  center: new google.maps.LatLng(43.344368,-90.396329),
							  mapTypeId: google.maps.MapTypeId.ROADMAP,
							  streetViewControl: false,
							  disableDefaultUI: true,
							  zoom: 16
	 						};

							var map = new google.maps.Map(document.getElementById('sidebar-map'),
								mapOptions);
					
							var MyPlace = new google.maps.LatLng(43.344368,-90.396329);
					
							var marker = new google.maps.Marker({
								position: MyPlace,
								url: 'https://richland.extension.wisc.edu/office-map-and-directions/',
								title: 'Office Map and Directions',
								map: map
							});
					
							google.maps.event.addListener(marker, 'click', function() {
							  window.location.href = marker.url;
							});

						  }
						  google.maps.event.addDomListener(window, 'load', initialize);
      </script>
      <p>
       <strong>
        Extension Richland County
       </strong>
       <br/>
       1000 Hwy 14 West
       <br/>
       Richland Center, WI 53581-1398
       <br/>
       Phone: 608-647-6148
       <br/>
       Fax: 608-647-9116
       <br/>
       <strong>
        Office Hours:
       </strong>
      </p>
      <p>
       M-F 8:30 AM to 4:30 PM
      </p>
      <p>
      </p>
      <p>
       In the case of inclement weather, Extension Richland County program cancellations are always announced on WRCO radio 100.9 FM on the day of the scheduled program. If you are unsure about whether a program or meeting is happening, please contact our office before leaving.
      </p>
