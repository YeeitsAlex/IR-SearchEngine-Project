        <div class="block-content">
         <p>
          The following members were new to the University of Chicago faculty during the year ending January 1, 2019.
         </p>
         <!-- Add alpha switcher -->
         <link href="/sites/newfaculty.uchicago.edu/files/uploads/custom.css" media="screen, projection" rel="stylesheet" type="text/css"/>
         <script src="https://webresources.uchicago.edu/js/jquery-1.3.2.js" type="text/javascript">
         </script>
         <script type="text/javascript">
          $(document).ready(function() {
/*$('ul.a2znav li a').click(function() {$('#notablealumni').children().hide(); return false;});*/

$('ul.a2znav a#ACtog').click(function() {$('#notablealumni').children().fadeOut(); $('ul.a2znav').children().removeClass('active'); $('.AC').addClass('active'); $('#notablealumni div#AC').fadeIn(); return false;});
$('ul.a2znav a#DKtog').click(function() {$('#notablealumni').children().fadeOut(); $('ul.a2znav').children().removeClass('active'); $('.DK').addClass('active'); $('#notablealumni div#DK').fadeIn(); return false;});
$('ul.a2znav a#LPtog').click(function() {$('#notablealumni').children().fadeOut(); $('ul.a2znav').children().removeClass('active'); $('.LP').addClass('active'); $('#notablealumni div#LP').fadeIn(); return false;});
$('ul.a2znav a#RZtog').click(function() {$('#notablealumni').children().fadeOut(); $('ul.a2znav').children().removeClass('active'); $('.RZ').addClass('active'); $('#notablealumni div#RZ').fadeIn(); return false;});

});
         </script>
