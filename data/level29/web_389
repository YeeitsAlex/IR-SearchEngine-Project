    <script>
     jQuery(document).ready(function($){
    setTimeout(animateIntro, 1000);
  });

  function animateIntro() {
      jQuery('.mooc-object-align.home').each(function(i){
        jQuery('.mooc-color-'+(i+1)).delay(i*250).animate({marginLeft: "0"}, 1500);
      });

      jQuery('.featured-moocs-icon').each(function(i){
        jQuery(this).hide(0).delay((i*250)+1100).show(0).animate({marginTop: "0"});
      });

      // jQuery('#mooc-home-centerpiece-background').delay(1500).fadeIn(2000);
      jQuery('#mooc-home-centerpiece-title').delay(1500).fadeIn(2000);      
      jQuery('#home-arrow-right').delay(1500).fadeIn(2000);      
    // } 
  }
    </script>
   </div>
