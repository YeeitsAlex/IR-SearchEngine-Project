// define the states in javascript
states = new Array();

var states;
$.getJSON( 'https://studentessentials.cornell.edu/js/state.json',function(data) {
    states = data;
});

var countrylist;
$.getJSON( 'https://studentessentials.cornell.edu/js/country.json',function(data) {
    countrylist = data;
});


// $psdata['home_address_country']

$("#home_country option").remove();
$("#resi_country option").remove();
$.each(countrylist, function(cindex, cvalue) {
    ccode = cvalue.code;
    cdesc = cvalue.description;
    current_home_country = ("" == ccode) ? " selected='SELECTED' ":"";
    current_resi_country = ("" == ccode) ? " selected='SELECTED' ":"";
    $('#home_country').append('<option value="'+ccode+'"'+current_home_country+'>'+cdesc+'</option>');
    $('#resi_country').append('<option value="'+ccode+'"'+current_resi_country+'>'+cdesc+'</option>');
});


// define the address field lists in Javascript
var countries;
// was /js/countries.json
$.getJSON( '/js/addrlblfrmt.json',function(data) {
    countries = data;
});


  $( "#home_country" ).change(function() {
     $("#home_state option").remove();
     $("#home_state").append($('<option>', {
        value: "",
        text: "-- select after country --"
     }));
     $.each(states, function(xindex,s) {
        index = $("#home_country").val();
        if (s.country == index) {
            if (s.state == '') {
                $("#home_state").append(
                    $("<option/>").text(s.description).val(s.state).attr("selected", true)
                );
            } else {
                $("#home_state").append(
                    $("<option/>").text(s.description).val(s.state)
                );
            }
        }
     });
    countrydata = countries[$("#home_country").val()];
    if (countrydata == undefined) {countrydata = [[]];}
    $(".homeaddress1, .homeaddress2, .homeaddress3, .homeaddress4, .homecity, .homenum1, .homenum2, .homefield1, .homefield2, .homefield3, .homecounty, .homestate").hide().find("input").attr("value", "");
    $.each(countrydata, function(xindex,row) {
        $.each(row, function(yindex, fielddata) {
            if (fielddata != null) {
                //alert(fielddata.field + " " + fielddata.label);
                if (fielddata.field == "ADDRESS1") {
                    $(".homeaddress1").show().find("input").val($("#home_original_address1").val());
                    $("label.homeaddress1").text(fielddata.label);
                }
                if (fielddata.field == "ADDRESS2") {
                    $(".homeaddress2").show().find("input").val($("#home_original_address2").val());
                    $("label.homeaddress2").text(fielddata.label);
                }
                if (fielddata.field == "ADDRESS3") {
                    $(".homeaddress3").show().find("input").val($("#home_original_address3").val());
                    $("label.homeaddress3").text(fielddata.label);}
                if (fielddata.field == "ADDRESS4") {
                    $(".homeaddress4").show().find("input").val($("#home_original_address4").val());
                    $("label.homeaddress4").text(fielddata.label);}
                if (fielddata.field == "CITY") {
                    $(".homecity").show().find("input").val($("#home_original_city").val());
                    $("label.homecity").text(fielddata.label);}
                if (fielddata.field == "NUM1") {
                    $(".homenum1").show().find("input").val($("#home_original_num1").val());
                    $("label.homenum1").text(fielddata.label);
                }
                if (fielddata.field == "NUM2") {
                    $(".homenum2").show().find("input").val($("#home_original_num2").val());
                    $("label.homenum2").text(fielddata.label);
                };
                if (fielddata.field == "ADDR_FIELD1") {
                    $(".homefield1").show().find("input").val($("#home_original_addr_field1").val());
                    $("label.homefield1").text(fielddata.label);
                }
                if (fielddata.field == "ADDR_FIELD2") {
                    $(".homefield2").show().find("input").val($("#home_original_addr_field2").val());
                    $("label.homefield2").text(fielddata.label);
                }
                if (fielddata.field == "ADDR_FIELD3") {
                    $(".homefield3").show().find("input").val($("#home_original_addr_field3").val());
                    $("label.homefield3").text(fielddata.label);
                }
                if (fielddata.field == "COUNTY") {
                    $(".homecounty").show().find("input").val($("#home_original_county").val());
                    $("label.homecounty").text(fielddata.label);
                }
                if (fielddata.field == "STATE") {
                    $(".homestate").show().find("input").val($("#home_original_state").val());
                    $("label.homestate").text(fielddata.label);
                }
                if (fielddata.field == "POSTAL") {
                    $(".homepostal").show().find("input").val($("#home_original_postal").val());
                    $("label.homepostal").text(fielddata.label);
                }
            }
        });
    });
  });


  $( "#resi_country" ).change(function() {
     $("#resi_state option").remove();
     $("#resi_state").append($('<option>', {
        value: "",
        text: "-- select after country --"
     }));
     $.each(states, function(xindex,s) {
        index = $("#resi_country").val();
        if (s.country == index) {
            if (s.state == '') {
                $("#resi_state").append(
                    $("<option/>").text(s.description).val(s.state).attr("selected", true)
                );
            } else {
                $("#resi_state").append(
                    $("<option/>").text(s.description).val(s.state)
                );
            }
        }
     });
    countrydata = countries[$("#resi_country").val()];
    if (countrydata == undefined) {countrydata = [[]];}
    $(".resiaddress1, .resiaddress2, .resiaddress3, .resiaddress4, .resicity, .resinum1, .resinum2, .resifield1, .resifield2, .resifield3, .resicounty, .resistate").hide().find("input").attr("value", "");
    $.each(countrydata, function(xindex,row) {
        $.each(row, function(yindex, fielddata) {
            if (fielddata != null) {
                //alert(fielddata.field + " " + fielddata.label);
                if (fielddata.field == "ADDRESS1") {
                    $(".resiaddress1").show().find("input").val($("#resi_original_address1").val());
                    $("label.resiaddress1").text(fielddata.label);
                }
                if (fielddata.field == "ADDRESS2") {
                    $(".resiaddress2").show().find("input").val($("#resi_original_address2").val());
                    $("label.resiaddress2").text(fielddata.label);
                }
                if (fielddata.field == "ADDRESS3") {
                    $(".resiaddress3").show().find("input").val($("#resi_original_address3").val());
                    $("label.resiaddress3").text(fielddata.label);
                }
                if (fielddata.field == "ADDRESS4") {
                    $(".resiaddress4").show().find("input").val($("#resi_original_address4").val());
                    $("label.resiaddress4").text(fielddata.label);
                }
                if (fielddata.field == "CITY") {
                    $(".resicity").show().find("input").val($("#resi_original_city").val());
                    $("label.resicity").text(fielddata.label);
                }
                if (fielddata.field == "NUM1") {
                    $(".resinum1").show().find("input").val($("#resi_original_num1").val());
                    $("label.resinum1").text(fielddata.label);
                }
                if (fielddata.field == "NUM2") {
                    $(".resinum2").show().find("input").val($("#resi_original_num2").val());
                    $("label.resinum2").text(fielddata.label);
                }
                if (fielddata.field == "ADDR_FIELD1") {
                    $(".resifield1").show().find("input").val($("#resi_original_addr_field1").val());
                    $("label.resifield1").text(fielddata.label);
                }
                if (fielddata.field == "ADDR_FIELD2") {
                    $(".resifield2").show().find("input").val($("#resi_original_addr_field2").val());
                    $("label.resifield2").text(fielddata.label);
                }
                if (fielddata.field == "ADDR_FIELD3") {
                    $(".resifield3").show().find("input").val($("#resi_original_addr_field3").val());
                    $("label.resifield3").text(fielddata.label);
                }
                if (fielddata.field == "COUNTY") {
                    $(".resicounty").show().find("input").val($("#resi_original_county").val());
                    $("label.resicounty").text(fielddata.label);
                }
                if (fielddata.field == "STATE") {
                    $(".resistate").show().find("input").val($("#resi_original_state").val());
                    $("label.resistate").text(fielddata.label);
                }
                if (fielddata.field == "POSTAL") {
                    $(".resipostal").show().find("input").val($("#resi_original_postal").val());
                    $("label.resipostal").text(fielddata.label);
                }
            }
        });
    });
  });

$("#resi_country").trigger("change");
$("#home_country").trigger("change");


// ========== deleteresiaddressform =====================================================================================================
var deleteresiaddressvalidator = $("#deleteresiaddressform").validate({
    rules: {

    },
    errorElement: "div",
    errorPlacement: function(error, element) {
            error.appendTo(element.parent());
    },
    // specifying a submitHandler prevents the default submit
    submitHandler: function() {
        var formData = $("#deleteresiaddressform").serializeArray();
        var URL = '/auth/data/deleteresiaddress';

        $.post(URL,
            formData,
            function(data, textStatus, jqXHR)
            {
               //data: Data from server.
                if (data.result == "success") {
                    //location.href = "/auth/portal/#data-addresses";
                    location.href = "/";
                }
               // alert(data.result);
            }).fail(function(jqXHR, textStatus, errorThrown)
            {
               // alert(textStatus);
            });
    },
    // set this class to error-labels to indicate valid fields
    success: function(label) {
        // set &nbsp; as text for IE
        $(label).addClass("valid").removeClass("error");
    },
    highlight: function(element, errorClass) {
         $(element).addClass(errorClass);
         //$("#phoneerror").addClass("errorclass");
         //$(element).parent().find("." + errorClass).remove();
         //$(element).parent().addClass(errorClass);
    },
    unhighlight: function(element, errorClass) {
         $(element).removeClass(errorClass);
         //$("#phoneerror").removeClass("errorClass").html("");
         $(element).parent().removeClass(errorClass);
    }
});

// ================= deletehomeaddressform ==============================================================================================
var deletehomeaddressvalidator = $("#deletehomeaddressform").validate({
    rules: {

    },
    errorElement: "div",
    errorPlacement: function(error, element) {
            error.appendTo(element.parent());
    },
    // specifying a submitHandler prevents the default submit
    submitHandler: function() {
        var formData = $("#deletehomeaddressform").serializeArray();
        var URL = '/auth/data/deletehomeaddress';

        $.post(URL,
            formData,
            function(data, textStatus, jqXHR)
            {
               //data: Data from server.
                if (data.result == "success") {
                    //location.href = "/auth/portal/#data-addresses";
                    location.href = "/";
                }
               // alert(data.result);
            }).fail(function(jqXHR, textStatus, errorThrown)
            {
               // alert(textStatus);
            });
    },
    // set this class to error-labels to indicate valid fields
    success: function(label) {
        // set &nbsp; as text for IE
        $(label).addClass("valid").removeClass("error");
    },
    highlight: function(element, errorClass) {
         $(element).addClass(errorClass);
         //$("#phoneerror").addClass("errorclass");
         //$(element).parent().find("." + errorClass).remove();
         //$(element).parent().addClass(errorClass);
    },
    unhighlight: function(element, errorClass) {
         $(element).removeClass(errorClass);
         //$("#phoneerror").removeClass("errorClass").html("");
         $(element).parent().removeClass(errorClass);
    }
});

// ===============================================================================================================
var homeaddressvalidator = $("#homeaddressform").validate({
    rules: {
        home_address1: {required: true},
        home_address2: {required: false},
        home_city: {required: true},
        home_state: "myHomeStateFormat",
        home_postal: "myHomePostalFormat",
        home_country: {required: true}
    },
    errorElement: "div",
    errorPlacement: function(error, element) {
            error.appendTo(element.parent());
    },
    // specifying a submitHandler prevents the default submit
    submitHandler: function() {
        var formData = $("#homeaddressform").serializeArray();
        var URL = '/auth/data/updatehomeaddressajax';

        $.post(URL,
            formData,
            function(data, textStatus, jqXHR)
            {
               //data: Data from server.
                if (data.result == "success") {
                    //location.href = "/auth/portal/#data-addresses";
                    location.href = "/";
                }
               // alert(data.result);
            }).fail(function(jqXHR, textStatus, errorThrown)
            {
               // alert(textStatus);
            });
    },
    // set this class to error-labels to indicate valid fields
    success: function(label) {
        // set &nbsp; as text for IE
        $(label).addClass("valid").removeClass("error");
    },
    highlight: function(element, errorClass) {
         $(element).addClass(errorClass);
         //$("#phoneerror").addClass("errorclass");
         //$(element).parent().find("." + errorClass).remove();
         //$(element).parent().addClass(errorClass);
    },
    unhighlight: function(element, errorClass) {
         $(element).removeClass(errorClass);
         //$("#phoneerror").removeClass("errorClass").html("");
         $(element).parent().removeClass(errorClass);
    }
});

