  <!-- This is a handlebars template for the list of emergency contacts -->
  <!-- It will be displayed in div id=contactlist                       -->
  <script id="contacts-template" type="text/x-handlebars-template">
   <h4>Your Emergency Contacts</h4>
<div>
When creating or updating an emergency contact you must include a phone number for the contact.
</div>
<div class="two-col">
{{#each contacts}}
<div class="contact">
        <h5>{{contact_name}}<span class="relationship"> ({{relationshipname}})</span></h5>
{{#is primary_contact "Y"}}
        <p class="primary"><span class="fa fa-check-circle-o"></span> Primary Contact</p>
{{/is}}
        <button class="showcontact" name="{{contact_name}}">Show</button>
        <button class="editcontact" name="{{contact_name}}">Edit</button>
{{#isnot primary_contact "Y"}}
        <button class="deletecontact" name="{{contact_name}}">Delete</button>
{{/isnot}}
</div>
{{/each}}
</div>
  </script>
  <script id="contact-display-template" type="text/x-handlebars-template">
   <h4>Display Contact</h4>

    <fieldset>

