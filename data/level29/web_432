        resi_phone_number: 'phone min 10, max 24 digits',
        cel1_phone_number: 'phone min 10, max 24 digits',
        perm_phone_number: 'phone min 10, max 24 digits',
        work_phone_number: 'phone min 10, max 24 digits',
        text_phone_number: 'phone min 10, max 24 digits',
        home_phone_extension: "extension 1-6 digits",
        resi_phone_extension: "extension 1-6 digits",
        cel1_phone_extension: "extension 1-6 digits",
        perm_phone_extension: "extension 1-6 digits",
        work_phone_extension: "extension 1-6 digits",
        text_phone_extension: "extension 1-6 digits"
    },
    errorPlacement: function(error, element) {
            error.appendTo(element.parent());
            //error.appendTo("div#phoneerrors");
    },
    // specifying a submitHandler prevents the default submit

    submitHandler: function(e) {
        var formData = $("#phoneform").serializeArray();
        var URL = '/auth/data/updatephonenumbersajax';

        //check for custom validation before submit 2044
        if (checkCWDState() > 2){
            //submited data via ajax
            $.post(URL,
                formData,
                function(data, textStatus, jqXHR)
                {
                   //data: Data from server.
                    if (data.result == "success") {
                        //location.href = "/auth/portal";
                        location.href = "/";
                    }
                    //alert(data.result);
                }).fail(function(jqXHR, textStatus, errorThrown)
                {
                    //alert(textStatus);
                });
        }
    },

    // set this class to error-labels to indicate valid fields
    success: function(label) {
        // set &nbsp; as text for IE
        $(label).addClass("valid").removeClass("error");
    },
    highlight: function(element, errorClass) {
         $(element).addClass(errorClass);
         //$("#phoneerror").addClass("errorclass");
         //$(element).parent().find("." + errorClass).remove();
         //$(element).parent().addClass(errorClass);
    },
    unhighlight: function(element, errorClass) {
         $(element).removeClass(errorClass);
         //$("#phoneerror").removeClass("errorClass").html("");
         $(element).parent().removeClass(errorClass);
    }
});

$.validator.addMethod("homePhoneFormat", function(value) {
    temp = value.replace(/\D/g,'');
    return (value=="" && $("#home_phone_country_code").val() =="" && $("#home_phone_extension").val() =="" ) || ((temp.length >=10) && (temp.length <= 24))
//                    return (value=="" && $("#home_phone_country_code").val() =="" && $("#home_phone_extension").val() =="" ) || /^(\(\d\d\d\)|\d\d\d)(\/|\s)*\d\d\d\-?\d\d\d\d$/.test(value);
                }, 'ddd/ddd-dddd');

$.validator.addMethod("resiPhoneFormat", function(value) {
    temp = value.replace(/\D/g,'');
    return (value=="" && $("#resi_phone_country_code").val() =="" && $("#resi_phone_extension").val() =="" ) || ((temp.length >=10) && (temp.length <= 24))
                }, 'ddd/ddd-dddd');

$.validator.addMethod("cel1PhoneFormat", function(value) {
    temp = value.replace(/\D/g,'');
    return (value=="" && $("#cel1_phone_country_code").val() =="" && $("#cel1_phone_extension").val() =="" ) || ((temp.length >=10) && (temp.length <= 24))
                }, 'ddd/ddd-dddd');

$.validator.addMethod("permPhoneFormat", function(value) {
    temp = value.replace(/\D/g,'');
    return (value=="" && $("#perm_phone_country_code").val() =="" && $("#perm_phone_extension").val() =="" ) || ((temp.length >=10) && (temp.length <= 24))
                }, 'ddd/ddd-dddd');

$.validator.addMethod("workPhoneFormat", function(value) {
    temp = value.replace(/\D/g,'');
    return (value=="" && $("#work_phone_country_code").val() =="" && $("#work_phone_extension").val() =="" ) || ((temp.length >=10) && (temp.length <= 24))
                }, 'ddd/ddd-dddd');

$.validator.addMethod("textPhoneFormat", function(value) {
    temp = value.replace(/\D/g,'');
    return (value=="" && $("#text_phone_country_code").val() =="" && $("#text_phone_extension").val() =="" ) || ((temp.length >=10) && (temp.length <= 24))
                }, 'ddd/ddd-dddd');

$.validator.addMethod("myPhoneFormat", function(value) {
                    return value=="" || /^(\(\d\d\d\)|\d\d\d)(\/|\s)*\d\d\d\-?\d\d\d\d$/.test(value);
                }, 'ddd/ddd-dddd');


$.validator.addMethod("myHomeStateFormat", function(value) {
        if ($('#home_state > option').length > 1) {
            return $('#home_state').val() != "";
        } else {
            return 1;
        }
    },
    'state selection required');
$.validator.addMethod("myResiStateFormat", function(value) {
        if ($('#resi_state > option').length > 1) {
            return $('#resi_state').val() != "";
        } else {
            return 1;
        }
    },
    'state selection required');

$.validator.addMethod("myHomePostalFormat", function(value) {
        if ($("#home_country").val() == "USA") {
            return /^\d\d\d\d\d$/.test(value);
        } else if ($("#resi_country").val() == "CAN") {
            return /^[A-Z]\d[A-Z]\s\d[A-Z]\d$/.test(value);
        } else {
            return 1;
        }
    },
    'valid postal code required');
$.validator.addMethod("myResiPostalFormat", function(value) {
    if ($("#resi_country").val() == "USA") {
        return /^\d\d\d\d\d$/.test(value);
    } else if ($("#resi_country").val() == "CAN") {
        return /^[A-Z]\d[A-Z]\s\d[A-Z]\d$/.test(value);
    } else {
        return 1;
    }
    },
    'valid postal code required');

        // add google analytics external link tracker

    function _gaLt(event){
        var el = event.srcElement || event.target;

        /* Loop up the tree through parent elements if clicked element is not a link (eg: an image inside a link) */
        while(el && (typeof el.tagName == 'undefined' || el.tagName.toLowerCase() != 'a' || !el.href))
            el = el.parentNode;

        if(el && el.href){
            if(el.href.indexOf(location.host) == -1){ /* external link */
                ga("send", "event", "Outgoing Links", el.href, document.location.pathname + document.location.search);
                /* if target not set then delay opening of window by 0.5s to allow tracking */
                if(!el.target || el.target.match(/^_(self|parent|top)$/i)){
                    setTimeout(function(){
                        document.location.href = el.href;
                    }.bind(el),500);
                    /* Prevent standard click */
                    event.preventDefault ? event.preventDefault() : event.returnValue = !1;
                }
            }

        }
    }

    /* Attach the event to all clicks in the document after page has loaded */
    var w = window;
    w.addEventListener ? w.addEventListener("load",function(){document.body.addEventListener("click",_gaLt,!1)},!1)
      : w.attachEvent && w.attachEvent("onload",function(){document.body.attachEvent("onclick",_gaLt)});

// Add some functionality to HandleBars

    Handlebars.registerHelper('is', function(a, b, opts) {
      //if (a == "Yes" || b == "Yes") alert("is: "+a+", "+b);
      if(a == b) // Or === depending on your needs
        return opts.fn(this);
      else
        return opts.inverse(this);
     });
    Handlebars.registerHelper('isnot', function(a, b, opts) {
      //if (a == "Yes" || b == "Yes") alert("is: "+a+", "+b);
      if(a != b) // Or === depending on your needs
        return opts.fn(this);
      else
        return opts.inverse(this);
     });

    Handlebars.registerHelper('address', function(options) {
      var a ='<div>';

      function aval(text) {
        if (text != "") {
            return text + "<br>";
        } else {
            return "";
        }
      }
       a = a + '</div>';
    return new Handlebars.SafeString(a);
});

// these lines display the list of contacts as a table, with buttons
var contacts_source   = $("#contacts-template").html();
var contacts_template = Handlebars.compile(contacts_source);
var contacts_html = contacts_template(contacts);
$("#contactlist").html(contacts_html);


// display one contact
$(".showcontact").click(function() {
        var name = $(this).attr("name");

        $.each(contacts['contacts'], function(index,contact) {
        if (contact['contact_name'] == name) {
            var contact_display_source   = $("#contact-display-template").html();
            var contact_display_template = Handlebars.compile(contact_display_source);
            var contact_html = contact_display_template(contact);
            $("#emergency-contact-display").html(contact_html);
        }
    });
});


$(".newcontact").click(function() {
    var name = $(this).attr("name");
    // The first contact should default to primary
    var beprimary = (contacts['contacts'].length == 0) ? "Y" : "N";
    contact = {
        contact_name: "",
        primary_contact: beprimary,
        relationship: "",
        same_address_empl: "",
        address_type: "HOME",
        country: "USA",
        address1: "",
        address2: "",
        address3: "",
        address4: "",
        city: "",
        num1: "",
        num2: "",
        addr_field1: "",
        addr_field2: "",
        addr_field3: "",
        state: "",
        postal: "",
        county: "",
        same_phone_empl: "N",
        main_phone_type: "HOME",
        main_phone_nbr: "",
        main_phone_country_code: "",
        main_extension: "",
        action: "NEW"
    };
    var contact_edit_source   = $("#contact-edit-template").html();
    var contact_edit_template = Handlebars.compile(contact_edit_source);
    var contact_html = contact_edit_template(contact);
    $("#emergency-contact-display").html(contact_html);


    $("#contact_same_address").hide();
    $("#contact_separate_address").show();


    // now that it's in the dom, set the click handler
    $('input[type="reset"].cancel').click(function(e) {
        $('#popup-close').trigger('click');
    });

    $("#contact_country").val(contact['country']);  // pick up default, not done in template

        // Now that the data is in the dom, we can adjust the display of the address fields

          $( "#contact_country" ).change(function() {
             $("#contact_state option").remove();
             $("#contact_state").append($('<option>', {
                value: "",
                text: "-- select after country --"
             }));
             $.each(states, function(xindex,s) {
                index = $("#contact_country").val();
                if (s.country == index) {
                    if (s.state == contact['contact_state']) {
                        $("#contact_state").append(
                            $("<option/>").text(s.description).val(s.state).attr("selected", true)
                        );
                    } else {
                        $("#contact_state").append(
                            $("<option/>").text(s.description).val(s.state)
                        );
                    }
                }
             });
            countrydata = countries[$("#contact_country").val()];
            if (countrydata == undefined) {countrydata = [[]];}
            //$(".contactaddress1, .contactaddress2, .contactaddress3, .contactcity, .contactcounty, .contactstate").hide();
            $.each(countrydata, function(xindex,row) {
                $.each(row, function(yindex, fielddata) {
                    if (fielddata != null) {
                        //alert(fielddata.field + " " + fielddata.label);
                        if (fielddata.field == "ADDRESS1") {
                            $(".contactaddress1").show();
                            $("label.contactaddress1").text(fielddata.label);
                        }
                        if (fielddata.field == "ADDRESS2") {
                            $(".contactaddress2").show();
                            $("label.contactaddress2").text(fielddata.label);
                        }
                        if (fielddata.field == "ADDRESS3") {
                            $(".contactaddress3").show();
                            $("label.contactaddress3").text(fielddata.label);
                        }
                        if (fielddata.field == "CITY") {
                            $(".contactcity").show().find("input").val($("#contact_city").val());
                            $("label.contactcity").text(fielddata.label);
                        }
                        if (fielddata.field == "COUNTY") {
                            $(".contactcounty").show().find("input").val($("#contact_county").val());
                            $("label.contactcounty").text(fielddata.label);
                        }
                        if (fielddata.field == "STATE") {
                            $(".contactstate").show().find("input").val($("#contact_state").val());
                            $("label.contactstate").text(fielddata.label);
                        }
                        if (fielddata.field == "POSTAL") {
                            $(".contactpostal").show().find("input").val($("#contact_postal").val());
                            $("label.contactpostal").text(fielddata.label);
                        }
                    }
                });
            });
          });

            $("#contact_country").trigger("change");
            $("#contact_state").val(contact['state']);  // set value now that select is generated

            // Done with the address fields, now we setup the form validator

    var contacteditvalidator = $("#contacteditform")
                .submit(function(e) {
                        e.preventDefault();
                })
                .validate({
                rules: {
                    contact_contact_name: {required: true},
                    contact_relationship: {required: true},
                    contact_main_phone_nbr: "contactMainPhoneNbrFormat"
                },
                errorElement: "div",
                errorPlacement: function(error, element) {
                        error.appendTo(element.parent());
                },
                // specifying a submitHandler prevents the default submit
                submitHandler: function(e) {
                    var contact = {
                        contact_name: $("#contact_contact_name").val(),
                        primary_contact: $("#contact_primary_contact").val(),
                        relationship: $("#contact_relationship").val(),
                        same_address_empl: "N",
                        address_type: $("#contact_address_type").val(),
                        country: $("#contact_country").val(),
                        address1: $("#contact_address1").val(),
                        address2: $("#contact_address2").val(),
                        address3: $("#contact_address3").val(),
                        address4: $("#contact_address4").val(),
                        city: $("#contact_city").val(),
                        num1: $("#contact_num1").val(),
                        num2: $("#contact_num2").val(),
                        addr_field1: $("#contact_addr_field1").val(),
                        addr_field2: $("#contact_addr_field2").val(),
                        addr_field3: $("#contact_addr_field3").val(),
                        state: $("#contact_state").val(),
                        postal: $("#contact_postal").val(),
                        county: $("#contact_county").val(),
                        country: $("#contact_country").val(),
                        same_phone_empl: "N",
                        main_phone_type: $("#contact_main_phone_type").val(),
                        main_phone_nbr: $("#contact_main_phone_nbr").val(),
                        main_phone_country_code: $("#contact_main_phone_country_code").val(),
                        main_extension: $("#contact_main_extension").val(),
                        cell_phone_nbr: $("#contact_cell_phone_nbr").val(),
                        home_phone_nbr: $("#contact_home_phone_nbr").val(),
                        work_phone_nbr: $("#contact_work_phone_nbr").val(),
                        action: $("#contact_action").val()
                    };
                    //alert(JSON.stringify(contact));
                    if (
                        ($("#contact_cell_phone_nbr").val() == "") &&
                        ($("#contact_home_phone_nbr").val() == "") &&
                        ($("#contact_work_phone_nbr").val() == "") &&
                        ($("#same_phone_empl").val() != "Y")
                        ) {
                        alert("You must supply either a cell, home or work phone number");
                        e.preventDefault();
                        return false;
                    }

                    contact["emplid"] = '';
                    var URL = '/auth/data/updatecontact';
                    if (contact["action"] == "NEW") {
                        URL = '/auth/data/newcontact';
                    }

                    $.post(URL,
                        contact,
                        function(data, textStatus, jqXHR)
                        {
                           //data: Data from server.
                            //if (data.result == "success") {
                            if (1) {
                               window.open("/auth/portal?p=ec", "_self");
                               return;
                            }
                            alert(JSON.stringify(data));
                        }).fail(function(jqXHR, textStatus, errorThrown)
                        {
                            alert(textStatus);
                        });
                }, // end of submithandler
                // set this class to error-labels to indicate valid fields
                success: function(label) { // lab1; as text for IE
                    $(label).addClass("valid").removeClass("error");
                },
                highlight: function(element, errorClass) {
                     $(element).addClass(errorClass);
                     //$("#phoneerror").addClass("errorclass");
                     //$(element).parent().find("." + errorClass).remove();
                     //$(element).parent().addClass(errorClass);
                },
                unhighlight: function(element, errorClass) {
                     $(element).removeClass(errorClass);
                     $(element).parent().removeClass(errorClass);
                }
            }); //end of validator




});

// populate the edit form
$(".editcontact").click(function() {
    var name = $(this).attr("name");
    $.each(contacts['contacts'], function(index,contact) {
        if (contact['contact_name'] == name) {
            contact['action'] = "UPDATE";
            var contact_edit_source   = $("#contact-edit-template").html();
            var contact_edit_template = Handlebars.compile(contact_edit_source);
            var contact_html = contact_edit_template(contact);
            $("#emergency-contact-display").html(contact_html);

            $("#contact_same_address").hide();
            $("#contact_separate_address").show();

            // now that it's in the dom, set the click handler
            $('input[type="reset"].cancel').click(function(e) {
                $('#popup-close').trigger('click');
            });

            $("#contact_relationship").val(contact['relationship']);  // need to set the values
            $("#contact_country").val(contact['country']);  // not done in template

            // Now that the data is in the dom, we can adjust the display of the address fields

          $( "#contact_country" ).change(function() {
             $("#contact_state option").remove();
             $("#contact_state").append($('<option>', {
                value: "",
                text: "-- select after country --"
             }));
             $.each(states, function(xindex,s) {
                index = $("#contact_country").val();
                if (s.country == index) {
                    if (s.state == contact['contact_state']) {
                        $("#contact_state").append(
                            $("<option/>").text(s.description).val(s.state).attr("selected", true)
                        );
                    } else {
                        $("#contact_state").append(
                            $("<option/>").text(s.description).val(s.state)
                        );
                    }
                }
             });
            countrydata = countries[$("#contact_country").val()];
            if (countrydata == undefined) {countrydata = [[]];}
            //$(".contactaddress1, .contactaddress2, .contactaddress3, .contactcity, .contactcounty, .contactstate").hide();
            $.each(countrydata, function(xindex,row) {
                $.each(row, function(yindex, fielddata) {
                    if (fielddata != null) {
                        //alert(fielddata.field + " " + fielddata.label);
                        if (fielddata.field == "ADDRESS1") {
                            $(".contactaddress1").show();
                            $("label.contactaddress1").text(fielddata.label);
                        }
                        if (fielddata.field == "ADDRESS2") {
                            $(".contactaddress2").show();
                            $("label.contactaddress2").text(fielddata.label);
                        }
                        if (fielddata.field == "ADDRESS3") {
                            $(".contactaddress3").show();
                            $("label.contactaddress3").text(fielddata.label);
                        }
                        if (fielddata.field == "CITY") {
                            $(".contactcity").show().find("input").val($("#contact_city").val());
                            $("label.contactcity").text(fielddata.label);
                        }
                        if (fielddata.field == "COUNTY") {
                            $(".contactcounty").show().find("input").val($("#contact_county").val());
                            $("label.contactcounty").text(fielddata.label);
                        }
                        if (fielddata.field == "STATE") {
                            $(".contactstate").show().find("input").val($("#contact_state").val());
                            $("label.contactstate").text(fielddata.label);
                        }
                        if (fielddata.field == "POSTAL") {
                            $(".contactpostal").show().find("input").val($("#contact_postal").val());
                            $("label.contactpostal").text(fielddata.label);
                        }
                    }
                });
            });
          });

            $("#contact_country").trigger("change");
            $("#contact_state").val(contact['state']);  // set value now that select is generated


            // Done with the address fields, now we setup the form validator

            var contacteditvalidator = $("#contacteditform")
                .submit(function(e) {
                        e.preventDefault();
                })
                .validate({
                rules: {
                    contact_contact_name: {required: true},
                    contact_main_phone_nbr: "contactMainPhoneNbrFormat"
                },
                errorElement: "div",
                errorPlacement: function(error, element) {
                        error.appendTo(element.parent());
                },
                // specifying a submitHandler prevents the default submit
                submitHandler: function() {
                    var contact = {
                        contact_name: $("#contact_contact_name").val(),
                        primary_contact: $("#contact_primary_contact").val(),
                        relationship: $("#contact_relationship").val(),
                        same_address_empl: "N",
                        address_type: $("#contact_address_type").val(),
                        country: $("#contact_country").val(),
                        address1: $("#contact_address1").val(),
                        address2: $("#contact_address2").val(),
                        address3: $("#contact_address3").val(),
                        address4: $("#contact_address4").val(),
                        city: $("#contact_city").val(),
                        num1: $("#contact_num1").val(),
                        num2: $("#contact_num2").val(),
                        addr_field1: $("#contact_addr_field1").val(),
                        addr_field2: $("#contact_addr_field2").val(),
                        addr_field3: $("#contact_addr_field3").val(),
                        state: $("#contact_state").val(),
                        postal: $("#contact_postal").val(),
                        county: $("#contact_county").val(),
                        country: $("#contact_country").val(),
                        same_phone_empl: "N",
                        main_phone_type: $("#contact_main_phone_type").val(),
                        main_phone_nbr: $("#contact_main_phone_nbr").val(),
                        main_phone_country_code: $("#contact_main_phone_country_code").val(),
                        main_extension: $("#contact_main_extension").val(),
                        cell_phone_nbr: $("#contact_cell_phone_nbr").val(),
                        home_phone_nbr: $("#contact_home_phone_nbr").val(),
                        work_phone_nbr: $("#contact_work_phone_nbr").val(),
                        action: $("#contact_action").val()
                    };
                    //alert(JSON.stringify(contact));

                    if (
                        ($("#contact_cell_phone_nbr").val() == "") &&
                        ($("#contact_home_phone_nbr").val() == "") &&
                        ($("#contact_work_phone_nbr").val() == "") &&
                        ($("#same_phone_empl").val() != "Y")
                        ) {
                        alert("You must supply either a cell, home or work phone number");
                        e.preventDefault();
                        return false;
                    }

                    contact["emplid"] = '';
                    var URL = '/auth/data/updatecontact';
                    if (contact["action"] == "NEW") {
                        URL = '/auth/data/newcontact';
                    }

                    $.post(URL,
                        contact,
                        function(data, textStatus, jqXHR)
                        {
                           //data: Data from server.
                            //if (data.result == "success") {
                            if (1) {
                               window.open("/auth/portal?p=ec", "_self");
                               return;
                            }
                            alert(JSON.stringify(data));
                        }).fail(function(jqXHR, textStatus, errorThrown)
                        {
                            alert(textStatus);
                        });
                }, // end of submithandler
                // set this class to error-labels to indicate valid fields
                success: function(label) { // lab1; as text for IE
                    $(label).addClass("valid").removeClass("error");
                },
                highlight: function(element, errorClass) {
                     $(element).addClass(errorClass);
                     //$("#phoneerror").addClass("errorclass");
                     //$(element).parent().find("." + errorClass).remove();
                     //$(element).parent().addClass(errorClass);
                },
                unhighlight: function(element, errorClass) {
                     $(element).removeClass(errorClass);
                     $(element).parent().removeClass(errorClass);
                }
            }); //end of validator

        }  // end of if ?
    }); // end of foreach
});

$.validator.addMethod("contactMainPhoneNbrFormat", function(value) {
    temp = value.replace(/\D/g,'');
    return  ((temp.length >=10) && (temp.length <= 24))
                }, 'ddd/ddd-dddd');



$(".deletecontact").click(function() {
        var name = $(this).attr("name");
        $.each(contacts['contacts'], function(index,contact) {
        if (contact['contact_name'] == name) {
            // call the function to delete the entry, when it's done refresh the screen
        contact["emplid"] = '';
        var URL = '/auth/data/deletecontact';
        $.post(URL,
            contact,
            function(data, textStatus, jqXHR)
            {
               //data: Data from server.
                if (1) {
                   //document.location.href("/auth/portal#data-emergencycontacts");
                   window.open("/auth/portal?p=ec", "_self");
                   return;
                }
                alert(JSON.stringify(data));
            }).fail(function(jqXHR, textStatus, errorThrown)
            {
                alert(textStatus);
            });
        }
    });
});



$( document ).ready(function() {
//var h = window.location.hash;
if (popup == "n") {
    $('a[href$="data-names"]').trigger("click");
}
if (popup == "a") {
    $('a[href$="data-addresses"]').trigger("click");
}
if (popup == "ec") {
    $('a[href$="data-emergencycontacts"]').trigger("click");
}

});
  </script>
