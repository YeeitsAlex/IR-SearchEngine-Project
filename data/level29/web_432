// impliments CWD custum Validation - cell phone warning

/*confirm checkbox state change event
 * 
 * if checked sets confirmed to true
 * hides the error message
 */
$("#infoConfirm").on('change', function(e){
    if(this.checked) {
        //remove the error warnings
        $('#infoConfirmError').removeClass('cwd-error');
        $(this).removeClass('cwd-custom-error');
    }else{
        // add inline error validation this is required if there is no cell phone
        $(this).addClass('cwd-custom-error');
        $('#infoConfirmError').addClass('cwd-error');
    }
});

/* when a kepress event occurs in the cellphone input
 * 
 * hide the warning bar
 * validation will be handled by jQuery Validate
 */
$('#cel1_phone_number').keypress(function(){
    hideInfoWarning();
});

/* Show custom warning dialog
 *
 */
function showInfoWarning(){
    //remove the info warning
    $('#infoWarning').fadeIn('fast', function(){
        document.getElementById('infoWarning').scrollIntoView(); 
        this.focus();
    }).css("display","inline-block").addClass('visible');
}

/* Hide custom warning dialog
 * unchecks the acknowledge checkbox
 * Hide the warning bar and remove class
 */
function hideInfoWarning(){
    //if check button is checked uncheck it
    if ($("#infoConfirm").is(':checked')){
        $("#infoConfirm").prop('checked', false);
    }
    //remove the info warning
    $('#infoWarning').removeClass('visible');
    $('#infoWarning').fadeOut('fast');

    //hide the error message
    $('#infoConfirmError').hasClass('cwd-error');
}

function showErrorMessage(){
    $('#infoConfirmError').addClass('cwd-error');
    //place error ring on checkbox
    $("#infoConfirm").addClass('cwd-custom-error');
    //move focus to checkbox
    $("#infoConfirm").focus(); 
}
// end of cell phone required implimentation
function checkCWDState(){
    var $checkbox = $("#infoConfirm");
    var $warning = $('#infoWarning')
    var $cellPhone = $('#cel1_phone_number');
    var $errorMsg = $('#infoConfirmError');
    
    if ($checkbox && $cellPhone && $errorMsg && $warning){
        //if checkbox is not checked and no cell phone number do not submit
        var checked = $checkbox.is(':checked');
        var cellVal = $cellPhone.val();
        var errorVisible = $errorMsg.hasClass('cwd-error');
        var warningVisable = $warning.hasClass('visible');

        if ( cellVal !='' ){
            //state 4
            //valid phone submit and reset state            
            return 4;
        }else if( (checked) && (cellVal =='' ) ){
            //state 3
            //valid phone submit and reset state
            return 3;
        }else if ( !(checked) && (cellVal =='') && !(errorVisible) && !(warningVisable)){
            //state 1 
            //invalid do not submit show warning
            showInfoWarning();
            return 0;
        }else if( !(checked) && (cellVal =='') && !(errorVisible) && (warningVisable) ){
            //state 2
            //invalid do not submit show warning retu
            //show error message
            showErrorMessage();
            return 1;
        }else if ( !(checked) && (cellVal =='') && (errorVisible) && (warningVisable) ){
            showInfoWarning();
            showErrorMessage();
            return 2
        }else{
            //error show all warnings just in case
            console.log('WARNING: invalid option');
            showInfoWarning();
            showErrorMessage();          
            return -1;  
        }

    }else{
        console.log('ERROR: required dom element is missing');
    }
    return -1;
}


