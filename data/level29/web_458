      <script type="text/javascript">
       jQuery(document).ready( function($) { if ($('#gateway-page').length) {

        $('#acalog-navigation br').next('div').addClass('nav-title');
        $('#acalog-navigation br').remove();

        $('#acalog-navigation .nav-title').each(function() {
                var navTitleText = $(this).children('a').text();
                $(this).empty().append(navTitleText);
        });

        var helpHref = $('.gateway-toolbar-help a').attr('href');
        var oid = helpHref.substring(16);


        if (oid == 33) {
                helpUrl = '/content.php?catoid=33&navoid=8507';
        } else if (oid == 31) {
                helpUrl = '/content.php?catoid=31&navoid=7977';
        } else if (oid == 28) {
                helpUrl = '/content.php?catoid=28&navoid=7289';
        } else if (oid == 26) {
                helpUrl = '/content.php?catoid=26&navoid=6776';
        } else if (oid == 22) {
                helpUrl = '/content.php?catoid=22&navoid=5824';
        } else if (oid == 18) {
                helpUrl = '/content.php?catoid=18&navoid=4364';
        } else if (oid == 14) {
                helpUrl = '/content.php?catoid=14&navoid=3459';
        } else if (oid == 12) {
                helpUrl = '/content.php?catoid=14&navoid=3459';
        } else {
                helpUrl = '/content.php?catoid=14&navoid=3459';
        }

        $('.gateway-toolbar-help a').attr('href', helpUrl);
        $('.gateway-toolbar-help a').removeAttr('onclick').removeAttr('onClick');

        }});
      </script>
