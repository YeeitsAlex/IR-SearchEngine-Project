  <script>
   $( document ).ready(function() {
                scLoad = function() {
                    var images = $('figure img');
                    for (var j = 0; j < images.length; j++) {
                        var i = $(images[j]),
                            iSrc = i.attr('src');
                        if (!i.is('.wraped')) {
                            i.wrap('<a href="' + iSrc + '" class="nivo-lb" />');
                            i.addClass('wraped');
                       }
                    }
                    $('a.nivo-lb').nivoLightbox({
                        effect: 'fadeScale'
                    });
                    var photosets = $('.photoset-grid');
                    imagesLoaded(photosets, function() {
                        photosets.photosetGrid({
                            highresLinks: true,
                            gutter: '2px',
                            
                            onComplete: function(){
                                var photosets = $('.photoset-grid');
                                for (var i = 0; i < photosets.length; i++) {
                                    var curSet = $(photosets[i]),
                                        pictures = curSet.find('a'),
                                        photosetID = curSet.attr('data-id');
                                    pictures.attr('data-lightbox-gallery', photosetID)
                                    .nivoLightbox({
                                        effect: 'fadeScale'
                                    });
                                }
                                var pictures = $('.photoset-grid a'),
                                    images = pictures.children();
                                for (var i = 0; i < pictures.length; i++) {
                                    var elem =  $(images[i]),
                                        elemAlt = elem.attr('Alt'),
                                        anchor = $(pictures[i]);
                                    if (elemAlt && elemAlt.length > 0) {
                                        anchor.attr('Title', elemAlt).addClass('has-caption');
                                    }
                                }
                            }
                        });
                    });
                    var iframes = $('.typography iframe, .video-wrapper iframe, .video-wrapper video, .single-column .audio-wrapper iframe, .masonry .audio-wrapper iframe, .masonry-2c .audio-wrapper iframe');
                    for (var i = 0; i < iframes.length; i++) {
                        var ci = $(iframes[i]),
                            ciW = ci.attr('width'),
                            ciH = ci.attr('height'),
                            aspect = ciH / ciW,
                            parentW = ci.parent().width();
                        ci.css({"width": parentW, "height": parentW * aspect});
                    }
                    
                }
                
                /* === Flickr FEED === */
                
                
                /* Instagram feed */
                
                
                /* === ELISE SCRIPT === */
                $(document).elisetheme({
                    disqusID: '',
                    postSize: 'size-x',
                    paginationStyle: 'load-more',
                    menuPin: true,
                    sidebarPin: true,
                    pinnedPosts: false,
                    headerAutoScroll: false,
                    photosetDelay: '6000',
                    postsLayout: 'masonry',
                    hideTag: 'hide',
                    pinTag: 'pinned'
                });
                $('.elise-slider').eliseSlider({
                    rotationDelay: '8000',
                    transition: 'slide'
                });
            });
