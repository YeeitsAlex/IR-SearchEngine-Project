    <section class="stats">
     <div class="grid">
      <h3>
       Who is Illinois International?
      </h3>
      <!-- InstanceBeginEditable name="Stats Info" -->
      <!-- InstanceEndEditable -->
      <div class="box">
       <p class="stat">
        <span>
         <!-- InstanceBeginEditable name="Box 1 Number" -->
         9
         <!-- InstanceEndEditable -->
        </span>
        <!-- InstanceBeginEditable name="Box 1 Text" -->
        units
        <!-- InstanceEndEditable -->
       </p>
       <!-- InstanceBeginEditable name="Box 1 Subtext" -->
       <p>
        dedicated to fostering the global classroom and enhancing the mobility of students, faculty, and staff
       </p>
       <!-- InstanceEndEditable -->
      </div>
      <div class="box">
       <p class="stat">
        <span>
         <!-- InstanceBeginEditable name="Box 2 Number" -->
         7
         <!-- InstanceEndEditable -->
        </span>
        <!-- InstanceBeginEditable name="Box 2 Text" -->
        awards &amp; grants
        <!-- InstanceEndEditable -->
       </p>
       <!-- InstanceBeginEditable name="Box 2 Subtext" -->
       <p>
        that facilitate education, research, and outreach on multiple cultures, languages, and histories
       </p>
       <!-- InstanceEndEditable -->
      </div>
      <div class="box">
       <p class="stat">
        <span>
         <!-- InstanceBeginEditable name="Box 3 Number" -->
         23
         <!-- InstanceEndEditable -->
        </span>
        <!-- InstanceBeginEditable name="Box 3 Text" -->
        scholarships
        <!-- InstanceEndEditable -->
       </p>
       <!-- InstanceBeginEditable name="Box 3 Subtext" -->
       <p>
        awarded to undergraduate and graduate students to support education abroad and research
       </p>
       <!-- InstanceEndEditable -->
      </div>
      <div class="box">
       <p class="buttons">
        <a class="button centered full" href="assets/docs/about/iip-org-chart-homepage.pdf" target="_blank">
         Learn more about our structure
        </a>
        <a class="button centered full" href="iiar-site/site/2018.html" target="_blank">
         2018 Annual Report
        </a>
       </p>
      </div>
     </div>
    </section>
