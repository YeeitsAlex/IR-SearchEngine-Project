    (function () {
        $(document).ready(function () {
            var state_base_color = '#D3D3D3';
            var state_fill_color = '#58A618';
            var internationalMapLoaded = false;

            var countries = [
                    {
                        'iso_code': 'US',
                        'name': 'United States',
                        'donors_count': 1144,
                        'rank': 1
                    },
                    {
                        'iso_code': 'IE',
                        'name': 'Ireland',
                        'donors_count': 2,
                        'rank': 2
                    },
            ];

            var countries_info = {};
            $.each(countries, function(index, country) {
                var iso_code = country.iso_code.toLowerCase();

                countries_info[iso_code] = country.donors_count;
            });

            function sizeMap() {
                var containerWidth = $('.panel-heatmap').width() - 20,
                    containerHeight = (containerWidth / 1.5);

                $('#vmap').css({
                    width: containerWidth,
                    height: containerHeight
                });
            }

            function onShowInfoTable(stateType) {
                if ($(window).width() <= 600) {
                    if (stateType === 'state') {
                        $("#country-info-table").hide();
                        $("#state-info-table").show();
                    } else {
                        $("#state-info-table").hide();
                        $("#country-info-table").show();
                    }
                } else {
                    $("#country-info-table").hide();
                    $("#state-info-table").hide();
                }
            }

            function loadInternationalMap() {
                $('#vmap').vectorMap({
                    map: 'world_en',
                    backgroundColor: '#fff',
                    color: '#D3D3D3',
                    hoverOpacity: 0.7,
                    selectedColor: state_base_color,
                    enableZoom: false,
                    showTooltip: false,
                    values: countries_info,
                    scaleColors: [state_base_color, state_fill_color],
                    normalizeFunction: 'linear',
                    onRegionOver: function (element, code, region) {
                        var value = countries_info[code];

                        if (typeof value === 'undefined') {
                            value = 0;
                        }

                        $('#state-title').text(region.toUpperCase());
                        $('#state-donors').text(value + " Donors");

                        var matching_country = countries.find(function(country) {
                            return country.iso_code === code.toUpperCase();
                        });
                        var rank = countries.length + 1;

                        if (matching_country) {
                            rank = matching_country.rank;
                        }
                        $('#state-rank').text('#' + rank);
                        $('.state-info-box').css('visibility', '');
                    },
                    onResize: function (element, width, height) {
                        console.log('Map Size: ' + width + 'x' + height);
                    }
                });

                    $('#state-title').text('United States');
                    $('#state-donors').text('1144' + " Donors");
                    $('#state-rank').text('#1');
                    $('.state-info-box').css('visibility', '');
            }

            $(".heatmap-nav").on('click', function () {
                $('.nav-tabs')
                    .find('li')
                    .each(function () {
                        $(this).removeClass('active');
                    });


                var activeMap = $(this).data('map');

                if (activeMap === 'int') {
                    if (countries.length === 0) {
                        $('.state-info-box').css('visibility', 'hidden');
                    }

                    $("#map").hide();
                    onShowInfoTable('country');

                    if (!internationalMapLoaded) {
                        $("#vmap").show();
                        sizeMap();

                        loadInternationalMap();
                        $(window).on("resize", sizeMap);
                        internationalMapLoaded = true;
                    } else {
                        $("#vmap").show();
                    }
                } else {
                    $("#map").show();
                    $("#vmap").hide();
                    onShowInfoTable('state');
                }

                $(this).parent().addClass('active');
            });
        });
    })();


    

    
