            <script>
             <!--//--><![CDATA[// ><!--

(function (){
// Please include the email domains you would like to not block in this list
var validDomains = ["@stanford.edu"];
MktoForms2.whenReady(function (form){ form.onValidate(function(){
var email = form.vals().Email; if(email){
if(!isEmailGood(email)) {
form.submitable(false);
var emailElem = form.getFormElem().find("#Email"); form.showErrorMessage("Stanford faculty & residential students only", emailElem);
}else{ form.submitable(true);
} }
}); });
function isEmailGood(email) {
for(var i=0; i < validDomains.length; i++) { var domain = validDomains[i];
if (email.indexOf(domain) != -1) {
return true; }
}
return false; }
