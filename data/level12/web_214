  <script type="text/javascript">
   $(function() {
            $(document).on("click", "#videoButton", function() {
                var iframe = document.createElement( "iframe" );
 
                iframe.setAttribute( "frameborder", "0" );
                iframe.setAttribute( "allowfullscreen", "" );
                iframe.setAttribute( "src", "https://www.youtube.com/embed/3-U3_q0pxiQ?rel=0&showinfo=0&autoplay=1" );
                iframe.setAttribute( "frameborder", "0");
                iframe.setAttribute( "class", "iframe-full");
                $("#videoContainer").append(iframe);
            });

            $(document).on("click", "#closeVideo", function() {
                $("#videoContainer").empty();
            });
        });
  </script>
