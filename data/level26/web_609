        req:(function(){
            var requirejs, callbacks=[];
            function req(){
                var args = Array.prototype.slice.call(arguments),
                    f= function(){requirejs.apply(undefined, args)}
                if (requirejs){f();}else{callbacks.push(f);}
            }
            req.init = function(req){
                if (requirejs){ return; }
                requirejs = req;
                for (var i=0; i<callbacks.length; i++){
                    callbacks[i]();
                }
            }
            return req;
        })()
    };
