//$(document).bind("galleriaDataBinding.esro", function() {galleriaJson.transition = "slide";});

function GalleriaPlay(imageDelay)
{
	 galleriaHandle.play(imageDelay);
}
function GalleriaStop()
{
	 galleriaHandle.pause();
}

function GalleriaTogglePlayState()
{
   galleriaHandle.playToggle()
}

function GalleriaFullScreen()
{
	 galleriaHandle.enterFullscreen();
}

function GetCurrentAnimatedImage()
{
    var gallery = $('#galleria');
    return $('.galleria-stage .galleria-image img',gallery).filter('[src="'+  $('.galleria-image.active img',gallery).attr('src')+'"]');
}

function SetGalleriaAriaLabel()
{
    window.setTimeout(function() {
        var galleriaAriaLabel = "Press ENTER to go to the {0} show. Press LEFT ARROW to display to the previous show. Press RIGHT ARROW to display to the next show.";
        galleriaAriaLabel = galleriaAriaLabel.format(GetCurrentAnimatedImage().attr("alt"));
        $('.ariaLiveMessages.galleria').text(galleriaAriaLabel);
    }, '2000');
}

//********************  transition options *******************************
//‘fade’ crossfade betweens images
//‘flash’ fades into background color between images
//‘pulse’ quickly removes the image into background color, then fades the next image
//‘slide’ slides the images depending on image position
//‘fadeslide’ fade between images and slide slightly at the same time

//********************  Crop options *******************************
//"true" means that all images will be scaled to fill the stage, centered and cropped.
//"false" will scale down so the entire image fits.
//"height’ will scale the image to fill the height of the stage.
//"width’ will scale the image to fill the width of the stage.
//"landscape’ will fill up images that has landscape proportions, but scale portrait images to fit inside the container.
//"portrait’ is like "landscape’ but the other way around.


// Load the classic theme
Galleria.loadTheme('style/Galleria/Themes/theatre/galleria.theatre.js');
// set alt attribute for galleria images
Galleria.ready(function() {
    //this.bind('image', function(e) {
    //    if (e.imageTarget) {
    //        e.imageTarget.alt = e.galleriaData.title;
    //    }
    //});

    var galleria = $('#galleria');
    galleria.on('keydown click', function (e) {
        if (e.which == 13 ){  
            GetCurrentAnimatedImage().trigger('click');

        }
        else if (e.which == 37 || e.which == 38){
            $('.galleria-image-nav-left',$(e.target)).trigger('click');
            SetGalleriaAriaLabel();
        }
        else if (e.which == 39 || e.which == 40){
            $('.galleria-image-nav-right',$(e.target)).trigger('click');
            SetGalleriaAriaLabel();
        }
    });
    var galleriaNavButtons = $('.galleria-image-nav>*',galleria);

    galleriaNavButtons.filter('.galleria-image-nav-left').attr('aria-label',"Display the previous Show");
    galleriaNavButtons.filter('.galleria-image-nav-right').attr('aria-label',"Display the next Show");

    galleria.attr('tabindex','-1');
    galleriaNavButtons.attr('tabindex','-1');

    galleriaNavButtons.on('keydown', function (e) {
        if (e.which == 13 ){
            e.stopPropagation();
            $(e.target).trigger('click');

            SetGalleriaAriaLabel();
            $('#galleria').trigger("focus");
        }
    });
});
// Fire binding event so others can alter the object
$(document).ready(function () {
    $(document).trigger("galleriaDataBinding.esro");    
}); 

// Initialize Galleria
var g = Galleria.run('#galleria', galleriaJson);


galleriaHandle = $('#galleria').data('galleria');
                                    
GalleriaPlay('3000');

$('#galleria').mouseover(function() {
    if ($('.toggleGalleriaAnimation').is('.animate')) {
        $(".galleria-info").css("visibility","visible");
        GalleriaTogglePlayState();
    }
});

$('#galleria').mouseout(function() {
    if ($('.toggleGalleriaAnimation').is('.animate')) {
        $(".galleria-info").css("visibility","hidden");
        GalleriaTogglePlayState();
        //galleriaHandle.next();
    }
});
$('.toggleGalleriaAnimation').on("click keydown",function(ev){
    if (ev.which == "13" || ev.which == "1"){
        $(this).toggleClass('animate');
        var isAnimated = $(this).is('.animate');
        var text = isAnimated?"The Galleria is animated. To use the galleria with keyboard, animation should stop. Stop galleria animation":"Galleria animation is turned OFF. Resume galleria animation";
        $(this).text(text);
        $(".galleria-info").css("visibility",isAnimated? "visible": "hidden");
        
        var galleria = $('#galleria');
        galleria.attr("tabindex",isAnimated? "-1" : "0");
        $('.galleria-image-nav>*',galleria).attr('tabindex',isAnimated? "-1" : "0");

        GalleriaTogglePlayState();
        
        if (!isAnimated) {
            SetGalleriaAriaLabel();
            galleria.trigger("focus");
        }
        else {
            $('.ariaLiveMessages.galleria').text("");
        }
    }
});
    </script>
