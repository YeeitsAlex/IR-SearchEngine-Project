           <a data-clickevent="shows/listening%20session%20with%20chris%20potter/info" data-clientclickevent="" href="shows/listening%20session%20with%20chris%20potter/info" id="ctl00$body$ShowsListControl_btnMoreInfo_19" onclick="" tabindex="0">
            More Info
            <img alt="Next" class="small_arrow_1_right" src="style/images/small_arrow_1_right.gif"/>
           </a>
          </div>
          <div class="button small_button_a addToBasket" data-disabledclass="disabled" data-origclass="button small_button_a addToBasket">
           <a data-clickevent="events/listening%20session%20with%20chris%20potter/2019-5-16_19.00/performance%20penthouse%2c%20logan%20center" data-clientclickevent="" href="events/listening%20session%20with%20chris%20potter/2019-5-16_19.00/performance%20penthouse%2c%20logan%20center" id="ctl00$body$ShowsListControl_btnAddToBasket_19" onclick="" tabindex="0">
