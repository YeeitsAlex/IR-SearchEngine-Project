  <script type="text/javascript">
   jQuery(document).ready(function($) {
	// Get parent menu items
	var mItems = $('nav.mk-main-navigation > ul#menu-main-nav > li.menu-item-has-children > a');
	// On main nav A focus
	mItems.on('focus',function(){
		// Hide all submenus
		$('nav.mk-main-navigation > ul#menu-main-nav > li.menu-item-has-children').removeClass('dropdownOpen');
		// Show only the focused submenu
		$(this).parent().addClass('dropdownOpen');
	// On main nav A hover
	}).on('hover',function(){
		// Place focus on it
		$(this).focus();
		$(this).css('border','none');
	});

	// When mouse leaves nav item, blur its focus
	$('.mk-main-navigation').on('mouseout', function(){
		mItems.blur();
	});
});
  </script>
