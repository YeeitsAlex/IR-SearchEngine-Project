   <script type="text/javascript">
    // <![CDATA[
	
var d = document.querySelectorAll('[id^=ptac-faq]');
var a = 0;
for (var i in d) {
    d[i].id = "ptac-faq" + a;
	a++;
} 
// ]]>
   </script>
   <script type="text/javascript">
    // <![CDATA[
function accordionClick(b) {
  	var x = document.getElementById(b);
  	for (var i = 0; i < x.childNodes.length; i++) {
    	var inner = x.childNodes[i];
		if(inner.className === 'ptac-faq-question') {
			for (var d = 0; d < inner.childNodes.Length; d++)
			{
				var child = inner.childNodes[d];
				if (child.className === 'ptac-sign')
				{
					if (child.style.content === "+")
					{
						child.style.content = "-";
					}
					else
					{
						child.style.content = "+";
					}
				}
			}
		}
		if (inner.className === 'ptac-faq-answer') {
			if (inner.style.visibility === "visible") {
				inner.style.visibility = 'hidden';
				inner.style.opacity = '0';
			} 
			else {
				inner.style.visibility = 'visible';
				inner.style.opacity = '1';
			}
		}
	}
} 
// ]]>
   </script>
