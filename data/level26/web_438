 function go(what,x) {
    for (var i=0;i<2;i++) {
        if (what.page[i].checked) {
           document.forms.page1.SEARCHTEXT.value = x
           document.forms.page2.SEARCHTEXT.value = x
           document.forms[what.page[i].value].submit();
        }
    }
}
</script>
