       <script>
        "use strict";
    var chat_link = document.getElementsByClassName("ceoel-chat"),
          main_nav_lis = document.getElementsByClassName("menu-item-has-children"),
          main_nav_links = document.getElementsByClassName("ceoel-main-nav"),
          ceoel_nav_trigger = document.getElementById("ceoel-mobile-menu-trigger"),
          ceoel_nav = document.getElementById("menu-primary"),
          main_nav_classes = ceoel_nav.classList,
          ceoel_current_page_link = document.getElementById("ceoel-current-page-item"),
          ceoel_sub_triggers = document.getElementsByClassName("ceoel-menu-toggle"),
          ceoel_no_children = document.getElementsByClassName("ceoel-no-children"),
          sub_nav_links = document.getElementsByClassName("ceoel-sub-nav"),
          uwhelp_win;


          //DESKTOP: Reveal subnav when parent main nav item gets focus
          if(main_nav_links.length) {
               for(var i = 0; i < main_nav_links.length; i++) {
                    main_nav_links[i].addEventListener("focus", function(e) {
                         var w = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
                         if(w > 625) {
                              console.log("menu item has focus");
                              var parent_li = ceoel_find_ancestor(this, "menu-item-has-children");
                              for(var j = 0; j < main_nav_lis.length; j++) {
                                   if( main_nav_lis[j] == parent_li) {
                                        console.log("am i here");
                                        main_nav_lis[j].classList.add("subnav-open");
                                   }
                                   else {
                                        main_nav_lis[j].classList.remove("subnav-open");
                                   }
                              }
                         }
                    });
               }
          }
          //DESKTOP: Reveal subnav when subnav item gets focus
          if(sub_nav_links.length) {
               for(var i = 0; i < sub_nav_links.length; i++) {
                    sub_nav_links[i].addEventListener("focus", function(e) {
                         var w = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
                         if(w > 625) {
                              console.log("sub menu item has focus");
                              var parent_li = ceoel_find_ancestor(this, "menu-item-has-children");
                              for(var j = 0; j < main_nav_lis.length; j++) {
                                   if( main_nav_lis[j] == parent_li) {
                                        console.log("am i here");
                                        main_nav_lis[j].classList.add("subnav-open");
                                   }
                                   else {
                                        main_nav_lis[j].classList.remove("subnav-open");
                                   }
                              }
                         }
                    });
               }
          }
          //DESKTOP: Closes all open subnavs when a main nav item that has not children gets focus
          if(ceoel_no_children.length){
               for(var m = 0; m < ceoel_no_children.length; m++) {
                    ceoel_no_children[m].addEventListener("focus", function(e){
                         ceoel_close_all_subnavs();
                    });
               }
          }
          //DESKTOP: Closes all open subnavs that have been tabbed to when a user
          //moves mouse away from navigation
          document.addEventListener("mouseover", function(e){
             var w = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
             if(w > 625 && !e.target.classList.contains("menu-item-link")) {
                   ceoel_close_all_subnavs();
             }
          });
          //DESKTOP:Close any subnavs that were opened on mobile when changing screen size from
          //mobile to desktop
          window.addEventListener("resize", function(){
             ceoel_hide_nav();
          });
          //MOBILE: Adds click functionality to hamburger
          ceoel_nav_trigger.addEventListener("click", function(e){
             e.preventDefault();
             ceoel_toggle_nav(ceoel_nav_trigger, ceoel_nav, ceoel_nav);
          });
          //MOBILE: Handles showing and hiding navigation
          function ceoel_toggle_nav(trigger, nav, ceoel_nav) {
             var trigger_classes = trigger.classList,
                 nav_classes = nav.classList,
                 main_nav_classes = ceoel_nav.classList;
             trigger_classes.toggle("trigger-open");
             if(nav_classes.contains("nav-open")) {
                   nav_classes.remove("nav-open");
                   trigger.setAttribute("aria-pressed", false);
                   //After transition is complete, set display
                   setTimeout(function(){
                        ceoel_set_style(nav, "display", "none");
                        //Return focus to appropriate link
                        if(nav_classes.contains("menu")) {
                             trigger.focus();
                        }
                        else if(main_nav_classes.contains("ceoel-full-side-nav")) {
                             trigger.parentNode.parentNode.firstChild.focus();
                        }
                        else {
                             trigger.parentNode.firstChild.focus();
                        }
                   }, 500);
             }
             else {
                   ceoel_set_style(nav, "display", "block");
                   var nav_style = window.getComputedStyle(nav);
                   setTimeout(function() {
                        nav_classes.add("nav-open");
                        trigger.setAttribute("aria-pressed", true);
                   }, 100);
                   setTimeout(function() {
                        if(nav_classes.contains("ceoel-sub-nav-wrapper")) {
                            nav.childNodes[1].childNodes[1].firstChild.focus();
                      }
                      else {
                            nav.firstChild.firstChild.focus();
                      }
                  }, 400);
             }
          };
          //MOBILE: This adds click functionality to the sub nav links
          if(ceoel_sub_triggers.length) {

             for(var i = 0; i < ceoel_sub_triggers.length; i++) {
                  ceoel_sub_triggers[i].addEventListener("click", function(e){
                      e.preventDefault();
                      var ceoel_sub_menu = this.nextSibling;
                         ceoel_toggle_nav(this, ceoel_sub_menu, ceoel_nav);
                    });
             }
          }
          //MOBILE: Closes the sub nav when current page link is clicked on
          if(ceoel_current_page_link) {
             ceoel_current_page_link.addEventListener("click", function(e){
                   var w = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
                   if(w <= 625) {
                      ceoel_toggle_nav(ceoel_nav_trigger, ceoel_nav, ceoel_nav);
                  }
             });
          }
          /*
          //MOBILE AND DESKTOP: Close subnavs as needed when tabbing around the page
          window.addEventListener("focus", function(e) {
             var w = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
             //if(e.keyCode === 9) {
                  ceoel_check_focus(w);
             //}
          });
          */
          //TOUCH DEVICES: Adds double click functionality to main nav items
          //when regular nav is showing -- for ipads, surface and the like
          //single click -- nothing happens; double click -- link activates
          var click_timer = null;
          var ceoel_handle_nav_touch = function(element) {
             if (click_timer == null) {
                  click_timer = setTimeout(function () {
                      clearTimeout(click_timer);
                      click_timer = null;
                 }, 500);
             } else {
                  clearTimeout(click_timer);
                  click_timer = null;
                  window.location = element.getAttribute("href");
             }
          };
          window.addEventListener("touchstart", function(e) {
               var w = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
               if(w > 625) {
                    var touch_target = e.target,
                         has_children = e.target.getAttribute("aria-haspopup");
                    if(has_children === "true") {
                         e.target.addEventListener("click", function(evt) {
                              evt.preventDefault();
                              ceoel_handle_nav_touch(evt.target)
                         });
                    }
               }
          });
          //MOBILE AND DESKTOP: UTILITY FUNCTIONS
          var ceoel_hide_nav = ceoel_debounce(function() {
             var w = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
             if(w > 625) {
                   ceoel_set_style(ceoel_nav, "display", "block");
                   ceoel_close_all_subnavs();
             }
          }, 250);
          function ceoel_check_focus(w) {
               var focused = document.activeElement;
               if (!focused || focused == document.body) {
                    focused = null;
               }
               else if (document.querySelector) {
                    focused = document.querySelector(":focus");
               }
               var focused_classes = focused.classList;

               //Focus is not in main nav -- DESKTOP & MOBILE
               if(!focused_classes.contains("menu-item-link")) {
                    //close all navigation
                    ceoel_close_all_subnavs();
                    if(w <= 625) { //MOBILE -- close main nav too
                         var trigger_classes = ceoel_nav_trigger.classList,
                            nav_classes = ceoel_nav.classList;
                         trigger_classes.remove("trigger-open");
                         nav_classes.remove("nav-open");
                         ceoel_nav_trigger.setAttribute("aria-pressed", false);
                         setTimeout(function(){
                              ceoel_set_style(ceoel_nav, "display", "none");
                         }, 500);
                    }
               }
               else { //DESKTOP only -- Focus is in main nav
                    if(w > 625) {
                         var item_parent = focused.parentNode,
                              parent_classes = item_parent.classList;
                         //close all subnavs, then open the correct one
                         ceoel_close_all_subnavs();

                         if(parent_classes.contains("menu-item-has-children"))  {
                              focused.parentNode.classList.add("subnav-open");
                         }
                         //custom for uwhelp -- campus profiles subnav
                         else if(focused_classes.contains("menu-item-profile")) {
                              focused.parentNode.parentNode.parentNode.parentNode.parentNode.parentNode.classList.add("subnav-open")
                         }
                         else {
                              focused.parentNode.parentNode.parentNode.parentNode.classList.add("subnav-open");
                         }
                    }
               }
          };
          function ceoel_find_ancestor (el, cls) {
             while ((el = el.parentElement) && !el.classList.contains(cls));
             return el;
          };
          //Closes subnavs
          function ceoel_close_all_subnavs() {
             for(var i = 0; i < main_nav_lis.length; i++) {
                   main_nav_lis[i].classList.remove("subnav-open");
                   //ceoel_sub_triggers[i].classList.remove("trigger-open");
             }
          };
          //Sets a particular style property
          function ceoel_set_style(el, prop, val) {
             var vendors = ["-webkit-", "-o-","-moz-", "-ms-",""];
             for(var i=0;i<vendors.length;i++) {
                   el.style[ceoel_to_camel_case(vendors[i] + prop)] = val;
             }
          };
          //Camel cases style declarations
          function ceoel_to_camel_case(str){
             return str.toLowerCase().replace(/(\-[a-z])/g, function($1){return $1.toUpperCase().replace("-","");});
          };
          //Debounce function
          function ceoel_debounce(func, wait, immediate) {
             var timeout;
             return function() {
                   var context = this, args = arguments;
                   var later = function() {
                        timeout = null;
                        if (!immediate) func.apply(context, args);
                   };
                   var callNow = immediate && !timeout;
                   clearTimeout(timeout);
                   timeout = setTimeout(later, wait);
                   if (callNow) func.apply(context, args);
             };
          };
          //Closes the chat window. Called from chat.htm
          function close_chat() {
               uwhelp_win.close();
          };
       </script>
       <noscript>
        <p id="ceoel-site-map">
         Enable JavaScript in your browser to use full site navigation. Or
         <a aria-label="Javascript is disabled in your browser. Use this link to navigate without Javascript." href="/site-map">
          view site map to navigate
         </a>
         .
        </p>
       </noscript>
      </div>
     </nav>
    </div>
