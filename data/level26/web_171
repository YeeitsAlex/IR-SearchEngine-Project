     <script type="text/javascript">
      jQuery(document).ready(function() {
		jQuery(".middle").each(function() {
			contentHeight = jQuery(this).height();
			parentHeight = jQuery(this).parent().height();
			if(parentHeight > contentHeight){
				topMargin = (parentHeight - contentHeight)/2;
				jQuery(this).css("padding-top", topMargin);
			}
		});
    });
	//box 1 slide parameters
	jQuery("#box1 .box_image").carouFredSel({
		circular: false,
		items: 1,
		auto:false,
		scroll: {
			fx:'crossfade'
		},
		pagination: {
			container: '#box1 .feature_dates',
			event: 'mouseenter',
			anchorBuilder: function( nr) {
				return '<li><a href="' + jQuery('img',this).data('link') + '" class="box_title">' + jQuery('img',this).data('title') + '</a></li>';
			}
		}
	});

	//box 2 slider parameters
	jQuery("#box2 .box_image").carouFredSel({
		circular: false,
		items: 1,
		auto:false,
		scroll: {
			fx:'crossfade'
		},
		pagination: {
			container: '#box2 .feature_dates',
			event: 'mouseenter',
			anchorBuilder: function( nr) {
				return '<li><a href="' + jQuery('img',this).data('link') + '" class="box_title">' + jQuery('img',this).data('title') + '</a></li>';
			}
		}
	});

	//box 3 slider parameters
	jQuery("#box3 .box_image").carouFredSel({
		circular: false,
		items: 1,
		auto:false,
		scroll: {
			fx:'crossfade'
		},
		pagination: {
			container: '#box3 .feature_dates',
			event: 'mouseenter',
			anchorBuilder: function( nr) {
				return '<li><div class="date">' + jQuery('img',this).attr('data-month') + '<br /><span>' + jQuery('img',this).data('day') +'</span></div><a class="box_title" target="_blank" href="'+ jQuery('img', this).data('link') +'">' + jQuery('img',this).data('title') + '</a></li>';
			}
		}
	});
     </script>
