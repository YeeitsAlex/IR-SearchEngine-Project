    <script type="text/javascript">
     function highlight(items) {
			var middleSlide = items.filter(":eq(1)");
			var slideTitle = jQuery('img', middleSlide).data("title");
			var slideSubTitle = jQuery('img', middleSlide).data("subtitle");
			var slideLink = jQuery('img', middleSlide).data("link");
			var linkTarget = jQuery('img', middleSlide).data("linkTarget");
			var popup = jQuery('img', middleSlide).data("popup");
			jQuery('img', middleSlide).animate({ opacity: 1 }, 500);
			jQuery("#slide_title").html(slideTitle);
			jQuery("#slide_title").attr('href',slideLink);
			jQuery("#slide_title").attr('target',linkTarget);
			jQuery("#slide_subtitle").html(slideSubTitle+" &raquo;");
			jQuery("#slide_subtitle").attr('href',slideLink);
			jQuery("#slide_subtitle").attr('target',linkTarget);
			if(popup){
				jQuery('#slide_title, #slide_subtitle, .slide_link').addClass('fancybox-media');
				middleSlide.append('<a class="playbtn fancybox-media" href="'+slideLink+'"></a>');
			}else{
				jQuery('#slide_title, #slide_subtitle, .slide_link').removeClass('fancybox-media');
			}
			jQuery("#slide_title, #slide_subtitle").animate({ opacity: 1 }, 500);
	
			return items;
		}
	
	
jQuery(function() {	
		jQuery("#home_slider").carouFredSel({
			width: '100%',
			align: 'center',
			items: {
				visible: 3
			},
			auto: 5000,
			scroll: {
				items: 1,
				duration: 1500,
				onBefore: function(data) {
					jQuery("#slide_title, #slide_subtitle").animate({ opacity: 0 }, 500);
					jQuery('.home_slide img').animate({ opacity: .40 }, 500);
					jQuery('.playbtn').remove();

				},
				onAfter	: function(data) {
					highlight( data.items.visible );
				}
			},
			prev: {
				key: "left",
				button: "#slide_left"
			},
			next: {
				key: "right",
				button: "#slide_right"
			},
			pagination: {
				container: '#pagination'
			},
			swipe		: {
				onTouch		: false
			}
		});
		
		jQuery('.home_slide img').animate({ opacity: .40 });
		highlight( jQuery('#home_slider > *') );
});

		//make playbutton appear on hover of title subtitle or image
		jQuery('#slide_title, #slide_subtitle, .home_slide').hover(function(e) {
			jQuery('.home_slide .playbtn').css('visibility','visible');
        }, function(){ 
			jQuery('.home_slide .playbtn').css('visibility','hidden');
		
		});
    </script>
   </div>
