    <div class="container">
     <div class="row">
      <div class="col-sm-4">
       <script type="text/javascript">
        // 
    $(function () {
            $.ajax({
                url: '//campusdata.uark.edu/apiv2/articles/SearchArticle?$orderby=publishDate+desc&$top=5&$filter=((Schools/any(s2:+s2/id+eq+2))+or+(Topics/any(t37:+t37/id+eq+37))+and+(Audiences/any(a2:+a2/id+eq+2)+and+Audiences/any(a3:+a3/id+eq+3)+and+Audiences/any(a6:+a6/id+eq+6)+and+Audiences/any(a1:+a1/id+eq+1)))&$top=3',
                data: {},
                contentType: "application/json; charset=utf-8",
			   	dataType: 'jsonp',
//jsonpCallback: 'Articles',
			   	async: "true",
			   	cache: "false",
                success: function (articles) {
                    dataDisplay = "";
                    $.each(articles, function (key, a) {
                        var briefText = "", image = "";
                        if (a.brief != null && a.brief != "") { briefText = a.brief.substr(0, 100) + '...';  briefText = briefText.replace(/(<p>|<\/p>)/g, "");}
                        if (a.image != null && a.image != "") {
                            image = a.image;
                        } else { image = "PMUA_Logo-web.jpg"; }
                        dataDisplay = "<div class='media'>" +
                                        "<a href='http://news.uark.edu/articles/" + a.id + "/'><img class='img-responsive pull-left padded-image' src='http://campusdata.uark.edu/resources/images/articles/" + image + "?w=100' alt='' /></a>" +
"<h4 style='margin-top: 0;'><a href='http://news.uark.edu/articles/" + a.id + "/'>" + a.headline + "</a></h4>" +
                                            "<p>" + briefText + "</p>"+ 
										"</div>";
                        $("#newsArticles").append(dataDisplay);
                    });
                }
            });

    });
//
       </script>
       <h2 id="news">
        News
       </h2>
       <div id="newsArticles">
       </div>
       <ul class="nav pull-right clearfix">
        <li>
         <a href="//news.uark.edu">
          More News &gt;
         </a>
        </li>
       </ul>
      </div>
      <div class="col-sm-4">
       <h2 id="events">
        Events
       </h2>
       <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.1.0/moment.min.js" type="text/javascript">
       </script>
       <script type="text/javascript">
        //  
$(function ()
{
$.ajax({
type: 'GET',
url: '/_resources/php/proxy.php',
data: { requrl: "https://calendars.uark.edu/RSSSyndicator.aspx?category=90-0" },
cache: 'true',
success: function (data)
{
var months = ["JAN", "FEB", "MAR", "APR", "MAY", "JUN", "JULY", "AUG", "SEPT", "OCT", "NOV", "DEC"]; 
var days = ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"];
var list_data = '';

var $xml = $(data).find('item:lt(6)'); 
$xml.each(function () { 
var $this = $(this); 
//var headline = this.title.replace(/ *\([^)]*\) */g, "");
var headline = $this.find('title').text(); 
var link = $this.find('link').text(); 
var publishedDate = $this.find('pubDate').text();
var content = $this.find('pubDate').text();

var d = new Date();
var date = new Date(publishedDate).getDate();
var time = moment(publishedDate).format("h:mm A");
var month = months[new Date(publishedDate).getMonth()];
var day = days[new Date(publishedDate).getDay()];

var year = new Date(publishedDate).getFullYear();   

if(time=='12:00 AM'){ time='All Day';}

list_data += '<div class="row"><div class="col-sm-3 col-md-3"><h4>' + month + ' ' + date + '</h4></div><div class="col-sm-9 col-md-9"><p><a href="'+ link +'">' + headline + '</a><br/>' + time + '</p></div></div>';

});

$(list_data).appendTo("#studyAbroadEvents");
}
});
});
//
       </script>
       <div>
        <ul id="studyAbroadEvents">
        </ul>
        <ul class="nav pull-right clearfix">
         <li>
          <a href="//calendars.uark.edu">
           More Calendar Events &gt;
          </a>
         </li>
        </ul>
       </div>
