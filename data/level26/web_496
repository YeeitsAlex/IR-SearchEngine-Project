                  <!-- HOMEPAGE INFO MESSAGE (rave alternative for non-emergency announcements) -->
                  <div id="xtraMessage" style="display: none;">
                  </div>
                  <script>
                   //extra homepage message
$().SPServices({
  operation: "GetListItems",
  async: false,
  listName: "Homepage Message",
  completefunc: function (xData, Status) {
    $(xData.responseXML).find("[nodeName='z:row']").each(function() {
		//do something
		var xtraMessageText = "<div class='xtracont'><div class='xiconCont'><div>&nbsp;</div></div><div class='xmessage'>" + $(this).attr("ows_Message") + "</div></div>";
		var xtraMessageStyle = $(this).attr("ows_Style");
		var xtraMessageIcon = $(this).attr("ows_Icon");
		console.log(xtraMessageIcon);
		$("#xtraMessage").append(xtraMessageText);
		if (xtraMessageStyle === "Red"){$("#xtraMessage").addClass("redMessage")};
		if (xtraMessageStyle === "Orange"){$("#xtraMessage").addClass("orangeMessage")};
		if (xtraMessageStyle === "Teal"){$("#xtraMessage").addClass("tealMessage")};
		if (xtraMessageStyle === "Purple"){$("#xtraMessage").addClass("purpleMessage")};
		if (xtraMessageStyle === "Yellow"){$("#xtraMessage").addClass("yellowMessage")};
		if (xtraMessageIcon != "None"){$(".xiconCont > div:first-child").addClass("mIcon")};
		if (xtraMessageIcon === "Snow"){$(".xiconCont > div:first-child").addClass("snw")};
		if (xtraMessageIcon === "Info"){$(".xiconCont > div:first-child").addClass("inf")};
		if (xtraMessageIcon === "Attention"){$(".xiconCont > div:first-child").addClass("attn")};
    });
  }
});
                  </script>
