<script>
 //hideme
if (navigator.appVersion.indexOf("MSIE 10") !== -1)
{
var elems = document.querySelectorAll(".hideme");
[].forEach.call(elems, function(el) {
    el.classList.remove("hideme");
});
}

$().SPServices({
  operation: "GetListItems",
  async: false,
  webURL: "/news",
  listName: "Homepage Feature One",
  completefunc: function (xData, Status) {
    $(xData.responseXML).find("[nodeName='z:row']").each(function() {
		//console.log( "Path: " + $(this).attr("ows_FileRef")); //path to image
  		//console.log( "Video: " + $(this).attr("ows_Video")); 
  		//console.log( "Link: " + $(this).attr("ows_Link"));
  		//console.log( "Description: " + $(this).attr("ows_Alt"));
  		
  		var headline = $(this).attr("ows_Title");
  		var aaa = $(this).attr("ows_FileRef");
  		var imgpth = aaa.split("#");
  		var lnk =   $(this).attr("ows_Link");
  		var alttxt = $(this).attr("ows_Alt");
  		var text = $(this).attr("ows_Text");		
  		
  		var textcont = "";
  		if ((headline != undefined) && (text != undefined)){
  			textcont = "<div class='feattxt'><h3>" + headline + "</h3><p>" + text + "</p></div>";
  		}
  		
		var sCell = "<a href='" + lnk + "'><img src='/" + imgpth[1] + "' border='0' alt='" + alttxt + "' /></a>" + textcont;
  		$("#featOne").append(sCell);
	
	 });
  }
});

