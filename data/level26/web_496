<script>
 function checkit(){
	if (!$.trim($("#emailAddressInfo").val())) {
		//alert("Please enter your email.");//fail
	} else {
		//createListItem();//submit form
		if(!SP.SOD.executeOrDelayUntilScriptLoaded(createListItem, 'sp.js')){ 
		  LoadSodByKey('sp.js');
		};

	}
}

var siteUrl = '/admissions';

function createListItem() {

var emailValue = document.getElementById('emailAddressInfo').value; //email from form

    var clientContext = new SP.ClientContext(siteUrl);
    var oList = clientContext.get_web().get_lists().getByTitle('Request Information - Simple Form');
        
    var itemCreateInfo = new SP.ListItemCreationInformation();
    this.oListItem = oList.addItem(itemCreateInfo);
        

    oListItem.set_item('Title', emailValue);

    
        
    oListItem.update();

    clientContext.load(oListItem);
        
    clientContext.executeQueryAsync(Function.createDelegate(this, this.onQuerySucceeded), Function.createDelegate(this, this.onQueryFailed));
}

function onQuerySucceeded() {

    //alert('Item created: ' + oListItem.get_id());
    document.getElementById("simpleMailForm").style.display = "none"; //hide form
   document.getElementById("submessage").style.display = "block"; //show thank-you

}

function onQueryFailed(sender, args) {

    alert('Request failed. ' + args.get_message() + '\n' + args.get_stackTrace());
}
//createListItem();

var onEnterViewPort = function(entries, observer) {
  entries.forEach(function(entry) {
    console.log(entry);
    // Fade in when we enter the viewport
    if (entry.intersectionRatio !== 0) {
      entry.target.classList.add('in');
    }
    // Fade back out when we leave the viewport
    else {
      entry.target.classList.remove('in');
    }
  })
}

var observer =  new IntersectionObserver(onEnterViewPort , {
});

var hidemes= document.querySelectorAll('.hideme');
for(var i = 0; i < hidemes.length; ++i) {
  observer.observe(hidemes[i]);
}
</script>
<script>
 var newzData = [];
var dataposition = "0";
//load the news data into an array from two different libraries
//get news releases
$().SPServices({
  operation: "GetListItems",
  async: false,
  webURL: "/ucm/News",
  listName: "Pages",
  viewName: "{1F8DCD82-9378-4BF3-B30A-E6EFC9844BDB}",
  completefunc: function (xData, Status) {
    $(xData.responseXML).find("[nodeName='z:row']").each(function() {
    	 //filter garbage from link
    	 var storyLink = $(this).attr("ows_FileRef");
    	 var linkSplit = storyLink.split("#");
    	 //filter date
    	 var rawDate = $(this).attr("ows_ArticleStartDate");
    	 var dateSplit = rawDate.split(" ");
    	 var dateParts = dateSplit[0].split("-");
    	 //is there an image, if not use default
    	 var nzimg;
    	 if ($(this).attr("ows_PublishingRollupImage") != undefined){
    	 	nzimg = $(this).attr("ows_PublishingRollupImage");
    	 	} else {
    	 	nzimg = "<img src='/SiteAssets/images/newsnophoto.png' border='0' />";
    	 	}
    	 //add data to array
         newzData.push({
      		title: $(this).attr("ows_Title"),
      		date: $(this).attr("ows_ArticleStartDate"),
      		year: dateParts[0],
      		month: dateParts[1],
      		day: dateParts[2],
      		summary: $(this).attr("ows_ArticleByLine"),
      		link: linkSplit[1],
      		image: nzimg
      		});
         dataposition = dataposition + 1;
    });
  }
});

//get UWSP in the News data
$().SPServices({
  operation: "GetListItems",
  async: false,
  webURL: "/ucm/uNews",
  listName: "Pages",
  viewName: "{6F068CF0-F609-44E0-B5DD-E9D267714838}",
  completefunc: function (xData, Status) {
    $(xData.responseXML).find("[nodeName='z:row']").each(function() {
         var storyLink = $(this).attr("ows_FileRef");
    	 var linkSplit = storyLink.split("#");
    	 var rawDate = $(this).attr("ows_ArticleStartDate");
    	 var dateSplit = rawDate.split(" ");
    	 var dateParts = dateSplit[0].split("-");
    	 var nzimg;
    	 if ($(this).attr("ows_PublishingRollupImage") != undefined){
    	 	nzimg = $(this).attr("ows_PublishingRollupImage");
    	 	} else {
    	 	nzimg = "<img src='/SiteAssets/images/newsnophoto.png' border='0' />";
    	 	}
         newzData.push({
      		title: $(this).attr("ows_Title"),
      		date: $(this).attr("ows_ArticleStartDate"),
      		year: dateParts[0],
      		month: dateParts[1],
      		day: dateParts[2],
      		summary: $(this).attr("ows_ArticleByLine"),
      		link: linkSplit[1],
      		image: nzimg
      		});
         dataposition = dataposition + 1;
    });
  }
});

//sort the data
function compare(a,b) {
  if (a.date > b.date)
    return -1;
  if (a.date < b.date)
    return 1;
  return 0;
}
newzData.sort(compare);
//data is now in ascending order

//write it out to the page in descending order
//for(i = (newzData.length - 1); i >= 0; i-- ){
for(i = 1; i >= 0; i-- ){
  //console.log(newzData[i].date);
  var liHtml = "<div class='newzrow'><div class='newzstory textstory'><h3><a href='/" + newzData[i].link + "'>" + newzData[i].title + "</a></h3><p>" + newzData[i].summary + "</p><span class='date'>" + newzData[i].month + "/" + newzData[i].day + "/" + newzData[i].year + "</span><br /></div></div>";
  $("#newslist").prepend(liHtml);
}
</script>
