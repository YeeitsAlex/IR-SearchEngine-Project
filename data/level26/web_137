  <!--   script to handle tooltip -->
  <script type="text/javascript">
   $(document).ready(function(){
           $("#tip1").attr("aria-hidden","true");
           $("#tip1").addClass("hidden");
           $("#tip1").removeClass("hidden");
   });
   
   
   $("#what_is_js").mouseover(function(){
        $("#tip1").attr("aria-hidden","false");
   }); 
   
   $("#what_is_js").mouseleave(function(){
        $("#tip1").attr("aria-hidden", "true");
   });
   
   
   $("#what_is_js").focus(function(){
        $("#tip1").attr("aria-hidden","false");
   });
   
   
   $("#what_is_js").keydown(function(ev){
        if (ev.which ==27) 
        {
            $("#tip1").attr("aria-hidden","true");
            ev.preventDefault();
            return false;
        }
   });
  </script>
