   function searchKeywordPrimo() {
 //var url_base = 'http://exl-impl-primo.hosted.exlibrisgroup.com/primo_library/libweb/action/dlSearch.do';
 var term2 = document.getElementById("primoQueryTemp").value.replace(/[,]/g, " ");
 //document.forms["searchPrimoTitleForm"].submit();
