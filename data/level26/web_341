<script>
 jQuery(document).ready(function($) {
		//Find the P&T date until June 30 of the next year//
		function daysUntil(year, month, day) {
          var now = new Date(),
          dateEnd = new Date(year, month - 1, day), // months are zero-based
          days = (dateEnd - now) / 1000/60/60/24;   // convert milliseconds to days
          return Math.round(days);
         }
//alert(daysUntil(2018, 12, 25));
$(".dayUntil").text(daysUntil(2019, 06, 30)); // 19 days!!
		
		
		//Append help//
			if ($(".whichPage")[0]){
				//"/faculty-development/lamp-program/"
				//$(".whichPage").att(URL,"https://helpform?id="+)
				
			}
		   var k=0;
		    $('.rotate').each(function(ii, obj) {
				 k++;
                 if (k==1){
					  $(this).removeClass( "rotatePiece");
					 ;
				 }
              });
          		   
	 function doRotation(){
		  i = 1; 
		  setInterval(function(){
            i = i == $('.rotate').length ? 0 : i;
			  $(".rotate").addClass( "rotatePiece");
             //$('.rotate').addClass("rotatePiece");
			  $('.rotate').each(function(ii, obj) {
				 // alert(i+" and "+ii);
                 if (i==ii){
					 $(this).fadeIn();
					$(this).removeClass( "rotatePiece");
				 }  else
			     {
					// $(this).hide();	 
				 }
              });
            i = i + 1;
          },8000);
	    }
	   
		doRotation();
						   
       // Handler for .ready() called.

