  <div aria-hidden="true" class="pum pum-overlay pum-theme-191 pum-theme-default-theme-2 popmake-overlay click_open" data-popmake='{"id":137,"slug":"mentored-research-and-artistry-program","theme_id":191,"cookies":[],"triggers":[{"type":"click_open","settings":{"extra_selectors":"","cookie_name":null}}],"mobile_disabled":null,"tablet_disabled":null,"meta":{"display":{"responsive_min_width":false,"responsive_max_width":false,"position_bottom":false,"position_right":false,"position_left":false,"stackable":false,"overlay_disabled":false,"scrollable_content":false,"disable_reposition":false,"size":"medium","responsive_min_width_unit":"px","responsive_max_width_unit":"px","custom_width":"640px","custom_width_unit":false,"custom_height":"380px","custom_height_unit":false,"custom_height_auto":false,"location":"center top","position_from_trigger":false,"position_top":"100","position_fixed":false,"animation_type":"fade","animation_speed":"350","animation_origin":"center top","overlay_zindex":"1999999998","zindex":"1999999999"},"close":{"text":"","button_delay":"0","overlay_click":false,"esc_press":"true","f4_press":"true"},"click_open":[]}}' id="pum-137" role="dialog">
   <div class="pum-container popmake theme-191 pum-responsive pum-responsive-medium responsive size-medium" id="popmake-137">
    <div class="pum-content popmake-content">
     <div id="fancybox-inner">
      <div class="popupContent">
       <div class="slidecontainer" id="ctl00_mainContent_ctl00_divContent">
        <p>
         <img alt="" class="alignnone size-full wp-image-338" height="155" src="http://www.lc.colostate.edu/wp-content/uploads/sites/2/2016/12/MRAPB.png" width="800"/>
        </p>
        <p>
         The
         <strong>
          Mentored Research and Artistry Program
         </strong>
         is an interdisciplinary, non-residential learning community designed to provide undergraduate students with the opportunity to participate in high impact, real world research or other creative works.
        </p>
        <h2>
         Community Requirements:
        </h2>
        <ul>
         <li>
          Complete a one-credit experiential learning seminar specific for Mentored Research and Artistry Participants (Fall only)
         </li>
         <li>
          Complete a minimum of two semesters of faculty mentored research or design projects
         </li>
         <li>
          Actively work toward the completion of all requirements associated with obtaining the
          <a href="http://tilt.colostate.edu/oura/formalResearch/mrap/index.cfm" rel="noopener" target="_blank">
           Mentored Research and Artistry transcript designation.
          </a>
         </li>
        </ul>
        <h2>
         Who Can Apply:
        </h2>
        <p>
         Any current undergraduate student in good academic standing with Colorado State University.
        </p>
        <h2>
         How to Apply:
        </h2>
        <p>
         Please contact Dr. Mark Brown at
         <a href="mailto:%20M.Brown@colostate.edu">
          M.Brown@colostate.edu
         </a>
         for questions or more information on applying to participate in the Mentored Research and Artistry Program.
        </p>
       </div>
      </div>
     </div>
    </div>
    <button aria-label="Close" class="pum-close popmake-close" type="button">
     CLOSE
    </button>
   </div>
