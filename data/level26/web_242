    <script>
     // Formats the date as a string how we want
function formatDate(date) {
  var month = date[1]-1;
  var fullDate = new Date(date[0], month, date[2]);
  var weekday=new Array(7);
      weekday[0]="Sunday";
      weekday[1]="Monday";
      weekday[2]="Tuesday";
      weekday[3]="Wednesday";
      weekday[4]="Thursday";
      weekday[5]="Friday";
      weekday[6]="Saturday";
  var displayDate = fullDate.toDateString().split(' ');
  var displayDay = weekday[fullDate.getDay()];
  return displayDay+', '+displayDate[1]+' '+displayDate[2];
}

// Formats the time as a string how we want
function formatTime(time) {
  var hours = time[0],
      mins = time[1],
      secs = time[2],
      afternoon = 'A.M.',
      time = hours+':'+mins
  
  if (hours>=13 && hours < 24) {
    afternoon = 'P.M.';
    hours-=12;
  } else if (hours>=0 && hours<13) {
    if (hours==='12') {
      afternoon = 'P.M.';
    } else if (hours==='00') {hours=12;}
  } else {
    console.log('Invalid time - '+hours+':'+mins+':'+secs);
    hours='';
    mins='00';
    secs='';
    afternoon='';
  }
  if (mins==='00') {
    time = hours+' '+afternoon;
  } else {
    time = hours+':'+mins+' '+afternoon;
  }
  return time;
}
function displayEvent (ev) {
  var current = ev;
  if (current !== undefined) {
    var logo = current.logo,
	    startDate = current.start.local.split('T'), // separate date and time
	    dateArray = startDate[0].split('-'), // separate year, month, day
	    timeArray = startDate[1].split(':'), // separate hour, min, sec
	    url = current.url,
	    title = current.name.text,
	    dateStr = formatDate(dateArray),
	    timeStr = formatTime(timeArray),
	    addressName = '',
	    address1 = '';
    if (typeof current.venue !== 'undefined') {
      if (typeof current.venue.name !== 'undefined') {
        addressName = current.venue.name + ',&nbsp;';
      }
      if (typeof current.venue.address !== 'undefined' && typeof current.venue.address.address_1) {
        address1 = current.venue.address.address_1;
      }
	  } 
	  // check whether there is a logo, if not add a default
	  if (!current.logo) {
      //console.log('no logo');
      logo = 'http://lghttp.60402.nexcesscdn.net/8046399/images/page/-/site/images/static/bsd_iop_events_entry_placeholder_298x258.png';
	  } else {
      logo = logo.url;
	  }

	  $('#events-list').prepend(
      '<article class="event">' + 
          '<a href="' + url + '" target="_blank"><img src="' + logo + '" alt="Event logo" class="event-logo" ></a>' + 
          '<div class="event-content"><p class="date">'+ dateStr + '&nbsp;|&nbsp;' + timeStr + '</p>' +
          '<h3 class="title"><a href="' + url + '"target="_blank">' + title + '</a></h3>' +
          '<addr class="event-location">' + addressName + address1 + '</addr></div>' +
          '<p><a href="' + url + '">RSVP &raquo;</a></p>' +
          '</article>');
  }
}
$(document).ready(function($){
  $.ajax({
      url: 'https://www.eventbriteapi.com/v3/users/me/owned_events/?token=67E5D3LMCG77D3KONN3X&status=live%2Cstarted&expand=venue',
      cache: true,
      dataType: 'json'
  }).done(function(response) {
      var totalItems = response.pagination.object_count;
      var totalPages = response.pagination.page_count;
      // Asychronously fetch additional pages of events, if there are any
      if (totalPages > 1) {
          var pageNum = 2;
          while(pageNum <= totalPages) {
              $.ajax({
                  url: 'https://www.eventbriteapi.com/v3/users/me/owned_events/?token=67E5D3LMCG77D3KONN3X&status=live%2Cstarted&expand=venue&page=' + pageNum,
                  cache: true,
                  dataType: 'json'
              }).done(function(response) {parseEvents(response.events);});
              pageNum++;
          }
      }
      // Get the first 18 events for immediate display
      var eventsToDisplay = response.events.slice(0,2);
      // $.each(eventsToDisplay, function(index, ev) {
      //     displayEvent(ev);
      // });
      displayEvent(eventsToDisplay[1]);
      displayEvent(eventsToDisplay[0]);
  }).fail(function() {
      $('#events-list').prepend("<h4>We weren't able to retrieve any events. Please check back shortly!</h4>");
	});
});
    </script>
