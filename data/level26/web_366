         function program_select_dropdown(){
    var sel = document.getElementById("progSelectDropDown").selectedIndex;
    var redirectURL = document.getElementById("progSelectDropDown").options[sel].title;
    $('#clickerGo').click(function(){
    window.location = redirectURL;
    return false;
    });
  }
