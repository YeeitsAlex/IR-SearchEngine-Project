    <script>
     $("ul",".toggler").hide();
$(".toggler").each(
 function() {
  if($("ul",this).length == 0){
   $("a",this).attr("class","noArrow");
  }
  else{
   $("a",this).attr("class","DcsSubAnchor");
   $(this).children().attr("href","#");
  }
 }
);

$(".DcsSubAnchor").click(function(){           
  
  // Start of new code added after dotcms finished their work
  var disp = $(this).next().attr("style");  
  if(disp.search('block') > 1){
 $(this).next().hide();
 $(this).css("background","transparent url(http://new.ipfw.edu/global/images/template/bg_liUnactive.gif) no-repeat scroll right center");
 return;
  }  
  $(".toggler").each(
    function() {
   if(($("ul",this).length != 0) && ($("ul",this).attr("style").search('block') > 1)){  
     $("ul",this).hide();
  $("a:first", this).css("background","transparent url(http://new.ipfw.edu/global/images/template/bg_liUnactive.gif) no-repeat scroll right center");
   }
   }
 );
 //end of new code. written by: Marat

  //this is the only line written by dotcms within this function
  $(this).next().slideToggle();
  
  //this is part of new code
  $(this).css("background","transparent url(http://ipfw.edu/global/images/template/bg_liActive.gif) no-repeat scroll right center");
  
});
    </script>
