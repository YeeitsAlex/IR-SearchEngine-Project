        <script>
         $(function() {
        $(".carousel").swiperight(function() {
            $(this).carousel('prev');
        });
        $(".carousel").swipeleft(function() {
            $(this).carousel('next');
        });

		function checkWidth(){
			console.log($('.content-container').eq(0).css('width'));
			$('.carousel:not(:eq(0))').css('margin-left', '0');
			$('.carousel:not(:eq(0))').css('min-width', parseFloat($('.content-container').eq(0).css('width')) - 14 + 'px');
			$('.carousel:not(:eq(0))>.carousel-inner>.item>a>img, .carousel:not(:eq(0))>.carousel-inner>.item>img').css('min-width', parseFloat($('.content-container').eq(0).css('width')) - 14 + 'px');
			$('.carousel:not(:eq(0))>.carousel-inner>.item>a>img, .carousel:not(:eq(0))>.carousel-inner>.item>img').css('max-width', parseFloat($('.content-container').eq(0).css('width')) - 14 + 'px');
		}
		
		if ($('.carousel').length > 1 && 'true' == 'false'){
			$('.carousel:not(:eq(0))').each(function(i) {
				$(this).attr('id', 'carousel' + (i+2));
				$(this).children($('.carousel-control')).attr('href', '#carousel' + (i+2));
			});
			
			// Execute on load
			checkWidth();
			// Bind event listener
			$(window).resize(checkWidth);
		}
		//else if ($('.carousel').length == 2 && 'true' == 'true'){
			//$('.carousel').eq(1).css('margin-left', '-2vw');
		//}

    });
        </script>
