  <script>
   var elemStr = "&lt;script async=&quot;async&quot; defer=&quot;defer&quot; src=&quot;https://www.youvisit.com/tour/Embed/js2&quot;&gt;&lt;/script&gt;";
				elemArr = elemStr.split('&lt;/script&gt;');
				
				for (var i = 0; i < elemArr.length-1; i++){
					var newText = elemArr[i].replace("&lt;script&gt;", "");
					newText = newText.replace("&lt;script", "");
					newText = newText.replace("&gt;", "");
					newText = newText.replace(/&quot;/g,'"');
					var tempScript = document.createElement('script');
					if(~newText.indexOf("async")){
						tempScript.setAttribute("async", "async");
						newText = newText.replace(/([\s\S]*?)async="[\s\S]*?"/g, '$1');
					}
					if(~newText.indexOf("defer")){
						tempScript.setAttribute("defer", "defer");
						newText = newText.replace(/([\s\S]*?)defer="[\s\S]*?"/g, '$1');
					}
					if(~newText.indexOf("src")){
						var tempText = newText.split('src=');
						tempText = tempText[1].replace(/"/g, '');
						tempScript.setAttribute("src", tempText);
						newText = newText.replace(/([\s\S]*?)src="[\s\S]*?"/g, '$1');
					}
					tempScript.innerHTML = newText;
					document.body.insertAdjacentElement('beforeend', tempScript);
				}
  </script>
 </body>
</html>

