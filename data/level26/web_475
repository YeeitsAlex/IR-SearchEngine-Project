   <script language="javascript">
    var isID = 0;
var isAll = 0;
var isLayers = 0;
var isDHTML = 0;

var domSMenu= null;
var oldDomSMenu= null;

var t=0;
var lCount = 0;
var lDelay = 3;
var pause = 500;

var evt = null;

if (document.getElementById && !document.all){isID = 1; isDHTML = 1;}
else 
{
  if (document.all){isAll = 1; isDHTML = 1;}
  else 
  {
    if (document.layers){isLayers = 1; isDHTML = 1;}
  }
}

//alert(isDHTML);

// return the correct address for object for user browser
function findDOM(objectID, withStyle)
{
  if (withStyle ==1)
  {
    if (isID){return (document.getElementById(objectID).style);}
    else 
    {
      if (isAll){return (document.all[objectID].style);}
      else 
      {
	if (isLayers){return (document.layers[objectID]);}
      }
    }
  }
  else
  {
    if (isID){return (document.getElementById(objectID));}
    else 
    {
      if (isAll){return (document.all[objectID]);}
      else 
      {
	if (isLayers){return (document.layers[objectID]);}
      }
    }
  }
}

// returns LEFT Object coordinate
function getLeft(object_ID)
{
  dom = findDOM(object_ID,0);
  domStyle = findDOM(object_ID,1);

  left_value = 0;
  if (domStyle.pixelLeft){left_value = domStyle.pixelLeft;}
  else 
  {
    if (dom.offsetLeft){left_value = dom.offsetLeft;}
    else { if (domStyle.left) {left_value = domStyle.left;}}
  }
  return left_value;
}
// returns TOP Object coordinate

function getTop(object_ID)
{
  dom = findDOM(object_ID,0);
  domStyle = findDOM(object_ID,1);

  top_value = 0;
  if (domStyle.pixelTop){top_value = domStyle.pixelTop;}
  else 
  {
    if (dom.offsetTop){top_value = dom.offsetTop;}
    else { if (domStyle.top) {top_value = domStyle.top;}}
  }
  return top_value;
}

function findLivePageWidth()
{
  var canvas = 'canvas';
  var elemLeft;
  var elemWidth = 1;
  var w;

  elemLeft = getLeft(canvas);
  //alert(canvasLeft);

  dom = findDOM(canvas,0);
  domStyle = findDOM(canvas,1);
  //alert(dom);

  if (dom.offsetWidth) {elemWidth=dom.offsetWidth;}
  else
  {
    if (dom.clip.width) {elemWidth=dom.clip.width;}
  }
  //alert(elemWidth);

  if (elemWidth==1){return null;}

  w = elemLeft + elemWidth ;
  //alert(w);
  
  return w;
}


function findPageWidth()
{
  var w;
  if (window.innerWidth != null)
  {
    w=window.innerWidth;
    //alert(w);
    return w;
  }
  if (document.body.clientWidth != null)
  {
    w=document.body.clientWidth;
    //alert(w);
    return w;
  }
  return (null);
}

function getImageTop(ObjectID,e)
{
  var anchorTop = 0;
  if (document.images[ObjectID].y)  // NS
  {
    anchorTop = document.images[ObjectID].y;
    //alert(anchorTop);
  }
  else
  {
    if (!e) {e = window.event;}
    myTarget = e.currentTarget;
  
    if (!myTarget) {myTarget = e.srcElement;}
    else if (myTarget == "undefined") {myTarget = e.srcElement;}
    //alert("myTarget => " + myTarget);

    while (myTarget != document.body)
    {
      anchorTop += myTarget.offsetTop;
      myTarget = myTarget.offsetParent;
    }
  }
  //alert("anchorTop: " + anchorTop);
  return anchorTop;
}

function getImageLeft(ObjectID,e)
{
  var anchorLeft = 0;
  if (document.images[ObjectID].x)  // NS
  {
    anchorLeft = document.images[ObjectID].x;
    //alert("anchorLeft: " + anchorLeft);
  }
  else
  {
    if (!e) {e = window.event;}
    myTarget = e.currentTarget;
  
    if (!myTarget) {myTarget = e.srcElement;}
    else if (myTarget == "undefined") {myTarget = e.srcElement;}
    //alert("myTarget => " + myTarget);

    while (myTarget != document.body)
    {
      anchorLeft += myTarget.offsetLeft;
      myTarget = myTarget.offsetParent;
    }
  }
  //alert("anchorLeft: " + anchorLeft);
  return anchorLeft;
}


function delayShowContent_Image(evt,objectID,imageID,top_shift,left_shift,left_offset)
{
  if(isDHTML)
  {
    //alert(isDHTML);
    var elemWidth = 1;
    var livePageWidth = findLivePageWidth();
    //alert(livePageWidth);    

    var Left = getImageLeft(imageID,evt);
    //alert(Left);

    var Top = getImageTop(imageID,evt);
    //alert(Top);

    var left_value = 0;
    var top_value = 0;
    var left_v = 0
    var top_v =0;

    t=2;

    dom = findDOM(objectID,0);
    //alert(objectID);
    //alert(dom);

    domStyle = findDOM(objectID,1);

    if(oldDomSMenu)
    {
      lCount = 0;
      oldDomSMenu.visibility = 'hidden';
      left_value = oldDomSMenu.left;
      top_value = oldDomSMenu.top;
    }
    //alert("Left => " + Left +  " ; Top => " + Top);

    if (dom.offsetWidth) 
    {
      //alert("dom.offsetWidth");  
      elemWidth=dom.offsetWidth;
      //alert(" elemWidth => " + elemWidth);
    }
    else
    {
      //alert("dom.clip.width");  
      if (dom.clip.width) {elemWidth=dom.clip.width;}
    }
   
    left_value = Left + left_shift;
    top_value = Top + top_shift;

    //alert("left_value => " + left_value +  " ; top_value => " + top_value);


    //alert("left_value => " + left_value +  " ; elemWidth => " + elemWidth);
    if ((left_value + elemWidth) > livePageWidth)
    {
      left_value = Left + left_offset - elemWidth;
    }

    //alert("left_value => " + left_value +  " ; top_value => " + top_value);
    
    domStyle.top = top_value;
    domStyle.left = left_value;
    oldDomSMenu = domStyle;

    left_v = getLeft(objectID);
    top_v = getTop(objectID);
    //alert("top: " + top_v);


    if (top_v >= 1 && left_v >= 1)
    {
      //alert("top: " + top_v);
      domStyle.visibility= 'visible';
      domStyle.zIndex = 100;

      //alert("visibility: " + domStyle.visibility);
    }
  }
  else {return null;}
}

function delayHideContent()
{
  //alert("start: t= " +t);
  if((oldDomSMenu) && (t == 0))
  {
    oldDomSMenu.visibility= 'hidden';
    oldDomSMenu.zIndex= '0';
    oldDomSMenu = null;
    lCount =0;
    // alert("t=0: t= " + t + "lCount= " + lCount);
    return false;
  }
  if (t == 2) {lCount =0; return false;}
  if (t == 1)
  {
    //alert("t=1: t= " +t);
    lCount =  lCount + 1;
    //alert("t=1: t= " + t + "lCount= " + lCount);
    if ( lDelay <=  lCount){t = 0;}
    if ( lDelay >=  lCount){setTimeout('delayHideContent('+t+')',pause);}
  }
}
   </script>
  </link>
