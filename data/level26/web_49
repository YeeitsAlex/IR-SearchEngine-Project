    <section class="two-columns-right" id="container">
     <div id="content" role="main">
      <div class="post-131 page type-page status-publish hentry" id="post-131">
       Home
       <div class="entry-content">
        <!-- MasterSlider -->
        <div class="master-slider-parent msl ms-parent-id-1" id="P_MS5c84b8522dcf3" style="max-width:1000px;">
         <!-- MasterSlider Main -->
