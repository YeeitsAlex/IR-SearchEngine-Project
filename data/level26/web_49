           <div class="ms-thumb">
            <div class="ms-tab-context">
             <div class='"ms-tab-context"'>
             </div>
            </div>
           </div>
          </div>
         </div>
         <!-- END MasterSlider Main -->
        </div>
        <!-- END MasterSlider -->
