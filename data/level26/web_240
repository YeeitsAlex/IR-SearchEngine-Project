    <script>
     (function($) {
    function endVideo() {
      var video = $("#ytiframe iframe").attr("src");
      $("#ytiframe iframe").attr("src", "");
      //$("#ytiframe iframe").attr("src", video);
    }
    $(document).on('opening', '.remodal', function() {
      console.log('opening');
    });
    $(document).on('opened', '.remodal', function() {
      console.log('opened');
    });
    $(document).on('closing', '.remodal', function(e) {
      console.log('closing' + (e.reason ?
        ', reason: ' + e.reason :
        ''));
      endVideo();
    });
    $(document).on('closed', '.remodal', function(e) {
      console.log('closed' + (e.reason ?
        ', reason: ' + e.reason :
        ''));
      endVideo()
    });
    $(document).on('confirmation', '.remodal', function() {
      console.log('confirmation');
    });
    $(document).on('cancellation', '.remodal', function() {
      console.log('cancellation');
    });
    //  Usage:  $(function() {
    //
    //    // In this case the initialization function returns the already created instance    var inst = $('[data-remodal-id=modal]').remodal();
    //
    //    inst.open();    inst.close();    inst.getState();    inst.destroy();  });  The second way to initialize:
    $('[data-remodal-id=modal2]').remodal({
      modifier: 'with-red-theme'
    });
    var inst = $('[data-remodal-id=modal]').remodal();
    /**
     * Opens the modal window
     */

    // inst.open();
    if (Cookies.get('name') != '1') {
      Cookies.set('name', '2', {
        expires: 7
      });
    }


    //remove underscore to enable - '1_'
    $(document).ready(function() {
      if ((Cookies.get('name') != '1') && (Cookies.get('name') != null)) {
        inst.open();
        Cookies.set('name', '1', {
          expires: 7
        });

        dataLayer.push({
          'event': 'Lightbox',
          'eventCategory': 'Lightbox',
          'eventAction': 'Mag Bag 2019',
          'eventLabel': 'ANG'
        });



      } else {
        endVideo();
        inst.destroy();
      }
    });
  })(jQuery);
    </script>
