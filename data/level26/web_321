      <article class="post-1674 page type-page status-publish hentry no-post-thumbnail" id="post-1674">
       <div class="entry-wrap">
        <div class="entry-content content">
         <div class="cs-content" id="cs-content">
          <div class="el1 x-section">
           <div class="el2 x-container max width">
            <div class="el3 x-column x-sm x-1-1">
             <div class="rev_slider_wrapper fullwidthbanner-container" data-source="gallery" id="rev_slider_1_1_wrapper" style="margin:0px auto;background:transparent;padding:0px;margin-top:0px;margin-bottom:0px;">
              <div class="rev_slider fullwidthabanner" data-version="5.4.8" id="rev_slider_1_1" style="display:none;">
               <ul>
