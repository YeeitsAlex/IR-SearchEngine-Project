<script>
 $(document).ready(function () {

    //if IE, disable video for version 10 and under since it can't seem to handle it
    if($.browser.msie)
    {
        if($.browser.versionNumber <= '10')
        {
            $('#header-unit').hide();
            $('.billboard-container-video').hide();
        }
        //end if
        
        var os = (function() {
            var ua = navigator.userAgent.toLowerCase();
            return {
                isWin2K: /windows nt 5.0/.test(ua),
                isXP: /windows nt 5.1/.test(ua),
                isVista: /windows nt 6.0/.test(ua),
                isWin7: /windows nt 6.1/.test(ua),
                isWin8: /windows nt 6.2/.test(ua),
                isWin81: /windows nt 6.3/.test(ua)
            };
        }());

        //if Windows 7 and IE 11 then hide the video, IE 11 is fine for displaying video on Win8 and above but not Win7
        if(os.isWin7 && $.browser.versionNumber == '11')
        {
            $('#header-unit').hide();
            $('.billboard-container-video').hide();
        }
        //end if
    }
    else
    {
        $('#faculty-video').lazyLoadXT();
    }
    //end if
});
</script>
