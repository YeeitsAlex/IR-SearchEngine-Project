   <script>
    $(document).ready(function () {

            $('.flexslider').flexslider({
                animation: "slide",
                randomize: true,
                pauseOnHover: true,
                controlNav: false,
                directionNav: false,
                nextText: "",
                prevText: ""
            });

            function socialHomeOver(myElement) {
                $(myElement).animate({
                    marginLeft: '-35px'
                }, 100);
            }

            function socialHomeOut(myElement) {
                $(myElement).animate({
                    marginLeft: '0px'
                }, 100);
            }

            //Begin make these navigate on click
            $('#social-home-facebook').click(function () {
                window.open(
                    'https://www.facebook.com/uwgreenbay',
                    '_blank'
                );
            });

            $('#social-home-twitter').click(function () {
                window.open(
                    'https://www.twitter.com/uwgb',
                    '_blank'
                );
            });

            /*
                $('#social-home-youtube').click(function () {
                    window.open(
                        'https://www.youtube.com/user/uwgb',
                        '_blank'
                    );
                });
            */

            $('#social-home-instagram').click(function () {
                window.open(
                    'https://instagram.com/uwgb/',
                    '_blank'
                );
            });
            //End make these navigate on click

            //Begin class switch for hover event, slide effect
            $(".social-home li").hover(function () {

                switch ($(this).attr('id')) {
                    case 'social-home-facebook':
                        socialHomeOver(this);
                        break;
                    case 'social-home-twitter':
                        socialHomeOver(this);
                        break;
                    /*
                case 'social-home-youtube':
                    socialHomeOver(this);
                    break;
                    */
                    case 'social-home-instagram':
                        socialHomeOver(this);
                        break;
                }
            },
                function () {

                    switch ($(this).attr('id')) {
                        case 'social-home-facebook':
                            socialHomeOut(this);
                            break;
                        case 'social-home-twitter':
                            socialHomeOut(this);
                            break;
                        /*
                    case 'social-home-youtube':
                        socialHomeOut(this);
                        break;
                        */
                        case 'social-home-instagram':
                            socialHomeOut(this);
                            break;
                    }
                });
            //End class switch for hover event, slide effect
        });
   </script>
