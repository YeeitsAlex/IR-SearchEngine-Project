<script>
 $(document).ready(function () {

        $('#connect').hide();

        var client_id = "1e7e3422ffe6400ca5bd81f368496866";
        var access_token = "3664713.1e7e342.9c34f50e4a8e422d93a587d5ecd95dcb";
        var resolution = "thumbnail"; // resolution option values are: "low_resolution", "thumbnail", "standard_resolution"
        var user_id = "3664713";
        var endpoint = "https://api.instagram.com/v1/users/" + user_id + "/media/recent?client_id=" + client_id + "&access_token=" + access_token;
        var count = 9;
        var instagramIcon = "<i class='fa fa-heart fa-lg'>";
        var selectTag = 'uwgb';
        var counter = 0;

        function loadInstagram(endpoint) {

            $(function () {
                $.ajax({
                    type: "GET",
                    dataType: "jsonp",
                    cache: false,
                    url: endpoint,
                    success: function (data) {

                        for (var h = 0; h < data.data.length; h++) {
                            //if (data.data[h].tags.indexOf(selectTag) > -1) {

                            switch (counter) {
                                case (0):
                                    $('#instagram-lg-1').attr('src', data.data[h].images.standard_resolution.url);
                                    $('#instagram-lg-1-text').html(instagramIcon + " " + data.data[h].likes.count + "<div id='instagram-lg-tags-1'>" + data.data[h].tags + "</div>");
                                    $('#instagram-lg-1-link').attr('href', data.data[h].link);
                                    //alert(data.data[h].link);
                                    counter++;
                                    break;
                                case 1:
                                    $('#instagram-sm-1').attr('src', data.data[h].images.low_resolution.url);
                                    $('#instagram-sm-1-text').html(instagramIcon + " " + data.data[h].likes.count + "<div id='instagram-sm-tags-1'>" + data.data[h].tags + "</div>");
                                    $('#instagram-sm-1-link').attr('href', data.data[h].link);
                                    counter++;
                                    break;
                                case 2:
                                    $('#instagram-sm-2').attr('src', data.data[h].images.low_resolution.url);
                                    $('#instagram-sm-2-text').html(instagramIcon + " " + data.data[h].likes.count + "<div id='instagram-sm-tags-2'>" + data.data[h].tags + "</div>");
                                    $('#instagram-sm-2-link').attr('href', data.data[h].link);
                                    counter++;
                                    break;
                                case 3:
                                    $('#instagram-sm-3').attr('src', data.data[h].images.low_resolution.url);
                                    $('#instagram-sm-3-text').html(instagramIcon + " " + data.data[h].likes.count + "<div id='instagram-sm-tags-3'>" + data.data[h].tags + "</div>");
                                    $('#instagram-sm-3-link').attr('href', data.data[h].link);
                                    counter++;
                                    break;
                                case 4:
                                    $('#instagram-sm-4').attr('src', data.data[h].images.low_resolution.url);
                                    $('#instagram-sm-4-text').html(instagramIcon + " " + data.data[h].likes.count + "<div id='instagram-sm-tags-4'>" + data.data[h].tags + "</div>");
                                    $('#instagram-sm-4-link').attr('href', data.data[h].link);
                                    counter++;
                                    break;
                                case 5:
                                    $('#instagram-sm-5').attr('src', data.data[h].images.low_resolution.url);
                                    $('#instagram-sm-5-text').html(instagramIcon + " " + data.data[h].likes.count + "<div id='instagram-sm-tags-5'>" + data.data[h].tags + "</div>");
                                    $('#instagram-sm-5-link').attr('href', data.data[h].link);
                                    counter++;
                                    break;
                                case 6:
                                    $('#instagram-sm-6').attr('src', data.data[h].images.low_resolution.url);
                                    $('#instagram-sm-6-text').html(instagramIcon + " " + data.data[h].likes.count + "<div id='instagram-sm-tags-6'>" + data.data[h].tags + "</div>");
                                    $('#instagram-sm-6-link').attr('href', data.data[h].link);
                                    counter++;
                                    break;
                                case 7:
                                    $('#instagram-sm-7').attr('src', data.data[h].images.low_resolution.url);
                                    $('#instagram-sm-7-text').html(instagramIcon + " " + data.data[h].likes.count + "<div id='instagram-sm-tags-7'>" + data.data[h].tags + "</div>");
                                    $('#instagram-sm-7-link').attr('href', data.data[h].link);
                                    counter++;
                                    break;
                                case 8:
                                    $('#instagram-sm-8').attr('src', data.data[h].images.low_resolution.url);
                                    $('#instagram-sm-8-text').html(instagramIcon + " " + data.data[h].likes.count + "<div id='instagram-sm-tags-8'>" + data.data[h].tags + "</div>");
                                    $('#instagram-sm-8-link').attr('href', data.data[h].link);
                                    counter++;
                                    break;
                            }
                            //end switch
                            //}
                            //end if
                        }
                        //end for
                    }
                }).done(function () {
                    //replace the commas so the tags aren't smashed together
                    //strip hard coded selectTag from the tags displayed
                    $('[id^=instagram-sm-tags-]').text(function () {
                        var strTags = $(this).text().replace(/,/g, " ");
                        return strTags.replace(selectTag, "");
                    });
                    $('[id^=instagram-lg-tags-]').text(function () {
                        var strTags = $(this).text().replace(/,/g, " ");
                        return strTags.replace(selectTag, "");
                    });

                    //$('[id^=instagram-sm-tags-]').matchHeight();

                    $('#connect').show();
                });
            });
        }

        loadInstagram(endpoint);

    });
</script>
