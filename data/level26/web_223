   <script>
    $(document).ready(function () {

    var events = []; // holds event objects for the 3 events highlighted on page
    var maxEventsListed = 3; // max number of events to be listed next to highlighted events
    var counter = 0; // used in RSS feed functions

    // get highlighted event elements on page and add to the array
    $(".upcoming-event").each(function() {
        var event = {
            image: $(this).find(".event-image"),
            title: $(this).find(".event-title"),
            date: $(this).find(".event-date"),
            location: $(this).find(".event-location")
        };

        events.push(event);
    });

    // pull events from RSS feed -- FOR LIST OF EVENTS
    $.ajax({
        url: 'https://calendar.uwgb.edu/RSSFeeds.aspx?data=mhalGvYQmVVe4idPkK%2bt7KLXLbqXZZDcpHppHzB45iBDqGEk%2bGwJWZgXSYOLpzitOulbY%2bI5blE%3d',
        type: 'GET',
        dataType: "xml"
    }).done(function(xml) {
        $(xml).find("item").each( function() {
            // get event details
            var eventLink = $(this).find("link").text();
            var eventTitle = $(this).find("title").text();
            var eventDate = $(this).find("mc\\:EventDate, EventDate").text();
            var eventTime = $(this).find("mc\\:StartTime, StartTime").text();

            // create <li> with event details
            if (counter < maxEventsListed) {
                $("#events-list").append('<li><a href="' + eventLink +
                    '" target="_blank">' + eventTitle +
                    '</a><br />' + eventDate + ', ' +
                    eventTime + '<li>');
            }

            counter++;
        });
    });

    counter = 0; // reset the counter

    // pull events from RSS feed -- FOR HIGHLIGHTED EVENTS
    $.ajax({
        url: 'https://calendar.uwgb.edu/RSSFeeds.aspx?data=H2GRpTFklveE6KQ%2b0BAw5CXrfSra8MTbrTQQlAJAjR7uRcqGhVH07TQB2QztU%2b9y',
        type: 'GET',
        dataType: "xml"
    }).done(function(xml) {
        $(xml).find("item").each( function() {
            // get event details
            var eventLink = $(this).find("link").text();
            var eventImage = $(this).find("enclosure").attr("url");
            var eventTitle = $(this).find("title").text();
            var eventDate = $(this).find("mc\\:EventDate, EventDate").text();
            var eventLocation = $(this).find("mc\\:Location, Location").text();
            var eventTime = $(this).find("mc\\:StartTime, StartTime").text();

            // add details to highlighted events on page
            if (counter < events.length) { // only do this for number of highlight objects on page (3)
                events[counter].image.find("img").attr("src", eventImage); // set event image
                events[counter].image.find("a").attr({"href": eventLink, "target": "_blank"}); // set event image's link
                events[counter].title.html('<a href="' + eventLink + '" target="_blank">' + eventTitle + '</a>'); // set event title and link
                events[counter].date.html(eventDate); // set event date
                if (eventTime.length != null) {
                    events[counter].date.append(", " + eventTime); // set event time (if exists)
                }
                events[counter].location.html(eventLocation); // set event location
            }

            counter++;
        });
    });
});
   </script>
