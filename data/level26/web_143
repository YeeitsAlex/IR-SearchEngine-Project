         function openClose(theID) {
            if (document.getElementById(theID).style.display == "block") {
                document.getElementById(theID).style.display = "none";
                $('#' + theID).prev().find('.col_arrow').attr('src', 'images/downarrow.png');

            } else {
                $('.texter').hide();
                document.getElementById(theID).style.display = "block";
                var test = $('#' + theID).prev().find('.col_arrow').attr('src');
                $('.col_arrow').attr('src', 'images/downarrow.png');
                $('#' + theID).prev().find('.col_arrow').attr('src', 'images/uparrow.png');

            }
        }
        
