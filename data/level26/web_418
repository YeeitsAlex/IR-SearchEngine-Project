	s.src = !("IntersectionObserver" in w) ? "https://prideresourcecenter.colostate.edu/wp-content/plugins/wp-rocket/inc/front/js/lazyload-8.15.2.min.js" : "https://prideresourcecenter.colostate.edu/wp-content/plugins/wp-rocket/inc/front/js/lazyload-10.17.min.js";
