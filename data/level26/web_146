         ​
         <script>
          $(document).ready(function() {
        $.ajax({

            dataType: 'json',
            url: 'https://chhs.source.colostate.edu/wp-json/wp/v2/posts?filter[tag]=prevention-research-center&per_page=3',
            type: 'GET',
            success: function(response) {

            // Here I'm building an article div with the title, excerpt and link to the live version.
            var divBody = '';
            $.each(response, function(index) {

                divBody += "<div class='articlewrapper' style='border: solid 1px #dcd9ce; background: #f8f7f0; padding:10px; margin-bottom:10px; font-size: 11px;'>";
                if (response[index].featured_image) {
                    divBody += "<img width='100px' src='" + response[index].featured_image.source_url + "' style='margin:5px 10px 5px 10px; float:left;' /><br />";
                }
                divBody += "<p><a href='" + response[index].link + "' target='_blank'>" + "<span style='font-weight:bold;'>"  + response[index].title.rendered + "</span></a>";
                divBody += "<br /><span style='font-weight:bold;'>" + response[index].excerpt.rendered + "<a href='" + response[index].link + "'>read more</span></a>";
                divBody += "</div>";

            });

        // Take our constructed div And put it into the above div
        $('.homeblog').html(divBody);
            }
        });
    });
         </script>
         <noscript>
          Your browser must support JavaScript to view this content. 
    Please enable JavaScript in your browser settings then try again.
