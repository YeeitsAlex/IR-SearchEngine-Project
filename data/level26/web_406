  <script>
   $(document).ready(function () {
            $("a.popload").fancybox();
            $(".expand").click(expand);

            navCheck();
            navMobile();
            $(".nav-button").click(function () {
                $(this).toggleClass("pressed");
                $(".nav-items").toggleClass("hidden");
            })
        });

        $(window).scroll(function () {
            navLock();
        })

        $(window).resize( function() {
            navCheck();
            navMobile();
        });

        function expand() {
            $(this).children(".expandee").toggleClass("expanded");
        }

        function isSmall() {
            if ($(window).innerWidth() < 1100)
                return true;
            else
                return false;
        }

        function navLock() {
            if (!isSmall()) {
                if ($(window).scrollTop() > $(".header").height()) {
                    $(".nav").addClass("nav-lock");
                    $(".holder").addClass("nav-lock");
                }
                else {
                    $(".nav").removeClass("nav-lock");
                    $(".holder").removeClass("nav-lock");
                }
                    
            }
        }

        function navMobile() {
            if (isSmall())
                $(".nav-items").css("height", ($(document).innerHeight() - 113) + "px");
            else
                $(".nav-items").css("height", "auto");
        }

        function navCheck() {
            $.each($(".sf-with-ul ul"), function () {
                if ($(this).offset().left + $(this).width() > $(window).innerWidth()) {
                    $(this).css("right", "0");
                }
                else {
                    if ($(this).css("right") == "0px") {
                        $(this).css("right", "initial");
                    }
                }
            });
        }
  </script>
