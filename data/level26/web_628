 $(document).ready(function () {
        ie8Fix();
        fixFacebookArea();
        $('#cal').datepicker({ onSelect: function (dateText, inst) {
            //redirect to the same page with the new parameters
            var url = "http://" + window.location.host + "/eventcalendar.aspx";
            var newurl = url + "?date=" + dateText;
            // window.location.replace(newurl);
            window.location.href = newurl;
        }
        });


        initGallery({
            listid: 'customgallery',
            holdingdiv: 'holdingdiv',
            hiddendiv: 'hiddendiv',
            displayarea: 'fullimage',
            thumbnailrotaterarea: 'thumbnails',
            slideclass: 'thumbarea',
            show: '5'
        });
    });
