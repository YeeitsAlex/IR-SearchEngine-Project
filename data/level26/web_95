    <script type="text/javascript">
     // 
    $(function () {
        $.ajax({
            url: "https://campusdata.uark.edu/apiv2/articles/SearchArticle?$orderby=publishDate+desc&$top=5&$filter=((Departments/any(d139:+d139/id+eq+139)+or+Departments/any(d23:+d23/id+eq+23)))&$top=3",
             data: {},
            dataType: 'jsonp',
            cache: 'true',
            success: function (articles) {
                dataDisplay = "";
                $.each(articles, function (key, a) {
                    var image = "", briefText = "";
                    if (a.brief != null && a.brief != "") { briefText = a.brief; briefText = briefText.replace(/(<p>|<\/p>)/g, ""); }
                    if (a.image != null && a.image != "") {
                        image = a.image;
                    } else { image = "PMUA_Logo-web.jpg"; }

                    dataDisplay = "<div class='col-md-4 col-sm-12'><div class='media'>" +
                                        "<a href='https://news.uark.edu/articles/" + a.id + "' class='media-left'><img alt='" + a.headline + "' src='https://campusdata.uark.edu/resources/images/articles/" + image + "' style='width:70px' class='media-object'/></a>" +

                                    "<div class='media-body'>" +
                                    "<h4 class='media-heading'><a href='https://news.uark.edu/articles/" + a.id + "'>" + a.headline + "</a></h4>" + briefText + 
                                    "</div></div><br>";

                    $("#newsArchive").append(dataDisplay);
                });
            }
        });

    });
//
    </script>
   </div>
