     <div id="page-title">
      <h2 class="title">
       Home
      </h2>
     </div>
     <div class="no-sidebars" id="content">
      <div id="support">
       <div class="messages error">
        <h2 class="element-invisible">
         Error message
        </h2>
        Brightcove client is not found
