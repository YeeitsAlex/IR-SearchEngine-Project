    //<![CDATA[


var SSPC2054_imgSlide = new SlideShow(5, 7);
SSPC2054_imgSlide.playPauseID = "PC2054_btnPlayPause";
SSPC2054_imgSlide.uniqueID = "PC2054_imgSlide";
SSPC2054_imgSlide.captionID = "PC2054_lblCaption";
SSPC2054_imgSlide.random = false;
SSPC2054_imgSlide.gSupportCaptions = false;

SSPC2054_imgSlide.images[0] = "https://connect.mbl.edu/image/slideshow/two.jpg";
SSPC2054_imgSlide.captions[0] = "";
SSPC2054_imgSlide.images[1] = "https://connect.mbl.edu/image/slideshow/eight.jpg";
SSPC2054_imgSlide.captions[1] = "";
SSPC2054_imgSlide.images[2] = "https://connect.mbl.edu/image/slideshow/four.jpg";
SSPC2054_imgSlide.captions[2] = "";
