SSPC2054_imgSlide.captions[3] = "";
SSPC2054_imgSlide.images[4] = "https://connect.mbl.edu/image/slideshow/seven.jpg";
SSPC2054_imgSlide.captions[4] = "";
var ipPC2054 = null;
ipPC2054 = new ImagePreloader(SSPC2054_imgSlide.images, SSPC2054_imgSlide.captions, onPreloadPC2054);

function onPreloadPC2054(aImages, aCaptions, nImages)
{
	gImageCapableBrowser = canManipulateImages();
	//SSPC2054_imgSlide.images = aImages;
	//SSPC2054_imgSlide.captions = aCaptions;
	SSPC2054_imgSlide.imageCount = nImages;
	SSPC2054_imgSlide.intervalID = setInterval("nextSlide(SSPC2054_imgSlide,false)",SSPC2054_imgSlide.slideInterval);
}

//]]>
   </script>
  </form>
 </body>
</html>

