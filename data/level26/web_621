<script>
 var firstName = '';
                        var lastName = '';
                        var emailAddress = '';
                        var academicProgram = '';
                        var academicProgramHidden = '';



                        $(document).ready(function () {

                            makeModalFormDirty();
							$(".pageForm #tfa_6").attr('checked', false);
							/*$('[id^=tfa_25]').hide();*/
                            // This is for Modal/ Popup

                            /*$('.md-overlay').click(function () {
                                makeModalFormDirty();
                                makePageFormSanitize();

                                $(".pageForm #tfa_1").val(firstName);
                                $(".pageForm #tfa_2").val(lastName);
                                $(".pageForm #tfa_3").val(emailAddress);
                                $(".pageForm #tfa_25").val(academicProgram);
                                $(".pageForm #tfa_521").val(academicProgramHidden);

                                $(".pageForm #tfa_6").attr('checked', false);

                               // $('.md-modal').removeClass('md-show');

                            });*/

                            $('.modal-header').click(function () {
                                makeModalFormDirty();
                                makePageFormSanitize();

                                $(".pageForm #tfa_1").val(firstName);
                                $(".pageForm #tfa_2").val(lastName);
                                $(".pageForm #tfa_3").val(emailAddress);
                                $(".pageForm #tfa_25").val(academicProgram);
                                $(".pageForm #tfa_521").val(academicProgramHidden);
                                

                                $(".pageForm #tfa_6").attr('checked', false);

                                $('.md-modal').removeClass('md-show');
								
								$('.md-overlay').css('visibility', 'hidden');
								$('.md-overlay').css('display', 'none');
								$('.md-overlay').css('opacity', '0');
								
								$('body').removeClass('noScroll');

                            });

                        });

                        function openModalForm() {

                            firstName = $(".pageForm #tfa_1").val();
                            lastName = $(".pageForm #tfa_2").val();
                            emailAddress = $(".pageForm #tfa_3").val();
                            academicProgram = $(".pageForm #tfa_25").val();
                            academicProgramHidden = $(".pageForm #tfa_521").val();
							
						

                            makePageFormDirty();
                            makeModalFormSanitize();

                            $(".modalForm #tfa_1").val(firstName);
                            $(".modalForm #tfa_2").val(lastName);
                            $(".modalForm #tfa_3").val(emailAddress);
                            $(".modalForm #tfa_25").val(academicProgram);
                            $(".modalForm #tfa_521").val(academicProgramHidden);

                            $(".modalForm #tfa_6").attr('checked', true);

                            if ($('.modalForm Div').hasClass('offstate')) {
                                $('.modalForm Div').removeClass('offstate')
                            }

                            $(".modalForm input:disabled").removeAttr("disabled");
                            $(".modalForm select:disabled").removeAttr("disabled");

                            	$('.md-modal').addClass('md-show');
								$('.md-overlay').css('visibility', 'visible');
								$('.md-overlay').css('display', 'block');
								$('.md-overlay').css('opacity', '1');
								
								$('body').addClass('noScroll');
                        }



                        function makeModalFormDirty() {
                            $('.modal-body [id^=tfa]').each(function() {
                                //console.log($( this ).attr('id'));
                                $( this ).attr('id', $( this ).attr('id') + '_modal_jsom');
                            });
                        }

                        function makeModalFormSanitize() {
                            $('.modal-body [id^=tfa]').each(function() {
                                //console.log($( this ).attr('id'));
                                $( this ).attr('id', $( this ).attr('id').replace("_modal_jsom", ""));
                            });
                        }

                        function makePageFormDirty() {
                            $('#pageFormContainer [id^=tfa]').each(function() {
                                //console.log($( this ).attr('id'));
                                $( this ).attr('id', $( this ).attr('id') + '_page_jsom');
                            });
                        }

                        function makePageFormSanitize() {
                            $('#pageFormContainer [id^=tfa]').each(function() {
                                //console.log($( this ).attr('id'));
                                $( this ).attr('id', $( this ).attr('id').replace("_page_jsom", ""));
                            });
                        }
</script>
