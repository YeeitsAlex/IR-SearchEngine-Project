 <article class="post-142 page type-page status-publish hentry no-post-thumbnail" id="post-142">
  <div class="entry-content">
   <div class="cs-content" id="cs-content">
    <div class="x-section" id="x-section-1" style="margin: 0px;padding: 0px; background-color: hsl(0, 0%, 0%);">
     <div class="x-container" style="margin: 0px auto;padding: 0px;">
      <div class="x-column x-sm x-1-1" data-fade="true" data-x-element="column" data-x-params='{"fade":true}' style="padding: 0px;opacity: 0;transition-duration: 750ms;">
       <div class="rev_slider_wrapper fullwidthbanner-container" data-source="gallery" id="rev_slider_4_1_wrapper" style="margin:0px auto;background:#0a0a0a;padding:0px;margin-top:0px;margin-bottom:0px;">
        <div class="rev_slider fullwidthabanner" data-version="5.4.8" id="rev_slider_4_1" style="display:none;">
         <ul>
          <li data-description="" data-easein="default" data-easeout="default" data-hideafterloop="0" data-hideslideonmobile="off" data-index="rs-7" data-masterspeed="300" data-param1="" data-param10="" data-param2="" data-param3="" data-param4="" data-param5="" data-param6="" data-param7="" data-param8="" data-param9="" data-rotate="0" data-saveperformance="off" data-slotamount="default" data-thumb="https://wgac.colostate.edu/wp-content/uploads/sites/21/2017/10/home-1024x312-100x50.jpg" data-title="Slide" data-transition="fade">
           <img alt="Get involved with the WGAC" class="rev-slidebg" data-bgparallax="15" data-bgposition="center center" data-blurend="0" data-blurstart="0" data-duration="10000" data-ease="Linear.easeNone" data-kenburns="on" data-no-retina="" data-offsetend="0 0" data-offsetstart="0 0" data-rotateend="0" data-rotatestart="0" data-scaleend="100" data-scalestart="100" height="312" src="https://wgac.colostate.edu/wp-content/uploads/sites/21/2017/10/home-1024x312.jpg" title="home" width="1024"/>
           <h1 class="tp-caption tp-resizeme man" data-frames='[{"delay":500,"speed":300,"frame":"0","from":"opacity:0;","to":"o:1;","ease":"Power2.easeInOut"},{"delay":"wait","speed":300,"frame":"999","to":"opacity:0;","ease":"nothing"}]' data-height="['122']" data-hoffset="" data-paddingbottom="[0,0,0,0]" data-paddingleft="[0,0,0,0]" data-paddingright="[0,0,0,0]" data-paddingtop="[0,0,0,0]" data-responsive_offset="on" data-textalign="['right','right','right','right']" data-type="text" data-width="['572']" data-x="right" data-y="324" id="slide-7-layer-1" style="z-index: 5; min-width: 572px; max-width: 572px; max-width: 122px; max-width: 122px; white-space: normal; font-size: 60px; line-height: 60px; font-weight: 500; color: rgba(255,255,255,1); letter-spacing: 3px;font-family:Montserrat;text-transform:uppercase;">
