      var step = 1;
		function slideit() {
			if (document.images) {		//if browser does not support the image object, exit.
				document.images.Image2.src = imageList[step - 1].image.src; 					//imageList[step - 1].src;
			
				var abcd = document.getElementById('z2');
				if (abcd) {
					abcd.innerHTML = imageList[step - 1].dsc; 				//imageDescription[step - 1];
				}

				if (step < imageList.length) {
					step++;
				} else {
					step = 1;
				}

				setTimeout("slideit()", 7000); 	//call function "slideit()" every 7.0 seconds; was 5000
			
			} else {
				return;
			}
		}

		slideit();
     </script>
