              <script type="text/javascript">
               setREVStartSize({c: jQuery('#rev_slider_9_1'), responsiveLevels: [1240,1024,778,480], gridwidth: [1240,1024,778,600], gridheight: [868,701,560,450], sliderLayout: 'fullscreen', fullScreenAutoWidth:'off', fullScreenAlignForce:'off', fullScreenOffsetContainer:'', fullScreenOffset:'60px'});
			
var revapi9,
	tpj=jQuery;
			
tpj(document).ready(function() {
	if(tpj("#rev_slider_9_1").revolution == undefined){
		revslider_showDoubleJqueryError("#rev_slider_9_1");
	}else{
		revapi9 = tpj("#rev_slider_9_1").show().revolution({
			sliderType:"standard",
