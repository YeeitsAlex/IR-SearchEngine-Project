    var closePopup = function() {
	$('#signup').hide();
	$.cookie('showPopup',false,{expires:1000});
};
$(document).ready(function() {
	if ($.cookie('showPopup') == "true") {
		$('#signup.popup').fadeIn(1000);
		$('#signup a.close').show();
	};
	$('#signup a.close').click(closePopup);
	$('#email-signup').validate({rules: {fldEmail: {required: true, email: true}}});
	$('#email-signup').submit(
		function() {
			if ($('#email-signup').valid()) {
				closePopup();
				ga('send', 'event', 'button', 'click', 'homepage popup');
			}
		}
	);	
});
   </script>
