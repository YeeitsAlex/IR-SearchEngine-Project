        timer_metaslider_500();
var metaslider_500 = function($) {
            $('#metaslider_500').addClass('flexslider'); /* theme/plugin conflict avoidance */
            $('#metaslider_500').flexslider({ 
                slideshowSpeed:10000,
                animation:"fade",
                controlNav:false,
                directionNav:true,
                pauseOnHover:false,
                direction:"horizontal",
                reverse:false,
                animationSpeed:500,
                prevText:"&lt;",
                nextText:"&gt;",
                slideshow:true,
                init: function(slider) {
                                $('#metaslider_500 .msHtmlOverlay').each(function() {
                $(this).css('display', 'block');
            });
                },
                before: function(slider) {
                    $('li:not(".flex-active-slide") .animated', slider).not('.disabled').each(function(index) {
                        var el = $(this);
                        var cloned = el.clone();
                        el.before(cloned);
                        $(this).remove();
                    });
                }
            });            
            $(window).resize(function(){
               $('#metaslider_500').metaslider_scale_layers({
                   downscale_only: false,
                   orig_width: 1080
               });
            });
            $('#metaslider_500').metaslider_scale_layers({
                downscale_only: false,
                orig_width: 1080
            });

            $('#metaslider_500 .msHtmlOverlay .layer[data-link], #metaslider_500 .msHtmlOverlay[data-link]').each(function() {
                var layer = $(this);

                layer.css('cursor', 'pointer').on('click', function(e) {
                    if( ! $(e.target).closest('a').length) {
                        window.open(layer.attr('data-link'), layer.attr('data-target'));
                    }
                 });
            });

            $('#metaslider_500 .msHtmlOverlay').each(function() {
                $(this).css('display', 'block');
            });
        };
        var timer_metaslider_500 = function() {
            var slider = !window.jQuery ? window.setTimeout(timer_metaslider_500, 100) : !jQuery.isReady ? window.setTimeout(timer_metaslider_500, 1) : metaslider_500(window.jQuery);
        };
        timer_metaslider_500();
  </script>
