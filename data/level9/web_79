       jQuery( document ).ready(function($) {

    var quotes = $(".quote-set");
    var quoteIndex = -1;
    
    function showNextQuote() {
        ++quoteIndex;
        quotes.eq(quoteIndex % quotes.length)
            .fadeIn(200)
            .delay(5000)
            .fadeOut(1000, showNextQuote);
    }
    
    showNextQuote();
    
})();
