  <script type="text/javascript">
   $(document).ready(function(){
			$(".division-slider").slick({
				dots: false,
				arrows: true,
				infinite: true,
				variableWidth: true,
				appendArrows: $("#news-nav"),
				slidesToShow: 3,
				responsive: [
				{
					breakpoint: 800,
					settings: {
						slidesToShow: 1,
						slidesToScroll: 1,
						infinite: true,
						dots: false
					}
				}
				]
			});

						//-----cycle through keywords
						//create array of words
			var items = ['a school for pushing limits', 'a school for intersectional inquiry', 'a school for amping activism', 'a school for cultural and social transformation'];
			var $text = $( '#home-branding span' ),
				delay = 1.5; //seconds

			//loop through each word
			function loop ( delay ) {
				$.each( items, function ( i, elm ){
					$text.delay( delay*1E3).fadeOut();
					$text.queue(function(){
						$text.html( items[i] );
						$text.dequeue();
					});
					$text.fadeIn();
					$text.queue(function(){
						if ( i == items.length -1 ) {
							loop(delay);   
						}
						$text.dequeue();
					});
				});
			}

			loop( delay );
						
			
			
		});
  </script>
