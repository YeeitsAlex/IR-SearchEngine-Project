var front = "auto";
if(front=="auto"){
    front = "Home";
     if(front.includes(":")){
         front = front.split(":")[0];
         if(front.includes(".")){
             front = front.split(".");
             front = front.map((int)=>int.includes("0")?parseInt(int,10):int).join(".");
         }
         front+=".";
     }
     else {
     front = "";
     }
}
front = front.replace(/_/g," ");

