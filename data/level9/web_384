     <!-- panel Research Facts -->
     <section class="panel" data-featured-post="none" data-posts-per-row="4" data-type="posts" data-width="full">
      <header>
       <h2 class="center">
        Stanford research for a better world
       </h2>
      </header>
      <div class="grid-container">
       <article>
        <figure class="landscape">
         <a aria-hidden="true" href="https://news.stanford.edu/2019/03/06/human-networks-drive-inequality-social-immobility/" tabindex="-1">
