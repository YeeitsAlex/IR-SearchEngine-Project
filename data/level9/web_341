            <div class="panel-frontpage-middle clearfix">
             <div class="panel-panel panel-banner">
              <div class="inside">
               <div class="rounded-corner">
                <div class="wrap-corner">
                 <div class="t-edge">
                  <div class="l">
                  </div>
                  <div class="r">
                  </div>
