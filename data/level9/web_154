     <div class="wpb_wrapper">
      <div class="wpb_raw_code wpb_raw_js">
       <div class="wpb_wrapper">
        <script type="text/javascript">
         function setInitialHeight(){
  setTimeout(function(){
    jQuery(".tweet-home").css({
      "height" : "initial",
    });
    jQuery(".latest-tweets").css({
      "padding" : "0",
    });
  }, 200);
}

function copyHeightFromPosts(){
  setTimeout(function(){
    var newHeight = jQuery(".latest-posts").height();
    jQuery(".tweet-home").height(newHeight);
    jQuery(".latest-tweets").css({"padding": "0 15px"});
  }, 200);
}

function chooseHeight(){
  var thisWidth = jQuery(window).width();
  if(thisWidth >= 720){ copyHeightFromPosts()}
    else { setInitialHeight() }
}

setTimeout(function(){ chooseHeight(); }, 200);
jQuery(window).resize(function(){ chooseHeight(); });
        </script>
       </div>
