  <script type="text/javascript">
   $(document).ready(function() {
    $('.submit_on_enter').keydown(function(event) {
        if (event.keyCode == 13) {
            this.form.submit();
            return false;
         }
    });

// populate search field with q from query string
	if ( $('#form_pub_search').length > 0 ) {
		
		// get query string variables 
		// https://css-tricks.com/snippets/javascript/get-url-variables/
		var vars = window.location.search.substring(1).split("&");
		for (var i=0; i < vars.length; i++) {
			 var pair = vars[i].split("=");
			 if (pair[0] == 'q') {
				// the q string is pair[1]
				$('input[name=q]').val(decodeURIComponent(pair[1]));
			 }
		}
	} // end populate search
  });
  </script>
  <script>
   $(document).ready(function(e) {
    var el = document.getElementById("success_message");
	var formEl = document.getElementById("signupElements");
    if ( formEl ) {
      if( el && el.style.display == 'none') {   
          formEl.style.display = 'block';
      } else { 
          formEl.style.display = 'none';
      }
    }
});
  </script>
