           <li class="media-object__meta-author" ng-if="item.listAuthorByLine">
            by
            <span ng-bind-html="item.listAuthorByLine">
            </span>
           </li>
           <li class="media-object__meta-pubdate" ng-if="item.listDate">
            {{:: item.listDate }}
           </li>
           <li class="media-object__meta-columns" ng-if="item.listColumns.length &gt; 0">
            <span ng-repeat="col in item.listColumns">
             <a href="{{:: col.linkUrl}}">
              {{:: col.linkTitle}}
             </a>
            </span>
           </li>
           <li class="media-object__meta-editor" ng-repeat="icon in item.listCollections">
            <svg aria-hidden="true" class="icon" ng-if="icon.iconImageName">
             <use xlink:href="{{ feed.getIcon(icon.iconImageName) }}" xmlns:xlink="http://www.w3.org/1999/xlink">
             </use>
            </svg>
            {{:: icon.iconTitle }}
           </li>
          </ul>
          <p ng-bind-html="item.listSummary" ng-if="item.listSummary">
          </p>
         </div>
        </article>
       </li>
      </ul>
     </div>
