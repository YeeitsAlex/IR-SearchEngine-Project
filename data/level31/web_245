  <script type="text/javascript">
   var analyticsCallbacks = [];
    SC.onGaLoaded = function(cb) {
      analyticsCallbacks.push(cb);
      triggerGaCallbacks();
    };

    function triggerGaCallbacks() {
      // to ensure we're still async everywhere we expect to be, defer handling these callbacks
      // until the next frame
      setTimeout(function() {
        if (ga) { // if ga is loaded
          analyticsCallbacks.forEach(function (cb) {
            cb();
          });
          analyticsCallbacks = [];
        }
      }, 0);
    }

    // Set this to true to enable Google Analytics debug mode
    var gaDebug = false;

    function loadGA(debug) {
      // insert Google Analytics, but use debug mode if specified
      // keep these two strings as separate variables so that it's easy to find
      // when grepping through the code base
      var gaScript = 'https://www.google-analytics.com/analytics.js';
      var gaDebugScript = 'https://www.google-analytics.com/analytics_debug.js';
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script', debug ? gaDebugScript : gaScript,'ga');
    }

    function initializeGA(debug) {
      ga('create', 'UA-3837893-1', 'auto', {allowLinker: true, name: 'classyTrack'});

      if (debug) {
        // when this is set, no hits will be sent to Google Analytics
        ga('set', 'sendHitTask', null);
      }

        triggerGaCallbacks();
    }

    loadGA(gaDebug);
    initializeGA(gaDebug);
  </script>
