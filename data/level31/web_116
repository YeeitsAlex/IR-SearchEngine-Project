       <h2 class="block__title block-title">
        Calendar
       </h2>
       <div class="view view-events-masonry-grid view-id-events_masonry_grid view-display-id-block_1 masonry-grid-view standard-page view-dom-id-b3a4b8c82b0364744af2beb23a8f8842">
        <div class="view-header">
         <p>
          The Northwestern research community hosts lectures, seminars, workshops, and educational events throughout the year.
         </p>
        </div>
        <div class="view-content">
         <div class="masonry-item views-row views-row-1 views-row-odd views-row-first col-xs-12 col-sm-12 col-md-6 col-lg-6">
          <article about="/events/ipr-colloquium-d-victorson-feinbergipr-shifting-illness-care-healthcare-through-mindfulness" class="node node-event node-teaser view-mode-teaser event event-listing clearfix" typeof="sioc:Item foaf:Document">
           <!-- regions -->
           <div class="ds-event_date event-date">
            <div class="field field-name-event-teaser-date-icon field-type-ds field-label-hidden">
             <div class="field-items">
              <div class="field-item even">
               <div class="month">
                Mar
               </div>
               <div class="day">
                11
               </div>
               <div class="year">
                2019
               </div>
              </div>
             </div>
            </div>
           </div>
           <div class="ds-content_area event-description feature-copy">
            <div class="field field-name-title field-type-ds field-label-hidden">
             <div class="field-items">
              <div class="field-item even" property="dc:title">
               <h4>
                <a href="/events/ipr-colloquium-d-victorson-feinbergipr-shifting-illness-care-healthcare-through-mindfulness">
                 IPR Colloquium: D. Victorson (Feinberg/IPR)  - Shifting from Illness Care to Healthcare Through Mindfulness-Based Self-Care
                </a>
               </h4>
              </div>
             </div>
            </div>
            <div class="field field-name-start-end-dates field-type-ds field-label-hidden">
             <div class="field-items">
              <div class="field-item even">
               <div class="event-time-location">
                Start Date: Mon, Mar 11, 2019 12:00 PM
                <br/>
                End Date: Mon, Mar 11, 2019 1:00 PM
               </div>
              </div>
             </div>
            </div>
           </div>
           <meta content="/events/ipr-colloquium-d-victorson-feinbergipr-shifting-illness-care-healthcare-through-mindfulness" property="schema:mainEntityOfPage"/>
           <meta content="https://research.northwestern.edu/events/ipr-colloquium-d-victorson-feinbergipr-shifting-illness-care-healthcare-through-mindfulness" property="schema:url"/>
           <div class="element-invisible">
            <div class="field field-name-event-time-location field-type-ds field-label-hidden">
             <div class="field-items">
              <div class="field-item even">
               <p class="event-time-location">
                12:00 PM
               </p>
              </div>
             </div>
            </div>
            <span class="date-display-single">
             Monday, March 11, 2019 -
             <div class="date-display-range">
              <span class="date-display-start" content="2019-03-11T12:00:00-05:00" datatype="xsd:dateTime" property="dc:date">
               12:00
              </span>
              to
              <span class="date-display-end" content="2019-03-11T13:00:00-05:00" datatype="xsd:dateTime" property="dc:date">
               13:00
              </span>
             </div>
            </span>
            <div class="field field-name-field-event-location field-type-location field-label-hidden">
             <div class="field-items">
              <div class="field-item even">
               <div class="location vcard" typeof="schema:Place">
                <div class="adr">
                 <div property="schema:address" typeof="PostalAddress">
                  <div class="street-address" datatype="xsd:string" property="schema:streetAddress">
