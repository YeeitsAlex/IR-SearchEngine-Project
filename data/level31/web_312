      jQuery(document).ready(function(){

        jQuery('#financial-aid .text').click(function(){
          var tab_id = jQuery(this).attr('data-tab');

          jQuery('#financial-aid .text').removeClass('current');
          jQuery('#financial-aid .number-cont').removeClass('current');

          jQuery(this).addClass('current');
          jQuery("#"+tab_id).addClass('current');
        });

      });
