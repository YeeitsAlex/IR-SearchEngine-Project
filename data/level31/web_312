      </div>
     </div>
     <script>
      jQuery( document ).ready(function() {
		if(window.outerWidth > 1024){
	        jQuery('#hero select').change(function(){
    	      var tab_id = jQuery(this).val();

	          jQuery('.wrapper').stop().animate({
	              scrollTop: jQuery("#"+tab_id).offset().top
	          }, 1000);
	          event.preventDefault();

	        });



	      jQuery("#to-top").click(function(){
	        jQuery('.wrapper').stop().animate({
	            scrollTop: jQuery(".wrapper").offset().top
	        }, 1000);
	        event.preventDefault();
	      });
		}else{
	        jQuery('#hero select').change(function(){
    	      var tab_id = jQuery(this).val();

	          jQuery('html,body').stop().animate({
	              scrollTop: jQuery("#"+tab_id).offset().top
	          }, 1000);
	          event.preventDefault();

	        });



	      jQuery("#to-top").click(function(){
	        jQuery('html,body').stop().animate({
	            scrollTop: jQuery(".wrapper").offset().top
	        }, 1000);
	        event.preventDefault();
	      });
		}

    jQuery('#scroll-wrapper').scrollbar();

	});
     </script>
    </div>
   </div>
