     <script>
      jQuery(document).ready(function(){

        var load_val = jQuery("#area select").val()

        jQuery("#area .img-bg").css({
          "background" : "url(/wp-content/uploads/2017/09/" + load_val + ".jpg) center center no-repeat",
          "background-size" : "cover",
          "-webkit-background-size" : "cover"
        });

        jQuery('#area select').change(function(){
          var tab_id = jQuery(this).val();

          jQuery("#area .tab-content").removeClass('current');

          jQuery("#"+tab_id).addClass('current');

          jQuery("#area .img-bg").css({
            "background" : "url(/wp-content/uploads/2017/09/" + tab_id + ".jpg) center center no-repeat",
            "background-size" : "cover",
            "-webkit-background-size" : "cover"
          });
        });

      });
     </script>
