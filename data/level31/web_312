     <script>
      jQuery(document).ready(function(){

        jQuery('#progress .text').click(function(){
          var tab_id = jQuery(this).attr('data-tab');

          jQuery('#progress .text').removeClass('current');
          jQuery('#progress .num-content').removeClass('current');

          jQuery(this).addClass('current');
          jQuery("#"+tab_id).addClass('current');
        });

