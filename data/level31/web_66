    <!-- ----------------------------------- for the rotating front page images-------------------------------------------- -->
    <script>
     $(document).ready(function(){
//rotating image code
		
		var items = $('.randomImageBox .randomImage').toArray();
		//var i = Math.floor(Math.random()*items.length);
		var i = 0;
		var timerIsOn = 1;
					
		function autoRotate() 
		{
			if( timerIsOn == 1)
				i = incrementI(Math.floor(Math.random()*items.length), items);
			
			window.setTimeout(function() {autoRotate()},10000)
		}

		autoRotate();
		
		for ( var j = 0; j < items.length; j++ )
		{
			$('#randomImageIndex').append('<div class="randomImageIndexButtons" id="rotate'+j+'">&bull;</div>');
		}
		$('#rotate'+i).addClass('randomImageIndexButtonsOn');
		
		$('.randomImageIndexButtons').click(function()
		{
			timerIsOn = 0;
			$('#playPause').find('img').attr('src', '/graphics/play.gif');
			
			$('.randomImage').removeClass('on');		
			$('.randomImageIndexButtons').removeClass('randomImageIndexButtonsOn')
			
			t = $(this).attr('id').substring(6);		
			
			$(items[t]).addClass('on');
			$('#rotate'+t).addClass('randomImageIndexButtonsOn')
			
			i = t;
		});
		$(window.parent.document).blur(function()
		{
				timerIsOn = 0;
				$('#playPause').find('img').attr('src', '/graphics/play.gif');
		});
		
		$('#playPause').click(function()
		{
			if( timerIsOn == 1 )
			{
				timerIsOn = 0;
				$('#playPause').find('img').attr('src', '/graphics/play.gif');
			}
			else
			{
				timerIsOn = 1;
				$('#playPause').find('img').attr('src', '/graphics/pause.gif');
			}
		});
		
		$('#nextButton').click(function()
		{
			i = incrementI(i, items);
			timerIsOn = 0;
			$('#playPause').find('img').attr('src', '/graphics/play.gif');

		});

		$('#previousButton').click(function()
		{
			i = decrementI(i, items);
			timerIsOn = 0;
			$('#playPause').find('img').attr('src', '/graphics/play.gif');
		});

		function setSLide(index, itemList) {

		}
	
		function incrementI(index, itemList)
		{
			$('.randomImage').removeClass('on');		
			$('.randomImageIndexButtons').removeClass('randomImageIndexButtonsOn')
			
			if ( index < itemList.length-1 )
				index++;
			else
				index = 0;	
			
			$(itemList[index]).addClass('on');
			$('#rotate'+index).addClass('randomImageIndexButtonsOn')
			
			return index;
		}
	
		function decrementI(index, itemList)
		{
			$('.randomImage').removeClass('on');		
			$('.randomImageIndexButtons').removeClass('randomImageIndexButtonsOn')

			if ( index > 0 )
				index--;
			else
				index = itemList.length-1;	
			
			$(itemList[index]).addClass('on');
			$('#rotate'+index).addClass('randomImageIndexButtonsOn')
			$('#readMore').attr('href', $(itemList[index]).find('a').attr('href'));

			return index;
		}
});
    </script>
