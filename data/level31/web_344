  <script>
   EdLabSSO.init({
  appName: "EdLabWebsite",
  autoLogin: true,
  //- debug: true,
  onLoggedIn: function(token) {
    /* Do something when logged in */
    //- console.info(token)
    if (token) {
      location.href = "/api/login/?return=" + encodeURIComponent(location.href.replace(location.hash, "")) + `&token=${token}`
    } else {
      console.warn("> Token not found, check and see if SSO is working")
    }
  },
  onLoggedInFailed: function(error, url) {
    /* Do something when manual login failed, the second parameter is the login url based on your application*/
    location.href = url
  }
})
  </script>
