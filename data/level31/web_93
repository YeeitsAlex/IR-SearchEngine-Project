         // Inject some minor style tweaks into the twitter timeline iframe
(function($) {
var cals_twitter_timeline_exists = setInterval(function() {
  if ($('footer iframe.twitter-timeline-rendered').length) {
    $("footer iframe.twitter-timeline-rendered").contents().find('head').append('<style>.timeline-Tweet-media, .timeline-Tweet-actions, .timeline-Tweet-brand, .TweetAuthor-avatar {display:none;} .timeline-Tweet-metadata {margin-top:-16px !important;} .timeline-Tweet-text {margin: 0 !important;} .timeline-Tweet-author {padding: 0 0 0 0 !important;} .timeline-Tweet--isRetweet .timeline-Tweet-author {padding: 0 0 0 20px !important;} .timeline-Tweet {position:relative !important; padding: 0 0 10px 10px !important; line-height: 1.3 !important;} .timeline-Tweet:hover {background-color: inherit !important;} .timeline-Tweet-retweetCredit {margin: 0 !important; font: 0/0 a !important; position: absolute !important; left: 10px; top: 0;}</style>');
    clearInterval(cals_twitter_timeline_exists);
  }
}, 500);
})(jQuery);

