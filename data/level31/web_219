   <script>
    function getCookie(name) {
  var value = '; ' + document.cookie
  var parts = value.split('; ' + name + '=')
  if (parts.length == 2) return parts.pop().split(';').shift()
}

if (getCookie('privacyNoticeAgree') !== 'true') {
  document.getElementById('privacyNotice').style.display = 'block'
  document.getElementById('privacyNoticeAgree').onclick = () => {
    var date = new Date()
    date.setTime(date.getTime() + (365 * 24 * 60 * 60 * 1000))
    document.cookie = 'privacyNoticeAgree=true; expires=' + date.toUTCString() + '; path=/'
    document.getElementById('privacyNotice').style.display = 'none'
  }
}
   </script>
