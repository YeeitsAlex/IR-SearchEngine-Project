   <script>
    $(function() {
		$('.item.past').each(function () {
			var $this = $(this);
			var $toggle = $('<button type="button" class="btn btn-xs btn-link text-muted">More &raquo;</button>');
			// console.log($toggle);
			$toggle.on('click', function () {
				$(this).hide().closest('div').find('p').slideDown();
			});
			$this.find('.col-sm-9 p').hide();
			$this.find('h3').on('click', function () {
				$(this).find('button').hide().closest('div').find('p').slideDown();
			}).append($toggle);
		})

	});
   </script>
