       <div id="main-content">
        <!-- !Main Content Header -->
        <!-- !Main Content -->
        <div class="region" id="content">
         <div class="block block-system no-title odd first last block-count-5 block-region-content block-main" id="block-system-main">
          <div class="panel-flexible panels-flexible-hero_space clearfix" id="index">
           <div class="panel-flexible-inside panels-flexible-hero_space-inside">
            <div class="panels-flexible-row panels-flexible-row-hero_space-main-row panels-flexible-row-first clearfix row1">
             <div class="inside panels-flexible-row-inside panels-flexible-row-hero_space-main-row-inside panels-flexible-row-inside-first clearfix">
              <div class="panels-flexible-region panels-flexible-region-hero_space-center panels-flexible-region-first panels-flexible-region-last region-single">
               <div class="inside panels-flexible-region-inside panels-flexible-region-hero_space-center-inside panels-flexible-region-inside-first panels-flexible-region-inside-last">
                <div class="panel-pane pane-block pane-block-34 no-title block" id="search-pane">
                 <div class="block-inner clearfix">
                  <div class="block-content">
                   <style>
                    /*Apply these styles to search-form select for top 5 browsers Chrome, Firefox, Edge, IE, Safari*/
