    html[data-useragent*="Mozilla/5.0 (iPad; CPU OS 11_0_3 like Mac OS X) AppleWebKit/604.1.38 (KHTML, like Gecko) Mobile/15A432 [FBAN/FBIOS;FBAV/147.0.0.46.81;FBBV/76961488;FBDV/iPad6,7;FBMD/iPad;FBSN/iOS;FBSV/11.0.3;FBSS/2;FBCR/;FBID/tablet;FBLC/en_US;FBOP/5;FBRV/0]"] #search-form select {
        padding: 7px 12px 7px 12px;
    }
                   </style>
                   <div class="selected">
                   </div>
                   <p id="search-info">
                    Search for journals, articles, books, magazines, and newspapers, all in one search.
                   </p>
                   <form action="javascript:submitSearch();" id="search-form" method="get" name="search-form">
                    <input id="searchQueryTemp" placeholder="Enter keywords..." size="35" type="text" value="">
                     <span class="select-wrapper">
                      <select id="searches" name="searches" onchange="libtabs_switch(lastSearch)">
                       <option value="primo">
                        One Search
                       </option>
                       <option value="catalog">
                        ASU Library Catalog
                       </option>
                       <option value="databases">
                        Databases
                       </option>
                       <option value="journals">
                        Journal Titles
                       </option>
                       <option value="scholar">
                        Google Scholar
                       </option>
                       <option value="libguides">
                        Guides
                       </option>
                       <option value="reserves">
                        Course Reserves
                       </option>
                      </select>
                     </span>
                     <input class="los-search" name="submit" type="submit" value="Search"/>
                    </input>
                   </form>
                   <p class="los-infos" id="extra-text">
                    <a href="https://arizona-asu-primo.hosted.exlibrisgroup.com/primo-explore/search?institution=01ASU_INST&amp;vid=01ASU&amp;search_scope=Everything&amp;mode=advanced&amp;lang=en_US">
                     Advanced Search
                    </a>
                   </p>
                   <script type="text/javascript">
                    // set default values
    var searchAction = "https://arizona-asu-primo.hosted.exlibrisgroup.com/primo-explore/search?query=any,contains,";
    var searchVars = "&institution=01ASU_INST&vid=01ASU&search_scope=Everything&mode=Basic&displayField=all&sortby=rank&lang=en_US";
    var searchInfo = "Search for journals, articles, books, magazines, and newspapers, all in one search.";
    var extraText = '<a href="https://arizona-asu-primo.hosted.exlibrisgroup.com/primo-explore/search?institution=01ASU_INST&vid=01ASU&search_scope=Everything&mode=advanced&lang=en_US">Advanced Search</a>';
    // grab the previous search if a user has gone back to this page
    var lastSearch = document.getElementById("searches").value;

    //Get documentElement
    var doc = document.documentElement;
    //Set the userAgent value of browser to data-useragent attribute
    doc.setAttribute('data-useragent', navigator.userAgent);
    //this is the value that we can use

    function libtabs_switch(lastSearch) {
        //var searchName = document.getElementById("searches").value;
        if (searchName == null){
            var searchName = document.getElementById("searches").value;
        } else {
            var searchName = lastSearch;
        }

        switch (searchName) {
            case "primo":
                searchAction = "https://arizona-asu-primo.hosted.exlibrisgroup.com/primo-explore/search?query=any,contains,";
                searchVars = "&institution=01ASU_INST&vid=01ASU&search_scope=Everything&mode=Basic&displayField=all&sortby=rank&lang=en_US";
                searchPlace = "Enter keywords...";
                searchInfo = "Search for journals, articles, books, magazines, and newspapers, all in one search.";
                extraText = '<a href="https://arizona-asu-primo.hosted.exlibrisgroup.com/primo-explore/search?institution=01ASU_INST&vid=01ASU&search_scope=Everything&mode=advanced&lang=en_US">Advanced Search</a>';
                break;

            case "catalog":
                searchAction = "https://arizona-asu-primo.hosted.exlibrisgroup.com/primo-explore/search?query=any,contains,";
                searchVars = "&institution=01ASU_INST&vid=01ASU&search_scope=default_scope&mode=Basic&displayField=all&sortby=rank&lang=en_US";
                searchPlace = "Enter keywords...";
                searchInfo = "Search the ASU Library Catalog.";
                extraText = '<a href="https://arizona-asu-primo.hosted.exlibrisgroup.com/primo-explore/search?institution=01ASU_INST&vid=01ASU&search_scope=default_scope&mode=advanced&lang=en_US">Advanced Search</a>';
                break;

            case "libguides":
                searchAction = "https://libguides.asu.edu/srch.php?q=";
                searchVars = "";
                searchPlace = "Search Library Guides...";
                searchInfo = "Best library resources for specific subjects and courses, created by ASU librarians.";
                extraText = '&nbsp;';
                break;

            case "journals":
                searchAction = "https://arizona-asu-primo.hosted.exlibrisgroup.com/primo-explore/jsearch?query=any,contains,&tab=jsearch_slot&institution=01ASU_INST&vid=01ASU&lang=en_US&offset=0&journals=any,";
                searchVars = "";
                searchPlace = "Enter title or ISSN";
                searchInfo = "Find specific online or print journals by title.";
                extraText = '&nbsp;';
                break;

            case "databases":
                searchAction = "https://libguides.asu.edu/az.php?q=";
                searchVars = "";
                searchPlace = "Search for Database Title";
                searchInfo = "Select specialized search engines to find articles and other materials.";
                extraText = '&nbsp;';
                break;

            case "scholar":
                searchAction = "https://scholar.google.com/scholar?hl=en&inst=1960582506653781529&q=";
                searchVars = "";
                searchPlace = "";
                searchInfo = "Broadly search for scholarly literature across many disciplines and types of sources.";
                extraText = '&nbsp;';
                break;

            case "reserves":
                searchAction = "https://arizona-asu-primo.hosted.exlibrisgroup.com/primo-explore/search?query=any,contains,";
                searchVars = "&tab=course_reserves&search_scope=Course_Reserves&vid=01ASU&lang=en_US&offset=0";
                searchPlace = "Item title, course number/name, instructor";
                searchInfo = "Find books and DVDs selected for a course by your instructor.";
                extraText = 'Looking for complete lists of reserves? Learn more <a href="/access/reserves">about ASU Library Reading Lists</a>.';
                break;
        }
        document.getElementById("search-info").innerHTML = searchInfo;
        document.getElementById("extra-text").innerHTML = extraText;
        document.getElementById("searchQueryTemp").placeholder = searchPlace;
    }

    function submitSearch() {
        if (lastSearch != null){
            libtabs_switch(lastSearch);
        }
        var searchTerm = document.getElementById('searchQueryTemp').value.replace(/[,]/g, " ");
        window.location = searchAction + encodeURIComponent(searchTerm) + searchVars;
    }
                   </script>
                  </div>
                 </div>
                </div>
                <div class="panel-separator">
                </div>
                <div class="panel-pane pane-views pane-random-bg no-title block">
                 <div class="block-inner clearfix">
