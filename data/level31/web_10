<!-- animation for menu icon -->
<script type="text/javascript">
 $(".mainmenu nav ul > li").on('mouseover', function() {
          //setTimeout(function changeImage() {
            $(".mainbg img").removeClass("opaque").eq($(this).index()).addClass("opaque");
          //}, 2400);
        });
        
        $('.search-close-wrapper').css('right', $('.navbar-rightset').width() - $('.aaum-search-close').width());

        $(".menu-desktop").children().on("click", function(e) {
          e.preventDefault;
          $('.menu-desktop .nav-icon').toggleClass('open');
          if($('.mainmenu').hasClass("opaque")){
            $('.mainmenu').removeClass("opaque");
            $('body').removeClass('modal-open');
          } else {
            $('.mainmenu').addClass("opaque");
            $('body').addClass('modal-open');
          }
          if($('.menutext1').hasClass('active')) {
            $('.menutext1.active').removeClass('active');
            $('.menutext2').addClass('active');
          } else {
            $('.menutext2.active').removeClass('active');
            $('.menutext1').addClass('active');
          }
        });

        
        $(".menu-mobile").children().on("click", function(e) {
          $('body').toggleClass('modal-open');
          $('html').removeClass('modal-open');
        });

        $('.canvas').on("click", function(e) {
          if($('.myNavmenu').hasClass('canvas-slid')) {
            $('.menu-mobile').trigger("click");
            $('body').removeClass('modal-open');
            $('html').removeClass('modal-open');
          }
        });

        $(".search-svg").on("click", function(e) {
          e.preventDefault;
          $(this).addClass('fade-out');
          $(".aaum-search-wrapper").addClass("aaum-search-wrapper-show");
          $('.aaum-suggestions-search').focus();
        });

        $(".aaum-search-close").on("click", function(e) {
          e.preventDefault;
          $(".aaum-search-wrapper").removeClass("aaum-search-wrapper-show");
          $(".search-svg").removeClass('fade-out');
        });

        $(function () {
          $('[data-toggle="popover"]').popover()
        });
</script>
