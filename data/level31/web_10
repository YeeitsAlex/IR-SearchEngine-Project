        <script type="text/javascript">
         //cdmay 20181205
function createCookie(name, value, days) {
    var date = new Date();
    var expires;
        if (days) {
            date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            expires = "; expires=" + date.toGMTString();
        }
        else expires = "";               

        document.cookie = name + "=" + value + expires + "; path=/";
}

function readCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') c = c.substring(1, c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
    }
    return null;
}

function eraseCookie(name) {
    createCookie(name, "", -1);
}

var data = {
    action: 'is_user_logged_in'
};
jQuery(document).ready(function() {
  jQuery('a').each(function() {
    var aURL = jQuery(this).attr("href");
    console.log('Found a link!');
    if (typeof aURL !== 'undefined') {
      console.log('The link is defined!');
      if (aURL.match(/^[%&_.0-z0-9-\/]*redirect_to=$/)) {
        console.log('Incomplete url found!');
        var currentURL = window.location.href;
        jQuery(this).attr('href', aURL + '' + currentURL);
      };
    };
  });
});



        jQuery(document).ready(function() {
          jQuery("a").each(function() {
            var aURL = jQuery(this).attr("href");
            if (typeof aURL !== 'undefined') {
              if (aURL.match(/^https:\/\/umalumni\.force\.com\/AAUMCustom[%&_.0-z0-9/-]*|^https:\/\/alumni\.umich\.edu\/wp\-login\.php[%&_.0-z0-9/-]*/)) {
              jQuery(this).attr('data-redirect', ''+aURL);
              jQuery(this).attr('href', 'https://umalumni.force.com/s/wp-login');
              };
            };
          });
          jQuery("a").click(function() {
            var dURL = jQuery(this).attr('data-redirect');
            console.log('Clicked link registered here: ' + dURL);
            if (dURL != "" && dURL != null) {
              eraseCookie('membershipTarget');
              createCookie('membershipTarget', dURL, 1);
              var cook = readCookie('membershipTarget');
              console.log('Since you are not logged in and are attempting to go to protected content, we are creating a cookie containing this URL: ' + cook + ' and instead taking you to a redirect page just before login.');
          };
        });
});



//end, cdmay 20181205
        </script>
