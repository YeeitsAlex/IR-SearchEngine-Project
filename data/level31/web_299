        <form action="https://performingarts.nd.edu/events/" id="blindeventsearch" method="get" style="display:inline-block;">
         <input id="category" name="eventtype" type="hidden" value="All"/>
         <input id="time" name="eventtime" type="hidden" value="2019-03-10"/>
         <input id="range" name="eventrange" type="hidden" value=""/>
         <input id="selecteddate" name="eventdate" type="hidden" value=""/>
         <input class="button black" style="margin-top: 1em;margin-bottom: 1em;" type="submit" value="GO"/>
        </form>
        <script type="text/javascript">
         jQuery(document).ready(function () {

                    //functionality pulled from scripts.js to pull available dates based on category
                    jQuery('#home-search-type .select-options p').click(function (e) {
                        var themeURL = WPURLS.themeurl;

                        var eventCategory = jQuery(this).data('keyword');

                        //AJAX to get available dates
                        jQuery.ajax( themeURL+"/pull-available-dates.php?category="+eventCategory )
                            .done(function(res) {
                                //console.log( res );
                                // An array of dates
                                /*var dates = res;*/
                                var dates = res.trim();
                                var dateSplit = dates.split(",");
                                var availableDays=[];
                                for (var i = 0; i < dateSplit.length; i++) {
                                    var temp = dateSplit[i].split('/');
                                    var newDateTimeFormat = temp[2]+"-"+temp[0]+"-"+temp[1];
                                    availableDays.push(newDateTimeFormat);
                                }
                                var uniqueDays = availableDays.filter(function(item, i, ar){ return ar.indexOf(item) === i; });
                                //var uniqueDays = new Set(availableDays);
                                //console.log(uniqueDays);

                                var dateToday = new Date();
                                jQuery( "#datepicker" ).datepicker('destroy');
                                jQuery( "#datepicker" ).datepicker(
                                    {
                                        minDate: dateToday,
                                        onSelect: function(dateText, inst) {
                                            var datePicked = jQuery(this).val();
                                            var dateAr = datePicked.split('/');
                                            var newDate = dateAr[2] + '-' + dateAr[0] + '-' + dateAr[1].slice(-2);
                                            jQuery("#home-search-time .select-selected").text(datePicked);
                                            jQuery('#blindeventsearch #time').val('');
                                            jQuery('#blindeventsearch #range').val('');
                                            jQuery('#blindeventsearch #selecteddate').val(newDate);
                                            jQuery('.select-options').removeClass('opened');

                                        },
                                        beforeShowDay: function (date) {
                                            var m = ('0' + (date.getMonth()+1)).slice(-2), d = ('0' + date.getDate()).slice(-2), y = date.getFullYear();
                                            //for (i = 0; i < uniqueDays.length; i++) {
                                                if (jQuery.inArray(y + '-' + m + '-' + d, uniqueDays) != -1) {
                                                    //return [false];
                                                    return [true, "event", 'Event today!'];
                                                }
                                            //}
                                            return [false, '', ''];
                                        }
                                    },
                                );

                            jQuery('.select-options.opened .ui-datepicker').addClass('active');

                            })



                        jQuery('#blindeventsearch #category').val(eventCategory);
                        var optionText = jQuery(this).text();
                        jQuery(this).siblings().removeClass('active');
                        jQuery(this).addClass('active');
                        jQuery(this).parent('.select-options').siblings('.select-selected').text(optionText);

                    });


                    //Set available date for calendar dropdown
                    

                    // An array of dates
                    var dates = '03/10/2019,03/15/2019,03/15/2019,03/16/2019,03/16/2019,03/16/2019,03/17/2019,03/19/2019,03/19/2019,03/20/2019,03/21/2019,03/22/2019,03/22/2019,03/22/2019,03/23/2019,03/23/2019,03/23/2019,03/23/2019,03/24/2019,03/24/2019,03/24/2019,03/24/2019,03/26/2019,03/27/2019,03/28/2019,03/29/2019,03/29/2019,03/29/2019,03/29/2019,03/30/2019,03/30/2019,03/30/2019,03/30/2019,03/30/2019,03/31/2019,03/31/2019,04/02/2019,04/03/2019,04/04/2019,04/04/2019,04/05/2019,04/05/2019,04/05/2019,04/05/2019,04/05/2019,04/06/2019,04/06/2019,04/06/2019,04/06/2019,04/07/2019,04/07/2019,04/07/2019,04/09/2019,04/10/2019,04/11/2019,04/11/2019,04/12/2019,04/13/2019,04/13/2019,04/14/2019,04/14/2019,04/14/2019,04/17/2019,04/18/2019,04/24/2019,04/28/2019,04/30/2019,05/02/2019,05/03/2019,05/04/2019,05/05/2019,05/11/2019,05/31/2019,06/01/2019,06/01/2019,06/02/2019,06/30/2019,';
                    var dateSplit = dates.split(",");
                    var availableDays=[];
                    for (var i = 0; i < dateSplit.length; i++) {
                        var temp = dateSplit[i].split('/');
                        var newDateTimeFormat = temp[2] + "-" + temp[0] + "-" + temp[1];
                        availableDays.push(newDateTimeFormat);
                    }
                    var uniqueDays = availableDays.filter(function(item, i, ar){ return ar.indexOf(item) === i; });
                    //var uniqueDays = new Set(availableDays);

                    //console.log(uniqueDays);

                var dateToday = new Date();
                jQuery( "#datepicker" ).datepicker(
                    {
                        minDate: dateToday,
                        onSelect: function(dateText, inst) {
                            var datePicked = jQuery(this).val();
                            var dateAr = datePicked.split('/');
                            var newDate = dateAr[2] + '-' + dateAr[0] + '-' + dateAr[1].slice(-2);
                            jQuery("#home-search-time .select-selected").text(datePicked);
                            jQuery('#blindeventsearch #time').val('');
                            jQuery('#blindeventsearch #range').val('');
                            jQuery('#blindeventsearch #selecteddate').val(newDate);
                            jQuery('.select-options').removeClass('opened');

                        },
                        beforeShowDay: function (date) {
                            var m = ('0' + (date.getMonth()+1)).slice(-2), d = ('0' + date.getDate()).slice(-2), y = date.getFullYear();
                            //for (i = 0; i < uniqueDays.length; i++) {
                                if (jQuery.inArray(y + '-' + m + '-' + d, uniqueDays) != -1) {
                                    //return [false];
                                    return [true, "event", 'Event today!'];
                                }
                            //}
                            return [false, '', ''];
                        }
                    },
                );

                });
        </script>
