          <script>
           //Alt tag injector script
	$(document).ready(function() {
		$("img").each(function () {
			var img = $(this);
			if (img.attr("class") == "asu_touch") {
				img.attr("alt", "asu touch"); 
			} else if (img.attr("alt") == "") {
				img.attr("alt", "null")
			} else if (img.attr("class") == "image-hero") 
				img.attr("alt", "Hero image")
		});
	});
	
	// Sets ARIA role for hero section
	$(document).ready(function() {
		$("section").each(function () {
			var section = $(this); 
			if (section.attr("class") == "hero-ratio  ") {
				section.attr("role", "banner"); 
			}
		});
	});
          </script>
