		var eventCallback;
		if (typeof newWindow === "undefined" || newWindow === false) {
			eventCallback = function(){document.location = url;};
		} else {
			eventCallback = function(){};
		}
		gtag('event', 'click', {
			'event_category': 'outbound',
			'event_label': url,
			'transport_type': 'beacon',
			'event_callback': eventCallback
		});
	}
