 <script>
  (function() {
  function dayOfYear(dt) {
    var today = new Date(dt.getFullYear(), dt.getMonth(), dt.getDate());
    var startYear = new Date(dt.getFullYear(), 0, 0);
    var oneDayInMS = 1000.0 * 60 * 60 * 24;
    var dateDiffInMs = Number(today) - Number(startYear);
    return parseInt(Math.floor(dateDiffInMs / oneDayInMS));
  }
  function getLeadPhotoByDayOfYear() {
    var photos = ['holi-festival', 'sunrise-west', 'basketball', 'deneve', 'hitch-sunset'];
    var daysToShowPhotos = 3;
    var now = new Date();
    var photoNumber = Math.round(dayOfYear(now) / daysToShowPhotos) % photos.length;
    return photos[photoNumber];
  }
  function setIndexLeadPhoto() {
    var photoName = getLeadPhotoByDayOfYear();
    document.body.classList.add('index-lead-' + photoName);
  }
  setIndexLeadPhoto();
})();
 </script>
