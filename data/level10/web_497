   <script>
    var category = '1';
 $(document).ready(function(){
	 /*
$('.slide-image-link').blur(function(){
	$(this).children("img").css('outline','none');
});

	 alert($('#left').height());
	$('#right').css('height', $('#left').height());
	 */
// when a category button is pressed, pull the appropriate nodes
$('.category-button').click(function(){
category = $(this).attr('id');
		$("#results").hide().load("/sites/all/themes/ucistories/templates/includes/browse_category.php?category=" + category).fadeIn('1000');
		$('#category-select').val(category);
		$(".category-button").removeClass("active");
		$(this).addClass("active");
});

// when a category is selected (small screens), pull the appropriate nodes
$('#category-select').change(function(){
category = $('#category-select').val();
		$("#results").hide().load("/sites/all/themes/ucistories/templates/includes/browse_category.php?category=" + category).fadeIn('1000');
});

// On page load, grab the default category
$.get( "/sites/all/themes/ucistories/templates/includes/browse_category.php?category=" + category, function( data ) {
  $( "#results" ).html( data );
});

$('.feature-list').slick({
  infinite: true,
 // fade: true,
  cssEase: 'linear',
  slidesToShow: 1,
  slidesToScroll: 1,
  prevArrow: $('#feature-prev'),
  nextArrow: $('#feature-next'),
  dots: true,
//  asNavFor: '.excerpt',
  focusOnSelect: true,
  accessibility:true,
  responsive: [
  {
  breakpoint: 992,
  settings: {
	    fade: false
  }
  }
  ]
});

 });
   </script>
