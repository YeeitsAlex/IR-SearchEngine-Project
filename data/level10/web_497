   $(document).ready(function() {
$('#nav-right').find('a').each(function(index) {
    var linkText = $(this).text();
    $(this).click(function() {
        ga('send', { hitType: 'event', eventCategory: 'UCI Stories Nav', eventAction: 'Click', eventLabel: ''+linkText+'' });
    });
});
$('#category-buttons').find('button').each(function(index) {
    var linkText = $(this).text();
    $(this).click(function() {
        ga('send', { hitType: 'event', eventCategory: 'UCI Stories Category Buttons', eventAction: 'Click', eventLabel: ''+linkText+'' });
    });
});
$('#results').find('a').each(function(index) {
    var linkText = $(this).find('span').text();
    $(this).click(function() {
        ga('send', { hitType: 'event', eventCategory: 'Result Video', eventAction: 'Click', eventLabel: ''+linkText+'' });
    });
});
$('#node-19').find('a').each(function(index) {
    var linkText = $(this).text();
    $(this).click(function() {
        ga('send', { hitType: 'event', eventCategory: 'Interviews Links', eventAction: 'Click', eventLabel: ''+linkText+'' });
    });
});
$('#node-type-interview').find('iframe').each(function(index) {
    var linkText = $(this).attr('title');
    $(this).click(function() {
        ga('send', { hitType: 'event', eventCategory: 'Video iframe', eventAction: 'Click', eventLabel: ''+linkText+'' });
    });
});
});
