  <script type="text/javascript">
   jQuery(document).ready(function()
{
	// make sure that all off-site links open in a new window
	jQuery("a[href^='http:']").not("[href*='']")
		.attr('target','_blank');
		//.filter(':not(.ignoreOffsiteLink)')
		//.toggleClass('offsiteLink');

	// make the mid section boxes the same height
	if ( jQuery('#midSection .midSectionBox').length > 0 )
	{
		var midHeight1 = jQuery('#midSection #msbox1 .widget-container:first-child').height();
		var midHeight2 = jQuery('#midSection #msbox2 .widget-container:first-child').height();
		var maxHeight = Math.max(midHeight1, midHeight2);
		jQuery('#midSection #msbox1 .widget-container:first-child').height(maxHeight);
		jQuery('#midSection #msbox2 .widget-container:first-child').height(maxHeight);
	}

	// this is my hack to horizontally center the saga, since I'm using display:table-cell to vertical center
	jQuery('#home-page-saga div').width(jQuery('#home-page-saga').width());

});
  </script>
