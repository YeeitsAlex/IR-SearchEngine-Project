  <script language="javascript" type="text/javascript">
   var url = window.location.href,
		  redirectDomains,
		  urlMatches,
		  appPrefix = "",
		  pathSuffix = "",
		  redirectRegex;

	  // trim out index.html if need be
      if (url.match(/index\.html$/)) {
        url = url.replace("index.html", "");
      }
	  
      // ensure the URL ends with a trailing slash.
      if (!url.match(/\/$/)) {
        url = url + "/";
      }

      // configure TDClient redirects
      // be sure to escape periods with \.
      redirectDomains = [
      'helpdesk\.fau\.edu',
      'fau\.teamdynamix\.com',
      'roadscholar\.teamdynamix\.com',
      'solutions\.teamdynamix\.com',
	  'support\.csuchico\.edu',
      'technology\.uwrf\.edu',
	  'help\.pointloma\.edu',
	  'helpdesk\.broward\.edu',
	  'bellevuecollege\.teamdynamix\.com',
	  'servicecenter\.twu\.edu',
	  'service\.uoregon\.edu',
	  'gethelp\.ddn\.com',
	  'servicedesk\.nscc\.ca',
	  'support\.dsu\.edu',
	  'support\.dom\.edu',
	  'ramsupport\.wssu\.edu',
	  'ask\.pgcc\.edu',
	  'support\.uidaho\.edu',
	  'helpdesk\.troy\.edu',
	  'tigertracks\.isu\.edu',
	  'services\.metrostate\.edu',
	  'govstech\.apsu\.edu',
	  'cabrillo\.teamdynamix\.com',
	  'csum\.teamdynamix\.com',
	  'foriowa\.teamdynamix\.com',
	  'servicedesk\.aub\.edu\.lb',
	  'services\.dartmouth\.edu',
	  'helpdesk\.cqc\.com',
	  'colby\.teamdynamix\.com'
      ];

      urlMatches = "(" + redirectDomains.join('|') + ")";

      // redirect to client portal as necessary
      var redirectRegex = new RegExp("^https:\/\/" + urlMatches + pathSuffix + "\/$", "i");
      if (redirectRegex.test(url) == true) {
        window.location.href = url.replace(new RegExp(pathSuffix, "i"), "") + appPrefix + "TDClient/Home/";
      }
  </script>
