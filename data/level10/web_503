        function base_photo_hover() {
            // Photo Hover
            jQuery('.image-zoom, .image-link').unbind().hoverIntent({
                over: function () {
                    $('img', jQuery(this)).stop().animate({
                        opacity: 0.4
                    }, 150);
                },
                out: function () {
                    jQuery('img', $(this)).stop().animate({
                        opacity: 1
                    }, 150);
                }
            });
        }
    });
	
jQuery(window).load(function () {
	var thisopts=$("#Effect_21_LinkAndZoom42439 li"),
