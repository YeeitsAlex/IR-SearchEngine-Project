 $(document).ready(function() {
        $('#search_fa').click(function() {
            $('#searchli_mobile').toggle();
        });
    });
    $(document).ready(function() {
        $('#fa-search_1').click(function() {
            $('#searchli_mobile').toggle();
        });
    });
    $(document).ready(function() {
        $('#replace_off_1').click(function() {
            $('#searchli_mobile').toggle();
        });
    });
    $(document).ready(function() {
        $('#replace_off_2').click(function() {
            $('#searchli_mobile').toggle();
        });
    });
    $(document).ready(function() {
        $('#mobileNav').click(function() {
            $('.mobileNavMenu').toggle();
        });
    });
    $(document).ready(function() {
        $('#desaparacedo').click(function() {
            $('.mobileNavMenu').toggle();
        });
    });
    $(document).mouseup(function(e) {
        var container = $("#mobile_nav");
        // if the target of the click is not the container nor a descendant of the container
        if (!container.is(e.target) && container.has(e.target).length === 0) {
            container.hide();
        }
    });
</script>
<script>
 $(document).ready(function() {
        $(".option-content").hide();
        $(".arrow-up").hide();
        $(".option-heading").click(function() {
            $(this).find(".arrow-up, .arrow-down").toggle();
        });
    });
</script>
