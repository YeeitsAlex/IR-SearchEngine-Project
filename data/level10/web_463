    body.iphone .flex_caption1 a.slide_btn {display:none;}
    body.iphone .flex_caption1 a.slide_btn:first-of-type {width: 80%; display:block; font-size: 1.5em; float:left;}

    #descriptiveText div p {
        line-height: 1.25em;}

    .tg_list-item img {
        width: 100%;
    }
      </style>
     </link>
    </meta>
   </meta>
  </meta>
