         <header class="entry-header">
          <h2 class="page-title">
           Home
          </h2>
         </header>
         <!-- .entry-header -->
         <div class="entry-content">
          <div class="panel-layout" id="pl-13">
           <div class="panel-grid panel-has-style" id="pg-13-0">
            <div class="siteorigin-panels-stretch panel-row-style panel-row-style-for-13-0" data-stretch-type="full-stretched">
             <div class="panel-grid-cell" id="pgc-13-0-0">
              <div class="so-panel widget widget_sow-hero panel-first-child panel-last-child" data-index="0" id="panel-13-0-0-0">
               <div class="hero-video-background panel-widget-style panel-widget-style-for-13-0-0-0">
                <div class="so-widget-sow-hero so-widget-sow-hero-default-ca3dee1b9725 so-widget-fittext-wrapper" data-fit-text-compressor="0.85">
                 <div class="sow-slider-base " style="display: none">
