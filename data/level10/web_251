// Comma separated list of images to rotate 
var imgs = new Array('http://arweb.sdsu.edu/es/images/veterans/home_14.png', 'http://arweb.sdsu.edu/es/images/veterans/home_17.png','http://arweb.sdsu.edu/es/images/veterans/home_13.png', 'http://arweb.sdsu.edu/es/images/veterans/home_12.png', 'http://arweb.sdsu.edu/es/images/veterans/home_15.png', 'http://arweb.sdsu.edu/es/images/veterans/home_16.png','http://arweb.sdsu.edu/es/images/veterans/home_1_v2.png', 'http://arweb.sdsu.edu/es/images/veterans/home_11_v2.png', 'http://arweb.sdsu.edu/es/images/veterans/home_5_v2.png', 'http://arweb.sdsu.edu/es/images/veterans/home_8_v2.png');
// delay in milliseconds between image swaps 1000 = 1 second 
var delay = 5000;
var counter = 0;

function preloadImgs(){
  for(var i=0;i<imgs.length;i++){
    MM_preloadImages(imgs[i]);
  }
}

function randomImages(){
  if(counter == (imgs.length)){
    counter = 0;
  }
  MM_swapImage('rotator', '', imgs[counter++]);
  setTimeout('randomImages()', delay);
}
