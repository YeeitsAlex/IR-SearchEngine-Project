         <div class="search_left">
          <label class="search_header" for="dpgMyPage$ctl06$txtSearch">
           Keyword Search
          </label>
          <div id="ncNews_newscenterHeader_pnlSearchBoxSublinksSpacesTop">
           <script type="text/javascript">
            var searchText = "search";

        $(document).ready(function() {
            var textBox = $("#ncNews_newscenterHeader_ctl00_txtSearch");

            textBox.attr('value', searchText);
            textBox.click(function() {
                var txt = $("#ncNews_newscenterHeader_ctl00_txtSearch");

                if (txt.attr("value") == searchText) {
                    txt.attr("value", "");
                }
            });
            textBox.blur(function() {
                var txt = $("#ncNews_newscenterHeader_ctl00_txtSearch");

                if (txt.attr("value") == "") {
                    txt.attr("value", searchText);
                }
            });
        });
           </script>
           <div class="search" id="ncNews_newscenterHeader_ctl00_pnlSearch" onkeypress="javascript:return WebForm_FireDefaultButton(event, 'ncNews_newscenterHeader_ctl00_btnSearch')">
