     $(document).ready(function () {

             /* Story Popups*/
             $(".storyLinkLarge").mouseover(function (e) {

                 e.preventDefault();

                 var largeID = $(this).attr("id").split("_")[0];
                 $("#" + largeID + "_large").show();
                 $("#" + largeID + "_largeTitle").hide();
                 $("#" + largeID + "_largeGradient").attr("class", "noGradient");

             });

             $(".largeTitle").mouseover(function (e) {

                 e.preventDefault();

                 var largeID = $(this).attr("id").split("_")[0];
                 $("#" + largeID + "_large").show();
                 $("#" + largeID + "_largeTitle").hide();
                 $("#" + largeID + "_largeGradient").attr("class", "noGradient");

             });

             $(".largePopup").mouseleave(function (e) {

                 e.preventDefault();

                 var largeID = $(this).attr("id").split("_")[0];
                 $("#" + largeID + "_large").hide();
                 $("#" + largeID + "_largeTitle").show();
                 $("#" + largeID + "_largeGradient").attr("class", "largeGradient");

             });

             $(".storyLinkMedium").mouseover(function (e) {

                 e.preventDefault();

                 var mediumID = $(this).attr("id").split("_")[0];
                 $("#" + mediumID + "_medium").show();
                 $("#" + mediumID + "_mediumTitle").hide();
                 $("#" + mediumID + "_mediumGradient").attr("class", "noGradient");

             });

             $(".mediumTitle").mouseover(function (e) {

                 e.preventDefault();

                 var mediumID = $(this).attr("id").split("_")[0];
                 $("#" + mediumID + "_medium").show();
                 $("#" + mediumID + "_mediumTitle").hide();
                 $("#" + mediumID + "_mediumGradient").attr("class", "noGradient");

             });

             $(".mediumPopup").mouseleave(function (e) {

                 e.preventDefault();

                 var mediumID = $(this).attr("id").split("_")[0];
                 $("#" + mediumID + "_medium").hide();
                 $("#" + mediumID + "_mediumTitle").show();
                 $("#" + mediumID + "_mediumGradient").attr("class", "mediumGradient");

             });

             $(".storyLinkSmall").mouseover(function (e) {

                 e.preventDefault();

                 var smallID = $(this).attr("id").split("_")[0];
                 $("#" + smallID + "_small").show();
                 $("#" + smallID + "_smallTitle").hide();
                 $("#" + smallID + "_smallGradient").attr("class", "noGradient");
             });

             $(".smallTitle").mouseover(function (e) {

                 e.preventDefault();

                 var smallID = $(this).attr("id").split("_")[0];
                 $("#" + smallID + "_small").show();
                 $("#" + smallID + "_smallTitle").hide();
                 $("#" + smallID + "_smallGradient").attr("class", "noGradient");

             });

             $(".smallPopup").mouseleave(function (e) {

                 e.preventDefault();

                 var smallID = $(this).attr("id").split("_")[0];
                 $("#" + smallID + "_small").hide();
                 $("#" + smallID + "_smallTitle").show();
                 $("#" + smallID + "_smallGradient").attr("class", "smallGradient");

             });

             // Filter Tablet
             $("#filter_tablet").toggle(function (e) {
                 e.preventDefault();
                 $(".keyword_tablet").show();
             }, function (e) {
                 e.preventDefault();
                 $(".keyword_tablet").hide();
             });

             // Filter Mobile
             $("#filter_mobile").toggle(function (e) {
                 e.preventDefault();
                 $(".keyword_mobile").show();
             }, function (e) {
                 e.preventDefault();
                 $(".keyword_mobile").hide();
             }); 

         var prm = Sys.WebForms.PageRequestManager.getInstance();

         prm.add_endRequest(function () {
             $(".storyLinkSmall").mouseover(function (e) {

                 e.preventDefault();

                 var smallID = $(this).attr("id").split("_")[0];
                 $("#" + smallID + "_small").show();
                 $("#" + smallID + "_smallTitle").hide();

