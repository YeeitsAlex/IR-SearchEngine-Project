      <script type="text/javascript">
       $(document).ready(function () {

                /* Browser Links TOP SEARCH*/
                $(".nav_link_across_search").mouseover(function (e) {

                    e.preventDefault();

                    $(".sublinks_search_top").show();
                    $("#searchtop_submenu").show();
                });

                $(".nav_link_across_search").click(function (e) {
                    e.preventDefault();
                });

                $(".page_nav_container_top").mouseleave(function (e) {
                    $(".sublinks_search_top").hide();
                    $("#searchtop_submenu").hide();
                });

                /* Mobile Links */

                $(".mobile_search_link").toggle(function (e) {
                    e.preventDefault();
                    $(".mobile_search").show();
                }, function (e) {
                    e.preventDefault();
                    $(".mobile_search").hide();
                });

                $(".mobile_category_link").toggle(function (e) {
                    e.preventDefault();
                    $(".mobile_category").show();
                }, function (e) {
                    e.preventDefault();
                    $(".mobile_category").hide();
                });

         });
      </script>
     </div>
