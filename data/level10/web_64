  <script language="javascript" type="text/javascript">
   $(document).ready(
		function()
		  {
			var handler = 
				function(event)
				{
					var elem = $(event.target);
					var parent = elem.closest('.highlight');
					var postData = {};
					postData['data'] = elem.attr('data-href');
					if(typeof postData['data'] != 'string')
					{
						console.log('unable to find data-href attribute');
						return;
					}
					parent.load('/resources/highlight_partial.php', postData,
						function() {
							$(this).find('area').click(handler);
						}
					);
				};
			
			$('area').unbind('click').click(handler);
		  }
	 );
  </script>
