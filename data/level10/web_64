   $(document).ready(function () {
                        $('#top_menu > li').hover(
                            function openSubMenu() {
                                $(this).find('ul').css('display', 'inherit');
                            },
                            function closeSubMenu() {
                                $(this).find('ul').css('display', 'none');
                            }
                            );
                        var menu_buttons = $('#top_menu > li');
                        var btns = menu_buttons.length;
                        var ctrw = $('#topNavMenu .main-width').width()-2*btns;
                        var btw = Math.floor(ctrw/btns);
                        console.log('number of buttons: ' + btns + '; width: ' + ctrw + '; button width: ' + btw);
                        //menu_buttons.css('min-width', btw);
                        console.log(menu_buttons.css('min-width'));                     
                    });
  </script>
