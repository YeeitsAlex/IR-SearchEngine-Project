            <script language="JavaScript">
             function ar_hov(matchClass) {
    var elems = document.getElementsByTagName('*'),i;
    for (i in elems) {
        if((" "+elems[i].className+" ").indexOf(" "+matchClass+" ") > -1) {
            elems[i].style.backgroundImage="url(/_images/nav_arrows_hover.png)";
            }
        }
    }
function ar_nohov(matchClass) {
    var elems = document.getElementsByTagName('*'),i;
    for (i in elems) {
        if((" "+elems[i].className+" ").indexOf(" "+matchClass+" ") > -1) {
            elems[i].style.backgroundImage="url(/_images/nav_arrows.png)";
            }
        }
    }
var x='nav_interior_indent';
            </script>
