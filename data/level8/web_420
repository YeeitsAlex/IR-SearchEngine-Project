  <script>
   var menuOpacity = "0.95";
            // <![CDATA[
// 
            
            var menuToggled = false;
            
            window.onresize = function(event) {
                if($(window).width() <= 767 && menuToggled) {
                    $(".btn-menu-row:first-child").css({
                        "margin-bottom": "-8px"
                    });
                }
                else if($(window).width() <= 767 && !menuToggled) {
                    $(".btn-menu-row:first-child").css({
                        "margin-bottom": "4px"
                    });
                }
                else if($(window).width() > 767 && menuToggled) {
                    $(".btn-menu-row:first-child").css({
                        "margin-bottom": "-15px"
                    });
                }
                else if($(window).width() > 767 && !menuToggled) {
                    $(".btn-menu-row:first-child").css({
                        "margin-bottom": "7px"
                    });
                }
            }
            
            $("#btn-menu").click(
                function() {
                    if(!menuToggled) {
                        $("body").css({"overflow":"hidden"});
                        var degrees = 45;
                        $(".btn-menu-row:nth-child(2)").css({"opacity": "0"});
                        $(".btn-menu-row:first-child").css({'-webkit-transform' : 'rotate('+ degrees +'deg)',
                        '-moz-transform' : 'rotate('+ degrees +'deg)',
                        '-ms-transform' : 'rotate('+ degrees +'deg)',
                        'transform' : 'rotate('+ degrees +'deg)'});
                        if($(window).width() <= 767) {
                            $(".btn-menu-row:first-child").css({
                                "margin-bottom": "-8px"
                            });
                        }
                        else {
                            $(".btn-menu-row:first-child").css({
                                "margin-bottom": "-15px"
                            });
                        }
                        $(".btn-menu-row:last-child").css({'-webkit-transform' : 'rotate(-'+ degrees +'deg)',
                        '-moz-transform' : 'rotate(-'+ degrees +'deg)',
                        '-ms-transform' : 'rotate(-'+ degrees +'deg)',
                        'transform' : 'rotate(-'+ degrees +'deg)'});
                        menuToggled = true;
                        $("#nav-main").css({"display": "block"});
                        $("#nav-main").animate({"opacity": menuOpacity}, 200);
                        setTimeout(function() {
                            $(".menu-items-container").animate({"opacity": "1"}, 200);
                        }, 200);
                    }
                    else {
                        $("body").css({"overflow":"visible"});
                        $(".btn-menu-row:nth-child(2)").css({"opacity": "1"});
                        $(".btn-menu-row:first-child").css({'-webkit-transform' : 'rotate(0deg)',
                        '-moz-transform' : 'rotate(0deg)',
                        '-ms-transform' : 'rotate(0deg)',
                        'transform' : 'rotate(0deg)'});
                        if($(window).width() <= 767) {
                            $(".btn-menu-row:first-child").css({
                                "margin-bottom": "4px"
                            });
                        }
                        else {
                            $(".btn-menu-row:first-child").css({
                                "margin-bottom": "7px"
                            });
                        }
                        $(".btn-menu-row:last-child").css({'-webkit-transform' : 'rotate(0deg)',
                        '-moz-transform' : 'rotate(0deg)',
                        '-ms-transform' : 'rotate(0deg)',
                        'transform' : 'rotate(0deg)'});
                        menuToggled = false;
                        $(".menu-items-container").animate({"opacity": "0"}, 200);
                        setTimeout(function() {
                            $("#nav-main").animate({"opacity": "0"}, 200);
                        }, 200);
                        setTimeout(function() {
                            $("#nav-main").css({"display": "none"});
                        }, 400);
                    }
                }
            );
            $("#nav-main").on('touchmove', function(e)
            {
                if (menuToggled)
                {
                    e.preventDefault();
                }
            });
            $(".submenu-toggle").click(function(e) {
                e.preventDefault();
                $(this).parent().find("ul").slideToggle();
            });


//
// ]]>
  </script>
