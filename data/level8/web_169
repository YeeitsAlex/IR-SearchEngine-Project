    $(document).ready(function () {
            
  $('.bxslider').bxSlider({
    autoStart: true,
    controls:false,
    auto:true,
    pager: false
    
});
  
 $('input.gsc-search-button[type="submit"]').attr("alt", "Search"); 
  
  $('.gsc-branding-img').attr("alt", "Google"); 
  
