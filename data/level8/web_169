    // Add the "focus" value to class attribute 
  $('ul#menu-top-navigation li').focusin( function() {
    $(this).addClass('focus');
  }
  );

  // Remove the "focus" value to class attribute 
  $('ul#menu-top-navigation li').focusout( function() {
    $(this).removeClass('focus');
  }
  );
});
   </script>
   <script type="text/javascript">
    $(function(){
	fixLastItemPosition();
});
$( window ).resize(function() {
  fixLastItemPosition();
});
function fixLastItemPosition(){
	/*	
	----------------------------------------------
	BEGIN Last nav item adjustment - push last item 
	to the right so it doesn't fall off the page
	----------------------------------------------
	TODO: 1) Make this a more dynmaic process based on screen size
	TODO: 2) Check that second to last nav doesn't need similar treatment
	TODO: 3) Review potential issues with responsive layouts
	*/
	var subNavWidth = $("header nav ul li:last-child ul").width();
	var style = $('<style>@media (min-width: 850px) {.lastTabContainer { left: ' + subNavWidth * (-.65) + 'px !important; } .lastTab:after { left: ' + subNavWidth * (.75) + 'px !important;  } }</style>');
	$('html > head').append(style);
	
	$('header nav ul li:last-child ul').addClass('lastTabContainer');
	$('header nav ul li:last-child ul').find('li:first-child > a').addClass('lastTab');
	/*	
	----------------------------------------------
	END Last nav item adjustment
	----------------------------------------------
	*/
}
   </script>
