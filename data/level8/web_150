function windowOpen(window_url) {
   helpWin = window.open(window_url,'','toolbar=yes,status=no,scrollbars=yes,menubar=yes,resizable=yes,directories=no,location=no ');
   if (document.images) { 
       if (helpWin) helpWin.focus()
   }
}
