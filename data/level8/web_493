        <script type="text/javascript">
         //<![CDATA[
$(window).load(function(){
var $one = $("#searchSdsuLibraryOnePrimo");
var $book = $("#searchSdsuBooksMediaPrimo");
var $article = $("#searchSdsuLibraryArticlesPrimo");
var $journal = $("#searchSdsuJournalsPrimo");
var $database = $("#searchSdsuDatabasesPrimo");
var $archive = $("#searchSdsuArchivesManuscriptsPrimo");
var $website = $("#searchSdsuLibraryWebsitePrimo");

$("#searchSdsuLibraryAllPrimo").keyup(function() {
    $one.val(this.value);
    $book.val(this.value);
    $article.val(this.value);
    $journal.val(this.value);
    $database.val(this.value);
    $archive.val(this.value);
    $website.val(this.value);
});
$("#searchSdsuLibraryAllPrimo").blur(function() {
    $one.val(this.value);
    $book.val(this.value);
    $article.val(this.value);
    $journal.val(this.value);
    $database.val(this.value);
    $archive.val(this.value);
    $website.val(this.value);
});
