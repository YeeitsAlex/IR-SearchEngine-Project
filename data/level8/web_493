   <div class="container">
    <div class="block block-system" id="block-system-main">
     <div about="/front-page" class="node node-general-library-page node-promoted clearfix" id="node-62" typeof="sioc:Item foaf:Document">
      <span class="rdf-meta element-hidden" content="Front Page" property="dc:title">
      </span>
      <div class="content">
       <div class="space40">
       </div>
       <h1 class="text-center" style="color:#000">
        Student Success Zones
       </h1>
       <div class="space40">
       </div>
       <div class="col-sm-4">
        <div class="item-box-2">
         <div class="media-container">
          <a href="https://buildit.sdsu.edu/">
           <img alt="table with books" src="/sites/all/themes/riley/img/thumb-buildit_sml.jpg"/>
          </a>
         </div>
         <div class="info-container text-center">
          <h2>
           <a href="https://buildit.sdsu.edu/">
            build IT
           </a>
          </h2>
          <div class="space10">
          </div>
          <!-- Description goes here -->
          <div class="space10">
