   $(function() {
            $('img.prPlaceholder').each(function() {
                var phImage = $(this); 
                var galleryID = phImage.attr('galleryID');
                var photoID = phImage.attr('photoID');
                var height = phImage.attr('height');
                var width = phImage.attr('width');
                var ajaxUrl = '/CMSControls/AjaxPhotoRotator.aspx?GalleryID=' + galleryID;
                ajaxUrl = ajaxUrl + '&PhotoID=' + photoID + '&width=' + width + '&height=' + height;

                $.ajax({ url: ajaxUrl, cache: false, success: function(data, textStatus) {
                    $(phImage).replaceWith(data);
                }
                });
            });
            LoadFeeds();
        });
  </script>
