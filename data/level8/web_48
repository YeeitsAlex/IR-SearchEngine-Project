/* Submenu on DOT Pages */


/* define a fixed width for the entire menu */
.dot-navigation {
  width: 300px;
}


/* make ALL links (main and submenu) have padding and background color */
.dot-mainmenu a {
  display: block;
  background-color: #fff; /* White background color */
  text-decoration: none;
  padding: 10px;
  color: #000;
  border-bottom: 1px solid #e5e5e5; /* lines between menu items */
}

/*hover behaviour */
.dot-mainmenu a:hover {
    background-color: #eaeaea;
}

/* active menu */
.dot-mainmenu a.active {
    background-color: #eaeaea; /* Add a blue color to the "active/current" link */
    color: black;
}


/* when hovering over a .dot-mainmenu item, display the submenu inside it. Submenu max-height 200px*/

.dot-mainmenu li:hover .dot-submenu {
  display: block;
  max-height: 200px;
}

/*Overwrite background-color for .submenu links only.*/

.dot-submenu a {
  background-color: #f9f9f9;
  padding-left: 20px;
}

/* hover behaviour for links inside .submenu */
.dot-submenu a:hover {
  background-color: #eaeaea;
}



