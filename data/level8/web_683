  <article class="entry post-3 page type-page status-publish has-post-thumbnail hentry et-bg-layout-dark" id="post-3" style="background-color: #003c6c;">
   <div class="container clearfix">
    <header class="entry-title">
     <h1>
      Games M.S. Programs at UCSC Games + Playable Media and Serious Games
     </h1>
    </header>
   </div>
   <!-- .container -->
  </article>
  <!-- .entry-->
  <article class="et-additional-content">
   <div id="breadcrumbs">
    <div class="container clearfix">
     <div id="breadcrumbs-links">
      <a class="breadcrumbs_home" href="https://gpm.soe.ucsc.edu/">
       Home
      </a>
      <span class="raquo">
       »
      </span>
     </div>
     <!-- #breadcrumbs-links -->
     <div id="et-search-form">
