      function startGallery() {
			var myGallery = new gallery($('myGallery'), {
				timed: true,
				showArrows: true,
				showInfopane: true,
				showCarousel: false,
				embedLinks: false,
				delay: 15000
			});
			document.gallery = myGallery;
