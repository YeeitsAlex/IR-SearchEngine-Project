           <section class="elementor-element elementor-element-726d24bc elementor-section-content-middle elementor-section-boxed elementor-section-height-default elementor-section-height-default elementor-section elementor-top-section" data-element_type="section" data-id="726d24bc">
            <div class="elementor-container elementor-column-gap-default">
             <div class="elementor-row">
              <div class="elementor-element elementor-element-1e14ba0e elementor-column elementor-col-100 elementor-top-column" data-element_type="column" data-id="1e14ba0e">
               <div class="elementor-column-wrap elementor-element-populated">
                <div class="elementor-widget-wrap">
                 <div class="elementor-element elementor-element-17ac1344 elementor-widget elementor-widget-shortcode" data-element_type="shortcode.default" data-id="17ac1344">
                  <div class="elementor-widget-container">
                   <div class="homeblog">
                   </div>
                   <script>
                    window.onload = function() {
		$.ajax({
			dataType: "json",
			// Change the tag or category_name to match the responses you would like.
			// An example using tags
			//url: "https://source.colostate.edu/wp-json/wp/v2/posts/?filter[tag]=college-of-engineering",
			url: "https://engr.source.colostate.edu/wp-json/wp/v2/posts/",
			type: "GET"
		})
		.fail(function(){
			console.log("Failed to get the news from source.colostate.edu");
		})
		.done(function(response) {
			// Building an article div with the title, excerpt and link to the live version.
			var divBody = "";
			// number of articles to return
			count = 4;
			for (var i=0; i < count; i++) {
				article = response[i];
				divBody+= "<div class='sourcewrapper'>";
				divBody += "<div class='image'><img src='" +article.featured_image.source_url+"' alt='" +article.title.rendered+ "'/></div>";
				divBody += "<div class='articlewrapper'>";
				divBody += "<h3><a target=_blank href='"+article.link+"'>"+article.title.rendered+"</a></h3>";
				divBody += "<p>"+article.excerpt.rendered+" <a target=_blank href='"+article.link+"' aria-label='"+article.title.rendered+"'>Read more&hellip;</a></p>";
				divBody += "</div>";
				divBody += "</div>";
			};

			// Take our four created divs and put it into the above div
			$(".homeblog").html(divBody);
		});
	};
                   </script>
                   <div class="elementor-shortcode">
                   </div>
                  </div>
                 </div>
                 <div class="elementor-element elementor-element-439058ab elementor-widget elementor-widget-heading" data-element_type="heading.default" data-id="439058ab">
                  <div class="elementor-widget-container">
                   <h4 class="elementor-heading-title elementor-size-default">
