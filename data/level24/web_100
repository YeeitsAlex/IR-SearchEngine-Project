		function addOnloadEvent(fnc)
		{
			if(typeof window.addEventListener != "undefined")
				window.addEventListener("load", fnc, false);
			else if(typeof window.attachEvent != "undefined")
			{
				window.attachEvent("onload", fnc);
			}
			else
			{
				if(window.onload!=null)
				{
					var oldOnload=window.onload;
					window.onload=function(e)
					{
						oldOnload(e);
						window[fnc]();
					};
				}
				else 
					window.onload = fnc;
			}
		}

		addOnloadEvent(function()
		{
			PIE.attach(document.getElementById('topContent'));
			tcc=document.getElementById('topNav').children;
			for(i=0;i<tcc.length;++i)
				if(tcc[i].nodeName=="UL")
				{
					tcca=tcc[i].getElementsByTagName("a");
					for(j=0;j<tcca.length;++j)
						PIE.attach(tcca[j]);
				}

			if(document.getElementById('frontPage'))
			{
				fpca=document.getElementById('frontPage').getElementsByTagName("a");
				for(j=0;j<fpca.length;++j)
					PIE.attach(fpca[j]);
			}

			if(document.getElementById('leftNav'))
			{
				PIE.attach(document.getElementById('leftNav'));
				lnca=document.getElementById('leftNav').getElementsByTagName("a");
				for(j=0;j<lnca.length;++j)
					j+1;//PIE.attach(lnca[j]);
