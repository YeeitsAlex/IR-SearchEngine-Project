       <!--[if !IE]> SEARCH <![endif]-->
       <script language="javascript" type="text/javascript">
        function ChangeOutput(strValue)
	{
		document.SearchForm.output.value=strValue;
	}
       </script>
       <form action="http://www.utmb.edu/search/search_parser.asp" id="SearchForm" method="get" name="SearchForm" style="margin:0px;padding:0px;">
        <input name="site" type="hidden" value="utmb"/>
        <input name="client" type="hidden" value="utmb"/>
        <input name="proxystylesheet" type="hidden" value="utmb"/>
        <input name="output" type="hidden" value="xml_no_dtd"/>
        <input name="hl" type="hidden" value="en"/>
        <input class="searchfield" id="textbox" maxlength="255" name="q" onblur="if(this.value=='') {this.value='Search UTMB';}" onfocus="if(this.value=='Search UTMB') {this.value='';}" type="text" value="Search UTMB"/>
        <input class="searchimagebtn" height="18" name="sa2" src="./App_Themes/Default/images/spacer_clear.gif" type="image" width="22"/>
        <a class="searchoptionsselector" href="" id="searchOptionsShow">
        </a>
        <a class="searchoptionsselector hide" href="" id="searchOptionsHide">
        </a>
        <div class="searchoptionsbox hide" id="divoptbox">
         <span class="searchtext">
          <input checked="checked" id="GSA" name="SearchOptions" onchange='ChangeOutput("xml_no_dtd");' type="radio" value="http://google.utmb.edu/search"/>
          <span id="searchopt_utmb">
           UTMB
          </span>
          <input id="google" name="SearchOptions" onchange='ChangeOutput("");' type="radio" value="http://google.com/search"/>
          <span id="searchopt_google">
           Google
          </span>
         </span>
        </div>
       </form>
       <script language="javascript">
        $("a#searchOptionsShow").click(function (event) {
        $("a#searchOptionsShow").addClass("hide");
        $("a#searchOptionsHide").removeClass("hide");
        $("div.searchoptionsbox").removeClass("hide");
		event.preventDefault();
    });
    $("a#searchOptionsHide").click(function (event) {
        $("a#searchOptionsShow").removeClass("hide");
        $("a#searchOptionsHide").addClass("hide");
        $("div.searchoptionsbox").addClass("hide");
		event.preventDefault();
    });
	
	$("span#searchopt_utmb").addClass("activeoption");
	
	$("input[name='SearchOptions']").change(function(){
		if ($("input[name='SearchOptions']:checked").val() == 'http://google.utmb.edu/search') {
			$("span#searchopt_utmb").addClass("activeoption");
			$("span#searchopt_google").removeClass("activeoption");
		}
		else
		{
			$("span#searchopt_utmb").removeClass("activeoption");
			$("span#searchopt_google").addClass("activeoption");
		}
	});
       </script>
