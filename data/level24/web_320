        <input class="searchfield" id="textbox" maxlength="255" name="q" onblur="if(this.value=='') {this.value='Search UTMB';}" onfocus="if(this.value=='Search UTMB') {this.value='';}" type="text" value="Search UTMB"/>
        <input class="searchimagebtn" height="18" name="sa2" src="/App_Themes/Default/images/spacer_clear.gif" type="image" width="22"/>
        <a class="searchoptionsselector" href="" id="searchOptionsShow">
