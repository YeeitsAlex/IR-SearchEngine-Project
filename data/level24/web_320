        <a class="searchoptionsselector hide" href="" id="searchOptionsHide">
        </a>
        <div class="searchoptionsbox hide" id="divoptbox">
         <span class="searchtext">
          <input checked="checked" id="GSA" name="SearchOptions" onchange='ChangeOutput("xml_no_dtd");' type="radio" value="https://search.utmb.edu:23356/apps/utmbclient/"/>
          <span id="searchopt_utmb">
           Search UTMB Health
          </span>
          <input id="google" name="SearchOptions" onchange='ChangeOutput("");' type="radio" value="http://www.google.com"/>
          <span id="searchopt_google">
           Google
          </span>
         </span>
        </div>
       </form>
       <script language="javascript">
        $("a#searchOptionsShow").click(function (event) {
        $("a#searchOptionsShow").addClass("hide");
        $("a#searchOptionsHide").removeClass("hide");
        $("div.searchoptionsbox").removeClass("hide");
        event.preventDefault();
    });
    $("a#searchOptionsHide").click(function (event) {
        $("a#searchOptionsShow").removeClass("hide");
        $("a#searchOptionsHide").addClass("hide");
        $("div.searchoptionsbox").addClass("hide");
        event.preventDefault();
    });
    
    $("span#searchopt_utmb").addClass("activeoption");
    
    $("input[name='SearchOptions']").change(function(){
        if ($("input[name='SearchOptions']:checked").val() == 'https://search.utmb.edu:23356/apps/utmbclient/') {
            $("span#searchopt_utmb").addClass("activeoption");
            $("span#searchopt_google").removeClass("activeoption");
        }
        else
        {
            $("span#searchopt_utmb").removeClass("activeoption");
            $("span#searchopt_google").addClass("activeoption");
        }
    });
       </script>
