                            PEPCForm.close()
                            alert(resp.content);
                            jQuery.magnificPopup.open({
                                items: {
                                    src: '#PEPCForm'
                                },
                                type: 'inline'
                            });
                            e.preventDefault();
                            return false;
                        } else {
                            e.preventDefault();
                            PEPCForm.location = resp.content;
                        }
                    }
                });
            })
        }
    });
   </script>
