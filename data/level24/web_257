   var utmbAppConfig = { usePagePreloader: true, siteRoot: "/", showArtboard: false, useMobileMenu: false, theme: 'utmb-modern', headerTitle: "School of Nursing", headerSubtitle: "The University of Texas Medical Branch", useUtmbAlertSystem: false, useNavigateMenu: true, navigateMenuTheme: 'nav-header', useExploreMenu: false, /*exploreMenuTheme: 'exp-mega', exploreMenuSource: 'institutional',*/ useSearchMenu: false };
        $(function () { offCanvasLockScroll(); var tandemNavs = ['navigate']; utmbWeb.init(utmbAppConfig); document.getElementById('masthead').appendChild(getMobileMenu(tandemNavs)); positionMobiNavButtons(tandemNavs); document.body.className += ' '; });
  </script>
  <script type="text/javascript">
   function showFullModal(b, ii, t, f, callback) { var fullModal = UIkit.modal('#fullModal', { bgClose: false }); $(document.getElementById('fullModal').querySelector('#fullModalBody')).html(""); $.get(b + ii).done(function (data) { $(document.getElementById('fullModal').querySelector('#fullModalBody')).html(data); }).fail(function () { $(document.getElementById('fullModal').querySelector('#fullModalBody')).html("<div class=\"alert alert-danger\"><button class=\"close\" type=\"button\" data-dismiss=\"alert\" style=\"margin-top: -10px; margin-right: -8px; opacity: 1\"><span class=\"fa fa-times-circle white\"></span></button><h5 class=\"text-center no-bold margin-top-10 margin-bottom-10 white\"><span class=\"fa fa-exclamation-triangle font-size-16 white\"></span>&nbsp;Unable to load specified resource (Server Error).  Please try again or contact support.</h5></div>") }).always(function () { /*$("#xLargeModal .modal-title").text(t); if (f == false) { $("#xLargeModal .modal-footer").remove(); } $("#xLargeModal").modal('show');*/ fullModal.toggle(); if (callback && typeof callback === "function") { callback(); } }); /*$("#mmBody").load(b + ii);*/ };
        $(document.body).on('click', '.modal_open', function () { showFullModal(this.getAttribute('data-val-url'), "", this.getAttribute('data-val-title'), false); });
        $(document).ready(function () {
            //var followPointer = new Event('mousemove');
            //var bigPointer = new Event('hover');
            //document.addEventListener('mousemove', function (e) { follow(e); }, false);
            //document.querySelectorAll('a').forEach(function (el) { el.addEventListener('hover', function () { document.getElementById('pointer').className += 'big'; }, false); });
            //document.dispatchEvent(followPointer);
            //document.dispatchEvent(bigPointer);
            if (window.location.href.toLowerCase().indexOf('#accreditations') > -1) { showAccreditations(); }
            if (window.location.href.toLowerCase().indexOf('#handbooks') > -1) { showHandbooks(); }
            if (window.location.href.toLowerCase().indexOf('#academic-calendar') > -1 || window.location.href.toLowerCase().indexOf('#academiccalendar') > -1) { showAcademicCalendar(); }
            if (window.location.href.toLowerCase().indexOf('#grading-policies') > -1 || window.location.href.toLowerCase().indexOf('#gradingpolicies') > -1) { showGradingPolicies(); }
            if (window.location.href.toLowerCase().indexOf('#student-forms') > -1 || window.location.href.toLowerCase().indexOf('#studentforms') > -1) { showStudentForms(); }
            changeID({ element: document.querySelector('.footer-logo a#brand'), newID: 'brand_footer' });
            repairKnownLinks([{ oldLink: "https://www.utmb.edu/healthcare", newLink: "https://www.utmb.edu/health" }, { oldLink: "https://www.utmb.edu/give", newLink: "https://innovationsinmind.utmb.edu/give" }]);
            stickyHeader(document.getElementById('header'), { showOnUp: true, animation: "uk-animation-slide-top" });
            var btp = document.querySelector('.back-to-top'); btp.setAttribute('uk-scroll', ''); btp.className += " btn btn-gray"; btp.innerHTML = "<span class=\"sr-only\">Back to Top</span><span class=\"uk-position-center\"><svg width=\"30\" height=\"30\" viewBox=\"0 0 20 20\" xmlns=\"http://www.w3.org/2000/svg\"><polyline fill=\"none\" stroke=\"#fff\" stroke-width=\"1.5\" points=\"4 13 10 7 16 13\"></polyline></svg></span>"; /*btp.style.bottom = (document.getElementById('footer').offsetHeight + 17) + "px"; });*/
            $(window).on('scroll', function () { var y = document.querySelector('#masthead a#brand').getBoundingClientRect().bottom; if (y < -10) { document.querySelector('.back-to-top').classList.add('active'); } else { document.querySelector('.back-to-top').classList.remove('active'); } });
        });
        function showAccreditations() { showFullModal("/About/Accreditations", "", "Our Accreditations", false); }
        function showAcademicCalendar() { showFullModal("/Academics/Calendar", "", "Academic Calendar", false); };
        function showStudentForms() { showFullModal("/Academics/Forms", "", "Student Forms", false); };
        function showGradingPolicies() { showFullModal("/Academics/Policies", "", "Grading Policies", false); };
        function showHandbooks() { showFullModal("/Academics/Handbooks", "", "Student Handbooks", false); }
        function changeID(obj) { if (obj) { obj.element.setAttribute('id', obj.newID); } }
        function repairKnownLinks(lks) { for (i = 0; i < lks.length; i++) { var elements = document.querySelectorAll('a[href="' + lks[i].oldLink + '"]'); for (ii = 0; ii < elements.length; ii++) { elements[ii].setAttribute('href', lks[i].newLink); } }        }
        function stickyHeader(el, options) { UIkit.sticky(el, options); }
        function uikitToggle(el, options) { var element = UIkit.toggle(el, options); element.toggle(); }
        function uikitDrop(el, options) { var element = UIkit.drop(el, options); element.toggle(); }
        function offCanvasLockScroll() { var events = ['mousewheel', 'DOMMouseScroll']; for (i = 0; i < events.length; i++) { document.documentElement.addEventListener(events[i], function (event) { if (this.className.indexOf('uk-offcanvas-page') > -1) { event.preventDefault(); event.stopPropagation(); } }); } }
        //function mouseX(evt) { if (!evt) evt = window.event; if (evt.pageX) return evt.pageX; else if (evt.clientX) return evt.clientX + (document.documentElement.scrollLeft ? document.documentElement.scrollLeft : document.body.scrollLeft); else return 0; }
        //function mouseY(evt) { if (!evt) evt = window.event; if (evt.pageY) return evt.pageY; else if (evt.clientY) return evt.clientY + (document.documentElement.scrollTop ? document.documentElement.scrollTop : document.body.scrollTop); else return 0; }
