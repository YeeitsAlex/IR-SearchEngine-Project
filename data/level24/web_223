		$('.milestone:odd').addClass('right-milestone');
		$('.right-milestone:first').css('margin-top', '100px');
		$('.milestone:even').addClass('left-milestone');
		
		$('#all').addClass('active-filter');
		
		$('.all').click(function() {
			$('#filters a').removeClass('active-filter');
			$('#all').addClass('active-filter');
			$('.client').removeClass('washedout');
		});
		
		$('.cat-Business').click(function() {
			$('#filters a').removeClass('active-filter');
			$('#business').addClass('active-filter');
			$('.cat-Business').removeClass('washedout');
			$('.client:not(.cat-Business)').addClass('washedout');
		});
		
		$('.cat-Education').click(function() {
			$('#filters a').removeClass('active-filter');
			$('#education').addClass('active-filter');
			$('.cat-Education').removeClass('washedout');
			$('.client:not(.cat-Education)').addClass('washedout');
		});
		
		$('.cat-Government').click(function() {
			$('#filters a').removeClass('active-filter');
			$('#government').addClass('active-filter');
			$('.cat-Government').removeClass('washedout');
			$('.client:not(.cat-Government)').addClass('washedout');
		});
		
		$('.cat-Organization').click(function() {
			$('#filters a').removeClass('active-filter');
			$('#organization').addClass('active-filter');
			$('.cat-Organization').removeClass('washedout');
			$('.client:not(.cat-Organization)').addClass('washedout');
		});
		
	});
  </script>
 </body>
</html>
