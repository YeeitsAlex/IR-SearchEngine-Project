	background-image: -webkit-linear-gradient(top, #FB2CE2, #fc74ec);
	background-image: -o-linear-gradient(top, #FB2CE2, #fc74ec);
	background-image: linear-gradient(to bottom, #FB2CE2, #fc74ec);
	background-repeat: repeat-x;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffFB2CE2', endColorstr='#fffc74ec', GradientType=0);
}

.btn-readmore a {
	color:white;
	text-shadow: 0 1px 0 #000;
}
