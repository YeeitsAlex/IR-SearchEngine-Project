   $(function(){

		$("#search_spyglass_button").on("click", function() {
			$("#header_top_search_box").toggle();
			return false;
		});

		$("#navigation .nav_programs").click(function(){
			$("#navigation .nav_programs").removeClass('active');
			$(this).addClass('active');
			var dropDown = $(this).attr('href');
			if ($(dropDown).parent().is(":visible")) {
				$(dropDown).parent().slideToggle(700,'easeOutQuad');
				$(this).removeClass('active');
			} else {

				if($(".dropdown_wrapper").is(":visible")) {
					$(".dropdown_wrapper").slideUp(700,'easeOutQuad');
					$(dropDown).parent().delay(700).slideDown(700,'easeOutQuad');
				} else {$(dropDown).parent().slideDown(700,'easeOutQuad');}
			}
			return false;
		});
	});
  </script>
