            <section class="block block-boxes block-boxes-os_sv_list_box" delta="1409838559" id="block-boxes-1409838559" module="boxes">
             <div class="block-inner clearfix">
              <h2 class="block-title" ng-non-bindable="">
               UPCOMING EVENTS
              </h2>
              <div class="block-content content" ng-non-bindable="">
               <div class="boxes-box" id="boxes-box-1409838559">
                <div class="boxes-box-content">
                 <div class="os-sv-list os_sv_list_box sv-list-event sv-list-view-sidebar_teaser sv-list-layout-list sv-grid-3" data-delta="1409838559" data-page="0" id="box-1409838559-page">
                  <article class="node node-event article event-start clearfix" id="node-1157346" role="article" target="_top">
                   <div class="event-start">
                    <span class="event-year">
                     2019
                    </span>
                    <span class="event-start-month">
                     Mar
                    </span>
                    <span class="event-start-day">
                     12
                    </span>
                   </div>
                   <div class="event-content">
                    <header class="node-header">
                     <h1 class="node-title" ng-non-bindable="" rel="nofollow">
