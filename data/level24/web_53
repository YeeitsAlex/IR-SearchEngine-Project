                          }
                          if (Array.isArray(window.addthis_layers_tools)) {
                            for (i = 0; i < window.addthis_layers_tools.length; i++) {
                              window.addthis.layers(window.addthis_layers_tools[i]);
                            }
                          }
                        }
                     },1000)
                    }());
  </script>
  <script async="async" data-cfasync="false" src="https://s7.addthis.com/js/300/addthis_widget.js#pubid=wp-2ce101a46a40a8f74f5632ef3c3ce405" type="text/javascript">
  </script>
  <script type="text/javascript">
   /* Fix Google Searchbox Accessibility Issues */
jQuery(document).ready(function() { 
  jQuery('.gsc-branding-img img').attr('alt', 'Google');  
  jQuery('.gsc-search-button input').attr('alt', 'Search');  
});
  </script>
