        <section class="odd" id="features-benefits">
         <h2>
          Features &amp; Benefits
         </h2>
         <div class="grid container" id="features-slider">
          <div class="col span4 feature-card-container">
           <div class="feature-card">
            <div class="center feature-item face front">
             <img height="64" src="https://png.icons8.com/book-shelf/dusk/64" title="Book Shelf" width="64"/>
             <h3 class="feature-item-title">
              <a href="/resources">
               Resources
              </a>
             </h3>
