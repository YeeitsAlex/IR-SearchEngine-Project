     <div class="row drawer hidden-xs" id="d_about">
      <div class="col-sm-12">
       <div class="row drawer_wrap">
        <div class="col-md-3 col-sm-4">
         <ul>
          <li>
           <a href="/about/history/">
            History
           </a>
          </li>
          <li>
           <a href="/about/accolades/">
            Accolades
           </a>
          </li>
