  <script type="text/javascript">
   $(document).ready(function() {
	

	function megaHoverOver(){
		$(this).find(".sub").stop().fadeTo('fast', 1).show();
			
		//Calculate width of all ul's
		(function($) { 
			jQuery.fn.calcSubWidth = function() {
				rowWidth = 0;
				//Calculate row
				$(this).find("ul").each(function() {					
					rowWidth += $(this).width(); 
				});	
			};
		})(jQuery); 
		
		if ( $(this).find(".row").length > 0 ) { //If row exists...
			var biggestRow = 0;	
			//Calculate each row
			$(this).find(".row").each(function() {							   
				$(this).calcSubWidth();
				//Find biggest row
				if(rowWidth > biggestRow) {
					biggestRow = rowWidth;
				}
			});
			//Set width
			$(this).find(".sub").css({'width' :biggestRow});
			$(this).find(".row:last").css({'margin':'0'});
			
		} else { //If row does not exist...
			
			$(this).calcSubWidth();
			//Set Width
			$(this).find(".sub").css({'width' : rowWidth});
			
		}
	}
	
	function megaHoverOut(){ 
	  $(this).find(".sub").stop().fadeTo('fast', 0, function() {
		  $(this).hide(); 
	  });
	}


	var config = {    
		 sensitivity: 1, // number = sensitivity threshold (must be 1 or higher)    
		 interval: 10, // number = milliseconds for onMouseOver polling interval    
		 over: megaHoverOver, // function = onMouseOver callback (REQUIRED)    
		 timeout: 0, // number = milliseconds delay before onMouseOut    
		 out: megaHoverOut // function = onMouseOut callback (REQUIRED)    
	};

	$("ul#topnav li .sub").css({'opacity':'0'});
	$("ul#topnav li").hoverIntent(config);
//form switcher-drop
$('select#filter,select#filter2,select#state').selectmenu(
	{width:160}); 
$('select#filter3,select#country').selectmenu(
	{width:185}); 	
$('input.input-search-right').defaultvalue("Search for...");
	
// show hides
//class
$('#btn_by_class').click(function() {
	$(this).addClass('selected');
	$('#btn_by_grad_alum,#btn_by_region,#btn_by_afilliation').removeClass('selected')
    $('#findgradalumni').hide('fast');
    $('#findmygroup').hide('fast');
    $('#findbyregion').hide('fast');
    $('#findmyclass').show('fast')
    return false;
  });	
 //grad alum 
$('#btn_by_grad_alum').click(function() {
	$(this).addClass('selected');
	$('#btn_by_class,#btn_by_region,#btn_by_afilliation').removeClass('selected')
    $('#findgradalumni').show('fast');
    $('#findmygroup').hide('fast');
    $('#findbyregion').hide('fast');
    $('#findmyclass').hide('fast')
    return false;
  });	 
 //region
$('#btn_by_region').click(function() {
	$(this).addClass('selected');
	$('#btn_by_class,#btn_by_grad_alum,#btn_by_afilliation').removeClass('selected')
    $('#findgradalumni').hide('fast');
    $('#findmygroup').hide('fast');
    $('#findbyregion').show('fast');
    $('#findmyclass').hide('fast')
    return false;
  });	 
 //affliation
$('#btn_by_afilliation').click(function() {
	$(this).addClass('selected');
	$('#btn_by_class,#btn_by_grad_alum,#btn_by_region').removeClass('selected')
    $('#findgradalumni').hide('fast');
    $('#findmygroup').show('fast');
    $('#findbyregion').hide('fast');
    $('#findmyclass').hide('fast')
    return false;
  });	 

