       <!-- News and Events -->
       <div id="news-events">
        <div class="row inner">
         <section class="large-9 small-12 columns" id="news">
          <div class="row">
           <div class="small-12 columns">
            <h2 class="title">
             <a href="https://warnercnr.colostate.edu/all-news">
              News
             </a>
            </h2>
           </div>
           <script>
            $(document).ready(function() {
							  $.ajax({
							    dataType: 'json',
							    url: 'https://warnercnr.source.colostate.edu/wp-json/wp/v2/posts?filter[category_name]=warner-home',
							    type: 'GET',
							    success: function(response) {

							      // Article div with the title, excerpt and link to the live version.
							      var newscard = '';
							      $.each(response, function(index) {
							      	if (index > 2) {
							          return false;
							        }

							        var monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "June", "July", "Aug", "Sept", "Oct", "Nov", "Dec"];
							        var date = new Date(response[index].date);
							        var formatDate = (monthNames[date.getMonth()]) + ' ' + date.getDate() + ', ' +  date.getFullYear();

					              	newscard += "<div class='medium-4 small-12 columns news-block end'>";
					        	      	newscard += "<a class='link-wrapper' href='" + response[index].link.replace("https:","http:") + "' target='_blank'>";
					        		      	if(response[index].featured_image) {
      							      			newscard += "<img src='" + response[index].featured_image.source_url + "' alt='" + response[index].featured_image.alt + "' />";
      							      			newscard += "<div class='newscard-mobile-image' style='background-image:url(&#39;" + response[index].featured_image.source_url + "&#39;);'></div>"
      								      	}
					        		      	newscard += "<div class='newscard-text'>";
					        			        newscard += "<h3>" + response[index].title.rendered + "</h3>";
					        			        newscard += "<div class='excerpt'>" + response[index].excerpt.rendered + "</div>";
					        				        // newscard += "<div class='newscard-bottom'>";
					        				        // newscard += "<p class='date'>" + formatDate + "</p>";
					        				        // newscard += "</div>";
					        		        newscard += "</div>";
					        	        newscard += "</a>";
					                newscard += "</div>";
							      });

							      // Take our created divs and put it into the above div
							      $('.news-wrapper').html(newscard);
							    }
							  });
							});
           </script>
           <!-- Display news stories from Source -->
           <div class="news-wrapper">
           </div>
          </div>
          <!-- end .row -->
         </section>
         <!-- end #news -->
