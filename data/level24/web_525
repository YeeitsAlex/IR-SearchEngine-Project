  <script>
   showPopup = function() 
            {
                var message = "Do not process the emergency loan/installment plan until your account reflects all charges for registered course(s), parking decal, health insurance and meal plan as you can only process one emergency loan/installment plan per term.  You will be responsible for paying, by January 18th, any charges that you add after you have processed this emergency loan/installment plan.";

                

                if (document.getElementById("rbBook").checked === true)
                {
                    message = "Do not process a book loan until your tuition and fees have been paid in full or through an emergency loan/installment plan. You can only process 1 book loan per term.";
                }
                
                return confirm(message);
            }
  </script>
