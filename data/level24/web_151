   <script>
    jQuery(function($)
            {
                $('.-do-toggleNav').on('click', function()
                {
                    $(this).toggleClass('-open');
                    $('#mobile-nav').toggleClass('-open');
                });

                $('.-do-search').on('click', function()
                {
                    if ( $('#searchInput').val() && $('.c-search').hasClass('-open') ) {
                        $('#searchInput').closest('form').submit();
                    }
                    else if ( ! $('.c-search').hasClass('-open') ) {
                        $('.c-search').addClass('-open');
                        var strLength = $('#searchInput').val().length * 2;
                        $('#searchInput')[0].focus();
                        $('#searchInput')[0].setSelectionRange(strLength, strLength);
                    }
                });

                $('#searchInput').on('blur', function()
                {
                    setTimeout(function()
                    {
                        $('.c-search').removeClass('-open');
                    },100);
                });

                $('#searchInput').keypress(function(e)
                {
                    if ( e.which == 13 ) // Enter key = keycode 13
                    {
                        if ( ! $('#searchInput').val() ) { return false; }
                    }
                });

                $('body').append('<div class="c-nav_mobile" id="mobile-nav"><ul></ul></div>');

                $('#primary-nav').find('ul:not(.sub-menu)').each(function(){
                    $('#mobile-nav ul').append($(this).html());
                    $('#mobile-nav').find('.-do-search').attr('href', 'http://graduateschool.colostate.edu/?s=');
                    $('#mobile-nav').find('svg').replaceWith('Search');
                });
            });
   </script>
