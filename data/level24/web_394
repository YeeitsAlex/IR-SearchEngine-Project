  <script type="text/javascript">
   function campstat() {
	var domain_url = 'https://now.ius.edu/wp-content/themes/ius_now_v2';
	var eFeed = domain_url + '/includes/campus_status.php';
	var eCont = 'div.campus-status-block div.campus_alerts';
	jQuery.fn.noIUSAlerts = function(strSel){
		jQuery(strSel + ' div.campus_alert').remove();
		jQuery(strSel + ' div.feed_item').remove();
		jQuery(strSel).append('<div class="feed_item"><a href="http://www.iu.edu/~iuinfo/backend/parser-cap.php?campus=ius&output=large&allclear=true">View Status</a></div>');
	}
	jQuery.fn.alertsError = function(strSel) {
		jQuery(strSel + ' div.campus_alert').remove();
		jQuery(strSel + ' div.feed_item').remove();
		jQuery(strSel).append('<div class="feed_item"><a href="http://www.iu.edu/~iuinfo/backend/parser-cap.php?campus=ius&output=large&allclear=true">View Status</a></div>');
	}
	jQuery.fn.updateStatus = function(strURL, strSel) {
		try {
			jQuery.ajax({
				url: strURL,
				type: 'GET',
				dataType: 'html',
				async: false, 
				error: function() {
					jQuery.fn.alertsError(strSel);
				}, 
				success: function(data) {
					// Remove old alerts
					jQuery(strSel + ' div.campus_alert').remove();
					jQuery(strSel + ' div.feed_item').remove();
					if(jQuery(data).length > 0) {
						var new_data = '<div>' + data + '</div>';
						var tmp_val = jQuery(new_data).find('div.campus_alert');
						if(tmp_val.length > 0) {
							if(jQuery(strSel).length > 0) {
								jQuery(strSel).append(data);
							}
						} else {
							jQuery.fn.noIUSAlerts(strSel);
						}
					} else {
						jQuery.fn.noIUSAlerts(strSel);
					}
				}
			});
		} catch(e) {
			jQuery.fn.alertsError(strSel);
		}
	}
	if(jQuery(eCont).length > 0) {
		jQuery.fn.updateStatus(eFeed, eCont);
		var feedUpdate = setInterval(function(){
				jQuery.fn.updateStatus(eFeed, eCont);
			},  
			120000);
	}
}

function sportsnewscolumns() {
	try {
		sportcolheight = 0;
		sportcol1 = jQuery('.se-sports').height();
		iunewscol2 = jQuery('.se-iu-news-container').height();
		sportcolheight = sportcol1;
		if(iunewscol2 > sportcolheight) {
			sportcolheight = iunewscol2;
		}
		jQuery('.se-sports').css({height: sportcolheight});
		jQuery('.se-iu-news-container').css({height: sportcolheight});
	} catch(errr) { }
}

(function(jQuery){
    jQuery(document).ready(function(e){
		IUSearch.init({ CX: { site: '013797427787343721755:xt34rixxvau', all: '016278320858612601979:ddl1l9og1w8' } });
		sportsnewscolumns();
		campstat();
        jQuery('#toggles div.row a.right-off-canvas-toggle').click(function() {
        	 if(jQuery('#mobile-nav').is(":visible")) {
    			  jQuery('#mobile-nav').hide();
    			  jQuery('#se-page-content').show();
                  jQuery('#site-titlebar').show();
                  jQuery('#site-header-image').show();
    		 } else{
    			jQuery('#mobile-nav').show();
    			jQuery('#se-page-content').hide();
                  	jQuery('#site-titlebar').hide();
                  	jQuery('#site-header-image').hide();
    		 }
    	});
        jQuery('#toggles div.row a.right-off-canvas-toggle').keyup(function(e){
			if(e.keyCode == 32) {
						// user has pressed space
				if(jQuery('#mobile-nav').is(":visible")) {
							jQuery('#mobile-nav').hide();
						jQuery('#se-page-content').show();
					jQuery('#site-titlebar').show();
					jQuery('#site-header-image').show();
					} else{
						jQuery('#mobile-nav').show();
						jQuery('#se-page-content').hide();
					jQuery('#site-titlebar').hide();
					jQuery('#site-header-image').hide();
				}
			}
        });
		jQuery('article.column-two').prepend(jQuery('#now-site-navigation').clone().attr('id','mobile-now-site-nav'));
		jQuery("#mobile-now-site-nav li").each(function() {
			jQueryzidtouse = jQuery(this).attr('id');
			jQueryzidtouse = 'se' + jQueryzidtouse;
			jQuery(this).attr('id', jQueryzidtouse);
		});
		jQuery(".now-nav-plus").click(function() {
			var txtvis = false;
			jQuery(this).parent().children('.sub-menu').each(function(){
				if(jQuery(this).is(":visible")) {
					jQuery(this).hide();
					txtvis = false;
				} else {
					jQuery(this).show();
					txtvis = true;
				}
			});
			if(txtvis) {
				jQuery(this).html('-');
			} else {
				jQuery(this).html('+');
			}
			return false;
		});
		jQuery(".breadcrumb a").each(function() {
			jQueryzhreftouse = jQuery(this).attr('href');
			jQueryzblogurl = 'https://now.ius.edu/';
			for (i = 2010; i < 2032; i++) {
				if(jQueryzhreftouse  == jQueryzblogurl + '' + i + '/') {
					jQueryappendformonth = '01/'
					switch (jQueryzhreftouse) {
						case jQueryzblogurl + '2010/':
						jQueryappendformonth = '06/';
						break;
					}
					jQuery(this).attr('href',jQueryzhreftouse + '/' + jQueryappendformonth)
				}
			}
		});
		jQuery(".se-now-month-selected a").each(function() {
			jQueryzurltouse = jQuery(this).attr('href');
			jQueryzurltouse = jQueryzurltouse.replace(/\/[0-9][0-9]\//,"/");
			//alert(jQueryzurltouse); // alert: http://blogs.ius.edu/dev-now/2014/
			jQuery("#menu-item-11").addClass('active current-menu-item');
			jQuery("#semenu-item-11").addClass('active current-menu-item');
			jQueryzurltouse = jQueryzurltouse.replace('https://now.ius.edu/',"/").replace('http://now.ius.edu/',"/");
			jQueryzurltouse = jQueryzurltouse.replace('http://now.ius.dev/',"/");
			jQueryzurltouse = jQueryzurltouse.replace('https://blogs.ius.edu/',"/").replace('http://blogs.ius.edu/',"/");
			//alert(jQueryzurltouse); // alert: /dev-now/2014/
			if(jQueryzurltouse == '/2010/' || jQueryzurltouse == '/dev-now/2010/') {
				jQueryzurltouse = jQueryzurltouse + '06/';
			} else {
				jQueryzurltouse = jQueryzurltouse + '01/';
			}
			jQuery("#menu-site-menu a[hrefjQuery='" + jQueryzurltouse + "']").each(function() {
				jQuery(this).parent().addClass('active current-menu-item');
			});
		});
		jQuery(".now-nav-plus").each(function() {
			if(jQuery(this).parent().hasClass('current-menu-ancestor')){
				jQuery(this).html('-');
			} else {
				if(jQuery(this).parent().hasClass('current-menu-item')) {
					jQuery(this).html('-');
				}
			}
		});
		
    });
})(jQuery);
  </script>
 </body>
</html>
