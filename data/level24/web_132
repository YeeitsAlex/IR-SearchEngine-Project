          $(document).ready( function () { $.getScript('/include/scripts/GMapFront.unpacked.js?v=3',function( data, textStatus, jqxhr ) { })  .done(function( jqxhr, settings, exception ) {  if($('#stations').length) {  links = document.getElementById('stations').getElementsByTagName('a'); if (links) {for (var i = 0; i < links.length; ++i) {str = 'a#' + links[i].id;$(str).fancybox({
