     <!-- JS for BrowZine checkbox -->
     <script>
      function searchJournals() {

if(jQuery("#nobrowzine").is(':checked'))
{
jQuery("#journals form").attr("action", "https://thslc-utxmed.primo.exlibrisgroup.com/discovery/jsearch");
//jQuery("#query").attr("name", "query");
document.getElementById("queryJ").value = ("any,contains," + document.getElementById("queryJ").value.replace(/[,]/g, " "));
document.forms["searchFormJ"].submit();
}
else
{
jQuery("#journals form").attr("action", "https://api.thirdiron.com/v2/libraries/684/external-search");
jQuery("#query").attr("name", "query");
}

}
//////////////
jQuery("#nobrowzine").click( function(){
searchJournals();
});

searchJournals();
     </script>
