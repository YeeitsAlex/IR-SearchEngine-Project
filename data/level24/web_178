                                                                                + "&V=" + document.getElementById("V").value
                                                                                + "&L=" + document.getElementById("L").value
                                                                                + "&S=" + document.getElementById("S").value;
                                                                
                                                                objSearchWindow = OpenChildWindow(strSearchURL, "Search")
                                                }


                                                function SearchLC()
                                                {
                                                                var objSearchWindow;
                                                                var strSearchURL;
                                                                var searchits =  document.getElementById("SEARCH4").value;

                                                                var pattern = / /g;

                                                                searchits = searchits.replace(pattern,'+')
                                                                
                                                                strSearchURL = 

                                                                                "http://utmb.thslc.net/vwebv/search?searchArg="
                                                                                + searchits + "&searchCode="
                                                                                + document.getElementById("SEARCHHOW").value
                                                                                + "&recCount=100&searchType=0";
                                                                
                                                                objSearchWindow = OpenChildWindow(strSearchURL, "SearchLC")
                                                }                                              


                                                function clearDefault(obj)
                                                {
                                                                obj.value = "";
                                                }
                                                
                                                function OpenChildWindow(fileName, windowName) 
                                                {
                                                                var childWindow = window.open(fileName, windowName, "");
                                                                childWindow.focus();
                                                                return childWindow;
                                                }
                                                
