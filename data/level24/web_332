           <script>
            // Accordion
(function ($) {
$(document).ready(function(){
  $('.views-row .accordion-item .accordion-content').css("display", "none");
  
  $('.views-row .accordion-item .accordion-heading').click(function() {
  
    var a = $(this).closest('.accordion-item');
    var b = $(a).hasClass('open');
    var c = $(a).closest('.view-grouping').find('.open');
      
    //Only one open at a time  
    // if(b != true) {
    //   $(c).find('.accordion-content').slideUp(200);
    //   $(c).removeClass('open');
    // }

    $(a).toggleClass('open');
    $(a).find('.accordion-content').slideToggle(200);



  });
});
})(jQuery);
           </script>
