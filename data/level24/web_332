           <script charset="utf-8" type="text/javascript">
            jQuery(document).ready(function($) {

  $("#nav-rpi").click(function(e) {
    e.preventDefault();
    if($("#rpi-mega").is(":visible")) {
      $("#rpi-mega").slideUp();

    } else {
      $("#rpi-mega").slideDown();
      $("#rpiQueryHolder").focus();

    }
  });
});
