    fx: 'fade',
    speed: 3000,
    slideResize: true,
    containerResize: false,
    width: '100%',
    fit: 1
  });

  // cycle doesn't run if there is only one item, and this can impact the positioning / padding of the 
  // banner (but only when authenticated) so when authenticated we need to accommodate the title styling 
  if ($('.view-banner-and-section-title .views-field-field-image .field-content .item-list ul li').length > 1) {
     $('body.logged-in .views-field.views-field-field-image').css('padding-bottom','20.8%');
  }
});
