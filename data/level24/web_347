   <script src="/_global/js/jquery.dataTables.min.js" type="text/javascript">
   </script>
   <script src="//cdn.datatables.net/responsive/2.0.2/js/dataTables.responsive.min.js" type="text/javascript">
   </script>
   <script src="https://cdnjs.cloudflare.com/ajax/libs/vanilla-lazyload/8.5.2/lazyload.min.js">
   </script>
   <script>
    if ($(".faculty").length != 0) {
      var table = $('table.display').DataTable({
        "paging": false
      });
      $('#allTableSearch').on( 'keyup', function () {
         table.search( this.value ).draw();
      });
      $.fn.dataTable.ext.errMode = 'none';
    }
   </script>
   <script>
    $(document).ready(function(){
        if ($(".faculty").length != 0) {
            new LazyLoad();  
            $('.az-filter').click(function(){
              setTimeout(function(){
                  window.scrollTo(window.scrollX, window.scrollY + 2);
                  window.scrollTo(window.scrollX, window.scrollY - 2);
              },800);
            });
            $("input#allTableSearch").keypress(function(){
              setTimeout(function(){
                  window.scrollTo(window.scrollX, window.scrollY + 2);
                  window.scrollTo(window.scrollX, window.scrollY - 2);
              },800);
            });
        }
    });
   </script>
  </div>
