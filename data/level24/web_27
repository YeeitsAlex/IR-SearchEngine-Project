  <script src="../_files/js/pagination.js" type="text/javascript">
  </script>
  <script type="text/javascript">
   (function ($, sr) { 
    var debounce = function (func, threshold, execAsap) {
        var timeout;

        return function debounced() {
            var obj = this, args = arguments;
            function delayed() {
                if (!execAsap)
                    func.apply(obj, args);
                timeout = null;
            };

            if (timeout)
                clearTimeout(timeout);
            else if (execAsap)
                func.apply(obj, args);

            timeout = setTimeout(delayed, threshold || 100);
        };
    }
    jQuery.fn[sr] = function (fn) { return fn ? this.bind('resize', debounce(fn)) : this.trigger(sr); };
})(jQuery, 'smartResize');

$(function () {
    $("a:has(> img)").addClass("undecorate");

    var $iframe = $("body.page-iframe-content.sfFrontEnd #content iframe");

    function setIframeHeight() {
        $($iframe.parents(":not(html, body)").get().reverse()).each(function () {
            var $el = $(this);
            var siblingsHeight = 0;
            $el.siblings(":not(style, script):visible").each(function () {
                siblingsHeight += $(this).outerHeight();
            });
            var height = $el.parent().outerHeight() - siblingsHeight;
            $el.outerHeight(height).css('overflow', 'hidden');
        });
    }

    $(window).smartResize(function () {
        setIframeHeight();
    });

    setIframeHeight();
});
  </script>
 </body>
