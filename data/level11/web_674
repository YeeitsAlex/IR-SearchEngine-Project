          function searchPrimo4() {
             document.getElementById("primoQueryTemp4").value = "any," + document.getElementById("primoQueryTemp4").value;
		document.getElementById("journalQuery").value = document.getElementById("primoQueryTemp4").value;
             document.forms["searchForm4"].submit();
      }
      
      function searchKeyPress(e) {
            if (typeof e == 'undefined' && window.event) {
                  e = window.event;
            }
            if (e.keyCode == 13) {
                  document.getElementById('go').click();
            }
      }
         </script>
