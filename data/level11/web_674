/*set the font size for University Libraries text*/
/*adjust the padding for today's hours*/
/*hide the new titles box*/

@media (min-width:768px) and (max-width:991px) {

	#header-nav a {
		font-size: 12px;
	}
	#header-nav a {
		padding-right: 5px;
	}

	#library-name h1 {
		font-size: 3em;
	}
	#todays-hours {
		padding-top: 0px;
		padding-bottom: 8px;
	}
	#s-lg-box-12821953-container {
		display: none;
	}
}


/*small devices (tablets, 768px and up)*/
/*add padding to the left of each University Libraries navbar links for spacing when they stack on small devices*/

@media (min-width: 768px) {

	#header-nav a {
		padding-left: 10px;
	}
}


/*large and medium devices (desktops, 992px and up)*/
/*display the full-size campus logo with seal and tagline, hide the small campus logo*/

@media (min-width: 992px) {

	.logo.desktop {
		display: block;
		max-width: 475px;
		width: 100%;
	}

	.logo.mobile {
		display: none;
	}
	
}  


/*medium and small devices (tablets, 991px and below)*/
/*display the small campus logo, hide the full-size campus logo*/
/*set the font size for the University Libraries text*/
/*adjust the padding for today's hours*/

@media (max-width: 991px) {

	.logo.desktop {
		display: none;
	}
	
	.logo.mobile {
		display: block;
		max-width: 154px;
		width: 100%;
	}

	#library-name h1 {
		font-size: 3em;
	}

	#todays-hours {
		padding-top: 10px;
	}
}


/*small and extra-small devices (tablets and phones, 768px and below)*/
/*adjust the padding for today's hours*/
/*hide the carousel box (box-12805768)*/
/*hide the new titles box (box-12821953*/

@media (max-width: 768px) {
	
	#todays-hours {
		padding-top: 0px;
		padding-bottom: 8px;
	}

	#s-lg-box-12805768-container {
		display: none;
	}

	#s-lg-box-12821953-container {
		display: none;
	}
	#library-name h1 {
		font-size: 2.5em;
	}
}
