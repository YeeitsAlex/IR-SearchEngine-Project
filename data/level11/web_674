         <script>
          function hours() {
var closures = new Array("11.31"); 
/*Create a check date for the current day*/
var d = new Date();
var day = d.getDate();
var month = d.getMonth() + 1;
var checkdate = month + "." + day;


/* if the date is in the array at all...*/
if (closures.indexOf(checkdate) >=0) {
document.getElementById("hours").innerHTML = "<table><tr><td>Hayward Campus:</td><td>  Closed</td></tr><tr><td>Concord Campus:</td><td>  Closed</td></tr></table>";
}
else
{
switch (new Date().getDay()) {
case 0:
	document.getElementById("hours").innerHTML = "<table><tr><td>Hayward Campus:</td><td>  12:00pm - 7:00pm</td></tr><tr><td>Concord Campus:</td><td>  Closed</td></tr></table>";
		break;
case 1:
	document.getElementById("hours").innerHTML = "<table><tr><td>Hayward Campus:</td><td>  8:00am - 10:00pm</td></tr><tr><td>Concord Campus:</td><td>  8:00am - 7:00pm</td></tr></table>";
		break;	
case 2:
	document.getElementById("hours").innerHTML = "<table><tr><td>Hayward Campus:</td><td>  8:00am - 10:00pm</td></tr><tr><td>Concord Campus:</td><td>  8:00am - 7:00pm</td></tr></table>";
		break;		
case 3:
	document.getElementById("hours").innerHTML = "<table><tr><td>Hayward Campus:</td><td>  8:00am - 10:00pm</td></tr><tr><td>Concord Campus:</td><td>  8:00am - 7:00pm</td></tr></table>";
		break;	
case 4:
	document.getElementById("hours").innerHTML = "<table><tr><td>Hayward Campus:</td><td>  8:00am - 10:00pm</td></tr><tr><td>Concord Campus:</td><td>  8:00am - 7:00pm</td></tr></table>";
		break;
case 5:
	document.getElementById("hours").innerHTML = "<table><tr><td>Hayward Campus:</td><td>  8:00am - 5:00pm</td></tr><tr><td>Concord Campus:</td><td>  Closed</td></tr></table>";
		break;
case 6:
	document.getElementById("hours").innerHTML = "<table><tr><td>Hayward Campus:</td><td>  11:00am - 5:00pm</td></tr><tr><td>Concord Campus:</td><td>  Closed</td></tr></table>";
		break;

}
  }
}
         </script>
         <script type="text/javascript">
          function searchPrimo() {
             document.getElementById("primoQuery").value = "any,contains," + document.getElementById("primoQueryTemp").value;
             document.forms["searchForm"].submit();
      }
      
      function searchKeyPress(e) {
            if (typeof e == 'undefined' && window.event) {
                  e = window.event;
            }
            if (e.keyCode == 13) {
                  document.getElementById('go').click();
            }
      }
         </script>
