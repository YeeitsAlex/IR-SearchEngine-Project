          (function($){
  $(window).resize(function() {
    $('.views_slideshow_singleframe_teaser_section, .views-slideshow-cycle-main-frame, .views-slideshow-cycle-main-frame-row').each(function(){
        var ratio = 360 / 1200 ; // height/width of home page images
        var minHeight = 288;
        var newHeight = Math.max($(this).width() * ratio, minHeight);
        $(this).height(newHeight);
    });
  });
})(jQuery);
