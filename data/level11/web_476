<!-- Simple jQuery to show and hide a div when its parent is clicked -->
<script type="text/javascript">
 $(document).ready(function(){
//			$(".h_rotate").click(function(){
//			 $(this).toggleClass("down"); 
//			 if($(".hours-plugin .hours-text").css('display') == 'block'){
//					$(".hours-plugin .hours-text").slideUp(400);
//				}else{
//					$(".hours-plugin .hours-text").slideDown(400);
//				}
//})
			$(".h_rotator").click(function() {
				$(".h_rotate").toggleClass("down");
				if($(".hours-plugin .hours-text").css('display') == 'block'){
					$(".hours-plugin .hours-text").slideUp(400);
				}else{
					$(".hours-plugin .hours-text").slideDown(400);
				}
			})
		});
</script>
