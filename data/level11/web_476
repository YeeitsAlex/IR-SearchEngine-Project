<!-- TO THE TOP! -->
<!--<div class="scroll-top">
            <a href="#menu-main">
            <i class="fa fa-caret-up"></i>
            <span>top</span>
            </a>
        </div> -->
<!-- END WRAP -->
<script type="text/javascript">
 var navFixed = true, navAnchored, navFixedWidth = true, scrollTopIcon = false;
            $(document).ready(function(){
                /* Check screen size and how far user is scrolled down initially.
                   - Changes Navigation based on screen size and depth into page
                   - Shows/hides the Scroll Top button depending on screen size and depth into page
                */
                chk_width();
                add_goto(); // adds links to each nav item to go to the landing page
                $(window).resize(chk_width); // Check if nav should be fixed after a resize
                $(window).scroll(chk_scroll); // Check if nav should be fixed after a scroll\
                margin_anchor();              // Setup scroll offset for anchor elements
            });

			/* Function to scroll the window to the hashed location from the URL
			   - Regular hash does not work because of the navbar always being on top,
			     so an offset is added with this function
		    */
			function margin_anchor(){
				// Find hashtag
				var hashtag = window.location.hash;

				// Get corresponding element
				if(hashtag){
                    var ele = $(hashtag);

                    // Figure out the element's offset from the top,
                    // minus the height of the navbar plus 10px of padding
                    var navHeight = $(".navbar").height() + 10; // Height of the navbar
                    var topOff = ele.offset().top - navHeight; // Location to scroll the window to

                    // Scroll the body to the correct element
                    $('html, body').animate({scrollTop: topOff}, 500);
                }
			}

            /* Function to anchor the nav on window width change.*/
            function chk_width(){
                var small_screen = 1007; // In px
                if($(window).width() <= small_screen){
                    // Remove the class if the window was resized
                    navFixed = false;
                    navFixedWidth = false;
                    if(navAnchored){
                        navFixed = false;
                        $(".navbar").removeClass("navbar-fixed-top");
                    }
                }
                else{
                    navFixedWidth = true;
                }
                chk_scroll();
            }

            /* Function to show/hide certain elements on scroll.
               - Navbar
               - Scroll Top button
            */
            function chk_scroll(){

                var supportPageOffset = window.pageXOffset !== undefined;
                var isCSS1Compat = ((document.compatMode || "") === "CSS1Compat");

                var y = supportPageOffset ? window.pageYOffset : isCSS1Compat ? document.documentElement.scrollTop : document.body.scrollTop;

                var nav_top = 196; // In px, from the top of the page

                if(y >= nav_top && navFixedWidth){
                  if(navAnchored !== true){
                      $(".navbar").addClass("navbar-fixed-top");
                      navAnchored = true;
                  }
                } else if(navAnchored){
                    $(".navbar").removeClass("navbar-fixed-top");
                    navAnchored = false;
                }

                if(!navFixedWidth){
                    if(y >= 525 && !scrollTopIcon){
                        $(".scroll-top").fadeIn(400);
                        scrollTopIcon = true;
                    }
                    else if (scrollTopIcon && y < 525){
                        scrollTopIcon = false;
                        $(".scroll-top").fadeOut(400);
                    }
                }
                else{
                    if(scrollTopIcon){
                        $(".scroll-top").fadeOut(400);
                        scrollTopIcon = false;
                    }
                }
            }

			/* Adds 'go to ...' links to each of the navigation items */
			function add_goto(){
				// Insert the link elements
				$(".dropdown-toggle").each(function(){
					var url = $(this).attr('href');
					var name = $(this).find('span').html();

					var toappend = "";
					toappend += "<a href='" + url + "' title='go to " + name + "' class='goto-link'>"; // open <a> tag
					toappend += "<i class='fa fa-external-link-square'></i></a>"; // insert <i> tag and close <a> tag

					$(this).after(toappend);
				});

				// Setup onclick listener to display the goto links
				$(".goto span, .goto i").click(function(){

					if($(this).parent().siblings('.dropdown-menu').is(':visible')){
						// The currently opened one was clicked again, just hide the arrow
						$(".goto-link").hide();
						return;
					}

					$(".goto-link").hide();
					$(this).parent().parent().find(".goto-link").show();
				});

				$(".nav .dropdown").click(function(){
					if($(this).children('.dropdown-menu').is(':visible')){
						$(".goto-link").hide();
						return;
					}
					$(".goto-link").hide();
					$(this).children('.goto-link').show();
				});

				// Setup listener to hide the links when not active
				$(document).mouseup(function(e){
					var arrows = $(".goto-link");
					var inner = $(".goto-link > i");

					if(!arrows.is(e.target) && !inner.is(e.target)){
						arrows.hide();
					}
				});

			}
</script>
