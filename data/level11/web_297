<script>
 $(function () {
            $('.list-group').hide();

            $('#slide-submenu').on('click', function () {
                $(this).closest('.list-group').fadeOut('slide', function () {
                    $('.mini-submenu').fadeIn();
                    $('.cover-body').fadeOut(200);

                });

            });

            $('.mini-submenu').on('click', function () {
                $(this).next('.list-group').toggle('slide');
                $('.mini-submenu').show();
                $('.cover-body').fadeIn(200);
            });

            $('.cover-body').on('click', function () {

                $('.list-group').hide();
                $('.cover-body').fadeOut(200);

            });

            $(document.body).on('click', '.dropdown-menu li', function (event) {

                var $target = $(event.currentTarget);

                $target.closest('.btn-group')
                    .find('[data-bind="label"]').text($target.text())
                    .end()
                    .children('.dropdown-toggle').dropdown('toggle');

                return false;

            });
        });
</script>
<script>
 var rowAnimationDelay = 50;
        var sortColumn = "displayName";
        var index = 0;
        var dropdownField = '';
        var filterByLetter = '';
        var domainDropdown = "";
        var urlParam = "";

        function rfc3986EncodeURIComponent(str) {
            return encodeURIComponent(str).replace(/[!'()*]/g, escape);
        }

        function LoadUsers(page) {
            var query = $("#searchtextBox").val();
            var letter = filterByLetter;
            var field = dropdownField;
            var domain = domainDropdown;
            var urlQuery = urlParam;
            var selectedDepartment = $("select[name=Department] option:selected").val();
            var selectedLocation = $("select[name=location] option:selected").val();

            // Set query if mobile search form visible
            if ($(".search-form-mobile input[type='text']").is(':visible')) {
                query = $(".search-form-mobile input[type='text']").val();
            }

            var url = "/Home/LoadUsers" + "?urlQuery=" + urlQuery + "&pageIndex=" + page + "&letter=" + (letter == null ? "" : letter) + "&query=" + query + "&field=" + (field == null ? "" : field) + "&domain=" + (domain == null ? "" : domain) + "&sortBy=" + (sortColumn == null ? "" : sortColumn);
            url = encodeURI(url);


            if (selectedDepartment && selectedDepartment != "*") {
                url += "&department=" + encodeURIComponent(selectedDepartment.replace("'", "''"));
            }
            if (selectedLocation && selectedLocation != "*") {
                url += "&location=" + encodeURIComponent(selectedLocation.replace("'", "''"));
            }

            //console.log(url);

            // Load retrieved data in div
            $("#collapsible-main-data").load(url, function () {
                if (index === 0) {
                    setPager();
                }
                index++;
            });
        }

        function setPager() {
            var totalPages = parseInt($("#hidTotalPages").val());
            if ($('#pager').data().twbsPagination != undefined) {
                $('#pager').twbsPagination('destroy');
            }
            $('#pager').twbsPagination({
                totalPages: totalPages,
                visiblePages: 5,
                onPageClick: function (event, page) {
                    $('#page-content').text('Page ' + page);
                    LoadUsers(page - 1);
                },
                prev: "<i class='fa fa-arrow-left'></i>",
                next: "<i class='fa fa-arrow-right'></i>"
            });
        }

        function searchText() {
            LoadUsers(0);
            index = 0;
        }

        $(document).ready(function () {



                
            $("#empty-grid-message").text("Please use the search feature");
            
            // Handle keypress events in desktop search input
            $("#searchtextBox").keypress(function (e) {
                if (e.which === 13) {
                    searchText();
                    e.preventDefault();
                }
            });



            $(".export").click(function () {
                var format = $(this).attr("title");
                var letter = filterByLetter;
                var query = $("#searchtextBox").val();
                var field = dropdownField;
                var domain = domainDropdown;
                var urlQuery = urlParam;
                var selectedDepartment = $("select[name=Department] option:selected").val();
                var selectedLocation = $("select[name=location] option:selected").val();

                var url = '/Home/Export' + '?urlQuery=' + urlQuery + '&format=' + format + (typeof letter != 'undefined' ? '&letter=' + letter : '') + (query != '' ? '&query=' + query : '')
                    + (typeof field != 'undefined' ? '&field=' + field : '') + (typeof domain != 'undefined' ? '&domain=' + domain : '') + (sortColumn != '' ? '&sortBy=' + sortColumn : '');
                url = encodeURI(url);


                if (selectedDepartment && selectedDepartment != "*") {
                    url += "&department=" + encodeURIComponent(selectedDepartment.replace("'", "''"));
                }
                if (selectedLocation && selectedLocation != "*") {
                    url += "&location=" + encodeURIComponent(selectedLocation.replace("'", "''"));
                }

                $('.a').html(field);
                
                window.open(url, '_blank');
            });

            //accordion
            $("#collapsible-main-data").delegate("[data-accordion]", "click", function () {
                var currentRowId = $(this).attr("href");
                var accordianName = currentRowId.substr(1, currentRowId.length).replace("ItemBody", "");

                var currentAccordianBody = $(currentRowId);
                //hide previous accordian body
                if (currentAccordianBody.find(".panel-body").css("display") == "none") {
                    currentAccordianBody.find(".panel-body").slideDown("slow");
                }
                else {
                    currentAccordianBody.find(".panel-body").slideUp("slow");
                }
                $(".panel-body").not(currentAccordianBody.find(".panel-body")).slideUp("slow");

                if (currentAccordianBody.html().length <= 22) {
                    var firstParam = "?path=" + currentAccordianBody.attr("data-path");
                    var secondParam = "&accordionName=" + accordianName;
                    var url = "/Home/Details" + firstParam + secondParam;
                    currentAccordianBody.load(url, function () {
                        // Bind hover event for element with class 'list'
                        $(currentAccordianBody).on({
                            mouseenter: function () {
                                if (!$(this).parent().hasClass("active"))
                                    $(this).attr('style', 'background-color:#860038 !important');
                            },
                            mouseleave: function () {
                                $(this).attr('style', 'background-color:#eaeaea');
                            }
                        }, ".list");
                    });
                }

                var current = $(this).find(".accordianDiv span");
                $(".accordianDiv span").not(current).removeClass("fa-plus fa-minus").addClass("fa-plus");
                if ($(this).find(".accordianDiv span").hasClass("fa-plus")) {
                    $(this).find(".accordianDiv span").removeClass("fa-plus").addClass("fa-minus");
                } else {
                    $(this).find(".accordianDiv span").removeClass("fa-minus").addClass("fa-plus");
                }

            });

            $("#collapsible-main-data").on("click", ".tabs .tab-links li.tabmenu", function () {
                var tabContentId = $($(this).attr("data-tab"));
                $(".tabmenu").removeClass("active");
                $(this).addClass("active");
                $(".tabContent").removeClass("active");
                tabContentId.addClass("active");
            });

            $("#selectSortBy").on('change', function () {
                sortColumn = $("#selectSortBy option:selected").val();
                var columnName = sortColumn.split(' ')[0];
                var isAsc = sortColumn.split(' ')[1] == 'ASC';

                LoadUsers(0);
                index = 0;


                $("#columnHeader .sortable").removeClass("sort-asc").removeClass("sort-desc");
                $("#columnHeader .sortable i").removeClass("fa-sort-desc").removeClass("fa-sort-asc");
                $("th[data-ldap=" + columnName + "]").addClass(isAsc ? "sort-asc" : "sort-desc")
                                                     .find("i").addClass(isAsc ? "fa-sort-asc" : "fa-sort-desc");
            });
            //$("#columnHeader").on('click', '.sortable a', function () {
            //    var sortContainer = $(this).parents(".sortable");
            //    var hasAlreadyAsc = sortContainer.hasClass("sort-asc");

            //    sortColumn = sortContainer.attr("data-ldap") + (hasAlreadyAsc ? " DESC" : " ASC");
            //    LoadUsers(0);
            //    index = 0;

            //    $("#columnHeader .sortable").removeClass("sort-asc").removeClass("sort-desc");
            //    sortContainer.addClass(hasAlreadyAsc ? "sort-desc" : "sort-asc");
            //});

            $("#searchFieldDropdown").on('click', '.item', function () {
                dropdownField = $(this).attr("data-name");
                LoadUsers(0);
                index = 0;
            });

            $("#domainDropdown").on('click', '.item', function () {
                domainDropdown = $(this).attr("data-name");
                LoadUsers(0);
                index = 0;
            });

            $("#letterFilters").on('click', '.letter-checkbox', function () {
                var currentLetter = $(this).text();
                filterByLetter = currentLetter;
                LoadUsers(0);
                index = 0;
            });

            $("#resetButton").click(function () {
                filterByLetter = '';
                LoadUsers(0);
                index = 0;

                setTimeout(function () {
                    $("#alphaText").html("A to Z");
                });

            });

            $("#ClearSearch").click(function () {
                var uri = window.location.toString();
                if (uri.indexOf("?") > 0) {
                    uri = uri.substring(0, uri.indexOf("?"));
                }
                window.history.replaceState({}, document.title, uri);
                $("#searchtextBox").val("");
                urlParam = "";
                index = 0;
                    
                $("#collapsible-main-data").html('<tr><td colspan="20" id="empty-grid-message"></td></tr>');
                $("#empty-grid-message").text("Please use the search feature");
                $('#pager').twbsPagination('destroy');
                
                //$("#searchtextBox").val("");
                //urlParam = "";
                //LoadUsers(0);
                //index = 0;

            });

            // Handle search focus and focusout for colors
            $("#searchtextBox").focus(function () {

                $(this).attr('style', 'background-color:#860038 !important;border:3px solid #006687 !important');
            });

            $("#searchtextBox").focusout(function () {

                $(this).attr('style', 'background-color:#808080 !important;border:3px solid #860038 !important');
            });

            // Handle hover colors for items in droplist
            $("#header .dropdown-menu li a").hover(function (e) {

                if (e.type === "mouseenter") {
                    $(this).attr('style', 'background-color:#6bb2cd !important');
                } else {
                    $(this).attr('style', 'background-color:transparent');
                }
            });

            $("a.clear-all").hover(function (e) {

                if (e.type === "mouseenter") {
                    $(this).attr('style', 'background-color:#6bb2cd !important');
                } else {
                    $(this).attr('style', 'background-color:transparent');
                }
            });

            //$("#columnHeader").on("click", ".sortColumn", function () {
            //    $(".sortColumn").find("i").removeClass("fa-sort-asc fa-sort-desc");
            //    if ($(this).hasClass("sort-desc")) {
            //        $(this).find("i").removeClass("fa-sort-asc");
            //        $(this).find("i").addClass("fa-sort-desc");
            //    } else {
            //        $(this).find("i").removeClass("fa-sort-desc");
            //        $(this).find("i").addClass("fa-sort-asc");
            //    }

            //});
        });
</script>
