  <script>
   /* Page Loader Script */
  
  $(window).on('load', function(){
      $(".page-loader").fadeOut("slow");
      $('.story-details').hide();
  });  
  
  
  $(document).ready(function(){

    // success stories dropdown boxes
    
    $('.ss-card-block').hover(function(){
    
      $(this).find('.card-background').toggleClass('card-background-hover');
      $(this).find('h2').toggle();
      
    });
    
    $('.ss-col').click(function(e){
      // prevent default anchor click behavior
      e.preventDefault();
      $('.fa-caret-down').hide();
      $(this).find('.fa-caret-down').show();
      $('.story-details').hide();
      $(this).find('.story-details').show(); 
      
      
    });

    // <a> tags with an href containing "#", wrapped in a parent with class of "scroll-nav"
    // will scroll to whatever element ID you place after "#" when clicked
    
    $(".scroll-nav [href^='#']").on('click', function(e) {
      
       // prevent default anchor click behavior
       e.preventDefault();

       // store hash
       var hash = this.hash;

       // animate
       $('html, body').animate({
         scrollTop: $(hash).offset().top
       }, 300, function(){

         // when done, add hash to url
         // (default click behaviour)
         window.location.hash = hash;
       });

    });
	
	// Meet the team profile photo click actions
	
	  $("#our-team a.vc_box_circle").attr({
		  "data-toggle":'tab',
		  "aria-expanded":'false'
	  });

	  $("#our-team .nav-tabs").attr({
		  "role":'tablist'

	  });

	  $("#our-team .tab-pane").attr({
		  "aria-expanded":'false'
	  });
	  
	  $('[data-toggle=tab]').click(function(e){
	  	//alert();
	  	var url = String(e.currentTarget);
	  	var tmp=url.split('#');
	  	var hash = tmp[1];
	  	$(".vc_row.wpb_row.vc_inner.vc_row-fluid.tab-pane.active").removeClass("active").attr("aria-expanded", false);
	  	$('#'+hash).addClass("active");
	  	
		  $('html, body').animate({
			  scrollTop: $(this.hash).offset().top
		  }, 500);
	  });

          
  });
  </script>
