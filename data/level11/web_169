function doGTranslate(lang_pair) {if(lang_pair.value)lang_pair=lang_pair.value;var lang=lang_pair.split('|')[1];var plang=location.pathname.split('/')[1];if(plang.length !=2 && plang != 'zh-CN' && plang != 'zh-TW')plang='en';if(lang == 'en')location.href='/'.replace('/'+plang+'/', '/');else location.href='/'+lang+'/'.replace('/'+plang+'/', '/');}
/* ]]> */
        </script>
