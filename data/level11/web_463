  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.5";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));
         </script>
         <div class="fb-page" data-adapt-container-width="true" data-height="600" data-hide-cover="true" data-href="https://www.facebook.com/sacstatecce" data-show-facepile="false" data-show-posts="true" data-small-header="true">
          <div class="fb-xfbml-parse-ignore">
           <blockquote cite="https://www.facebook.com/sacstatecce">
            <a href="https://www.facebook.com/sacstatecce">
             Sacramento State – College of Continuing Education
            </a>
           </blockquote>
          </div>
         </div>
        </div>
       </div>
      </div>
