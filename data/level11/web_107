      <script>
       function getSelectedPage() {
    var deptObj = {};
    
    
   
           deptObj[0] = ["Communication", "communication", "{http://www.cpp.edu/~class/}"];
            
            deptObj[1] = ["Economics", "economics", "{http://www.cpp.edu/~class/}"];
            
            deptObj[2] = ["English and Modern Languages", "english-Modern-languages", "{http://www.cpp.edu/~class/}"];
            
            deptObj[3] = ["Geography and Anthropology", "geography-anthropology", "{http://www.cpp.edu/~class/}"];
            
            deptObj[4] = ["History", "history", "{http://www.cpp.edu/~class/}"];
            
            deptObj[5] = ["Music", "music", "{http://www.cpp.edu/~class/}"];
            
            deptObj[6] = ["Philosophy", "philosophy", "{http://www.cpp.edu/~class/}"];
            
            deptObj[7] = ["Political Science", "political-science", "{http://www.cpp.edu/~class/}"];
            
            deptObj[8] = ["Psychology and Sociology", "psychology-sociology", "{http://www.cpp.edu/~class/}"];
            
            deptObj[9] = ["Theatre and New Dance", "theatre-new-dance", "{http://www.cpp.edu/~class/}"];
            
        
    
    
    


    var showdept = false;
     var maincontent = "";
     var fullurl ="";
     var matches = 0;
    
    Object.keys(deptObj).forEach(function(key) {
 
    if(window.location.pathname.indexOf(deptObj[key][1]) > -1)  {
        fullurl = deptObj[key][2] + "/" + deptObj[key][1]
        fullurl = fullurl.replace(/[{}]/g, "");
        if(matches == 0) {
            maincontent = "<h2><a href='" + fullurl + "'>" + deptObj[key][0] +  "</a></h2>";
        }
        matches++;
        showdept = true;
         console.log(maincontent); 
    }
 

    
});
    if(showdept == true) { 
        $( "#deptheading" ).html(maincontent);
        $( "#college-header-title" ).addClass("with-dept");
        $( "#college-navigation" ).css("display","none");
    }
    
        
}
      </script>
