  .light-green .goto-link{background: url(../common/resources/img/goto-arrow2.png) 0 center no-repeat transparent; 
                    font-size: 12px; 
                    background-position:right 3px; 
                    height:30px; 
                    float:left;}
  
  .tan .goto-link, .tan .goto-link a,
  .white .goto-link a,
  .light-green .goto-link a{color:#666; margin-left:-15px; padding-right:15px;}

   /*White Go To Links*/
   .blue .goto-link,
   .green .goto-link,
  .gold .goto-link,
  .red .goto-link{background: url(../common/resources/img/goto-arrow3.png) 0 center no-repeat transparent; 
                    font-size: 12px; 
                    background-position:right 3px; 
                    height:30px; 
                    float:left;}
  
  .blue .goto-link a,
  .green .goto-link a,
  .gold .goto-link a,
  .red .goto-link, .red .goto-link a{color:#fff; margin-left:-15px; padding-right:15px;}
  
  .none, .none .goto-link a{background:transparent !important;} 
 .rig .goto-link.yes, .rig .goto-link.no{margin-top:10px;}
 
@media (max-width: 1199px) {
  .rig.columns-1 .goto-link{width:44%; text-align:center;}
}  
@media (max-width: 767px) {
  ul.rig.columns-4 li {width: 49%; /* 22.5 this value + 1 should = 25% */}
  ul.rig h3{line-height:30px;}
  .rig.columns-1 .goto-link{width:90%; text-align:center;}
}
  @media (max-width: 638px) {
  .rig.columns-1 .goto-link{width:90%; text-align:center;}
}
@media (max-width: 549px) {
    ul.rig.columns-3 li {width: 100% !important; /* over-ride all li styles */	margin: 0 0 20px;}

}
@media (max-width: 480px) {
	ul.grid-nav li {display: block; margin: 0 0 5px;}
	ul.grid-nav li a {display: block;}
	ul.rig {margin-left: 0;}
	ul.rig li {width: 100% !important; /* over-ride all li styles */	margin: 0 0 20px;}
}
