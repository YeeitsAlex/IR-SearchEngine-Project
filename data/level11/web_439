             <h2>
              CRC Person of the Month
             </h2>
             <div class="content">
              <div about="/block/crc-person-of-the-month-block" class="entity entity-bean bean-stanford-testimonial-block clearfix" typeof="">
               <div class="content">
                <div class="postcard-right">
                 <div>
                  <div class="group-s-circle-image-style field-group-div circle-image">
                   <div class="field-collection-container clearfix">
                    <div class="field field-name-field-s-image-info field-type-field-collection field-label-hidden">
                     <div class="field-items">
                      <div class="field-item even">
                       <div class="field-collection-view clearfix view-mode-stanford-medium-square field-collection-view-final">
                        <div about="/field-collection/field-s-image-info/2373" class="entity entity-field-collection-item field-collection-item-field-s-image-info clearfix" typeof="">
                         <div class="content">
