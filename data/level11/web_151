   $( document ).ready(function() {						
				$.magnificPopup.open({
					items:{
						src: '#popup-box',
						type: 'inline'						
					},
					closeOnBgClick: false,
					closeOnContentClick: false,
					showCloseBtn: false
				});	
			$(document).on('click', '#tour', function (e) {
				$.magnificPopup.close();
			});
			$(document).on('click', '.popup-modal-dismiss', function (e) {
				e.preventDefault();
				$.magnificPopup.close();
			});
