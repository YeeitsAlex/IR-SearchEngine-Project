         <script type="text/javascript">
          $(document).ready(function () {
		//SEARCH BOX
		$('#searchbox-input').focus(function() {
			var searchValue = $(this).attr('value');
			if(searchValue == 'Search Site')
				$(this).attr('value','');
		});
		
		$('#searchbox-input').blur(function() {
			var searchValue = $(this).attr('value');
			if(searchValue == '')
				$(this).attr('value','Search Site');
		});
	});
	
	function searchSubmit () {
		var searchValue = $('#searchbox-input').attr('value');
		if(searchValue == 'Search Site') {
			alert('Search Value is Empty');
			return false;
		} else
			return true;
	}
