  <script>
   // Enable nav bar hover:
jQuery('ul.nav li.dropdown').hover(function() {
  jQuery(this).find('.dropdown-menu').stop(true, true).delay(200).fadeIn();
}, function() {
  jQuery(this).find('.dropdown-menu').stop(true, true).delay(200).fadeOut();
});
  </script>
  <script>
   $(document).ready(function(){
$('#Sub_Preference').find('input[type="radio"][value="NO"]').parent().remove(); 
$('#Sub_Preference').find('input[type="radio"][value="YES"]').parent().html('<input type="radio" name="SUB_OVERALL" value="YES">');
$('#Sub_Preference').find('input[type="radio"][value="YES"]').prop("checked", true);
$('#Sub_Preference').find('input[type="radio"][value="YES"][name="SUB_OVERALL"]').prop("hidden", true);
$('a[data-toggle="popover"]').each(function(){
	var content = $(this).attr('data-content');
	content = content.replace('Change Substitution Preference', 'Substitution Preference');
	content = content.replace("<div class='radio'><label><input type='radio' name='SUB_IND' value = 'NO'>NO</label></div>", '');
	content = content.replace("<div class='radio'><label><input type='radio' name='SUB_IND' value='NO'>NO</label></div>", '');
	$(this).attr('data-content', content);
});

});
  </script>
  <script type="text/javascript">
   $(document).ready(function () {
                //ADA fix keeps .ui-dialog from loading twice on the page
                $('.ui-dialog').each(function (e) {
                    if (e != 0) {
                        $(this).remove();
                    }
                });
            });
  </script>
  <script type="text/javascript">
   //allowing maintain scroll position on postbacks for mvc
        $(document).scroll(function(){
            localStorage['ControllerPageName'] = document.URL;
            localStorage['MaintainScrollPosition'] = $(document).scrollTop();
        });
        $(document).ready(function () {
            if (localStorage['ControllerPageName'] == document.URL) {
                $(document).scrollTop(localStorage['MaintainScrollPosition']);
            }
        });
  </script>
