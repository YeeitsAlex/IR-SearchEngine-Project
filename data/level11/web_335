count = 29;







function next() {







count++;







if (count > maxPic) {







count = 1;







}







eval("document.p.src=p" + count + ".src");







}







function back() {







count--;







if (count == 0) {







count = maxPic;







}







eval("document.p.src=p" + count + ".src");







}







function link() {







location.href=eval("link" + count);







}







