      $("#main .section").hide();
      $(".current-section").show();

      $(".section-header").click(function(){
        $(".current-header").removeClass("current-header");
        $(this).addClass("current-header");
        $(".current-section").hide();
        $(".current-section").removeClass("current-section");
        $("#" + $(this).attr("id") + "_sec").addClass("current-section")
        $(".current-section").show();
      })
    });
