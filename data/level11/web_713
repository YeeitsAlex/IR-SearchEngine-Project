             <script>
              var playCarousel = function(e){
    //console.log('Play');
    jQuery('.carousel').carousel('cycle');
    jQuery('.carousel').carousel('next');
  }
  var pauseCarousel = function(e){
   // console.log("PAUSE");
    jQuery('.carousel').carousel('pause');
  }

 jQuery('.carousel-slider-pause').on('click', function(e){
    // pause slider videos
    var theparentid = jQuery(this).parents(".carousel").attr("id");
    jQuery('video').each(function() {
      if(jQuery(this).parents(".carousel").attr("id") == theparentid){
      jQuery(this).get(0).pause();
      }//pause videos in the selected carousel
    });
  });

  jQuery('.carousel-slider-play').on('click', function(e){
    // play slider videos
    var theparentid = jQuery(this).parents(".carousel").attr("id");
    jQuery('video').each(function() {
      if(jQuery(this).parents(".carousel").attr("id") == theparentid){
        jQuery(this).get(0).play();
      }//play videos in the selected carousel
    });
  });
             </script>
