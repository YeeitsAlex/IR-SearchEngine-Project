              <script>
               jQuery('#testimonial-slider').on('slide.bs.carousel', function(e){
    var prev = jQuery(e.currentTarget);
    var next = jQuery(e.relatedTarget);
    //console.log(next.index());
    var first = (prev.index() == '0') ? '-240px' : '0';
    var last = (next.index() == '0') ? '-240px' : '0';
    //console.log('working, FIRST(' + first + ') : NEXT(' + last + ')');

    var offset = '240';
    if(jQuery(window).width() < 768) {
      offset = '220';
    }

    if(next.index() == '0') {
      next.css('top', offset + 'px');
      prev.css('top','-' + offset + 'px');
    } else {
      next.css('top','-' + offset + 'px');
    }
  });
  jQuery('#testimonial-slider').on('slid.bs.carousel', function(e){
    var prev = jQuery(e.currentTarget);
    var next = jQuery(e.relatedTarget);
    prev.removeAttr('style');
    next.removeAttr('style');
  });
              </script>
