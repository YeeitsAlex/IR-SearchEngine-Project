      <h2 class="page-header">
       Homepage
      </h2>
      <div class="region region-content">
       <section class="block block-system clearfix" id="block-system-main">
        <div about="/homepage" class="ds-1col node node-paragraphs-page node-promoted view-mode-full clearfix" typeof="sioc:Item foaf:Document">
         <div class="paragraphs-items paragraphs-items-field-basic-paragraphs paragraphs-items-field-basic-paragraphs-full paragraphs-items-full">
          <div class="field field-name-field-basic-paragraphs field-type-paragraphs field-label-hidden">
           <div class="field-items">
            <div class="field-item even">
             <style>
              .carousel-slider-control { left: calc(100% - 168px); }
  .carousel-indicators { left: calc(100% - 120px) !important; }
             </style>
             <div class="carousel slide" data-interval="7000" data-ride="carousel" id="carousel-component-1">
              <!-- Wrapper for slides -->
              <div class="carousel-inner" role="listbox">
               <div class="item active">
                <div about="" class="carousel-slide-item entity entity-paragraphs-item paragraphs-item-slide" typeof="">
                 <div class="content" style="height: 400px;">
                  <div class="carousel-slide-top-overlay">
                  </div>
                  <!-- check for mp4 video...-->
                  <!-- image -->
                  <img src="/sites/default/files/20160919_COFFieldTrip_HO-0640.jpg" style="height: 400px; object-fit: cover;"/>
                  <!-- BEGIN BOTTOM OVERLAY -->
                  <div class="carousel-slide-bottom-overlay-with-text">
                   <div class="paragraphs-items paragraphs-items-field-overlay paragraphs-items-field-overlay-full paragraphs-items-full">
                    <div class="field field-name-field-overlay field-type-paragraphs field-label-hidden">
                     <div class="field-items">
                      <div class="field-item even">
                       <div class="slide-overlay">
                        <h2>
                         <a href="https://visitosu.oregonstate.edu/">
                          Welcome to Oregon State
                         </a>
                        </h2>
                        <p>
                         <p>
                          Interested in one of our programs? We encourage you to visit campus and take the Natural Resources Tour!
                         </p>
                        </p>
                       </div>
                      </div>
                     </div>
                    </div>
                   </div>
                  </div>
                  <div class="carousel-slide-links">
                   <a class="carousel-slide-link" href="http://www.forestry.oregonstate.edu/ug-more-info?fbclid=IwAR2YUJuqXjaoCIflT13glBYP9WYi-ZYP9cTkFxk1bz8rEQVTeHXQZIBRPbE">
                    Request more info
                   </a>
                   <a class="carousel-slide-link" href="https://visitosu.oregonstate.edu/">
                    Visit campus
                   </a>
                  </div>
