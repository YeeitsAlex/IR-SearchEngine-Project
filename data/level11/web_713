                 // VIDEO SLIDER JS
      jQuery(document).ready(function(){
        var winx= jQuery(window).width();
			  var winy = jQuery(window).height();
        if (winx > 750) {
          jQuery('video.nomobileautoplay').attr('autoplay','true')
          jQuery('video.yesmobileautoplay').attr('autoplay','true');
        }else{
          jQuery("video.nomobileautoplay").prop("onclick", null).off("click");
          jQuery('video.yesmobileautoplay').attr('autoplay','true');
        }//only play video if we're over 750px wide or if autoplay on mobile box checked
      });
