          <script type="text/javascript">
           $(function() {
                                                  function matchHeights() {
                                                    var $elems = $(".home-page .block, .home-page .sfeventsList");
                                                    $elems.height("auto");
                                                    
                                                    var $responsiveElem = $(".home-page .blocks .sf_colsOut");
                                                    $responsiveElem.parent().hide(); // prevent JS from using computed value
                                                    var stacked = $responsiveElem.css("width") == "100%";
                                                    $responsiveElem.parent().show();
                                                    if (!stacked) {
                                                      var max = 0;
                                                      $elems.each(function (i, elem) {
                                                        max = Math.max(max, $(elem).height());
                                                      });
                                                      $elems.height(max);
                                                    }
                                                  }
                                                
                                                  matchHeights();
                                                  
                                                  var timeout;    
                                                  $(window).resize(function() {
                                                    clearTimeout(timeout);
                                                    timeout = setTimeout(matchHeights, 100);
                                                  });
                                                })
          </script>
         </div>
        </div>
       </div>
      </div>
     </div>
    </div>
