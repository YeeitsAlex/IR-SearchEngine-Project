    <a class="btn btn-sm btn-warning fancybox ajax" href="index.cfm?FuseAction=Public.cookiePolicy" title="Read cookie policy">
     Read cookie policy
    </a>
    Accept?
    <button class="btn btn-sm btn-warning" data-dismiss="alert" onclick="setCookie( cname = 'no-cookies', cvalue = 'false' );">
     Yes
    </button>
    <button class="btn btn-sm btn-warning" data-dismiss="alert" onclick="setCookieEndOfSession( cname = 'no-cookies', cvalue = 'true' );">
     No
    </button>
