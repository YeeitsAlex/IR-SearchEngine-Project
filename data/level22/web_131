    <!-- Alert Bar Control 
<div id="utmbAlertSystem" class="panel">
    <div id="utmb-alert-bar">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div id="alert-container" class="clearfix">
                        <a id="lnkAlertSite" href="https://www.utmb.edu/alert/" class="alert-bar-title" title="UTMB Alert Site">
                            <i class="fa fa-volume-up " aria-hidden="true"></i>
                            <span>Alerts</span>
                        </a>
