<main aria-label="main search for the library homepage" class="featuresearch" id="main-container">
 <div class="featuresearch-feature">
  <ul id="slider2" role="marquee" tabindex="-1">
  </ul>
  <script>
   $(document).ready(function() {
  $.ajax({
    dataType:'json',
    url:'https://lib.wvu.edu/admin/image_manager/includes/ajax/getImages.php',
    data: '',
    async: false,
    success:function() {
        console.log('success');
    },
    error: function (jqXHR, textStatus, errorThrown) {
        console.log(textStatus + ': ' + errorThrown);
    },
    complete: function(data){
        var jsonData = data.responseJSON;

        for (var i = 0; i < jsonData.length; i++) {
            var buildList = '<li class="slider-object"><a href="' + jsonData[i].actionURL +'">';
            buildList += '<img src="'+ jsonData[i].imageAd +'" alt="' + jsonData[i].altText + '" title="' + jsonData[i].name + '" />';
            buildList += '</a></li>';

            $('#slider2').append(buildList);
        }
    },
  });

  $('#slider2').bxSlider({
      auto: true,
      controls: true,
      minSlides: 1,
      maxSlides: 1,
      slideWidth: 360,
      slideMargin: 0,
      touchEnabled: true,
      autoHover: true,
  });
});
  </script>
 </div>
