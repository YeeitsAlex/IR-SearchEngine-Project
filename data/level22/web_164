 <script type="text/javascript">
  $(document).on('focusin', '.featuresearch-search-tab', function() {
      $(this).click();
    });

    $(document)
        .on('click',  '#as_sitesearch_wvrhc',        setSiteSearch)
        .on('submit', '#bookMediaSearchForm',        buildQuery)
        .on('submit', '#bookMediaSearchForm_summon', buildQuery_summon)
        .on('keyup',  '.sync-input',                 sync_input)
    function sync_input() {
        $(".sync-input").val($(this).val());
    }
    function setSearchQueryName() {
        var affectID = $(this).data("affectid");
        if ($(this).is(":checked")) {
            $("#"+affectID).attr('name', $(this).data("newname"));
        }
    }
    function setSiteSearch() {
        if ($(this).is(":checked")) {
            $("#as_sitesearch").attr('value', "wvrhc.lib.wvu.edu");
        }
        else {
            $("#as_sitesearch").attr('value', "lib.wvu.edu");
        }
    }
    function buildQuery() {
        if ($('#booksMediaByAuthor').is(":checked")) {
            $('#abjm3').val("au:"+($('#abjm3').val()));
        }
        else if ($('#booksMediaByTitle').is(":checked")) {
            $('#abjm3').val("ti:"+($('#abjm3').val()));
        }
    }
    function buildQuery_summon() {
        if ($('#booksMediaByAuthor').is(":checked")) {
            $('#booksAndMedia').val("(AuthorCombined:("+$('#booksAndMedia').val()+"))");
        }
        else if ($('#booksMediaByTitle').is(":checked")) {
            $('#booksAndMedia').val("(TitleCombined:("+$('#booksAndMedia').val()+"))");
        }
    }
 </script>
 <!-- End Search Box -->
