<script>
 ( function( $ ) {

	"use strict";

	var $show_hide = $( ".show-hide" );

	// Restructure things for show/hide behavior on sections with the `show-hide` class.
	$show_hide.each( function() {
        var $section = $( this ),
            $trigger = $section.find( "> header" );

        // Add a button with the `aria-expanded` attribute inside section headers.
		$trigger.find( ">:first-child" ).wrapInner( "<button aria-expanded='false'></button>" );
        
        // Add a class to the section headers and move them outside the section.
        $trigger.find( ">:first-child" ).unwrap().addClass( "collapsible-section-header" ).insertBefore( $section );
        
        // Add a class to the section and hide it.
		$section.addClass( "collapsible-section" ).hide();
	} );

	// Handle section display toggling.
	$( ".collapsible-section-header" ).on( "click", "button", function() {
        var $trigger = $( this ),
            $section = $( this ).closest( ".collapsible-section-header" ).next( ".collapsible-section" );
        
		if ( "false" === $trigger.attr( "aria-expanded" ) ) {
			$trigger.attr( "aria-expanded", "true" );
			$section.slideDown( "fast" );
		} else {
			$trigger.attr( "aria-expanded", "false" );
			$section.slideUp( "fast" );
		}
	} );
}( jQuery ) );
</script>
