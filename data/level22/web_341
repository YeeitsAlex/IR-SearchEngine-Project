//PIVOT SEARCHBAR


function pivot_search() {
var url="https://pivot.proquest.com/funding/results?src=widget&full=";
var full=document.getElementById("pivot_search_txt");
document.location=url + encodeURIComponent(full.value);
}
function handleKeyPress(e,form){ 
var key=e.keyCode || e.which; 
if (key==13) pivot_search();
} ;
