(function () {
    "use strict";

    function Build_ReportChart(path, chartname, charttitle) {
        jQuery('#Reports').append("<h3>" + charttitle + "</h3>");
        jQuery('#Reports').append("<div id='" + chartname + "_legend'></div>");
        jQuery('#Reports').append('<canvas id="' + chartname + '" width="600" height="400"></canvas>');
        var terms = [];
        var cahnrs = [];
        var cea = [];
        var cas = [];
        var vetmed = [];
        var pharm = [];
        var educ = [];
        var nursing = [];
        var medsci = [];
        var admin = [];
        var wsu = [];

        jQuery.ajax({
url: "https://ecodevoportal.wsu.edu/OcService.svc/rest/json/" + path,
            dataType: 'jsonp',
            success: function (json) {

                var bCahnrs = false;
                var bCea = false;
                var bCas = false;
                var bVetmed = false;
                var bPharm = false;
                var bEduc = false;
                var bNursing = false;
                var bMedsci = false;
                var bAdmin = false;
                var bWsu = false;

                var bFirstRun = true;
                jQuery.each(json, function (i, item) {

                    var d = new Date();
                    var fy = d.getFullYear();
                    var fyTerm = "FY" + item.Term;

                    if (d.getMonth() > 5) {
                        fy++;
                        if (fy == item.Term) {
                            fyTerm += "*";
                        }
                    }

                    if (jQuery.inArray(fyTerm, terms) === -1) {
                        terms.push(fyTerm);
                        if (!bFirstRun) {
                            if (!bCahnrs) {
                                cahnrs.push("0");
                            }
                            if (!bCea) {
                                cea.push("0");
                            }
                            if (!bCas) {
                                cas.push("0");
                            }
                            if (!bVetmed) {
                                vetmed.push("0");
                            }
                            if (!bPharm) {
                                pharm.push("0");
                            }
                            if (!bEduc) {
                                educ.push("0");
                            }
                            if (!bNursing) {
                                nursing.push("0");
                            }
                            if (!bMedsci) {
                                medsci.push("0");
                            }
                            if (!bAdmin) {
                                admin.push("0");
                            }
                            if (!bWsu)
                                wsu.push("0");
                        }
                        bFirstRun = false;
                        bCahnrs = false;
                        bCea = false;
                        bCas = false;
                        bVetmed = false;
                        bPharm = false;
                        bEduc = false;
                        bNursing = false;
                        bMedsci = false;
                        bAdmin = false;
                        bWsu = false;
                    }
                    if (item.Org == "COLLEGE OF AGRICULTURE HUMAN & NATURAL RESOURCE SCIENCES (0003)") {
                        cahnrs.push(Math.round(item.Value));
                        bCahnrs = true;
                    }
                    if (item.Org == "COLLEGE OF ENGINEERING & ARCHITECTURE (0005)") {
                        cea.push(Math.round(item.Value));
                        bCea = true;
                    }
                    if (item.Org == "COLLEGE OF ARTS AND SCIENCES (0031)") {
                        cas.push(Math.round(item.Value));
                        bCas = true;
                    }
                    if (item.Org == "COLLEGE OF VETRINARY MEDICINE (0011)") {
                        vetmed.push(Math.round(item.Value));
                        bVetmed = true;
                    }
                    if (item.Org == "COLLEGE OF PHARMACY (0015)") {
                        pharm.push(Math.round(item.Value));
                        bPharm = true;
                    }
                    if (item.Org == "COLLEGE OF MEDICAL SCIENCES (0008)") {
                        medsci.push(Math.round(item.Value));
                        bMedsci = true;
                    }
                    if (item.Org == "COLLEGE OF EDUCATION (0013)") {
                        educ.push(Math.round(item.Value));
                        bEduc = true;
                    }
                    if (item.Org == "COLLEGE OF NURSING (0006)") {
                        nursing.push(Math.round(item.Value));
                        bNursing = true;
                    }
                    if (item.Org == "ADMIN (0000)") {
                        admin.push(Math.round(item.Value));
                        bAdmin = true;
                    }
                    if (item.Org == "WSU") {
                        wsu.push(Math.round(item.Value));
                        bWsu = true;
                    }

                });
                if (!bCahnrs) {
                    cahnrs.push("0");
                }
                if (!bCea)
                    cea.push("0");
                if (!bCas)
                    cas.push("0");
                if (!bVetmed)
                    vetmed.push("0");
                if (!bPharm) {
                    pharm.push("0");
                }
                if (!bEduc) {
                    educ.push("0");
                }
                if (!bNursing) {
                    nursing.push("0");
                }
                if (!bMedsci) {
                    medsci.push("0");
                }
                if (!bAdmin) {
                    admin.push("0");
                }
                if (!bWsu)
                    wsu.push("0");

                if (!bWsu)
                    BuildChart(chartname);
                else
                    BuildWsuSummaryChart(chartname);
            }
        });

        function BuildChart(chartname) {
            var ctx = document.getElementById(chartname).getContext("2d");
            var data = {
                labels: terms,
                datasets: [
                    {
                        label: "CAHNRS",
                        fillColor: "rgba(152,30,50,1)",
                        strokeColor: "rgba(152,30,50,1)",
                        highlightFill: "rgba(152,30,50,0.8)",
                        highlightStroke: "rgba(152,30,50,1)",
                        data: cahnrs
                    },
                    {
                        label: "CEA",
                        fillColor: "rgba(220,220,220,1)",
                        strokeColor: "rgba(220,220,220,1)",
                        highlightFill: "rgba(220,220,220,0.8)",
                        highlightStroke: "rgba(220,220,220,1)",
                        data: cea
                    },
                    {
                        label: "CAS",
                        fillColor: "rgba(0,60,105,1)",
                        strokeColor: "rgba(0,60,105,1)",
                        highlightFill: "rgba(0,60,105,0.8)",
                        highlightStroke: "rgba(0,60,105,1)",
                        data: cas
                    },
                    {
                        label: "VETMED",
                        fillColor: "rgba(236,122,8,1)",
                        strokeColor: "rgba(236,122,8,1)",
                        highlightFill: "rgba(236,122,8,0.8)",
                        highlightStroke: "rgba(236,122,8,1)",
                        data: vetmed
                    },
                    {
                        label: "PHARM",
                        fillColor: "rgba(182,191,0,1)",
                        strokeColor: "rgba(182,191,0,1)",
                        highlightFill: "rgba(182,191,0,0.8)",
                        highlightStroke: "rgba(182,191,0,1)",
                        data: pharm
                    }
                    ,
                    {
                        label: "EDUC",
                        fillColor: "rgba(60,182,206,1)",
                        strokeColor: "rgba(60,182,206,1)",
                        highlightFill: "rgba(60,182,206,0.8)",
                        highlightStroke: "rgba(60,182,206,1)",
                        data: educ
                    }
                    ,
                    {
                        label: "NURSING",
                        fillColor: "rgba(219,206,172,1)",
                        strokeColor: "rgba(219,206,172,1)",
                        highlightFill: "rgba(219,206,172,0.8)",
                        highlightStroke: "rgba(219,206,172,1)",
                        data: nursing
                    }
                    ,
                    {
                        label: "MEDSCI",
                        fillColor: "rgba(94,106,113,1)",
                        strokeColor: "rgba(94,106,113,1)",
                        highlightFill: "rgba(94,106,113,0.8)",
                        highlightStroke: "rgba(94,106,113,1)",
                        data: medsci
                    }
                    ,
                    {
                        label: "ADMIN",
                        fillColor: "rgba(0,0,0,1)",
                        strokeColor: "rgba(0,0,0,1)",
                        highlightFill: "rgba(0,0,0,0.8)",
                        highlightStroke: "rgba(0,0,0,1)",
                        data: admin
                    }
                ]
            };

            var options = {
                //Boolean - Whether the scale should start at zero, or an order of magnitude down from the lowest value
                scaleBeginAtZero: true,

                //Boolean - Whether grid lines are shown across the chart
                scaleShowGridLines: true,

                scaleLabel: "<%=value %>",

                //String - Colour of the grid lines
                scaleGridLineColor: "rgba(0,0,0,.05)",

                //Number - Width of the grid lines
                scaleGridLineWidth: 1,

                //Boolean - If there is a stroke on each bar
                barShowStroke: true,

                //Number - Pixel width of the bar stroke
                barStrokeWidth: 2,

                //Number - Spacing between each of the X value sets
                barValueSpacing: 10,

                //Number - Spacing between data sets within X values
                barDatasetSpacing: 1,

                responsive: true,

                //String - A legend template
                legendTemplate: "<ul class=\"<%=name.toLowerCase()%>-legend\"><% for (var i=0; i<datasets.length; i++){%><li><span style=\"background-color:<%=datasets[i].fillColor%>\">   </span><%if(datasets[i].label){%><%=datasets[i].label%><%}%></li><%}%></ul>",

                multiTooltipTemplate: "<%= datasetLabel %> - <%=value%>"
            }
            var myBarChart = new Chart(ctx).Bar(data, options);
            //var legend = myBarChart.generateLegend();
            //$('#'+ chartname + '_legend').append(legend);
        }

        //WSU Summary Chart
        function BuildWsuSummaryChart(chartname, datalabel) {
            var ctx = document.getElementById(chartname).getContext("2d");

            var options = {
                //Boolean - Whether the scale should start at zero, or an order of magnitude down from the lowest value
                scaleBeginAtZero: true,

                //Boolean - Whether grid lines are shown across the chart
                scaleShowGridLines: true,

                scaleLabel: "<%=value %>",

                //String - Colour of the grid lines
                scaleGridLineColor: "rgba(0,0,0,.05)",

                //Number - Width of the grid lines
                scaleGridLineWidth: 1,

                //Boolean - If there is a stroke on each bar
                barShowStroke: true,

                //Number - Pixel width of the bar stroke
                barStrokeWidth: 2,

                //Number - Spacing between each of the X value sets
                barValueSpacing: 10,

                //Number - Spacing between data sets within X values
                barDatasetSpacing: 1,

                responsive: true,

                //String - A legend template
                legendTemplate: "<ul class=\"<%=name.toLowerCase()%>-legend\"><% for (var i=0; i<datasets.length; i++){%><li><span style=\"background-color:<%=datasets[i].fillColor%>\">   </span><%if(datasets[i].label){%><%=datasets[i].label%><%}%></li><%}%></ul>",

                multiTooltipTemplate: "<%= datasetLabel %> - <%=value%>"
            }

            var data = {
                labels: terms,
                datasets: [
                    {
                        label: datalabel,
                        fillColor: "rgba(152,30,50,1)",
                        strokeColor: "rgba(152,30,50,1)",
                        highlightFill: "rgba(152,30,50,0.8)",
                        highlightStroke: "rgba(152,30,50,1)",
                        data: wsu
                    }]
            };
            var myBarChart = new Chart(ctx).Bar(data, options);
            //myBarChart.addData(wsu, datalabel);
            //var legend = myBarChart.generateLegend();
            //var legend = myBarChart.generateLegend();
            //$('#'+ chartname + '_legend').append(legend);
        }

    };

if(jQuery("#Reports").length > 0) {
