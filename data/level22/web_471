              <div class="region region-three-33-second">
               <div class="region-inner clearfix">
                <section class="panel-pane pane-views pane-news buttons-middle block">
                 <div class="block-inner clearfix">
                  <h2 class="pane-title block-title">
                   News
                  </h2>
                  <div class="block-content">
                   <div class="view view-News view-id-News view-display-id-block_1 fp-news view-dom-id-c010d9d1c8c00cf515db2bb976d294e5">
                    <div class="view-content">
                     <table class="views-table cols-0">
