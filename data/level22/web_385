   $(document).ready(function(){
						$('#shareSearchBtn').click(function(){
							var textArea = document.createElement('textarea');
							// Place in top-left corner of screen regardless of scroll position.
							textArea.style.position = 'fixed';
							textArea.style.top = 0;
							textArea.style.left = 0;
							textArea.style.width = '2em';
							textArea.style.height = '2em';
							textArea.style.padding = 0; //reducing the size if it does flash render.
							textArea.style.border = 'none';
							textArea.style.outline = 'none';
							textArea.style.boxShadow = 'none';
							textArea.style.background = 'transparent'; // Avoid flash of white box if rendered for any reason.
							textArea.value = 'https://www.uthct.edu/events?start_date=2019-03-10&end_date=2020-03-10&search=';
							document.body.appendChild(textArea);
							textArea.select();

							try {
								var successful = document.execCommand('copy');
								alert('This search is now copied to your clipboard.');
							} catch (err) {
								console.log('Oops, unable to copy');
							}

							document.body.removeChild(textArea);
						});
					});
  </script>
