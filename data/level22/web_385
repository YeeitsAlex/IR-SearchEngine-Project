                            function watchMainMenuItemsForHover(){
                               $('#sid-primary-menu ul.nav li.dropdown').hover(function() {
                                 $(this).addClass('active').find('.dropdown-menu').stop(true, true).delay(200).fadeIn().addClass('shown');
                                }, function() {
                                  $(this).removeClass('active').find('.dropdown-menu').stop(true, true).delay(200).fadeOut().removeClass('shown');
                                });
                            }
                            if(window.innerWidth > 991){
                                watchMainMenuItemsForHover();
                            }
                        
		
    $(document).ready(function() {
        $(".release").dotdotdot({
            height: 80,
            fallbackToLetter: true,
            watch: true,
        });
    });

		
	$('.video-playlist iframe,.video-playlist object').filter(':not(.non-responsive-object)').each(function() {
		if($(this).parent().is(':not(div.embed-responsive)')){
			if($(this).is("iframe[src*='//player.vimeo.com'], iframe[src*='//www.youtube.com']")){
				$(this).wrap('<div class="embed-responsive embed-responsive-16by9"></div>');
			}
		}
		if($(this).is(':not(embed-responsive-item)')){
			$(this).addClass("embed-responsive-item");
		}
	});

		
	$('#page iframe,#page object').filter(':not(.non-responsive-object)').each(function() {
		if($(this).parent().is(':not(div.embed-responsive)')){
			if($(this).is("iframe[src*='//player.vimeo.com'], iframe[src*='//www.youtube.com']") || ($(this).is("iframe[src*='//www.facebook.com/']") && $(this).is("iframe[src*='video.php']"))){
				$(this).wrap('<div class="embed-responsive embed-responsive-16by9"></div>');
			}
		}
		if($(this).is(':not(embed-responsive-item)')){
			$(this).addClass("embed-responsive-item");
		}
	});
	$('#page table:not(.non-responsive-object)').each(function() {
		if($(this).parent().is(':not(div.table-responsive)')){
			$(this).wrap('<div class="table-responsive"></div>');
		}
		if($(this).is(':not(table)')){
			$(this).addClass("table");
		}
	});
	$('#page img:not(.img-responsive, .non-responsive-image)').addClass("img-responsive").css('height','auto').css('width', 'auto');

	})
  </script>
