  <script>
   (function(i,s,o,g,r,a,m){i["GoogleAnalyticsObject"]=r;i[r]=i[r]||function(){(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)})(window,document,"script","//www.google-analytics.com/analytics.js","ga");ga("create", "UA-16999842-1", {"cookieDomain":".twin-cities.umn.edu"});ga("send", "pageview");(function ($) {
  $(document).ready(function () {
    var beaconExists = 'sendBeacon' in navigator;
    function createFunctionWithTimeout(callback, opt_timeout) {
      var called = false;
      setTimeout(callback, opt_timeout || 300);
      return function() {
        if (!called) {
          called = true;
          callback();
        }
      }
    }
    function evtTrack(category, action, label) {
      console.log("Logging event: {" + category + ", " + action + ", " + label + "}");
      ga('send', 'event', {
        eventCategory: category, 
        eventAction: action, 
        eventLabel: label,
        transport: 'beacon'
      });
    }
    function evtTrackWithCompletion(category, action, label, onCompletion) {
      if(onCompletion !== null) {
        console.log("Logging event: {" + category + ", " + action + ", " + label + "}");
        ga('send', 'event', {
          eventCategory: category, 
          eventAction: action, 
          eventLabel: label,
          transport: 'beacon',
          hitCallback: createFunctionWithTimeout(onCompletion)
        });
      } else {
        evtTrack(category, action, label);
      }
    }
    // In analytics: Log the event in 'homesite' Category, link (URL of page clicked from),
    function evtTrackLink(link, onCompletion) {
      evtTrackWithCompletion('homesite', link, String(document.location), onCompletion);
    }
    function evtTrackRegionLinks(regionName, link, onCompletion) {
      evtTrackWithCompletion("semantic homesite", link, regionName, function() {
        evtTrackLink(link, onCompletion);
      });
    }

    var pagename = document.location.pathname;
    // selector: region name
    var regions = {
      '.home-image-mast > a, .image-mast > a, .image-mast-text a, .video-mast a': pagename + " - hero links",
      'ul.maroon-buttons a': pagename + " - maroon buttons",
      'ul.linkset-inpage a': pagename + " - anchor links",
      '.top-mast a': pagename + " - gold bar links",
      '.umn-mast a': pagename + " - masthead links",
      '#umn-footer a': pagename + " - footer links",
      '.social-media a': pagename + " - social media links",
      '.news-cards article:eq(0) a': pagename + " - news cards - card 1",
      '.news-cards article:eq(1) a': pagename + " - news cards - card 2",
      '.news-cards article:eq(2) a': pagename + " - news cards - card 3"
    };
    // The anchor tag classes not being tracked
    var anot = [".flex-next", ".flex-prev", '[href="#"]', '.see-more', '.see-less'];
    $.each(regions, function(selector, regionName) {
      anot.push(selector);
      $(document).on('click', selector, function(e) {
        var callback = null;
        if(!beaconExists) {
          e.preventDefault();
          callback = function() {
            document.location.href = link;
          }
        }
        var link = $(this).attr('href');
        evtTrackRegionLinks(regionName, link, callback);
      });
    });
    $(document).on('click', 'a.see-more', function(e) {
      var id = $(this).parents('article.news-items').attr('id');
      evtTrack("semantic homesite", '#' + id, pagename + " - news see more");
    });
    $(document).on('click', 'a.see-less', function(e) {
      var id = $(this).parents('article.news-items').attr('id');
      evtTrack("semantic homesite", '#' + id, pagename + " - news see less");
    });
    $(document).on('click', 'a', function (e) {
      var go = true;
      for(var i = 0; i < anot.length; i++) {
        if($(this).is(anot)) {
          go = false;
          break;
        }
      }
      if(go) {
        var callback = null;
        if(!beaconExists) {
          e.preventDefault();
          callback = function() {
            document.location.href = link;
          }
        }
        var link = $(this).attr('href');
        evtTrackLink(link, callback);
    }
    });

    $(document).on('click', '.flex-next, .flex-prev', function () {
      evtTrack('homesite', String('Slideshow ' + window.location.host + window.location.pathname + ' ' + jQuery(this).closest('.flexslider').attr('id')), String(((jQuery(this).closest('.flexslider').find('.flex-active-slide').index()) + 1)));
    });

    $(document).on('click', 'li.quiz-final', function () {
      evtTrack('goldyquiz', 'submitted', 'goldy-quiz');
    });

   $(document).on('click', '#slide-next', function () {
       console.log("shekedom");
      evtTrack('homesite', 'next-slide', 'umndriven-slider');
    });

    $(document).on('click', 'ul.linkset > li > a', function(e) {
      var id = $(this).attr('href');
      evtTrack("homesite", id, pagename);
    });

    $(document).on('submit', '#undergrad-majors-search, #grad-majors-search', function() {
        var prefix = "";
        if($(this).is("#undergrad-majors-search")) {
          prefix = "under";
        }
        var category = 'homesite ' + prefix + 'graduate search'
        var action = $.trim($(this).find('#' + prefix + 'grad-majors-search-text').val());
        if(action == "") {
          action = "No value";
        }
        var label = $.trim($(this).find('#' + prefix + 'graduate-colleges-field').val());
        if(label == "") {
          label = "No value";
        }
        evtTrack(category, action, label);
    });
  });
})(jQuery);
  </script>
