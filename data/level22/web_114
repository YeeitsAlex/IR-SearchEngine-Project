  <script>
   function gpacalc() {
	var gr = new Array(9);
	var cr = new Array(9);
	
	cr[0] = 4.0;
	cr[1] = 3.7;
	cr[2] = 3.3;
	cr[3] = 3.0;
	cr[4] = 2.7;
	cr[5] = 2.3;
	cr[6] = 2.0;
	cr[7] = 1.7;
	cr[8] = 1.3;
	cr[9] = 1.0;
	cr[10] = 0;
	
	gr[0] = "A";
	gr[1] = "A-";
	gr[2] = "B+";
	gr[3] = "B";
	gr[4] = "B-";
	gr[5] = "C+";
	gr[6] = "C";
	gr[7] = "C-";
	gr[8] = "D+";
	gr[9] = "D";
	gr[10] = "F";
	
	var ingr = new Array(5);
	ingr[0] = document.getElementById("gform_1").GR1.value;
	ingr[1] = document.getElementById("gform_1").GR2.value;
	ingr[2] = document.getElementById("gform_1").GR3.value;
	ingr[3] = document.getElementById("gform_1").GR4.value;
	ingr[4] = document.getElementById("gform_1").GR5.value;
	ingr[5] = document.getElementById("gform_1").GR6.value;
	ingr[6] = document.getElementById("gform_1").GR7.value;
	ingr[7] = document.getElementById("gform_1").GR8.value;
	
	var incr = new Array(5);
	incr[0] =  document.getElementById("gform_1").CR1.value;
	incr[1] =  document.getElementById("gform_1").CR2.value;
	incr[2] =  document.getElementById("gform_1").CR3.value;
	incr[3] =  document.getElementById("gform_1").CR4.value;
	incr[4] =  document.getElementById("gform_1").CR5.value;
	incr[5] =  document.getElementById("gform_1").CR6.value;
	incr[6] =  document.getElementById("gform_1").CR7.value;
	incr[7] =  document.getElementById("gform_1").CR8.value;
	
    grcount = 11;
    allgr = 0;
    allcr = 0;
	gpa = 0;
    
	for (var x = 0; x < 5 + 3; x++) {
		if (ingr[x]) {
			for (var xx = 0; xx < grcount; xx++) {
				if (ingr[x] == gr[xx]) {
					allgr = allgr + (parseInt(incr[x],10) * cr[xx]);
					allcr = allcr + parseInt(incr[x],10);
					validgrcheck = 1;
					break;
				}
			}
		}
	}
	
	if (allcr == 0) {
		alert("Error - You did not enter any credit values! GPA = N/A");
        
		return 0;
	}
	
	gpa = allgr / allcr;
    
    gpa = round2Fixed(gpa);
	
	alert("GPA = " + gpa);
	
	return 0;
}

function round2Fixed(value) {
  value = +value;

  if (isNaN(value))
    return NaN;

  // Shift
  value = value.toString().split('e');
  value = Math.round(+(value[0] + 'e' + (value[1] ? (+value[1] + 2) : 2)));

  // Shift back
  value = value.toString().split('e');
  return (+(value[0] + 'e' + (value[1] ? (+value[1] - 2) : -2))).toFixed(2);
}
