  <!-- HEADER SLIDE ON PAGE SCROLL DOWN -->
  <script type="text/javascript">
   $(function() {
		var bar = $('#headerSlideContainer');
		var top = bar.css('top');
		$(window).scroll(function() {
			if($(this).scrollTop() > 120) {
				bar.stop().animate({'top' : '0px'}, 0);
			} else {
				bar.stop().animate({'top' : top}, 0);
			}
		});
	});
  </script>
