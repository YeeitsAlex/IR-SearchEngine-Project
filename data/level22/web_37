  <script type="text/javascript">
   function theRotator() {
    //Set the opacity of all images to 0
    jQuery('.rotator ul li').css({opacity: 0.0});
    
    //Get the first image and display it (gets set to full opacity)
    jQuery('.rotator ul li:first').css({opacity: 1.0});
    jQuery('.banner_nav ul li:first').addClass('current');
      
    //Call the rotator function to run the slideshow, 6000 = change to next image after 6 seconds   
    update = setInterval('rotate()',6000);    
  }

  function rotate() { 
    //Get the first image
    var current = (jQuery('.rotator ul li.show') ?  jQuery('.rotator ul li.show') : jQuery('.rotator ul li:first'));
    var currentDot = ($('.banner_nav ul li.current') ?  $('.banner_nav ul li.current') : $('.banner_nav ul li:first'));

    if (current.length == 0){
      current = jQuery('.rotator ul li:first');
      currentDot = $('.banner_nav ul li:first');
    }

    //Get next image, when it reaches the end, rotate it back to the first image
    var next = ((current.next().length) ? ((current.next().hasClass('show')) ? jQuery('.rotator ul li:first') : current.next()) : jQuery('.rotator ul li:first'));
    var nextDot = ((currentDot.next().length) ? ((currentDot.next().hasClass('current')) ? $('.banner_nav ul li:first') : currentDot.next()) : $('.banner_nav ul li:first')); 

    //To rotate randomly    
    /*var sibs = current.siblings();
    var rndNum = Math.floor(Math.random() * sibs.length );
    var next = jQuery(sibs[ rndNum ]);*/  

    //Set the fade in effect for the next image, the show class has higher z-index
    next.css({opacity: 0.0}).addClass('show').animate({opacity: 1.0}, 1000);
    nextDot.addClass('current');

    //Hide the current image
    current.animate({opacity: 0.0}, 1000)
    .removeClass('show');
    currentDot.removeClass('current');  
  };
  
  jQuery('.banner_nav ul li').click(function(){
    var current_index = jQuery('.banner_nav ul li').index(this);
    jQuery('.banner_nav ul li').each(function(){
      jQuery(this).removeClass('current');
    });
    jQuery('.rotator ul li.show').each(function(){
      jQuery(this).animate({opacity: 0.0}, 1000).removeClass('show');
    });
    
    jQuery(jQuery('.banner_nav ul li').get(current_index)).addClass('current');
    jQuery(jQuery('.rotator ul li').get(current_index)).css({opacity: 0.0}).addClass('show').animate({opacity: 1.0}, 1000);
    clearInterval(update);
    update = setInterval('rotate()',6000);
  });

  jQuery(document).ready(function() {   
    //Load the slideshow
    theRotator();
    jQuery('.rotator').fadeIn(1000);
    jQuery('.rotator ul li').fadeIn(1000); // tweek for IE

  });
  </script>
  <script>
   $(document).ready(function(){ 
  $('#cres_content a img').parent().css('border','0px');
  });
  </script>
