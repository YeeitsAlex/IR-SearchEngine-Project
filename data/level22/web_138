   <section id="main" role="main">
    <div class="row clearfix">
     <div id="page-title">
      <h2 class="title">
       Front Page
      </h2>
     </div>
     <div class="no-sidebars" id="content">
      <div class="region region-content block-count1 clearfix">
       <div class="block block-system block-region-weight-1 odd" id="block-system-main">
        <div class="content block-body clearfix">
         <div class="panel-flexible panels-flexible-1 clearfix" id="inventure-front-design">
          <div class="panel-flexible-inside panels-flexible-1-inside">
           <div class="panels-flexible-row panels-flexible-row-1-1 panels-flexible-row-first clearfix row-venture-slideshow">
            <div class="inside panels-flexible-row-inside panels-flexible-row-1-1-inside panels-flexible-row-inside-first clearfix">
             <div class="panels-flexible-region panels-flexible-region-1-region_venture_slideshow panels-flexible-region-first panels-flexible-region-last region-venture-slideshow">
              <div class="inside panels-flexible-region-inside panels-flexible-region-1-region_venture_slideshow-inside panels-flexible-region-inside-first panels-flexible-region-inside-last">
               <div class="panel-pane pane-views pane-front-slide">
                <div class="pane-content">
                 <div class="view view-front-slide view-id-front_slide view-display-id-default view-dom-id-090733323684ca949574be1241e35017">
                  <div class="view-content">
                   <div class="views-row views-row-1 views-row-odd views-row-first views-row-last">
                    <div class="views-field views-field-field-slide-slide-image">
                     <div class="field-content">
                      <a href="https://inventureprize.gatech.edu/inventure-prize-ticket-request-form">
                       <img alt="InVenture Prize ticket request" height="600" src="https://inventureprize.gatech.edu/sites/default/files/styles/slideshow_slide__1500_x_600_/public/slides/request_ticket_innovation_1600x700px.png?itok=cZhnUFEA" typeof="foaf:Image" width="1500"/>
                      </a>
