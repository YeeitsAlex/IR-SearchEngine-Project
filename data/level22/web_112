   jQuery(document).ready(function($){

	// Set current link
	$("ul.stelter-nav > li > a").each(function(){
		
		var cLink = $(this);	

		path = $(this).attr("href").toString();

		if(window.location.pathname.match(path)){

			cLink.parent("li").addClass("dogeared");
		}

	});

	if(window.location.pathname != "/"){
		$("ul.stelter-nav > li.overview").removeClass("dogeared");	
	}

});
  </script>
