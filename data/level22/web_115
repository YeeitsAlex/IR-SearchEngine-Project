 <script>
  var dropdown_menu = document.getElementById('dropdown-menu');

// change the button based on the search profile
function type_change(anchor) {
	document.getElementById('search_type_display').innerText = anchor.innerText;
	
	// reset aria-selected for all links except the active one so the checkbox moves with the selected profile
	var links = dropdown_menu.querySelectorAll('a');
	for (var i = 0; i < links.length; i++) {
		links[i].setAttribute('aria-selected', 'false');
	}
	
	// reset "selected" class for all links except the active one so the checkbox moves with the selected profile
	var list_items = dropdown_menu.querySelectorAll('li');
	for (var i = 0; i < list_items.length; i++) {
		list_items[i].classList.remove('selected');
		list_items[i].classList.remove('active');
	}
	
	// add selected attributes to selected anchor/li
	anchor.classList.add('selected');
	anchor.querySelector('a').setAttribute('aria-selected', 'true');
}
 </script>
</section>
