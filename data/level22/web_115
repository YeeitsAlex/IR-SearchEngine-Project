       <div class="tab-pane fade in active" id="tab-1">
        <form action="https://utsa.primo.exlibrisgroup.com/discovery/search" id="homepage-quicksearch" method="get" style="margin-top: 0 !important; padding: 0" target="_blank">
         <div class="input-group" style="margin-top: 0 !important">
          <label class="formLabel quickSearchLabel" for="quickSearch">
           Quick Search
          </label>
          <input id="quickSearch" name="s" onblur="this.placeholder = 'Find Articles, Books &amp; More'" onfocus="this.placeholder = ''" placeholder="Find Articles, Books &amp; More" type="text"/>
          <input id="keywords" name="search" onfocus="" type="hidden" value=""/>
          <input class="submit btn btn-default" onclick="searchPrimo()" type="submit" value="Search"/>
         </div>
         <div class="input-group" style="margin-top: 0 !important">
          <label class="full-text" for="full-text">
           <input id="full-text" name="facet" type="checkbox" value="tlevel,include,online_resources"/>
           Available Online
          </label>
          <label class="peer-reviewed" for="peer-reviewed">
           <input id="peer-reviewed" name="facet" type="checkbox" value="tlevel,include,peer_reviewed"/>
           Peer Reviewed
          </label>
          <a class="advancedLink" href="https://utsa.primo.exlibrisgroup.com/discovery/search?vid=01UTXSANT_INST:DEFAULT&amp;sortby=rank&amp;mode=advanced" style="font-size: 127%" target="blank">
           Advanced Search
          </a>
         </div>
         <input name="vid" type="hidden" value="01UTXSANT_INST:DEFAULT"/>
         <input name="tab" type="hidden" value="Everything"/>
         <input name="search_scope" type="hidden" value="MyInst_and_CI"/>
         <input name="mode" type="hidden" value="Basic"/>
         <input name="displayMode" type="hidden" value="full"/>
         <input name="highlight" type="hidden" value="true"/>
         <input id="primoQuery" name="query" type="hidden" value="any,contains"/>
         <input name="displayField" type="hidden" value="all"/>
         <input name="facet" type="hidden" value="rtype,exclude,newspaper_articles"/>
         <input name="facet" type="hidden" value="rtype,exclude,reviews"/>
        </form>
        <script type="text/javascript">
         function searchPrimo() {
												document.getElementById("primoQuery").value = "any,contains," + document.getElementById("quickSearch").value.replace(/[,]/g, " ");
												
												document.forms["homepage-quicksearch"].submit();
											}
        </script>
       </div>
       <div class="tab-pane fade " id="tab-2">
        <form action="https://utsa.primo.exlibrisgroup.com/discovery/search" id="catalog-quicksearch" method="get" style="margin-top: 0 !important; padding: 0" target="_blank">
         <div class="input-group" style="margin-top: 0 !important">
          <label class="formLabel quickSearchLabel" for="catalog">
           Catalog
          </label>
          <input id="catalog" name="s" onblur="this.placeholder = 'Find Print Books &amp; DVDs'" onfocus="this.placeholder = ''" placeholder="Find Print Books &amp; DVDs" type="text"/>
          <input id="keywords" name="search" onfocus="" type="hidden" value=""/>
          <input class="submit btn btn-default" onclick="searchPrimoCatalog()" type="submit" value="Search"/>
         </div>
         <div class="input-group" style="margin-top: 0 !important">
          <a class="advancedLink" href="https://utsa.primo.exlibrisgroup.com/discovery/search?vid=01UTXSANT_INST:DEFAULT&amp;sortby=rank&amp;mode=advanced&amp;tab=LibraryCatalog&amp;search_scope=MyInstitution" style="font-size: 127%" target="blank">
           Advanced Search
          </a>
         </div>
         <input name="vid" type="hidden" value="01UTXSANT_INST:DEFAULT"/>
         <input name="tab" type="hidden" value="LibraryCatalog"/>
         <input name="search_scope" type="hidden" value="MyInstitution"/>
         <input name="highlight" type="hidden" value="true"/>
         <input id="primoQueryCatalog" name="query" type="hidden" value="any,contains"/>
         <input name="displayField" type="hidden" value="all"/>
        </form>
        <script type="text/javascript">
         function searchPrimoCatalog() {
											document.getElementById("primoQueryCatalog").value = "any,contains," + document.getElementById("catalog").value.replace(/[,]/g, " ");
											
											document.forms["catalog-quicksearch"].submit();
										}
        </script>
       </div>
       <div class="tab-pane fade " id="tab-3">
        <form action="https://utsa.primo.exlibrisgroup.com/discovery/search" id="textbooks-quicksearch" method="get" style="margin-top: 0 !important; padding: 0" target="_blank">
         <div class="input-group" style="margin-top: 0 !important">
          <label class="formLabel quickSearchLabel" for="textbooks">
           Textbooks/Reserves
          </label>
