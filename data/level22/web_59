  <script type="text/javascript">
   jQuery(function(){
    // Set submenu items' width to the width of the parents
    jQuery("li.sf-depth-2").parent().each(function() {
        jQuery(this).width(jQuery(this).parent().width());
    });
    
    jQuery.fn.extend({
        hideSuperfishUl : function(){
            var $ul = jQuery(['li.sfHover'].join(''),this).add(this)
            return this;
        }
    });
    
    // Fill the empty space for menus without items
    jQuery(".sf-depth-1.sf-no-children").append('<ul class="sf-megamenu sf-hidden"></ul>');
    
    // Trigger opening up the menus
    var menu_open = false;
    jQuery("ul.sf-menu").hoverIntent(
        // Mouse in
        function() {
            jQuery(this).find('li.sf-depth-1').addClass('sfHover');
            jQuery(this).find('ul').removeClass('sf-hidden');
        },
        // Mouse out
        function() {
            jQuery(this).find('li.sf-depth-1').removeClass('sfHover');
            jQuery(this).find('ul').addClass('sf-hidden');
            menu_open = false;
        }
    );
    
    /*
     * Set the menu height
     */
    // Get the height of the tallest coumn and add 24 pixels for a padding at the bottom
    var maxHeight = Math.max.apply(null, jQuery("li.sf-depth-1 ul.sf-hidden").map(function () {
        return jQuery(this).height() + 24;
    }).get());
    // Apply it to all menu columns
    jQuery("li.sf-depth-1 ul").height(maxHeight);
    
    // Apply dropdown css when mouse over main menu tabs
    jQuery('li.sf-depth-1').mouseover( function() {
        jQuery(this).children("ul:first").addClass('hovered').height(maxHeight + 1);
    });
    
    jQuery('li.sf-depth-1').mouseout( function() {
        jQuery(this).children("ul:first").removeClass('hovered').height(maxHeight);
    });

    // For tablets, make first click open the menu.
    // Second click => open link or close the menu
    jQuery("ul.sf-menu").click(function(event) {
        if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
            if (event.target.nodeName == 'A' && menu_open == false) {
                event.preventDefault();
            }
            if( menu_open ) {
                // Close
                jQuery(this).find('li.sf-depth-1').removeClass('sfHover');
                jQuery(this).find('ul').addClass('sf-hidden');
            } else {
                // Open
                jQuery(this).find('li.sf-depth-1').addClass('sfHover');
                jQuery(this).find('ul').removeClass('sf-hidden');
            }
            menu_open = !menu_open;
        }
    });
});
  </script>
