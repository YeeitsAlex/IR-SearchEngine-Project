function OnSubmitSearchForm()
{
  if(document.searchForm.collection[0].checked == true)
  {
    var searchits = document.searchForm.strSearchInput.value;
var pattern = / /g;
searchits = searchits.replace(pattern,'+');
document.searchForm.action ="http://morris.law.yale.edu/search/a?searchtype=X&searcharg=" + searchits + "&searchscope=1&SORT=D";
document.searchForm.method ="post";
  }
  else
