     <script>
      $( document ).ready(function() {
  //$(".component-container").filter(":contains('Operational')").remove();
  
   var mobileQuery = "56em";
    var fullMobileQuery = "40em";
    var windowWidth = $(window).width();
    
    // Fix show/hide nav & header nav on resize
    $(window).resize(function() {
        // show header nav
        if (window.matchMedia("(min-width: "+fullMobileQuery+")").matches){
            //console.log("min-width: "+fullMobileQuery+" - true");
            var headerNav = $("#header_nav");
            if (!headerNav.is(":visible"))
                headerNav.show();
        }
        // hide header nav when going to mobile
        if (window.matchMedia("(max-width: "+fullMobileQuery+")").matches){
            if (windowWidth == $(window).width()) return false;     // fixes resize event firing on mobile devices when scrolling
            //console.log("max-width: "+fullMobileQuery+" - true");
            var headerNav = $("#header_nav");
            if (headerNav.is(":visible"))
                headerNav.hide();
        }

        // hide mobile nav on desktop
        if (window.matchMedia("(min-width: "+mobileQuery+")").matches){
            var mobileNav = $("#mobile_high_council_chamber");
            if (mobileNav.is(":visible"))
                mobileNav.hide();
        }
    });

  
    $("#search").click(function() {
        var expandableRegion = $(".mobile_search");
        if (expandableRegion.is(":visible"))
            expandableRegion.slideUp("fast");
        else{
            expandableRegion.slideDown("fast");
            expandableRegion.find("input[type='text']").focus();
        }
    });

    $("#menu").click(function() {
        var expandableRegion = $("#mobile_high_council_chamber");
        var extraExandableRegions = $("#header_nav");

        if (expandableRegion.is(":visible")){
            expandableRegion.slideUp("fast");
            if (window.matchMedia("(max-width: "+fullMobileQuery+")").matches){
                extraExandableRegions.slideUp("fast");
            }
        }
        else{
            expandableRegion.slideDown("fast");
            if (window.matchMedia("(max-width: "+fullMobileQuery+")").matches){
                extraExandableRegions.slideDown("fast");
            } 
        }
         });
  
  
});
     </script>
    </div>
