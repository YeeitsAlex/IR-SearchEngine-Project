          <script type="text/javascript">
           function sticktothetop() {
    var window_top = jQuery(window).scrollTop();
    var top = jQuery('#stick-here').offset().top;
    if (window_top > top) {
        jQuery('.block-menu').addClass('stickyMenu');
        jQuery('#stick-here').height(jQuery('.block-menu').outerHeight());
    } else {
        jQuery('.block-menu').removeClass('stickyMenu');
        jQuery('#stick-here').height(0);
    }
}
jQuery(function() {
    jQuery(window).scroll(sticktothetop);
    sticktothetop();
});
          </script>
