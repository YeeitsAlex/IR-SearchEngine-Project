// Holly 12-1-2017 Updates

//Minimize tabs for smaller screens
jQuery(document).ready(function($){
    $("#tabs").tabs();
    $(window).resize(function() {
        // This will fire each time the window is resized:
        if($(window).width() >= 600) {
            // larger screen - adds css class with flex display
            $('#tabs').removeClass('tabsa');
            console.log("tab large");
        } else {
            // if smaller - removes class with flex display
            $('#tabs').addClass('tabsa');
            console.log("tab small");
        }
    }).resize();
  });

//Minimize contact side bar for smaller screens
jQuery(document).ready(function($){
    $(window).resize(function() {
        // This will fire each time the window is resized:
        $('.ico-text').removeClass('ico-text-sm-md');
        $('.ico-text').removeClass('ico-text-lg-xl');
        $('#ct-container').removeClass('ct-contact-sm');
        $('#ct-container').removeClass('ct-contact-med');
        $('#ct-container').removeClass('ct-contact-lg');
        $('#ct-container').removeClass('ct-contact-xl');
        
        if($(window).width() <= 640) {
            // larger screen - adds css class with flex display
            $('.ico-text').addClass('ico-text-sm-md');
            $('#ct-container').addClass('ct-contact-med');
            $('.ct-rent').removeClass('ct-columns');
            console.log("sm");
        } else if($(window).width() <= 960) {
            // if smaller - removes class with flex display
            $('.ico-text').addClass('ico-text-sm-md');
            $('#ct-container').addClass('ct-contact-med');
            $('.ct-rent').removeClass('ct-columns');
            console.log("med");
        } else if($(window).width() <= 1200) {
            // if smaller - removes class with flex display
            $('.ico-text').addClass('ico-text-lg-xl');
            $('#ct-container').addClass('ct-contact-lg');
            $('.ct-rent').addClass('ct-columns');
            console.log("lg");
        } else if($(window).width() > 1200) {
            // if smaller - removes class with flex display
            $('.ico-text').addClass('ico-text-lg-xl');
            $('#ct-container').addClass('ct-contact-xl');
            $('.ct-rent').addClass('ct-columns');
            console.log("xl");
        }
    }).resize();
  });

