           <header class="clearfix" id="main-content-header">
            <h1 id="page-title">
             HOME
            </h1>
           </header>
           <div id="content">
            <div class="block block-system no-title odd first block-count-4 block-region-content block-main" id="block-system-main">
             <article class="node node-page article odd node-lang-en node-full clearfix" id="node-1394" role="article">
              <div class="node-content">
              </div>
             </article>
            </div>
            <div class="block block-panels-mini no-title even block-count-5 block-region-content block-two-column-first" id="block-panels-mini-two-column-first">
             <div class="two-50 at-panel panel-display clearfix" id="mini-panel-two_column_first">
              <div class="region region-two-50-first">
               <div class="region-inner clearfix">
                <section class="panel-pane pane-views pane-spotlight thespotlight block">
                 <div class="block-inner clearfix">
                  <h2 class="pane-title block-title">
                   Research Resources
                  </h2>
                  <div class="block-content">
                   <div class="view view-spotlight view-id-spotlight view-display-id-block_2 view-dom-id-6ad5ae7d0ee63b7e0fc6039f61e06d54">
                    <div class="view-content">
                     <div class="flexslider optionset-wide-slider" id="flexslider-1">
                      <ul class="slides">
                       <li>
                        <div class="views-field views-field-field-spotlight-image">
