  <button class="pp-headlineblock-nav__button pp-headlineblock-nav__less button_less-2194555" onclick="backnews2194555();" tabindex="-1">
   <span class="pp-visually-hidden" lang="en">
    Show previous items
   </span>
   <span class="pp-headlineblock-nav__icon pp_icon pp_icon_angle-up">
   </span>
