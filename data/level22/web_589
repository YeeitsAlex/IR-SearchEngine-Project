<script>
 //hidden input values should be set with js to prevent wrong inputvalues when navigating back to this page
            pp_jquery("#hv_headlines_lastshown-2227800").val("3");
            pp_jquery("#hv_headlines_loaded-2227800").val("4");
            var stopservice2227800 = 0;
            var last_shown2227800 = 3;
            var loaded_items2227800 = 4;
            var totalreleases2227800 = 50;
            var corrected_totalreleases2227800 = totalreleases2227800 - 3;
            var pp_scrollHeight2227800 = pp_jquery(".pp_bigheadlines_height-2227800 .mm_listitem").outerHeight(true);
            var bigheadlinescontainerHeight2227800 = pp_scrollHeight2227800 * 3 - 10;

            var count = 0;

            function morenews2227800() {
                var last_shown2227800 = pp_jquery('#hv_headlines_lastshown-2227800').val();
                var loaded_items2227800 = pp_jquery('#hv_headlines_loaded-2227800').val();

                last_shown2227800 = parseInt(last_shown2227800) + 1;
                pp_jquery('#hv_headlines_lastshown-2227800').val(last_shown2227800);

                pp_jquery(".button_less-2227800").css("opacity",1);
                pp_jquery(".button_less-2227800").attr("onclick","backnews2227800();").attr('tabindex', '0');
                // Calculate height of scrolldiv
                var pp_scrollHeight2227800 = pp_jquery(".pp_bigheadlines_height-2227800 .mm_listitem").outerHeight(true);
                count++;

                pp_jquery(".button_more-2227800").removeAttr("onclick").attr('tabindex', '-1');
                pp_jquery(".pp_bigheadlines_items-2227800").animate({
                    marginTop: "-=" + pp_scrollHeight2227800                  }, 400, function() {
                    if (last_shown2227800 < totalreleases2227800){
                        pp_jquery(".button_more-2227800").attr("onclick","morenews2227800();").attr('tabindex', '0');

                    } else {
                        pp_jquery(".button_less-2227800").focus();
                    }

                    // update tabindex of headline-item links so only visible items are part of the tabindex.
                    var nonVisibleItems = pp_jquery('#pp-headline-2227800 .mm_listitem .pp_bigheadlines_heading a[tabindex="-1"]');
                    if (nonVisibleItems.length === 0) {
                        pp_jquery('#pp-headline-2227800 .mm_listitem:first-child a').attr('tabindex', -1);
                    } else {
                        var lastNonVisibleItem = nonVisibleItems[nonVisibleItems.length -1];
                        pp_jquery(lastNonVisibleItem).closest('.mm_listitem').next('.mm_listitem').find('a').attr('tabindex', -1);
                    }
                  });

                if (stopservice2227800 == 0) {

                    show_headlines2227800(loaded_items2227800,1);

                }

                if (last_shown2227800 >= totalreleases2227800) {

                    pp_jquery(".button_more-2227800").css("opacity",0.2);
                    stopservice2227800 = 1;

                } else {

                    if(loaded_items2227800 < totalreleases2227800){
                        limitstartnew = parseInt(loaded_items2227800) + 1;
                        pp_jquery('#hv_headlines_loaded-2227800').val(limitstartnew);
                    }
                }

            }

            function backnews2227800(){
                last_shown2227800 = parseInt(pp_jquery('#hv_headlines_lastshown-2227800').val()) - 1;
                pp_jquery('#hv_headlines_lastshown-2227800').val(last_shown2227800);
                pp_jquery(".button_more-2227800").css("opacity",1);
                pp_jquery(".button_more-2227800").attr("onclick","morenews2227800();").attr('tabindex', '0');
                // Calculate height of scrolldiv
                var pp_scrollHeight2227800 = pp_jquery(".pp_bigheadlines_height-2227800 .mm_listitem").outerHeight(true);
                count--;

                pp_jquery(".button_less-2227800").removeAttr("onclick").attr('tabindex', '-1');
                pp_jquery(".pp_bigheadlines_items-2227800").animate({
                    marginTop: "+=" + pp_scrollHeight2227800                  }, 400, function() {

                    if (last_shown2227800 > 3){
                        pp_jquery(".button_less-2227800").attr("onclick","backnews2227800();").attr('tabindex', '0');
                    }else {
                        pp_jquery(".button_more-2227800").focus();
                    }

                    // update tabindex of headline titles and other links so only visible items are part of the tabindex.
                    var nonVisibleItems = pp_jquery('#pp-headline-2227800 .mm_listitem .pp_bigheadlines_heading a[tabindex="-1"]');
                    var lastNonVisibleItem = nonVisibleItems[nonVisibleItems.length -1];
                    pp_jquery(lastNonVisibleItem).closest('.mm_listitem').find('a').not('.pp_bigheadlines_more').attr('tabindex', 0);
                  });

                if (last_shown2227800 == 3){
                    pp_jquery(".button_less-2227800").css("opacity",0.2);

                }
            }

            function getHeight2227800(){
                pp_jquery('.pp_bigheadlines_height-2227800').css("height", bigheadlinescontainerHeight2227800);
            }

            pp_jquery(document).ready(function() {

                getHeight2227800();

                                    try {
                        addthis.toolbox('.addthis_toolbox');
                    } catch(err){
                        //do nothing
                    }
                
            });


	function show_headlines2227800(limitstart, limitend){

		var tags_addon = '';
		var multimedia_count = '';
		var sharerelease = '';
				
		pp_jquery.getJSON('//newsroom.usra.edu/services/getheadlines.php?releases=["341445","338866","338183","335977","329155","329485","328255","324209","322662","322078","314668","313191","311807","310323","310318","301709","300487","289758","289611","288807","287153","285679","285575","284836","274446","270506","268757","267575","260370","243434","242042","232809","231266","213808","213168","212324","203866","202145","201984","201803","197319","195420","194908","195025","194522","194424","193445","193194","192911","190863"]' + tags_addon + multimedia_count + '&preview=&subtitle=&substring=&video=1&limitstart=' + limitstart + '&limitend=' + limitend, function(data) {

		if (!data) {

			stopservice2227800 = 1;

		} else {

			var items = '';

			pp_jquery.each(data, function(key, val) {

				if (key != 'error'){

						var date = val['date'];
						var date_split = date.split(" ");
						var date_day = date_split[0];
						var date_month = date_split[1];
						var date_year = date_split[2];
						var tags_items = '';
						var multimedia_count = '';
						var sharerelease = '';

                        var addthisDescription = val['message'].replace(/"/g, "");

						
													sharerelease += "<div class=\"pp_bigheadlines_share\">";
							sharerelease += "<div class=\"addthis_toolbox addthis_default_style\" addthis:title=\"" + val['title'] + "\" addthis:description=\"" + addthisDescription + "\" addthis:url=\"" + val['shareurl'] + "\">";
							sharerelease += "<a class=\"addthis_button_more\"><div class=\"pp_sharethis_custom pp_icon pp_icon_share\"><span class=\"pp-visually-hidden\">Share this release <span class=\"pp-visually-hidden\" lang=\"en\">(opens in dialog)</span></span></div></a>";
							sharerelease += "</div>";
							sharerelease += "</div>";

												
						if(val['subtitle']){
							var subtitle = "<h4 class=\"pp_bigheadlines_subheading\">" + val['subtitle'] + "</h4>";
						} else {
							var subtitle = "";
						}

						items += "<div class=\"mm_listitem\">";
												items += "<div class=\"pp_unit pp_one-of-four pp_bigheadlines_id-2227800\">";
						items += "<div class=\"pp_bigheadlines_image\" onclick=\"window.location='" + val['caseurl'] + "'\" style=\"" + val['featuredimage'] + "\" aria-hidden=\"true\" role=\"presentation\" tabindex=\"-1\">" + val['videothumbnail'] + "</div>";
						items += "</div>";
						items += "<div class=\"pp_unit pp_three-of-four pp_bigheadlines_data\">";
																		items += "<div class=\"pp_bigheadlines_date\"><div class=\"pp_date_month\">" + date_month + "</div><div class=\"pp_date_day\">" + date_day + "<span class=\"pp_date_day_comma\">,</span></div><div class=\"pp_date_year\">" + date_year + "</div></div>";
						
						items += "<div class=\"pp_bigheadlines_title\">";
						items += "<h3 class=\"pp_bigheadlines_heading\" title=\"" + val['title'] + "\" class=\"";
						items += 'pp_bigheadlines_title--share';
						items += "\">";
						items += "<a href=\"" + val['caseurl'] + "\">" + val['title'] + "</a>";
						items += "</h3>" + subtitle + "</div>";
						items += sharerelease;
						items += "<div class=\"pp_bigheadlines_text\">" + val['message'] + "</div>";
						items += "<a class=\"pp_bigheadlines_more\" title=\"" + val['title'] + "\" href=\"" + val['caseurl'] + "\" aria-hidden=\"true\" role=\"presentation\" tabindex=\"-1\">Read more from this release</a>";
						items += tags_items;
						items += multimedia_count;
						items += "</div>";
						items += "</div>";
				}

			});

				//pp_jquery(".preloader").fadeOut();
				pp_jquery(".pp_bigheadlines_items-2227800").append(items);
									try {
						addthis.toolbox('.addthis_toolbox');
					} catch(err){
						//do nothing
					}
				
				getHeight2227800();
			}
		});
	}
                           pp_jquery(window).resize(function() {

               getHeight2227800();
               pp_jquery(".pp_bigheadlines_items-2227800").css("marginTop", -pp_scrollHeight2227800 * count);

           });
</script>
