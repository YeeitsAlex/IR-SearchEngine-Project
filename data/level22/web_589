<script>
 //hidden input values should be set with js to prevent wrong inputvalues when navigating back to this page
            pp_jquery("#hv_headlines_lastshown-3025398").val("3");
            pp_jquery("#hv_headlines_loaded-3025398").val("4");
            var stopservice3025398 = 0;
            var last_shown3025398 = 3;
            var loaded_items3025398 = 4;
            var totalreleases3025398 = 8;
            var corrected_totalreleases3025398 = totalreleases3025398 - 3;
            var pp_scrollHeight3025398 = pp_jquery(".pp_bigheadlines_height-3025398 .mm_listitem").outerHeight(true);
            var bigheadlinescontainerHeight3025398 = pp_scrollHeight3025398 * 3 - 10;

            var count = 0;

            function morenews3025398() {
                var last_shown3025398 = pp_jquery('#hv_headlines_lastshown-3025398').val();
                var loaded_items3025398 = pp_jquery('#hv_headlines_loaded-3025398').val();

                last_shown3025398 = parseInt(last_shown3025398) + 1;
                pp_jquery('#hv_headlines_lastshown-3025398').val(last_shown3025398);

                pp_jquery(".button_less-3025398").css("opacity",1);
                pp_jquery(".button_less-3025398").attr("onclick","backnews3025398();").attr('tabindex', '0');
                // Calculate height of scrolldiv
                var pp_scrollHeight3025398 = pp_jquery(".pp_bigheadlines_height-3025398 .mm_listitem").outerHeight(true);
                count++;

                pp_jquery(".button_more-3025398").removeAttr("onclick").attr('tabindex', '-1');
                pp_jquery(".pp_bigheadlines_items-3025398").animate({
                    marginTop: "-=" + pp_scrollHeight3025398                  }, 400, function() {
                    if (last_shown3025398 < totalreleases3025398){
                        pp_jquery(".button_more-3025398").attr("onclick","morenews3025398();").attr('tabindex', '0');

                    } else {
                        pp_jquery(".button_less-3025398").focus();
                    }

                    // update tabindex of headline-item links so only visible items are part of the tabindex.
                    var nonVisibleItems = pp_jquery('#pp-headline-3025398 .mm_listitem .pp_bigheadlines_heading a[tabindex="-1"]');
                    if (nonVisibleItems.length === 0) {
                        pp_jquery('#pp-headline-3025398 .mm_listitem:first-child a').attr('tabindex', -1);
                    } else {
                        var lastNonVisibleItem = nonVisibleItems[nonVisibleItems.length -1];
                        pp_jquery(lastNonVisibleItem).closest('.mm_listitem').next('.mm_listitem').find('a').attr('tabindex', -1);
                    }
                  });

                if (stopservice3025398 == 0) {

                    show_headlines3025398(loaded_items3025398,1);

                }

                if (last_shown3025398 >= totalreleases3025398) {

                    pp_jquery(".button_more-3025398").css("opacity",0.2);
                    stopservice3025398 = 1;

                } else {

                    if(loaded_items3025398 < totalreleases3025398){
                        limitstartnew = parseInt(loaded_items3025398) + 1;
                        pp_jquery('#hv_headlines_loaded-3025398').val(limitstartnew);
                    }
                }

            }

            function backnews3025398(){
                last_shown3025398 = parseInt(pp_jquery('#hv_headlines_lastshown-3025398').val()) - 1;
                pp_jquery('#hv_headlines_lastshown-3025398').val(last_shown3025398);
                pp_jquery(".button_more-3025398").css("opacity",1);
                pp_jquery(".button_more-3025398").attr("onclick","morenews3025398();").attr('tabindex', '0');
                // Calculate height of scrolldiv
                var pp_scrollHeight3025398 = pp_jquery(".pp_bigheadlines_height-3025398 .mm_listitem").outerHeight(true);
                count--;

                pp_jquery(".button_less-3025398").removeAttr("onclick").attr('tabindex', '-1');
                pp_jquery(".pp_bigheadlines_items-3025398").animate({
                    marginTop: "+=" + pp_scrollHeight3025398                  }, 400, function() {

                    if (last_shown3025398 > 3){
                        pp_jquery(".button_less-3025398").attr("onclick","backnews3025398();").attr('tabindex', '0');
                    }else {
                        pp_jquery(".button_more-3025398").focus();
                    }

                    // update tabindex of headline titles and other links so only visible items are part of the tabindex.
                    var nonVisibleItems = pp_jquery('#pp-headline-3025398 .mm_listitem .pp_bigheadlines_heading a[tabindex="-1"]');
                    var lastNonVisibleItem = nonVisibleItems[nonVisibleItems.length -1];
                    pp_jquery(lastNonVisibleItem).closest('.mm_listitem').find('a').not('.pp_bigheadlines_more').attr('tabindex', 0);
                  });

                if (last_shown3025398 == 3){
                    pp_jquery(".button_less-3025398").css("opacity",0.2);

                }
            }

            function getHeight3025398(){
                pp_jquery('.pp_bigheadlines_height-3025398').css("height", bigheadlinescontainerHeight3025398);
            }

            pp_jquery(document).ready(function() {

                getHeight3025398();

                
            });


	function show_headlines3025398(limitstart, limitend){

		var tags_addon = '';
		var multimedia_count = '';
		var sharerelease = '';
				
		pp_jquery.getJSON('//newsroom.usra.edu/services/getheadlines.php?releases=["303129","302556","282283","283337","281625","230746","222173","167880"]' + tags_addon + multimedia_count + '&preview=&subtitle=&substring=&video=&limitstart=' + limitstart + '&limitend=' + limitend, function(data) {

		if (!data) {

			stopservice3025398 = 1;

		} else {

			var items = '';

			pp_jquery.each(data, function(key, val) {

				if (key != 'error'){

						var date = val['date'];
						var date_split = date.split(" ");
						var date_day = date_split[0];
						var date_month = date_split[1];
						var date_year = date_split[2];
						var tags_items = '';
						var multimedia_count = '';
						var sharerelease = '';

                        var addthisDescription = val['message'].replace(/"/g, "");

						
												
						if(val['subtitle']){
							var subtitle = "<h4 class=\"pp_bigheadlines_subheading\">" + val['subtitle'] + "</h4>";
						} else {
							var subtitle = "";
						}

						items += "<div class=\"mm_listitem\">";
												items += "<div class=\"pp_unit pp_one-of-four pp_bigheadlines_id-3025398\">";
						items += "<div class=\"pp_bigheadlines_image\" onclick=\"window.location='" + val['caseurl'] + "'\" style=\"" + val['featuredimage'] + "\" aria-hidden=\"true\" role=\"presentation\" tabindex=\"-1\">" + val['videothumbnail'] + "</div>";
						items += "</div>";
						items += "<div class=\"pp_unit pp_three-of-four pp_bigheadlines_data\">";
																		items += "<div class=\"pp_bigheadlines_date\"><div class=\"pp_date_month\">" + date_month + "</div><div class=\"pp_date_day\">" + date_day + "<span class=\"pp_date_day_comma\">,</span></div><div class=\"pp_date_year\">" + date_year + "</div></div>";
						
						items += "<div class=\"pp_bigheadlines_title\">";
						items += "<h3 class=\"pp_bigheadlines_heading\" title=\"" + val['title'] + "\" class=\"";
						items += 'pp_bigheadlines_title--share';
						items += "\">";
						items += "<a href=\"" + val['caseurl'] + "\">" + val['title'] + "</a>";
						items += "</h3>" + subtitle + "</div>";
						items += sharerelease;
						items += "<div class=\"pp_bigheadlines_text\">" + val['message'] + "</div>";
						items += "<a class=\"pp_bigheadlines_more\" title=\"" + val['title'] + "\" href=\"" + val['caseurl'] + "\" aria-hidden=\"true\" role=\"presentation\" tabindex=\"-1\">Read more from this release</a>";
						items += tags_items;
						items += multimedia_count;
						items += "</div>";
						items += "</div>";
				}

			});

				//pp_jquery(".preloader").fadeOut();
				pp_jquery(".pp_bigheadlines_items-3025398").append(items);
				
				getHeight3025398();
			}
		});
	}
                           pp_jquery(window).resize(function() {

               getHeight3025398();
               pp_jquery(".pp_bigheadlines_items-3025398").css("marginTop", -pp_scrollHeight3025398 * count);

           });
</script>
