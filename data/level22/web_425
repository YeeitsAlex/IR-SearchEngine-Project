  <script>
   $(function(){$('#username').attr('placeholder','Username');$('#password').attr('placeholder','Password');});
    function open_alert(obj){
        if(obj.checked){
            var r = confirm('Do not enable Remember Me on a Public/Shared System')
        }
        if(r == true){
            return true;
        }
        else{
            obj.checked = false ;
        }
    }
  </script>
