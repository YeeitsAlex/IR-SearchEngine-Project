  <script>
   ( function( $ ) {

	"use strict";

	var $show_hide = $( "dl.folded" );

	// Restructure DOM elements for collapsible section behavior.
	$show_hide.each( function() {
        var $list = $( this ),
            $headings = $list.find( "> dt" ),
			$sections = $list.find( "> dd" );

        // Replace the `dl` tag with a div containt the `show-hide` class.
        $list.wrapInner( "<div class='show-hide'></div>" ).contents().unwrap();
        
        // Replace `dt` tags with buttons containing the `aria-expanded` attribute.
		$headings.wrapInner( "<button aria-expanded='false'></button>" ).contents().unwrap();
        
        // Replace `dd` tags with divs containing the `closed` class.
		$sections.wrapInner( "<div class='closed'></div>" ).contents().unwrap();
	} );

	// Handle section display toggling.
	$( ".show-hide" ).on( "click", "button", function() {
        var $heading = $( this ),
            $section = $( this ).next( "div" );
        
		if ( "false" === $heading.attr( "aria-expanded" ) ) {
			$heading.attr( "aria-expanded", "true" );
			$section.removeClass( "closed" );
		} else {
			$heading.attr( "aria-expanded", "false" );
			$section.addClass( "closed" );
		}
	} );
}( jQuery ) );

