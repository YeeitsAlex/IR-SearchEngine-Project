		 var closeTD = '</td>';
		 document.getElementById('theCarousel').innerHTML ='<table width="100%"><tr>'+ openTD1 + cell1Txt + closeTD + openTD2 + cell2Txt + closeTD + '</tr></table>';
		}
	}
