   if (navigator.userAgent.toLowerCase().indexOf("msie")>-1 && document.documentMode < 9){
		if (document.getElementById("nasTopMenu")){
		document.getElementById('nasTopMenu').innerHTML = topmenuTbl;
		}
		if (document.getElementById("theCarousel")){
		 document.getElementById('carousel-leftcolumn').getElementsByTagName('ol')[0].innerHTML = "";
		 var cell1Txt = document.getElementById('carousel-leftcolumn').innerHTML;
		 var cell2Txt = '<table id="carrouselHeadlines">' + document.getElementById('carousel-rightcolumn').getElementsByTagName('table')[0].innerHTML + '</table>'
		 var openTD1 = '<td style="vertical-align:top" width="450px">';
