              <div class="block-content content" ng-non-bindable="">
               <div class="boxes-box" id="boxes-box-1509636200">
                <div class="boxes-box-content">
                 <div class="flexslider-text-overlay">
                  <div class="flexslider-text-overlay-inner">
                   <h2>
                   </h2>
                   <p>
                   </p>
                  </div>
                 </div>
                 <div class="flexslider-container">
                  <div class="flexslider">
                   <ul class="slides">
