  <script id="js-scrollTo">
   (function (collection, toElements) { function scrollToEl(list) { list.map(function (item) { $('html, body').animate({ scrollTop: $(item).offset().top }, 1100); }); } function delegator(event) { scrollToEl(toElements); } collection.map(function (item) { return item.addEventListener('click', delegator, false); }); })(Array.from(SWLXWS_DOC.querySelectorAll('.tabs li')), Array.from(SWLXWS_DOC.querySelectorAll('#explore-ut-southwestern')));
  </script>
  <script type="text/javascript">
   $(window).scroll(lazyLoadJuicer); var feed = $('.juicer-feed'); var juicerFeedScrollDistance = feed.offset().top; var juicerLoaded = false; function lazyLoadJuicer() { var scrollDistance = $(window).scrollTop(); var windowHeight = $(window).height(); if ((scrollDistance >= (juicerFeedScrollDistance - windowHeight)) && !juicerLoaded) { $.getScript('//assets.juicer.io/embed.js', function(data, textStatus, jqxhr) { console.log(data); console.log(textStatus); console.log(jqxhr.status); console.log("load was performed"); }); $('head').append('<link rel="stylesheet" media="all" type="text/css" href="//assets.juicer.io/embed.css">'); juicerLoaded = true; } }; lazyLoadJuicer();
  </script>
