   <iframe height="0" src="//www.googletagmanager.com/ns.html?id=GTM-K5CHVG" style="display:none;visibility:hidden" width="0">
   </iframe>
  </noscript>
  <script src="https://iacuc.wsu.edu/wp-content/themes/spine/js/spine-theme.js?ver=0.29.0-1.7.9-44092" type="text/javascript">
  </script>
  <script src="https://iacuc.wsu.edu/wp-includes/js/wp-embed.min.js?ver=4.9.9" type="text/javascript">
  </script>
  <!-- Custom JavaScript Editor -->
  <script>
   (function ($) {
    'use strict';

    if ($('.accordion')[0]) {

        $('.accordion').find('h2 + div').each(function (index) {
			var content_id = 'accordion-content-' + index;

            $(this).attr('id', content_id).attr('aria-hidden', 'true').hide().
                prev('h2').attr('aria-controls', content_id).attr('aria-expanded', 'false');
        });

        $('.accordion').on('click', 'h2', function () {
            if ('false' === $(this).attr('aria-expanded')) {
                $(this).attr('aria-expanded', 'true').
                    next('div').attr('aria-hidden', 'false').slideDown('fast');
            } else {
                $(this).attr('aria-expanded', 'false').
                    next('div').attr('aria-hidden', 'true').slideUp('fast');
            }
        });

	}
}(jQuery));
  </script>
 </body>
</html>

