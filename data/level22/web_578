  <script>
   function validate(event) {
      var $form = $(event.target);
      var $email = $form.find("input[name=email]");
      $email.val( $.trim($email.val()) );
      $section = $form.closest('section');
      if (!validEmail( $email.val() )) {
        displayError('The email address you entered is not a valid email address.');
        return false;
      }
      if ($section.is('#register, #guest') && !$section.find('input[name=tou]').is(':checked')) {
        displayError('You must agree to the terms of use.');
        return false;
      }
      return true;
    }

    function validEmail(email) {
      emailRegex = /^[a-z0-9\.\_\%\+\-\']+@[a-z0-9\.-]+\.[a-z]{2,}$/i;
      return emailRegex.test(email);
    }

    function displayError(message) {
      $(".alert span").text(message);
      $('.alert').show();
      $(document).trigger('displayError');
    }

    $(function() { 
      // Validate forms on submit.
      $("form").on('submit', validate);

      // Change page title on tab switch.
      $("a[data-toggle='tab']").on('show.bs.tab', function() {
        var section = $( $(this).attr('href') ).attr('data-title') || 'Login';
        document.title = section + ' | National Academies Press';
      });

      // Apply placeholder text polyfill to older browsers.
      $("input, textarea").placeholder(); 

      // Keep email address form field value across sections.
      $("input[name='email']").on('change', function(event) {
        $("input[name='email']").val( $(event.target).val() );
      });
      $("input[name='mailings']").on('change', function(event) {
        $("input[name='mailings']").val( $(event.target).val() );
      });

      // Dismiss the aler t if the user navigates to a different section or clicks the close button.
      $("a, button, .close").on('click', function() {
        $(".alert").hide();
      });

      /////////////////////////////////////
      // Google Analytics Event Tracking //
      /////////////////////////////////////

      // NAP: login page errors
      //   error > [active tab id] > [error message]
      $(trackErrorEvent);
      $(document).on('displayError', trackErrorEvent);
      function trackErrorEvent(event) {
        if ($(".alert:visible").length > 0) {
          // trackEvent('error', $("section.active").attr('id'), $(".alert:visible span").text());
        }
      }

      // NAP: login nav
      //   login > nav > [current tab ID -> target tab ID]
      $("a[data-toggle='tab']").on('click', function(e) {
        // trackEvent('login', 'nav', $(this).closest('section').attr('id') + ' -> ' + $(this).attr('href').replace('#', ''));
      });

      // NAP: login submit
      //   login > submit > [current tab ID]
      // NAP: email signup
      //   email > subscribe > [current tab ID]
      $("form").on('submit', function(e) {
        // trackEvent('login', $(this).closest('section').attr('id'), 'submit');
        if ($("input[name='mailings']").is(":checked")) {
          // trackEvent('email', 'subscribe', $(this).closest('section').attr('id'));
          window.dataLayer = window.dataLayer || [];dataLayer.push({ 
            'mailing': "yes",
          });
        }
      });
      $("form").on('submit', function(e) {
        // trackEvent('login', $(this).closest('section').attr('id'), 'submit');
        if ($("#guestMailings").is(":checked")) {
          // trackEvent('email', 'subscribe', $(this).closest('section').attr('id'));
          window.dataLayer = window.dataLayer || [];dataLayer.push({ 
            'mailing': "yes",
          });
        }
      });

    });
  </script>
