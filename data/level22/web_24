  <script language="JavaScript" type="text/javascript">
   //<![CDATA[
var mycookie = document.cookie;

function clearCookie(name) {
  document.cookie=name+"=; path=/ ; domain=.utexas.edu";
  location.reload();
  getCookie(name);
}

function getCookie(name) {
    var index = mycookie.indexOf(name + '=');
    if (index == -1)
    return null;
    index = mycookie.indexOf("=", index) + 1;
    var endstr = mycookie.indexOf(';', index);
    if (endstr == -1)
    endstr = mycookie.length;
    return unescape(mycookie.substring(index, endstr));
}

function setCookie(name, value) {
    if (value == '') {
        document.cookie=name + "=; path=/; domain=.utexas.edu";
    } else {
        var oldvalue = getCookie(name);
        if (oldvalue != null) {
            var index = oldvalue.indexOf(value);
        } else {
            var index = -1;
        }

        if (index == -1) {
            if (value != null && value != "") {
                if (oldvalue != null) {
                    document.cookie=name + "=" + oldvalue + escape(value) + "; path=/ ; domain=.utexas.edu";
                } else {
                        document.cookie=name + "=" + escape(value) + "; path=/ ; domain=.utexas.edu";
                }
                mycookie = document.cookie;
            }
            mycookie = document.cookie;
            document.forms['cartform'].submit();
            // this.document.cartform.submit();
        } else {
            alert("Cart contains this course already!");
        }
    }
}
//]]>
  </script>
