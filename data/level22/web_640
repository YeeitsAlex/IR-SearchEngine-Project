       var system = {"name":"Research Guides"};
                    springSpace.publicObj = new springSpace.public.Public({
                        constant: {
                            PROCESSING: {
                                
                            },
                            CONTENT: {
                            }
                        },
                        system_name: system.name,
                        customer: {"name":"East Carolina University Libraries"}
                    });
					springSpace.Session.CONST.ACTION_AUTH_SET_LG_SESSION_COOKIE = 750;
					jQuery(function() {
						springSpace.Session.getLGSessionStatus({
							url: "https://ecu.libapps.com/libapps/login_process.php",
							data: {
								action: 5,
								app_id: 1,
								domain: "libguides.ecu.edu",
								site_id: 1386
							}
						});
					});
				jQuery(function() {
					setInterval(function() {
						springSpace.Session.getLGSessionStatus({
							url: "https://ecu.libapps.com/libapps/login_process.php",
							data: {
								action: 5,
								app_id: 1,
								domain: "libguides.ecu.edu",
								site_id: 1386
							}
						})
					}, 600000);
				});
