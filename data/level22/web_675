     <section id="page-content-section">
      <div class="page-content col-xs-12 col-sm-12 col-md-9 col-lg-9">
       <p class="left-navigation-trigger visible-xs visible-sm text-center">
        <button class="department-navigation-button btn btn-primary btn-md" data-toggle="offcanvas" type="button">
