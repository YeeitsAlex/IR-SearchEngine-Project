    .lw_widget>.lw_events_day>ul>li, .lw_widget>.lw_widget_column>ul>li, .lw_widget>noscript>ul>li, .lw_widget>ul>li {
        padding: 0;
        border-bottom: 2px solid #CCCCCC;
        padding: 0 0.5em 0.5em;
        margin-bottom: 0.5em; }
    .lw_widget>.lw_events_day>ul>li:last-of-type, .lw_widget>.lw_widget_column>ul>li:last-of-type, .lw_widget>noscript>ul>li:last-of-type, .lw_widget>ul>li:last-of-type {
        border-bottom: none; }
    .lw_events_day .date-day-name,
    .lw_events_day .date-day-number,
    .lw_events_day .date-month {
       display: inline-block;
       text-align: center;
       font-size: 1em;
       font-weight: normal;
       padding: 0;
       margin-right: 0.5em;
       color: inherit;
       background: none;
       vertical-align: baseline; }
    .lw_events_day .date-day-name {
        float: left; }
      .lw_events_day .date-day-name:after {
          content: ',';
      }
    .lw_events_day .date-month {
        margin-right: 0; }
    .lw_events_day .date-day-number {
        font-weight: bold; }
    

}
