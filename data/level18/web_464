        function googleTranslateElementInit() {
  new google.translate.TranslateElement({pageLanguage: 'en', autoDisplay: false}, 'google_translate_footer');
  jQuery('img.goog-te-gadget-icon').attr('alt','Google Translate');
  jQuery('div#goog-gt-tt div.logo img').attr('alt','translate');
  jQuery('div#goog-gt-tt .original-text').css('text-align','left');
  jQuery('.goog-te-gadget-simple .goog-te-menu-value span').css('color','#000000');
  jQuery('.goog-te-combo').attr('aria-label','google translate languages');
  jQuery('a.goog-logo-link img').attr('alt','Google');
  
  jQuery("#goog-gt-tt h1").remove();
  
}
       </script>
