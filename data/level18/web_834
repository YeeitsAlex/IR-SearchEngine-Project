 <script type="text/javascript">
  /* onLoad callback to add listeners to the search form */
            function addFormListeners() {
                (function ($) {
                    var searchField = $("input#gsc-i-id1");
                    /* Respond to search button */
                    var searchButton = $("input.gsc-search-button");
                    searchButton.click(function(){
                        adjustArguments(searchField.val());
                    });
                    /* Respond to enter key on textfield */
                    searchField.bind("enterKey",function(e){
                        adjustArguments(searchField.val());
                    });
                    searchField.keyup(function(e){
                        if(e.keyCode == 13) {
                            $(this).trigger("enterKey");
                        }
                    });
                }(jQuery));
            }

            /* Designated callback for performing action after google javascript has been loaded. */                                  
            var myCallback = function() {
                if (document.readyState == 'complete') {
                    addFormListeners();
                } else {
                    // Document is not ready yet, when CSE element is initialized.
                    google.setOnLoadCallback(function() {
                        addFormListeners();
                    }, true);
                }
            };

            /* Insert it before the CSE code snippet so that cse.js can take the script parameters, like parsetags, callbacks */
            window.__gcse = {
                parsetags: 'onload',
                callback: addFormListeners
            };

            (function() {
                var cx = '010409771836061691994:qytwch7xu20'; // Insert your own Custom Search engine ID here
