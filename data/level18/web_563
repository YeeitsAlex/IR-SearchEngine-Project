      <div class="uk-width-1-1@m">
       <h2 class="uk-text-center uk-heading-hero">
        Homecoming 2018
       </h2>
      </div>
     </div>
     <div class="uk-grid-margin" uk-grid="">
      <div class="uk-width-expand@m">
       <div class="uk-margin uk-text-right">
        <img alt="Homecoming word cloud" class="el-image" data-height="492" data-sizes="(min-width: 426px) 426px" data-src="/magazine/2018_fall/wp-content/themes/yootheme/cache/homecoming_word_cloud-8cdceadf.jpeg" data-srcset="/magazine/2018_fall/wp-content/themes/yootheme/cache/homecoming_word_cloud-8cdceadf.jpeg 426w" data-width="426" uk-img=""/>
