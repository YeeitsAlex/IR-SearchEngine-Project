  });

  $('a.socialicon.facebook').on('click', function(e) {
    console.log('Share on Facebook icon clicked.');
    e.preventDefault(); 
    FB.ui({
      method: 'share',
      href: document.URL,
      display: 'popup'
    }, function(response) {
        console.info('Sending analytics for sharing of facebook.');
