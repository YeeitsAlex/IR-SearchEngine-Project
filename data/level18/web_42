               <div id="ctl00_PlaceHolderMain_UIVersionedContent6_ctl00_label" style="display:none">
                Page Content 2
               </div>
               <div aria-labelledby="ctl00_PlaceHolderMain_UIVersionedContent6_ctl00_label" class="ms-rtestate-field" id="ctl00_PlaceHolderMain_UIVersionedContent6_ctl00__ControlWrapper_RichHtmlField" style="display:inline">
                <div style="text-align:center;">
                 <img alt="Heading for News and Highlights" class="ms-rtePosition-3" src="/fwf/PublishingImages/NewsHighlights.jpg" style="margin:0px;width:90%;"/>
                </div>
                <div class="ms-rtestate-read ms-rte-wpbox">
                 <div class="ms-rtestate-notify ms-rtestate-read dac898e3-6bb6-4317-ac9a-a5d162ef5260" id="div_dac898e3-6bb6-4317-ac9a-a5d162ef5260" unselectable="on">
                  <table cellpadding="0" cellspacing="0" style="width:100%">
                   <tr>
                    <td class="s4-wpcell-plain " id="MSOZoneCell_WebPartWPQ3" valign="top">
                     <table border="0" cellpadding="0" cellspacing="0" class="s4-wpTopTable " width="100%">
                      <tr>
                       <td valign="top">
                        <div allowdelete="false" allowremove="false" class="ms-WPBody " haspers="false" id="WebPartWPQ3" style="height:450px;overflow:auto;" webpartid="ab704ded-3b02-4bf0-a271-8fb980542e23" webpartid2="dac898e3-6bb6-4317-ac9a-a5d162ef5260" width="100%">
                         <!-- working with local spservice file -->
                         <!--  script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script -->
                         <!--  script type="text/javascript" src="https://ag.tennessee.edu/fwf/scripts/jquery.SPServices-2013.01.min.js"></script -->
                         <style>
                          /* custom styles for page if needed */
#imageList{
	list-style-type:none;
}
/* make the image float to the left */
#imageList img{
	float:left;
	margin-right:10px;
	max-width:80px;
	border:1px solid #bbb;
	}
#imageList li{
	clear:both;
	padding-bottom:2em;
	overflow:auto;
	}	
	
/* styles for paragraph tags in the list*/	
#imageList p{
	margin-left:0;
	}

/* this is for the paragraph heading */
#imageList p.listHead{
	font-weight:bold;
	font-size:130%;
	margin-left:0;
	padding-left:0;
}
                         </style>
                         <script language="javascript" type="text/javascript">
                          //get current date  used to check again dates in list
dt = new Date();

//main site url - makes sure the script would work anywhere in the site directory
var baseURL = window.location.protocol + "//" + window.location.host + "/"
console.log (baseURL);
$(document).ready(function() {

	
  $().SPServices({
    operation: "GetListItems",
    async: false,
    
    //** edit below with the library site url
	//webURL: "https://ag.tennessee.edu/fwf/",
	
	// **** edit below with your library name
    listName: "FWFHighlights",
    
    CAMLQuery: "<Query>" +
    
    			/* get all records - no conditional needed
    			//can use this to just grab active records, etc.
    			"<Where>" +
    				"<And>" + //Check and make sure employee is active
    					"<Eq>"+
    						"<FieldRef Name='Active' /><Value Type='Boolean'>1</Value>" +
    					"</Eq>"+
    					"<Eq>"+ //Check and make sure Department is correct (Pull from page?)
    						"<FieldRef Name='Department' /><Value Type='Text'>Biomedical and Diagnostic Sciences</Value>"+
    					"</Eq>"+
    				"</And>" +
    			"</Where>" +
    			*/
    			
    			/* this is just the sorting */
    			"<OrderBy>"+
    				"<FieldRef Name='StartDate' Ascending='FALSE' />"+
    			"</OrderBy>"+
    			"</Query>",    						
    	CAMLViewFields: "<ViewFields><FieldRef Name='Title' /><FieldRef Name='Link' /><FieldRef Name='Author' /><FieldRef Name='StartDate' /><FieldRef Name='EndDate' /><FieldRef Name='Info' /><FieldRef Name='Heading' /><FieldRef Name='FileDirRef' /><FieldRef Name='FileLeafRef' /></ViewFields>",
    	completefunc: function (xData, Status) {
		//the alert below will let you know if you are connecting to the data and returning anything
		//alert(xData.responseText);
		//uncomment the errors if things don't work right
/*
--------------   TESTING CODE BELOW  -------------------
		if (Status == "error") {
            alert ('Unable to communicate with Sharepoint Server!');
        }else{
        	alert ('Status = '+Status);
		}            
------------------  END OF TESTING CODE --------------------
*/

        var newGroup = 0;
        

		$(xData.responseXML).SPFilterNode('z:row').each(function() {
			//this is where you build your item blocks (loop through)
			
			
			//=== the block below checks for undefined fields and returns an empty string to prevent display error
			//get title	
			var title='';
			if ($(this).attr('ows_Title') !==undefined){
				title = $(this).attr('ows_Title');
			}else{
				title = '';
			}
			
			var author = $(this).attr('ows_Author');
			if (author===undefined){
				author = '';
			}
		
			var heading = $(this).attr('ows_Heading');
			if (heading===undefined){
				heading = '';
			}

			var info = $(this).attr('ows_Info');
			if (info===undefined){
				info = '';
			}
			var link = $(this).attr('ows_Link');
			if (link===undefined){
				link = '';
			} else {

				//Edit by Derek Evans - 12/15/2016
				//
				//There are two fields in ows_Link, one for the actual link an another is a description of the link.
				//ows_Link return the link and the description as a comma delimited string (if there was a link and description).
				//this parses the link and the description into an array and sets the link var to first item 

				link = link.split(",")[0];
			}

			console.log(link);

			
			// make sure filename is not empty 
			if ($(this).attr('ows_FileLeafRef')!==undefined){
				var tmpFile = $(this).attr('ows_FileLeafRef').split('#');
				fileName=tmpFile[1];
				
				var tmpDir = $(this).attr('ows_FileDirRef').split('#');
				var fileDir = tmpDir[1];
				
				//var picurl = '../../../'+fileDir+'/'+fileName;
				var picurl = baseURL+fileDir+'/'+fileName;
				
				/* if you created an asset gallery, it would generate thumbs and you could use them here
				var thumbFileName = (fileName.substring(0,fileName.length-4))+'_'+fileName.substr(-3,3);
				//var thumb ='../../../'+fileDir+'/_t/'+thumbFileName+'.jpg';
				var thumb =baseURL+fileDir+'/_t/'+thumbFileName+'.jpg';
				*/

				//date checker
				var startDate = $(this).attr('ows_StartDate');
				var tmpMonth='';
				var tmpDay='';
				var tmpYear ='';
				if (startDate===undefined){
					startDate = new Date();
				}else{
					//break date down
					var tmpMonth= parseInt(startDate.substring(5,7));
					var tmpDay=parseInt(startDate.substring(8,10));
					var tmpYear =parseInt(startDate.substring(0,4));
					tmpMonth --;
					if (tmpMonth<0){
						tmpMonth=11;
						tmpYear = tmpYear-1;
					}
					startDate = new Date(tmpYear,tmpMonth,tmpDay);					
				}
				
				var endDate = $(this).attr('ows_EndDate');
				if (endDate===undefined){
					endDate = new Date();
				}else{
					//break date down
					var tmpMonth= parseInt(endDate.substring(5,7));
					var tmpDay=parseInt(endDate.substring(8,10));
					var tmpYear =parseInt(endDate.substring(0,4));
					tmpMonth --;
					if (tmpMonth<0){
						tmpMonth=11;
						tmpYear = tmpYear-1;
					}
					endDate = new Date(tmpYear,tmpMonth,tmpDay);					
				}
				
				console.log(startDate.getTime() +', '+endDate.getTime() +'('+endDate+'), '+dt.getTime()+'('+dt+')');
								
				// build string 
				var liHtml='';
				
				if  ((startDate.getTime()<dt.getTime()) && (endDate.getTime()>dt.getTime())){
					liHtml = '<li><p class="listHead">'+heading+'</p><image src="'+picurl+'"><p>'+info+'</p><p>';
					if (link!==''){
						liHtml += '<a href="'+link+'">For more info...</a>';
					}
					liHtml += '</li>';
					//this spits it out below and builds the ul listed below
			        $("#imageList").append(liHtml);	        
			    }
	        }
	      });
    	}	
	});   
});
                         </script>
                         <ul id="imageList">
                         </ul>
                        </div>
                       </td>
                      </tr>
                     </table>
                    </td>
                   </tr>
                  </table>
                 </div>
                 <div id="vid_dac898e3-6bb6-4317-ac9a-a5d162ef5260" style="display:none;" unselectable="on">
