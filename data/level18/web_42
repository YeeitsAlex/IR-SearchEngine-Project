var _fV4UI = true;
function _RegisterWebPartPageCUI()
{
    var initInfo = {editable: false,isEditMode: false,allowWebPartAdder: false,listId: "{5ca7d634-7f10-4e80-9f9d-9ef43278b482}",itemId: 1,recycleBinEnabled: true,enableMinorVersioning: true,enableModeration: false,forceCheckout: true,rootFolderUrl: "\u002ffwf\u002fPages",itemPermissions:{High:16,Low:200769}};
