                Page Content 3
               </div>
               <div aria-labelledby="ctl00_PlaceHolderMain_UIVersionedContent6_ctl01_label" class="ms-rtestate-field" id="ctl00_PlaceHolderMain_UIVersionedContent6_ctl01__ControlWrapper_RichHtmlField" style="display:inline">
                <div style="text-align:center;">
                 <img alt="Heading for Calendar of Events" src="/fwf/PublishingImages/Calendarbutton1.jpg" style="margin:0px;width:90%;"/>
                </div>
                <div class="ms-rtestate-read ms-rte-wpbox">
                 <div class="ms-rtestate-notify ms-rtestate-read 80c4f9ea-e04c-4d01-936f-35c343a9827c" id="div_80c4f9ea-e04c-4d01-936f-35c343a9827c" unselectable="on">
                 </div>
                 <div id="vid_80c4f9ea-e04c-4d01-936f-35c343a9827c" style="display:none;" unselectable="on">
                 </div>
                </div>
                <div>
                </div>
                <div class="ms-rtestate-read ms-rte-wpbox">
                 <div class="ms-rtestate-notify ms-rtestate-read 5af7f98c-4653-433c-bad0-104d3fc47933" id="div_5af7f98c-4653-433c-bad0-104d3fc47933" unselectable="on">
                  <table cellpadding="0" cellspacing="0" style="width:100%">
                   <tr>
                    <td class="s4-wpcell-plain " id="MSOZoneCell_WebPartWPQ2" valign="top">
                     <table border="0" cellpadding="0" cellspacing="0" class="s4-wpTopTable " width="100%">
                      <tr>
                       <td valign="top">
                        <div allowdelete="false" allowremove="false" class="ms-WPBody " haspers="false" id="WebPartWPQ2" style="" webpartid="09de1cbe-6b88-45f7-9f33-b7fbf7be6824" webpartid2="5af7f98c-4653-433c-bad0-104d3fc47933" width="100%">
                         <script src="/scripts/jquery.SPServices-2013.02a.min.js" type="text/javascript">
                         </script>
                         <script type="text/javascript">
                          // change listName to fit calendar name on site
$(document).ready(function() {
$().SPServices({
    operation: "GetListItems",
    async: false,
    listName: "Calendar of Events",
    CAMLRowLimit: 7,
    CAMLViewFields: "<ViewFields>" +
            "<FieldRef Name='Title' />" +
            "<FieldRef Name='EventDate' />" +
            "<FieldRef Name='EndDate' />" +
            "<FieldRef Name='Location' />" +
            "<FieldRef Name='Description' />" +
            "<FieldRef Name='fRecurrence' />" +
            "<FieldRef Name='RecurrenceData' />" +
            "<FieldRef Name='fAllDayEvent' />" +
            "<FieldRef Name='EncodedAbsUrl' />" +
            "<FieldRef Name='ID' />" +
        "</ViewFields>",
    CAMLQuery: '<Query>' +
            '<Where>' +
                '<DateRangesOverlap>' +
                    '<FieldRef Name="EventDate" />' +
                    '<FieldRef Name="EndDate" />' +
                    '<FieldRef Name="RecurrenceID" />' +
                    '<Value Type="DateTime">' +
                        '<Now />' +
                    '</Value>' +
                '</DateRangesOverlap>' +
            '</Where>' +
            '<OrderBy>' +
                '<FieldRef Name="EventDate" />' +
            '</OrderBy>' +
        '</Query>',
    CAMLQueryOptions: '<QueryOptions>' +
            '<RecurrencePatternXMLVersion>v3</RecurrencePatternXMLVersion>' +
            '<ExpandRecurrence>TRUE</ExpandRecurrence>' +
        '</QueryOptions>',
    completefunc: function (xData, Status) {
    
		// define our variables before the looping begins
		var tempMonth;
		var tempDay; //if you need it - Sunday, Monday,Tuesday,etc
		//define monthNames  (the var statement should all be on one line)
		var monthName=["January","February","March","April","May","June","July","August","September","October","November","December"]; 
		// **  remember - January will be monthName[0] NOT monthName[1] ** 
		// if you want January to be month1 then, add "", before "January"
		
		// format time from 24hr to 12hr
		var getFormattedTime = function (fourDigitTime){
    		var hours24 = parseInt(fourDigitTime.substring(0,2), 10);
    		var hours = ((hours24 + 11) % 12) + 1;
    		var amPm = hours24 > 11 ? ' pm' : ' am';
    		var minutes = fourDigitTime.substring(2);

    		return hours + minutes + amPm;
			};

       	// function for returning each row
       	$(xData.responseXML).SPFilterNode("z:row").each(function() {
      	
      		// split date/time up to usuable parts
       		var ddd = ($(this).attr("ows_EventDate").split(" ")[0]);
       		var ttt = ($(this).attr("ows_EventDate").split(" ")[1]);
			tArr = ttt.split(":");		
			dArr = ddd.split("-");  // ex input "2013-12-30" ;
       		var isodd= dArr[1] + "/" + dArr[2] + "/" +dArr[0];
			var isott = tArr[0] + ":" + tArr[1];		
        	var idd= new Date(isodd);   
        
        	var dd    = idd.getDate();
        	var mm = idd.getMonth(); //January is 0!
        	var allday = $(this).attr("ows_fAllDayEvent");
        	if (allday == 1) {
         	var newtime = "All Day Event"
         	}else{
            	var newtime = getFormattedTime(isott);
				}
        	
			// get Month name and Day
			tempMonth = monthName[mm]; //this will store the MonthName
			tempDay = dd;
			
			// get URL for the link to item
			var url = $(this).attr("ows_EncodedAbsUrl") + "";
			if(($(this).attr("ows_EncodedAbsUrl") + "").match(/000$/))
				{
					url = url.substring(0, url.lastIndexOf("/", url.length));
					url = url + "/DispForm.aspx?ID=" + $(this).attr("ows_ID");
				}
			var link = "<a href='" + url + "'><span class='title'>" + $(this).attr("ows_Title") + "</span></a>";       
		
			// if field is undefined then leave it blank
			if (typeof($(this).attr("ows_Location")) == 'undefined') {
			var Location = "";
			}else{ 
			var Location = "Location: "  + $(this).attr("ows_Location");
				}
        	
        	// write out html 
	  		var Title = $(this).attr("ows_Title");
	    	var liHtml = "<li class='dfwp-item'><div class='xslCalendar' xmlns:ddwrt='http://schemas.microsoft.com/WebParts/v2/DataView/runtime'>" +
	   		"<div class=item link-item'  style='min-height:50px'><div class='description xslCal'><div class='left'><span class='month'>" + tempMonth + "</span><span class='day'>" + 
	   		dd + "</span></div><div class='right'>" + link +"<span class='location'>" + Location + "</span><span class='time'>Time: " + newtime + "</span></div></div></div></div></li>";	
	   
	   
	  		$(".calUpcomingEvents, dfwp-list").append(liHtml);
	   
         });
    }
});
});
                         </script>
                         <ul class="calUpcomingEvents dfwp-list" style="width:100%">
                         </ul>
                        </div>
                       </td>
                      </tr>
                     </table>
                    </td>
                   </tr>
                  </table>
