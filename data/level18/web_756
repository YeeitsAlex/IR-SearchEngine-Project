    <script type="text/javascript">
     var calDate = new Date(), millisInDay = 8.64e+7;
    	var millisInWeek = millisInDay*7;
    	function loadWeek(dir) {
    		document.getElementById("loadingOverlay").style.display = 'block';
    		
		var xhttp = new XMLHttpRequest();
		xhttp.onreadystatechange = function() {
			if (this.readyState == 4 && this.status == 200) {
				document.getElementById("calendarEventsList").innerHTML = this.responseText;
				console.log(this.responseText);
				
				[].slice.call(document.getElementsByClassName("dateHeader")).forEach(function(div, i) {
				    div.getElementsByTagName('div')[0].innerHTML = ['SUN', 'MON', 'TUE', 'WED', 'THU', 'FRI', 'SAT'][new Date(calDate.getTime() + (millisInDay * (i)) ).getDay()];
					div.getElementsByTagName('div')[1].innerHTML = new Date(calDate.getTime() + (millisInDay * (i)) ).getDate();
				});
				
				document.getElementById("loadingOverlay").style.display = 'none';
			}
		};
		calDate = new Date(calDate.getTime() + (millisInWeek*dir));
		//xhttp.open("GET", "https://sa.uncg.edu/wp-content/themes/sa/calendar.ajax.php?start=" + ((calDate.getTime()-(millisInDay*2))/1000) + "&end=" + ((calDate.getTime()+(millisInDay*3))/1000), true);
		console.log(calDate.getTime()/1000);
		console.log((calDate.getTime()+(millisInDay*6))/1000);
		xhttp.open("GET", "https://sa.uncg.edu/wp-content/themes/sa/calendar.ajax.php?start=" + (calDate.getTime()/1000) + "&end=" + ((calDate.getTime()+(millisInDay*6))/1000), true);
			console.log(new Date((calDate.getTime())));
			console.log(new Date(((calDate.getTime()+(millisInDay*6)))))
		xhttp.send();
	}
	
	document.getElementById('lastWeekButton').addEventListener('click', function() {loadWeek(-1);});
	document.getElementById('nextWeekButton').addEventListener('click', function() {loadWeek(1);});
    </script>
