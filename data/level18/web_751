     <div>
      <!-- why is this here ITS? -->
      <article>
       <div class="container-fluid no-bottom-padding ">
        <div id="content-wrapper">
         <div class="row for-sidebar">
          <div class="main-content-inner col-sm-12" id="content">
           <article class="post-7 page type-page status-publish hentry" id="post-7">
            <header class="entry-header">
             <h1 class="entry-title">
              Summer Session
             </h1>
            </header>
            <!-- .entry-header -->
            <div class="entry-content">
             <section class="dates-links-block">
              <div class="session-date-block">
               <h2>
                Session I
               </h2>
               <p>
                May 15–June 19
               </p>
              </div>
              <div class="link-block">
               <img class="img-laptop" src="https://summer.uncg.edu/wp-content/themes/summersession_uncgwp/images/laptop.svg"/>
               <div>
                <a class="link-button" href="https://summer.uncg.edu/find-a-course/">
                 Courses &gt;&gt;
                </a>
               </div>
               <p>
                Find the course you need during Summer Session — online or on campus.
               </p>
              </div>
              <div class="link-block">
