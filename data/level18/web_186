  position: relative;
  left: .1em; 
  font-size:.80em!important; 
  font-weight:bold!important;
}
.mytab [type=radio] {
  display: none;   
}
.mytab .content {
  position: absolute;
  top: 31px;
  left: 0;
  background: white;
  padding: .5em;
  border: .1em solid #ccc; 
  border-radius: 0 0 .25em .25em;
  display:none;
  min-width:93%;
  font-size:.8em!important;
}
.mytab [type=radio]:checked ~ label {
  background: white;
  border-bottom: .1em solid white;
  z-index: 2;
}
.mytab [type=radio]:checked ~ label ~ .content {
  display:block;
}
.mytabs {
        height:100%!important;
        min-height: 225px!important;
        max-height:auto!important;
}
.mytabs .content{
	overflow: auto!important;
}
