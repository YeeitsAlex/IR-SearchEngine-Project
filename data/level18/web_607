  <script type="text/javascript">
   ;(function() {
        var counter = 0;
        var loadingPageDom = document.getElementById('loading_page');
        var loadingPageDomMessage = document.getElementById('loading-page__message');
        var applicationDom = document.getElementsByClassName('application');
        
        var showHideLoadingPage = function showHideLoadingPage() {
          counter++;
          loadingPageDom.classList = 'loading-page show-flex';

          if (counter > 5) {
            console.log('slow network detected...');
            loadingPageDomMessage.classList = 'loading-page__message show-flex';
          }

          setTimeout(function() {
            applicationDom.length
              ? loadingPageDom.classList = 'loading-page hide'
              : showHideLoadingPage()
          }, 500);
        };

        setTimeout(function() {
          if (!applicationDom.length) {
            console.log('app not ready yet...');
            showHideLoadingPage();
          } else {
            console.log('app ready!');
          }
        }, 250);
      })();
  </script>
