   <!--

function GoodDate(field,theText,allowBlank)
// Modified from original JavaScript code by Chirp Internet: www.chirp.com.au
// Test the overall date format and the format of each part (day, month, year).  Pass in whether or not to allow blank dates.
{
    var minYear = 1970;
    var maxYear = (new Date()).getFullYear()+20;
    var errorMsg = "";

    // regular expression to match required date format
    re = /^(\d{1,2})\/(\d{1,2})\/(\d{4})$/;

    if(field.value != '') {
		if(regs = field.value.match(re)) {
			if(regs[1] < 1 || regs[1] > 12) {
				errorMsg = "The value for month (" + regs[1] + ") is invalid in the " + theText + " field.";
			} 
			else 
				if(regs[2] < 1 || regs[2] > 31) {
					errorMsg = "The value for day (" + regs[2] + ") is invalid in the " + theText + " field.";
				} 
				else 
					if(regs[3] < minYear || regs[3] > maxYear) {
						errorMsg = "The value for year (" + regs[3] + ") must be between " + minYear + " and " + maxYear + " in " + theText + " field.";
					}
		} 
		else {
			errorMsg = "The date format (" + field.value + ") is invalid in the " + theText + " field.";
		}
    } 
	else 
		if(!allowBlank) {
			errorMsg = "An empty date is not allowed in the " + theText + " field.";
    	}

    if(errorMsg != "") {
		alert(errorMsg);
		field.focus();
		return false;
    }

    return true;
}

-->
