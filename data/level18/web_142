        <div class="content-inner">
         <a name="main-content">
         </a>
         <!--front panel regions beg-->
         <div class="at-panel gpanel panel-display content clearfix" id="content-panels">
          <div class="region region-content-top">
           <div class="region-inner clearfix">
            <div class="block block-boxes block-boxes-os_slideshow_box no-title" delta="1459280318" id="block-boxes-1459280318" module="boxes">
             <div class="block-inner clearfix">
              <div class="block-content content" ng-non-bindable="">
               <div class="boxes-box" id="boxes-box-1459280318">
                <div class="boxes-box-content">
                 <div id="layout_3_1_overlay">
                  <div class="slide">
                   <ul class="rslides">
                    <li>
                     <a href="https://trainingportal.harvard.edu/Saba/Web_spf/NA1PRD0068/common/leclassview/dowbt000000000003520">
                      <img alt="Collaboration, email &amp; calendar" class="image-style-os-slideshow-31-980" height="327" src="//static.hwpi.harvard.edu/files/styles/os_slideshow_3%3A1_980/public/mso/files/ax105_4a77_9.jpg?m=1515603540&amp;itok=jEAacMdk" title="Collaboration, email &amp; calendar" width="980"/>
                     </a>
                     <div class="caption slide-copy">
                      <h2>
