jQuery(window).on('load', function() { // After page loads...

  function updateDate() {
   setTimeout(function() {
   console.log("updateDate");
    var dateDay = jQuery(".lw_cal_today_day").text();
    var $dateMonth = jQuery(".lw_cal_today_month");
    var dateYear = jQuery("#lw_mini_cal > h3 > span").text();
    var dateString = dateDay + " " + $dateMonth.text() + " " + dateYear;
    var date = new Date(dateString);
    var dateDayName = date.toLocaleString('en-us', {  weekday: 'long' });
    var dateMonthName = date.toLocaleString('en-us', { month: 'long' });
    var dateHeaderString = dateDayName + ", " + dateMonthName + " " + dateDay;
    if ( dateDayName == "Invalid Date" || dateMonthName == "Invalid Date" ) { 
      dateDayName = dateMonthName = 0; } 
    else {
      $dateMonth.text(dateDayName + ", " + dateMonthName);
    }
   }, 200);
  }

  setTimeout(updateDate, 1000);
  // add event listener to mini call during capture phase to circumvent stopPropagation in bubble phase
  var miniCal = document.getElementById("lw_mini_cal");
  miniCal.addEventListener("click", updateDate, false);
});

//--><!]]>
