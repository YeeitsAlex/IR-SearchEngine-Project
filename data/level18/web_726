  <script type="text/javascript">
   $(document).ready(function(){
			$('.sidebar_widget li:has(ul)').prepend('<span class="show">Show</span>');
			$('.sidebar_widget li:has(ul)').addClass('sub');

			
			$(document).on('click', '.sidebar_widget .show',function(event){
			$(this).siblings('.sub ul').slideToggle(); //or whatever show-function you want to use
				$(this).removeClass('show').addClass('hide');
				$(this).text('Hide');
			});

			$(document).on('click', '.sidebar_widget .hide',function(event){
			$(this).siblings('.sub ul').slideToggle(); //or whatever show-function you want to use
				$(this).removeClass('hide').addClass('show');
				$(this).text('Show');
			});


		});
  </script>
  <script type="text/javascript">
   $(document).ready(function(){

			if ($('.selected ul').is(':visible'))    
   			 $('.selected span').removeClass('show').addClass('hide');    
			else
   			 $('.selected span').addClass('show');
});
  </script>
  <script>
   function includeAlert() {
  var z, i, elmnt, file, xhttp;
  /*loop through a collection of all HTML elements:*/
  z = document.getElementsByTagName("*");
  for (i = 0; i < z.length; i++) {
    elmnt = z[i];
    /*search for elements with a certain atrribute:*/
    file = elmnt.getAttribute("alert-include");
    if (file) {
      /*make an HTTP request using the attribute value as the file name:*/
      xhttp = new XMLHttpRequest();
      xhttp.onreadystatechange = function() {
        if (this.readyState == 4) {
          if (this.status == 200) {
            elmnt.innerHTML = this.responseText;
          }
          if (this.status == 404) {elmnt.innerHTML = "Page not found.";}
          /*remove the attribute, and call this function once more:*/
          elmnt.removeAttribute("emergency-alert");
          includeHTML();
        }
      }      
      xhttp.open("GET", file, true);
      xhttp.send();
      /*exit the function:*/
      return;
    }
  }
};
  </script>
