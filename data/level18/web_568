        <div class="block block-block block-javascript" id="block-block-19">
         <div class="block-inner clearfix">
          <div class="block-content content no-title">
           <!-- IE test -->
           <script type="text/javascript">
            <!--//--><![CDATA[//><!--
if (typeof document.documentMode == 'number' && document.documentMode > 0) {
  document.documentElement.className+=' is-ie';
}
//--><!]]>
           </script>
           <script type="text/javascript">
            <!--//--><![CDATA[//><!--
/*
 * MIT Title IX website javascript
 * rjw 10/17
 *
 * Requires:
 *   jquery.cycle.js              http://malsup.com/jquery/cycle/
 *   jquery.onfontresize.js       http://www.tomdeater.com/jquery/onfontresize/
 *
 */

jQuery(document).ready(function() {

  var $ = jQuery;

  var sitedomain = window.location.hostname;



  ////
  //// adjust ACTIVE AND ACTIVE-TRAIL CLASSES in menus
  ////

  // on pages using context for menu position, context marks parents active-trail rather than active -
  // so make sure something is active
  //
  $('.block-primary-nav a.active-trail').addClass('active');
  $('.block-secondary-nav a.active-trail').addClass('active');

  // make sure only one active menu item in menus, and that all parents are marked active-trail
  //
  $('.block-primary-nav a.active').parent('li').removeClass('active').addClass('active-trail').parents('li').addClass('active-trail').children('a').removeClass('active').addClass('active-trail');
  $('.block-secondary-nav a.active').parent('li').removeClass('active').addClass('active-trail').parents('li').addClass('active-trail').children('a').removeClass('active').addClass('active-trail');



  ////
  //// SEARCH ADJUSTMENTS (obsolete as of 6/21/18)
  ////

  // change text of search button to "go"
  //
  //$('#block-google-appliance-ga-block-search-form .form-submit').attr('value','Go');

  // set search default
  //
  //$('#block-google-appliance-ga-block-search-form .form-item-search-keys input').attr('placeholder', 'search...');



  ////
  //// MOBILE SEARCH
  ////

  // create container for mobile menu and search
  //
  $('#top-panels-wrapper .container').append('<div class="region region-mobile-menu"></div>');


  // create search toggle and container
  //
  $searchtoggle = $('<div id="divmobearchtoggle" class="block-mobile-opener"><a href="#" id="mobearchlink">Search</a></div>');
  $mobilesearch = $('<div id="divmobsearch"><div id="divmobsearchinner"></div></div>');

  // clone search
  //
  $searchclone = $('.block-search form').clone();
  $mobilesearch.find('#divmobsearchinner').append($searchclone);

  // toggle mobile search on/off
  //
  $searchtoggle.find('#mobearchlink').on('click', function(e) {
    e.preventDefault();
    $('#divmobsearch').slideToggle();
    $(this).toggleClass('active');
  });

  // insert into page
  //
  $('.region-three-33-third').append($searchtoggle);
  $('.region-mobile-menu').append($mobilesearch);


  ////
  //// MOBILE MENU
  ////

  // create menu toggle and container
  //
  $mobilemenutoggle = $('<div id="divmobtoggle" class="block-mobile-opener"><a href="#" id="mobmenulink">Menu</a></div>');
  $mobilemenu = $('<div id="divmobmenu"><div id="divmobmenuinner"></div></div>');

  // clone main menu
  //
  var $menuclone = $('.block-primary-nav .menu-block-wrapper > ul').clone();
  $mobilemenu.find('#divmobmenuinner').append($menuclone);

  // set all li with a child ul to openable but closed
  //
  $mobilemenu.find('#divmobmenuinner li li').children('ul').parent().addClass('openable closed');

  // ...but then open up the active trail
  //
  $mobilemenu.find('#divmobmenuinner a.active, #divmobmenuinner a.active-trail').parent().removeClass('closed').addClass('open');

  // create opener/closer buttons
  // note: top level always open, so only added to second level and lower
  //
  $mobilemenu.find('#divmobmenuinner li li.openable > a').append('<span class="opener"></span><span class="closer"></span>');


  // toggle mobile menu on/off
  //
  $mobilemenutoggle.find('#mobmenulink').on('click', function(e) {
    e.preventDefault();
    $('#divmobmenu').slideToggle();
    $(this).toggleClass('active');
  });

  // toggle submenus on/off
  //
  $mobilemenu.find('span.opener').on('click', function(e) {
    e.preventDefault();
    $(this).parent('a').parent('li').removeClass('closed').addClass('open');
  });
  $mobilemenu.find('span.closer').on('click', function(e) {
    e.preventDefault();
    $(this).parent('a').parent('li').removeClass('open').addClass('closed');
  });

  // insert into page
  //
  $('.region-three-33-third').append($mobilemenutoggle);
  $('.region-mobile-menu').append($mobilemenu);



  ////
  //// set LINK CLASSES and open links in new windows
  ////

  // links starting with "http" (but not to this site)
  //
  $('a[href^="http"]').not('[href*="'+sitedomain+'"]').attr('target','_blank').addClass('external');

  // links starting with "mailto"
  //
  $('a[href^="mailto"]').addClass('mailto');

  // links ending with ".pdf"
  //
  $('a[href$=".pdf"]').attr('target','_blank').addClass('pdf');

  // links ending with ".ppt"
  //
  $('a[href$=".ppt"]').attr('target','_blank').addClass('ppt');

  // links ending with ".doc"
  //
  $('a[href$=".doc"]').attr('target','_blank').addClass('doc');



  ////
  //// REWRITE LINKS to http to links to https for logged-in users
  ////
  if ($('body').hasClass('logged-in')) {
    $('a[href^="http://'+sitedomain+'"]').each(function() {
      var href = $(this).attr('href');
      href = href.replace('http', 'https');
      $(this).attr('href', href);
    });
  }



  ////
  //// prepare HOME PAGE SLIDESHOW
  ////
  if ($("body").hasClass("page-type-homepage") && $('.block-home-slideshow-desktop .slide').length > 0) {

    // create container for controls
    //
    $('<div id="divslidecontrols"><ul id="slidepager"></ul></div>').prependTo('.block-home-slideshow-desktop');

    // assign id to slide container
    //
    $('.block-home-slideshow-desktop .view-content').attr('id', 'divslideshow');

    // start slideshow
    //
    $('#divslideshow').cycle({
      containerResize: 0,
      slideResize: 0,
      fx: 'fade',
      speed: 500,
      timeout: 7000,
      pause: 0,
      //startingSlide: Math.floor(Math.random()*$('#divslideshow div.slide').length),
      startingSlide: 0,
      pager:  '#slidepager',
      pagerAnchorBuilder: function(idx, slide) { return '<li><a href="#"></a></li>';  }

    });

    // pause when hovering over caption
    $('#divslideshow .text').mouseover( function() {$('#divslideshow').cycle('pause');} );
    $('#divslideshow .text').mouseout( function() {$('#divslideshow').cycle('resume');} );

    // pause when hovering over pager
    $('#slidepager').mouseover( function() {$('#divslideshow').cycle('pause');} );
    $('#slidepager').mouseout( function() {$('#divslideshow').cycle('resume');} );

  }



  ////
  //// Miscellaneous HTML REWRITING
  ////

  // (if any...)



  ////
  //// RESPONSIVE EMBEDDED IFRAMES for videos, google maps, etc.
  //// adapted from http://css-tricks.com/NetMag/FluidWidthVideo/demo.php
  ////

  // include iframes in content area without class 'noresize', and with both width and height attributes, but not in percent
  var $allVideos = $("#content-wrapper iframe")
                   .not('.noresize')
                   .filter('[width]').not('[width*="%"]')
                   .filter('[height]').not('[height*="%"]');

  // initialize
  //
  $allVideos.each(function() {
    $(this)
      // jQuery .data does not work on object/embed elements
      .attr('data-aspectRatio', this.height / this.width)
      .removeAttr('height')
      .removeAttr('width')
      .wrap('<div class="videowrapper"></div>');
  });

  // react to window resize event
  //
  function _videoresize() {
    $allVideos.each(function() {
      var $el = $(this);
      var newWidth = $el.parent().width();
      $el
        .width(newWidth)
        .height(newWidth * $el.attr('data-aspectRatio'));
    });
  }
  $(window).resize(_videoresize);



  ////
  //// MONITOR FOR FONT RESIZE and call window resize when found
  //// Requires jquery.onfontresize.min.js
  ////
  $(document).bind('fontresize', function () {
    $(window).resize();
  });


  $(window).resize(); // kick off resize-triggered events when page loaded

});
//--><!]]>
           </script>
          </div>
         </div>
        </div>
       </div>
      </footer>
     </div>
    </div>
   </div>
  </div>
