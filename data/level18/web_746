   $(document).ready(function () {
            setupColorBox();
            setupResponsiveVideo();
            setupBuckeyeAlerts();
        });
        $(window).resize(function () {
            setupColorBox();
        });
        function setupColorBox() {
            var vWidth = ($(window).width() < 960 ? "80%" : "auto");
            $(".colorbox").colorbox({ rel: "gal", width: vWidth });
        }
        function setupResponsiveVideo() {
            $("#content iframe").wrap("<div class=\"video-container\"></div>");
        }
        function setupBuckeyeAlerts() {
            $("#buckeye_alert").buckeyeAlert({
                url: "//www.osu.edu/feeds/emergency-alert.rss",
                messageClass: "container"
            });
        }
  </script>
