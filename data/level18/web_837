<script>
 (function($) {
  var container = document.getElementById('mm_live_pane_5119_container');
  var template = document.getElementById('mm_live_pane_5119_template').innerHTML;

  $.getJSON('https://mediamagnet.osu.edu/api/v1/items.json?channel_id=384&per_page=4', function(data) {
    $.each(data.items, function(idx, item) {
      item['formatted_published_at'] = moment(item['published_at'], 'YYYY-MM-DD HH:mm:ss Z').fromNow();
      var html = Mustache.to_html(template, item);
      $(html).hide().appendTo(container).fadeIn();
    });
  }).fail(function() {
    $(container).append('<p>Cannot load content.</p>');
  });
})(jQuery);
</script>
