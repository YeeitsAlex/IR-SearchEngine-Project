   google.load("feeds", "1");
		
		 function OnLoad() {
		  var feed = new google.feeds.Feed("http://www.utk.edu/events/rss/feed.php?l=14");
		  feed.setNumEntries(7);
		  
		   feed.load(function(result) {
			if (!result.error) {
			  var container = document.getElementById("feed");
			  for (var i = 0; i < result.feed.entries.length; i++) {
				var entry = result.feed.entries[i];
				var li = document.createElement("li");
				var a = document.createElement("a");
				var url = entry.link;
				a.appendChild(document.createTextNode(entry.title));
				a.setAttribute('href', url);
				container.appendChild(li);
				li.appendChild(a);
			  }
			}
		  });
	
		 var feed2 = new google.feeds.Feed("http://www.utk.edu/tntoday/category/headlines/feed/");
		 
		  feed2.setNumEntries(7);
		  feed2.load(function(result) {
			if (!result.error) {
			  var container2 = document.getElementById("feed2");
			  for (var i = 0; i < result.feed.entries.length; i++) {
				var entry = result.feed.entries[i];
				var li = document.createElement("li");
				var a = document.createElement("a");
				var url = entry.link;
				a.appendChild(document.createTextNode(entry.title));
				a.setAttribute('href', url);
				container2.appendChild(li);
				li.appendChild(a);
			  }
			}
		  });
		}
		google.setOnLoadCallback(OnLoad);
