             <img alt="GSW Blue" class="logo img-responsive hide-for-small-only" id="topLogo" src="/Assets/Template/logo-gswprimary-white.svg" style="background-color: #1c3f7b;"/>
            </a>
           </div>
           <script type="text/javascript">
            // <![CDATA[
function logoInit() {
		window.addEventListener('scroll', function(e) {
			var distanceY = window.scrollY,
				shrinkOn = 60,
				logo = document.getElementById("topLogo");
			if (distanceY > shrinkOn) {
				logo.classList.add("shrink");
			} else {
				if (logo.classList.contains("shrink")) {
					logo.classList.remove("shrink");
				}
			}
		});
	}
	window.onload = logoInit();
// ]]>
           </script>
