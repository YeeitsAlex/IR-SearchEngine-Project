			window.COMPUTED_SEARCH = window.COMPUTED_SEARCH.map(function(post) {
				post.fulltext = post.post_title + post.post_content+ ' ';
				Object.values(post.acf).forEach(function(v) {
					if(typeof v === 'string' && v.indexOf(' ') !== -1) {
						post.fulltext += v.toLowerCase()+' ';
					}
					if(typeof v === 'object' && v.length) {
						v.forEach(function(c) {
							if (c.content) {
								post.fulltext += c.content.toLowerCase()+' ';
							}
							if (c.title) {
								post.fulltext += c.title.toLowerCase()+' ';
							}
							if (c.blocks) {
								c.blocks.forEach(function(b) {
									if(typeof b === 'string' && b.indexOf(' ') !== -1) {
										post.fulltext += b.toLowerCase()+' ';
									}
								})
							}
						});
					}
				})
				return post;
			})
		} catch(err) {
			window.COMPUTED_SEARCH = [];
		}
</script>

