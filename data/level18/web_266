  <script language="javascript" type="text/javascript">
   function test1()
    {
       return confirm("This will change the EMail address that is notified whenever " +
                      'Passphrase Maintenance is used for the PirateID, .\n\n' +
                      "Are you sure you want to do this?")
    }
    function test2() {
       return confirm("Registering again will delete your previous questions and answers, so " +
                      'you will need to enter new ones.\n\n' +
                      "Are you sure you want to do this?")
    }
    function test3()
    {
       return confirm('This will generate an Activation Code and send it to . Are you sure you want to do this?')
    }
    function test4()
    {
       return confirm('Passphrase Maintenance will no longer have your Texting Number and will no longer be able to text you an Activation Code. Are your sure you want to do this?')
    }
    function DCCancel(n)
    {
       //       return confirm('Are you sure you want to cancel?');

       if (confirm('Are you sure you want to cancel?'))
       {
          if (n == 2)
          {
             return CheckBadPW2()
          }
          else { return true }
       }
       else { return false }
    }
    function NoAgree()
    {
       return confirm('Your PirateID can not be activated until you agree. Are you sure you do not want to agree?');
    }
    function EnableAgree(v)
    {
       var btnAgree = document.getElementById('cphContent_ActivateWizard___CustomNav29_btnAgree');
       if (null != btnAgree)
       {
           btnAgree.disabled = v;
       }
     }
     function CheckBadPW(Code)
     {
        //        alert("CheckBadPW called");
        var p0, p1, p2, p1nv, p2nv;
        if (Code == 'C')
        {
           p1 = document.getElementById('cphContent_ActivateWizard_ctl41_txtNewPass1');
           p2 = document.getElementById('cphContent_ActivateWizard_ctl41_txtNewPass2');
           p1nv = encodeURIComponent(p1.value); // escape(p1.value)
           p2nv = encodeURIComponent(p2.value); // escape(p2.value)
//           alert('p1nv=' + p1nv + '; p2nv=' + p2nv);
           p1.value = p1nv;
           p2.value = p2nv;
        }
        else if (Code == 'E')
        {
           p0 = document.getElementById('cphContent_ActivateWizard_ctl44_txtCurrentPass');
           p1 = document.getElementById('cphContent_ActivateWizard_ctl44_txtNewPassA');
           p2 = document.getElementById('cphContent_ActivateWizard_ctl44_txtNewPassB');
           //        alert("p1: " + p1.value);
           p1nv = encodeURIComponent(p1.value); // escape(p1.value)
           p2nv = encodeURIComponent(p2.value); // escape(p2.value)
           //        alert('p1nv ' + p1nv);
//           alert('p1nv=' + p1nv + '; p2nv=' + p2nv);
           p0.value = encodeURIComponent(p0.value); // escape(p0.value)
           p1.value = p1nv;
           p2.value = p2nv;
        }
        else if (Code == 'A')
        {
           p1 = document.getElementById('cphContent_ActivateWizard_ctl36_txtCurPass');

           //        alert("p1: " + p1.value);
           p1nv = encodeURIComponent(p1.value); // escape(p1.value)
//           alert('p1nv ' + p1nv);
           p1.value = p1nv;
        }
        else if (Code == 'U')
        {
           p1 = document.getElementById('cphContent_ActivateWizard_ctl46_txtUsePass')
           p1nv = encodeURIComponent(p1.value); // escape(p1.value)
//           alert('p1nv=' + p1nv);
           p1.value = p1nv;
        }
        else if (Code == 'T')
        {
           p1 = document.getElementById('cphContent_ActivateWizard_ctl42_txtPWToTest')
           p1nv = encodeURIComponent(p1.value); //escape(p1.value)
//           alert('p1nv=' + p1nv);
           p1.value = p1nv;
        }
        return true
     }
     function CheckBadPW2()
     {
        //        alert("CheckBadPW called");
        var p0 = document.getElementById('cphContent_ActivateWizard_ctl44_txtCurrentPass');
        var p1 = document.getElementById('cphContent_ActivateWizard_ctl44_txtNewPassA');
        var p2 = document.getElementById('cphContent_ActivateWizard_ctl44_txtNewPassB');
        //        alert("p1: " + p1.value);
        var p1nv = encodeURIComponent(p1.value); // escape(p1.value)
        var p2nv = encodeURIComponent(p2.value); // escape(p2.value)
        //        alert('p1nv ' + p1nv);
        p0.value = encodeURIComponent(p0.value); // escape(p0.value)
        p1.value = p1nv;
        p2.value = p2nv;
        return true
     }
     function CheckBadPW3()
     {
        //        alert("CheckBadPW called");
        var p1 = document.getElementById('cphContent_ActivateWizard_ctl36_txtCurPass');

        //        alert("p1: " + p1.value);
        var p1nv = encodeURIComponent(p1.value); // escape(p1.value)
        //        alert('p1nv ' + p1nv);
        p1.value = p1nv;

        return true
     }
