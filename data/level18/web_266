  <script language="javascript" type="text/javascript">
   function PWCheck(sender, args)
     {
        //       alert("PWCheck Called " + sender.id + '; ' + args)
        var IsStudentPW = 'False' == 'True'
        var ctl = sender.id.toString().replace("cvCheck", "txt");
        var cTxt = document.getElementById(ctl)
        var VVal = cTxt.value;
        //       alert("VVal: " + VVal);
        if (VVal == undefined || VVal == null || VVal.trim().length == 0)
        {
           args.IsValid = false;
           //          sender.innerHTML = "<br />You must supply a value for the passphrase!!!!";
           return;
        }
        //       if (VVal.indexOf('&') > -1 || VVal.indexOf('<') > -1)
        //       {
        //          args.IsValid = false;
        //          sender.innerHTML = "<br />The value you entered contains an illegal character!";
        //          return;
        //       }
        if (IsStudentPW)
        {
           if (VVal.length < 8 || VVal.length > 16)
           {
              args.IsValid = false;
              sender.innerHTML = "<br />JThe passphrase entered is too short or too long!";
              return;
           }
        }
        else
        {
           if (VVal.length < 8)
           {
              args.IsValid = false;
              sender.innerHTML = "<br />JThe passphrase entered is too short!";
              return;
           }
        }
        var patU = /[A-Z]/
        var patL = /[a-z]/
        var patD = /[0-9]/
        var patSS = /^[A-Za-z0-9\!\@\#\$\%\^\&\*\(\)\_\-\+\=\{\}\[\]\|\\\?\/\<\>\~\:\;\`\.\,\']{8,16}$/;
        var patSL = /[\!\@\#\$\%\^\&\*\(\)\_\-\+\=\{\}\[\]\|\\\?\/\<\>\~\:\;\`\.\,\']/;
        var patSI = /[^A-Za-z0-9 ]/
        var NT = 0
        if (patU.test(VVal))
        {
           NT = NT + 1
        }
        if (patL.test(VVal))
        {
           NT = NT + 1
        }
        if (patD.test(VVal))
        {
           NT = NT + 1
        }
        if (IsStudentPW)
        {
           if (patSL.text(VVal))
           {
              NT = NT + 1
           }
           if (!patSS.test(VVal))
           {
              args.IsValid = false;
              sender.innerHTML = "<br />The passprhase contains an invalid character!";
              return;
           }
        }
        else
        {
           if (patSI.test(VVal))
           {
              NT = NT + 1
           }
        }
        if (NT < 3)
        {
           args.IsValid = false
           sender.innerHTML = "<br />The passphrase entered is not strong!";
           return
        }
        var TPass = VVal.toUpperCase()
        if (TPass.indexOf('') > -1)
        {
           args.IsValid = false;
           patA = /@students\.ecu\.edu/
           if (IsStudentPW && ('' == 'ALUMNI' ))
           {
              sender.innerHTML = "<br />Your passphrase cannot contain your Alumni username!";
           }
           else
           {
              sender.innerHTML = "<br />Your passphrase cannot contain your PirateID!";
           }
           return;
        }
        args.IsValid = true
     }
  </script>
  <script language="javascript" type="text/javascript">
   function ShowPWHelp()
    {
       var newWin = null;
       try
       {
          newWin = window.open("PWInfo.aspx");
          newWin.focus()
          return false;
       }
       finally
       {
          newWin = null;
       }
    }
    function ShowRMFHelp()
    {
       var newWin = null;
       try
       {
//          newWin = window.open("https://itcs.ecu.edu/multi-factor-authentication-mfa/");
//          newWin = window.open("https://itcs.ecu.edu/services/accounts-passwords/multi-factor-authentication-mfa/");
          newWin = window.open("https://itcs.ecu.edu/services/accounts-passwords/multi-factor-authentication/")
          newWin.focus()
          return false;
       }
       finally
       {
          newWin = null;
       }
    }
    function ShowTCHelp()
    {
       var newWin = null;
       try
       {
          newWin = window.open("TCInfo.aspx");
          newWin.focus()
          return false;
       }
       finally
       {
          newWin = null;
       }
    }

