     <script>
      jQuery(window).load(function() {
	header_height = jQuery("header").height() + jQuery("#top-header").height();
	content_height = jQuery("#main-content").prop('scrollHeight') + header_height;
	window_height = jQuery(window).height();
	footer_height = jQuery("#main-footer .container").prop('scrollHeight');
	footer_top = jQuery("#main-footer").position().top;
	if (window_height-footer_height > content_height) {
		jQuery("#main-footer").attr("style", "position: absolute !important;");
		jQuery("#main-footer").css("top", (window_height-footer_height).toString() + "px");
	}
});
     </script>
