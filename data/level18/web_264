<script>
 (function($) {
  var container = document.getElementById('mm_live_pane_589_container');
  var template = document.getElementById('mm_live_pane_589_template').innerHTML;

  $.getJSON('https://mediamagnet.osu.edu/api/v1/items.json?%2Fapi%2Fv1%2Fitems_json%3Fchannel_id[]=410&channel_id[]=584&channel_id[]=409&channel_id[]=412&channel_id[]=595&channel_id[]=594&tags=osucyber&per_page=4', function(data) {
    $.each(data.items, function(idx, item) {
      item['formatted_published_at'] = moment(item['published_at'], 'YYYY-MM-DD HH:mm:ss Z').fromNow();
      var html = Mustache.to_html(template, item);
      $(html).hide().appendTo(container).fadeIn();
    });
  }).fail(function() {
    $(container).append('<p>Cannot load content.</p>');
  });
})(jQuery);
</script>
