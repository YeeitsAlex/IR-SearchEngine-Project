     <section class="block block-block clearfix" id="block-block-3">
      <script type="text/javascript">
       function buildAPPsearchURL() {
    var unencodedQuery = document.getElementById("APPsearchInput").value;
    var encodedQuery = encodeURIComponent(unencodedQuery);
    window.location = "https://login.proxy006.nclive.org/login?qurl=http%3A%2F%2Fsearch.ebscohost.com%2Flogin.aspx%3Fdirect%3Dtrue%26site%3Deds-live%26scope%3Dsite%26group%3Dmain%26profile%3Deds%26authtime%3Dcookie%2Cip%2Cuid%26bQuery%3D" + encodedQuery;
  }

  function buildGoogleScholarURL() {
    var unencodedQuery = document.getElementById("googleScholarInput").value;
    var encodedQuery = encodeURIComponent(unencodedQuery);
    window.location = "https://login.proxy006.nclive.org/login?qurl=https%3A%2F%2Fscholar.google.com%2Fscholar%3Fhl%3Den%26q%3D" + encodedQuery + "%26btnG%3DSearch";
  }

  function buildVideoURL() {
      var searchType = document.getElementById("searchtype");
      var selectedSearchType = searchType.options[searchType.selectedIndex].value;
      var inputTextQuery = document.getElementById("videoInput").value;
      if (selectedSearchType == "X") {
          window.location = "http://wncln.wncln.org/search~S1/X?SEARCH=(" + inputTextQuery + ")&m=v&m=1&m=g&m=u";
      }
      else {
          window.location = "http://wncln.wncln.org/search~S1/X?SEARCH=" + selectedSearchType + ":(" + inputTextQuery + ")&m=v&m=1&m=g&m=u";
      }
  }
      </script>
     </section>
     <!-- /.block -->
    </div>
   </div>
  </div>
