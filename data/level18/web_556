  <!-- START of script to filter event slider to "featured" category on load -->
  <script>
   $(document).ready(function () {
		var $eventCarousel = $('.events-carousel__filter-link');
		var $li = $("<li></li>");
		var $invisibleLink = $(
		'<a class="events-carousel__filter-link" href="./events/featured" data-filter="featured">Featured</a>');
		$eventCarousel.first().parent().before($li.append($invisibleLink));
		$invisibleLink.click();
		$li.hide();
		});
  </script>
  <script>
   if($(".flickity-slider").html().length ==0)
    {
    $(".events-carousel__filter-link").hide();
    }
  </script>
  <!-- END of script to filter event slider to "featured" category on load -->
  <!-- Last published by Lee Augsburger (laugsbur) at 2019-02-07-05:00-->
 </body>
</html>
