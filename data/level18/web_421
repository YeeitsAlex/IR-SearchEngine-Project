      <p class="text-bold text-center">
       Processing...please wait a moment...
      </p>
      <div class="progress">
       <div aria-valuemax="100" aria-valuemin="0" aria-valuenow="0" class="progress-bar progress-bar-striped active" role="progressbar" style="width: 0%">
       </div>
      </div>
     </div>
    </div>
   </div>
  </div>
  <script type="text/javascript">
   $(document).ready(function() {
		linkToken = 20190310010024;
	
		$("body").on('click mouseover mousedown', ".file-link", function(event){
			$(".file-link").each(function() {
				if($(this).attr('href').search(linkToken) == -1) {
					myLink = $(this).attr('href') + "/" + linkToken;
					$(this).attr('href', myLink);
				}
			});
		});
			
		$("body").on('click', ".file-link", function(event){
			// show a load message
			$('#loadLinkMessage').modal({
				show: 'false',
				backdrop: 'static',
				keyboard: 'false'
			});
	
			progressValue = 0;
			
			fileDownloadCheckTimer = window.setInterval(function () {
				var cookieValue = $.cookie('FILEDOWNLOADTOKEN');
				
				if (cookieValue == linkToken) {
					window.clearInterval(fileDownloadCheckTimer);
					$.removeCookie('FILEDOWNLOADTOKEN', { path: '/' }); //clears this cookie value
					$("#loadLinkMessage").modal('hide');
				}
				else {
					progressValue = progressValue + 2;

					$('.progress-bar').width(progressValue + '%');

					if (progressValue == 100) {
						progressValue = 0;
					}
				}
			}, 1000);
		});
	});
  </script>
 </body>
</html>

