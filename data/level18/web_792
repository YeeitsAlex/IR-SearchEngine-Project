        <script type="text/javascript">
         $(document).ready(function(){
    $.ajax({
      url: '/JSON/spotlight.php',
      cache: false,
      dataType: "json"
    })
    .done(function(data) {
      try {
        var spotlightOutput = '';

        // Loop through rows
        $.each(data, function (key, val) {
        
          var OfferingName = val.OfferingName;
          var OfferingID = val.OfferingID;
          var SectionID = val.SectionID;
          var Start_Month_Day_Year = val.Start_Month_Day_Year;
      
          spotlightOutput += '<div class="headline-block">';//  Start of date
          spotlightOutput += Start_Month_Day_Year;
          spotlightOutput += '</div>';// End of date

          spotlightOutput += '<div class="headline-block feed-block-feature">';// Start Program Name and Link

          spotlightOutput += '<a target="_blank" href="';
          spotlightOutput += 'https://register.ece.emory.edu/modules/shop/index.html?';
          spotlightOutput += 'action=section&OfferingID='+OfferingID+'&SectionID='+SectionID;
          spotlightOutput += '">'+OfferingName+'</a>';

          spotlightOutput += '</div>';// End of Program Name and Link
        });// $.each(data, function (key, val)


        $('#ECE_Course_Spotlight').html(spotlightOutput);
      } catch(err) {
        alert(err.message);
      };
    })    
    .fail(function( jqXHR, textStatus ) {  
      //alert( "Triggered fail callback: " + textStatus );  
    });
    
  $('#row2-group3').css('overflow-y','auto'); // Add vertical scrollbars, if necessary
  });
        </script>
