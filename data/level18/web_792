        <script type="text/javascript">
         $(document).ready(function(){
  
    var contactData = '<div id="contactInfoDiv" class="container" style="';
    contactData += 'text-align: right; background-color:#002878; ';
contactData += ' padding-bottom: 5px;';
    contactData += '">';
    contactData += '<span style="font-family: Verdana,Arial,sans-serif; font-weight: bold; color:#ffffff; padding-right: 10px;">';
    contactData += 'ece@emory.edu&nbsp\|&nbsp404.727.6000';
    contactData += '</span>';
    contactData += '</div>';
    
    $('#main-nav').before(contactData);
  
  
    $.ajax({
      url: '/JSON/infosessions.php',
      cache: false,
      dataType: "json"
    })
    .done(function(data) {
      try {

        var infoSessionOutput = '';

        // Loop through rows
        $.each(data, function (key, val) {
        
          var Info_Session_Number = val.Info_Session_Number;
          var ECE_Program_Name = val.ECE_Program_Name;
          var Info_Session_Date_Time_Format = val.Info_Session_Date_Time_Format;
          var Info_Session_Date_Time = val.Info_Session_Date_Time;
          var Session_Month_Day_Year = val.Session_Month_Day_Year;
          var Session_Time = val.Session_Time;
          var Location = val.Location;
          var Webinar_URL = val.Webinar_URL;
          var Webinar_ID = val.Webinar_ID;
          var Status = val.Status;

          //  Start of class="headline-block feed-block-feature"
          infoSessionOutput += '<div class="headline-block feed-block-feature">';
          infoSessionOutput += Session_Month_Day_Year + ' | '
          infoSessionOutput += Session_Time;
          infoSessionOutput += '<br>';
          infoSessionOutput += '<a href="';
          /*
          if (Location == 'Webinar') {
            infoSessionOutput += Webinar_URL;            
          } else {
            infoSessionOutput += 'https://ece.emory.edu/info_session/info_session_signup.php?';
            infoSessionOutput += 'infosession=' + Info_Session_Number;          
          }// if (Location == 'Webinar')
          */
          infoSessionOutput += 'https://ece.emory.edu/info_session/info_session_signup.php?';
          infoSessionOutput += 'infosession=' + Info_Session_Number;
          infoSessionOutput += '">'+ECE_Program_Name+'</a>';
          
          //  End of class="headline-block feed-block-feature"
          infoSessionOutput += '</div>';
        });// $.each(data, function (key, val)


        $('#ECE_Info_Session_List').html(infoSessionOutput);
      } catch(err) {
        alert(err.message);
      };
    })    
    .fail(function( jqXHR, textStatus ) {  
      //alert( "Triggered fail callback: " + textStatus );  
    });
    
    $('#row2-group2').css('overflow-y','auto'); // Add vertical scrollbars, if necessary
  });
        </script>
