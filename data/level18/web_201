         <div class="block-inner clearfix">
          <h2 class="block-title">
           College News
          </h2>
          <div class="content clearfix">
           <div class="view view-news view-id-news view-display-id-block view-dom-id-e0333921044055cbd6af0d4cd44eecfe">
            <div class="view-content">
             <div class="views-row views-row-1 views-row-odd views-row-first">
              <div class="views-field views-field-nothing">
               <span class="field-content">
                <div class="topnews">
                 <div class="newsphoto">
                  <picture>
                   <!--[if IE 9]><video style="display: none;"><![endif]-->
                   <source media="(min-width: 1220px)" srcset="https://medicine.arizona.edu/sites/default/files/styles/1x_widescreen/public/howe-teaser.jpg?itok=KstJTsFo&amp;timestamp=1551977568 1x"/>
                   <source media="(min-width: 980px)" srcset="https://medicine.arizona.edu/sites/default/files/styles/1x_desktop/public/howe-teaser.jpg?itok=QAKADRa1&amp;timestamp=1551977568 1x"/>
                   <source media="(min-width: 740px)" srcset="https://medicine.arizona.edu/sites/default/files/styles/1x_tablet/public/howe-teaser.jpg?itok=wJbvg-FE&amp;timestamp=1551977568 1x"/>
                   <source media="(min-width: 740px)" srcset="https://medicine.arizona.edu/sites/default/files/styles/1x_small_tablet/public/howe-teaser.jpg?itok=fSTNqYcc&amp;timestamp=1551977568 1x"/>
