         <div class="block-inner clearfix">
          <div class="content clearfix">
           <div class="view view-home-slide-shows view-id-home_slide_shows view-display-id-block view-dom-id-958718038d02a69d1c6fabac44de284a">
            <div class="view-content">
             <div class="skin-default">
              <div class="views-slideshow-controls-top clearfix">
               <div class="views-slideshow-pager-fields widget_pager widget_pager_top views_slideshow_pager_field" id="widget_pager_top_home_slide_shows-block">
                <div class="views-slideshow-pager-field-item views_slideshow_pager_field_item views-row-odd views-row-first" id="views_slideshow_pager_field_item_top_home_slide_shows-block_0">
                 <div class="views-field-field-slide-image">
                  <div class="views-content-field-slide-image">
