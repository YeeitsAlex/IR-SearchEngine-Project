     <div id="contentdiv">
      <div id="text1">
       <h1>
        Home
       </h1>
       <script>
        jQuery(document).ready(function ($) {  
  
	var slideSource = new Array();
	var slideCount = 0;
	var slideInterval;
	var currentSlide = 0;
   	window.console && console.log('first');

  	$.ajax({
      	url: "https://sa.ccga.edu/videos/getSlides.php",
      	success: function(data) {
          	wrap = "<div>" + data + "</div>";
          	$(wrap).find(".getSlideDiv").each(function() {
              	fname = $(this).attr('data-fname');
              	atext = $(this).attr('data-atext');
              	alink = $(this).attr('data-alink');
              	tempArr = new Array(fname,atext,alink);
              	window.console && console.log('test');
              	slideSource.push(tempArr);
                slideCount++;
            });
            beginShow();
        },
      	fail: function() {
			slideSource[0]= Array("uploaded/images/test/No9AffordableSlide2.jpg","College of Coastal Georgia was ranked the number 9 most affordable small college in the US","http://www.ccga.edu/page.cfm?p=534&newsid=520");
            slideCount++;
            beginShow();
        }
    });
//  slideSource[0]= Array("uploaded/images/test/2019SpringReg.jpg","College of Coastal Georgia Spring Registration is open","https://portal.ccga.edu");
//	slideSource[1]= Array("uploaded/images/test/No9AffordableSlide2.jpg","College of Coastal Georgia was ranked the number 9 most affordable small college in the US","http://www.ccga.edu/page.cfm?p=534&newsid=520");
    
function beginShow() {
//	slideCount = slideSource.length;
  
	slideDivStyle = "display: none; width: 100vw; height: 100vh; position: relative; background: #000; background: -webkit-linear-gradient(#005480,#000,#005480); background: linear-gradient(#005480,#000,#005480);";
	fullHeight = $(window).height();
	halfHeight = parseInt(fullHeight/2);
	slideTop = halfHeight + 53;
	slideStyle = "position: absolute; max-width: 100vw; max-height: 100%; top: 50%; left: 50%; transform: translate(-50%,-50%);";

	slideDiv = "<div id='slideOverlay' style='" + slideDivStyle + "'>";
	for (i=0; i < slideCount; i++) {
		slideDiv += "<a href='" + slideSource[i][2] + "'><img id='slideContent" + i + "' class='slideContent' src='" + slideSource[i][0] + "' alt='" + slideSource[i][1] + "' style='";
		if (i != 0) {
			slideDiv += "display: none;";
		}
		slideDiv += slideStyle + "' role='button' tabindex='1' title='Read more' /></a>";
	}
	slideDiv += "</div>";
   
	$('.slideshow-container').html(slideDiv);
    
    $('.slideContent').click(function () {
      	slideID = $(this).attr('id');
        slideNum = substr(slideID,12);
        window.location.href(slideSource[slideNum][2]);
    });
    
	
	$('#slideOverlay').fadeIn(750, function () {
		slideHeight = $('#slideContent0').height();
		slideHeight+= 20;
		$('#slideOverlay').css('height',slideHeight + 'px');
	});
	slidePlay(currentSlide);
	slideInterval = setInterval(function () {
		currentSlide++;
		if (currentSlide == slideCount) {
			currentSlide = 0;
			slidePlay(currentSlide);
		} else {
			slidePlay(currentSlide);
		}
	}, 6000);
}

	function slidePlay(slideNum) {
		if (slideNum == 0) {
			lastSlide = slideCount - 1;
			$('#slideContent' + lastSlide).fadeOut(750);
			$('#slideContent' + slideNum).fadeIn(750);
		} else {
			prevSlide = slideNum - 1;
			$('#slideContent' + prevSlide).fadeOut(750);
			$('#slideContent' + slideNum).fadeIn(750);
		}
	}
  
});
       </script>
      </div>
