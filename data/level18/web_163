              <script src="https://online.uncg.edu/hubfs/UNCG%20Online%20Jan%202018/js/jquery.beefup.min.js">
              </script>
              <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet"/>
              <script>
               $(document).ready(function () {
    $('.two-column-accord').beefup(); 

    if($(window).innerWidth() > 1024){
        $(".beefup__head").click()
    }
});

$(window).resize(function () {
    if($(window).innerWidth() > 1024){
        if($(".beefup__body").is(':visible') == false){
            $(".beefup__head").click()
        }
    } else {
        $(".beefup.two-column-accord.is-open").removeClass("is-open");
        if($(".beefup__body").is(':visible') == true){
        	$(".beefup__body").hide();
        }
    }
});
              </script>
