       <script type="text/javascript">
        $('#form-archives #cat').attr('id', 'cat-archives');
					    var dropdown = document.getElementById("cat-archives");
					    function onCatChange() {
							if ( dropdown.options[dropdown.selectedIndex].value > 0 ) {
								location.href = "https://newsandfeatures.uncg.edu/?cat="+dropdown.options[dropdown.selectedIndex].value;
							}
					    }
					    dropdown.onchange = onCatChange;
       </script>
