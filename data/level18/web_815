       <script>
        $(document).ready(function () {
            

                                url = encodeURIComponent('https://scholarblogs.emory.edu/ecds/feed/');
                                url = url.replace('amp%3B','');
            
                           $('#news-row2-group1').emoryFeed({
                        
                        preset: "4 - Simple Headlines Only",
                        url: url,
                        addClass: ["emory-feed", "simple-headlines"]
                
                    , 
                    feature: {
                        readMoreText: 'Continue Reading'
                    }
               
                        ,
                        alsoTitle: {
                            text:"Recent Blog Posts"
                        }
                    
                        ,
                        
                            title: {
                                text: 'ECDS Blog'
                            },
                            viewAll: {
                                text: 'View All'
                            }
                        
                , 
                maxItems: 3
                        ,
                        openLinksInNewWindow: true
                    
             }); 

            });
       </script>
