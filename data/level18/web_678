     <script>
      var analytics_path = window.location.pathname.substring(1);		// full pathname (does not start with /)
	var analytics_dir = analytics_path.split("/")[0];					// first directory in pathname
	
	// default top-level code
	var analytics_code = 'UA-20993389-1';
	
	// overrides
	switch(analytics_dir){
			case 'azindex':
			analytics_code = 'UA-64660617-1';
			break;
			
			case '404':
			analytics_code = 'UA-20993389-10';
			break;
			
			case 'mobile':
			analytics_code = 'UA-20993389-2';
			break;
	}

	//console.log("analytics code: ", analytics_code);
	
	// send code
	ga('create', analytics_code, 'auto');
    ga('send', 'pageview');
	
	// homepage rotator click analytics event
	$(document).ready(function(){
		$(".flexslider .slides a").click(function( e ) {
			var title = $(".flex-active-slide").find("h1").text();
			// send analytics
			ga('send', 'event', 'Homepage', 'rotator click', title);
		});
	});
     </script>
