           <script id="w4pl-js-984" type="text/javascript">
            var shorten_excerpt = function() {

var excerpts = document.getElementsByClassName('post-excerpt');
if ( document.documentElement.clientWidth > 950 || document.documentElement.clientWidth < 750) {
	var max_chars = 100;
} else {
var max_chars = 200;
}

	for (var i = 0; i < excerpts.length; ++i) {
		var excerpt = excerpts[i];
		var excerpt_string = excerpt.innerHTML;
		var words = excerpt_string.split(' ');
		var num_words = words.length;
		word_lens = [];

		for (var j = 0; j < words.length; ++j) {
			word_lens.push(words[j].length);
		}

		var display_amount = 0;
		var total = 0;
		var index = 0;
		while (total < max_chars) {
			display_amount++;
			total += word_lens[index];
			index++;
		}

		words = words.slice(0, display_amount);
		new_excerpt = words.join(' ');

		if (display_amount < num_words) {
			new_excerpt = new_excerpt + '...';
		}
		excerpt.innerHTML = new_excerpt;
	}

	return;
}


window.onload = function() {
	var titles = document.getElementsByClassName('post_title');
if ( document.documentElement.clientWidth > 950) {
	var max_chars = 60;
} else {
var max_chars = 100;
}

	for (var i = 0; i < titles.length; ++i) {
		var title = titles[i];
		var title_string = title.innerHTML;
		var words = title_string.split(' ');
		var num_words = words.length;
		word_lens = [];

		for (var j = 0; j < words.length; ++j) {
			word_lens.push(words[j].length);
		}

		var display_amount = 0;
		var total = 0;
		var index = 0;
		while (total < max_chars) {
			display_amount++;
			total += word_lens[index];
			index++;
		}

		words = words.slice(0, display_amount);
		new_title = words.join(' ');

		if (display_amount < num_words) {
			new_title = new_title + '...';
		}
		title.innerHTML = new_title;
	}

        shorten_excerpt();
	return;
}



