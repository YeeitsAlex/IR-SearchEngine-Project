        <div class="row row-campus uppercase">
         Available at Main Campus
        </div>
        <footer>
         <a class="compare fl compare-button" data-marray='{"mident":"WLITBA-NONE-NONE","mtitle":"World Literature","mdegree":"Bachelor of Arts","mprog":"UHMNT","ccollege":"College of Humanities","cslug":"college-of-humanities","murl":"https:\/\/pah.arizona.edu\/","maddt":"No Additional Requirements","mtext":"The new interdisciplinary major in World Literature offers you opportunities to explore your interests in regional literatures (Eastern Europe, Latin America, East Asia, West Africa, the Middle East) or national literatures (Chinese, French, Russian, Filipino, German, Italian, Arabic, Turkish) with more topical studies (genre, period, gender, ethnicity, indigenous critique, cultural studies, postcolonial studies). You will also learn about literary theory, criticism, and history. World literature is literature with an impact beyond its original language and cultural area. It is the product of intercultural historical events, and the circulation of ideas that connect people from across the globe. Combined with an intensive foreign language study, the degree program will provide in-depth insight into the literatures that live on in those languages.","mslug":"world-literature","mfees":"No","mprem":"No","mmath":"General","mlang":"4 Semesters","mcampus":"Available at Main Campus","mholland":"H3","mmymf":"MY07","msalary":"No data available at this time."}' href="#">
          + Save
         </a>
         <a class="fl details-button" href="/major/world-literature" onclick="ga('send', 'event', 'Degree', 'View Details', 'World Literature');">
          See Details »
         </a>
        </footer>
       </div>
      </div>
     </section>
     <!--END DEGREE-LIST-->
    </div>
   </div>
   <script>
    //total majors loaded initially.
		var total = 283;
		//hide welcome banner
		$('#hideBanner').click(function(){
			$.cookie('hideBanner','yes',{ expires : 30 });
			$(this).parent().fadeOut();
		});
		//sort-bar
		dl = $('.degree-list');
		b = $('#bars');

		//add cursor to tile icon
		$('#sort-tile').addClass('active');
		b.addClass('tile-active');

		//tile button, active by default, add tile class and remove row
		$('.sort-style').click(function(){
			if(!$(this).hasClass('active')){
				$(this).addClass('active').siblings('.sort-style').removeClass('active');
      	$('#style-filter-field').val($(this).data('style'));
      	filter_query();
				ga('send', 'event', 'Filter', 'Style', $(this).data('style'));
			}
		});

		//query function for refreshing data
		function filter_query(){
			$('.degree-list').fadeOut(100, function(){
				if($('#style-filter-field').val() == 'table'){
					b.removeClass('tile-active').addClass('row-active');
				}else{
					b.removeClass('row-active').addClass('tile-active');
				}
				$.get("/core/functions/filter?"+$("#search_form").serialize(), function(data) {
					$('.degree-container').html(data);
					$('.results-number').html($('.degree-block').length);
					$('.degree-list').fadeIn(100);
				});
			});
		}

		//#filter-box title that collapses
		$('#narrow').addClass('opened').click(function(){
			if(!$(this).hasClass('opened')){
				$(this).removeClass('closed').addClass('opened');
			}else{
				$(this).removeClass('opened').addClass('closed');
			}
			$(this).parent().next('.filter-box').slideToggle();
		}).css({cursor:"pointer"});

		//filter row headings
		$('.filter h4').addClass('open').click(function(){
			if($(this).hasClass('open')){
				$(this).parent().parent().find('.filter h4').removeClass('open');
			}else{
				$(this).parent().parent().find('.filter h4').addClass('open');
			}
    	$(this).parent().parent().find('.filter .box').slideToggle();
    }).css({cursor:"pointer"});

		//cluetips, both math and language, will be used by holland
		$('.info-icon').hover(function(){
			info = "#" + $(this).attr('id');
			$(info+'-cluetip').stop().fadeIn('slow');
		},function(){
			$(info+'-cluetip').stop().fadeOut('fast');
		}).mousemove(function(e){
			var mousex = e.pageX - 250;
			var mousey = e.pageY + 10;
			$(info+'-cluetip').css({top: mousey, left: mousex});
		});

		var stickyHeaderTop = $('.search-results-bar').offset().top;
 		//add sticky if past top of selectors
    $(window).scroll(function(){
      if( $(this).scrollTop() > stickyHeaderTop ) {
        //   $('.saved-degree-block').addClass('sticky');
          $('#bars').addClass('sticky')
      } else {
        //   $('.saved-degree-block').removeClass('sticky');
          $('#bars').removeClass('sticky')
      }
      //scrollup arrow if top past 800px
      if ($(this).scrollTop() > 800) {
				$('.scrollup').fadeIn();
			} else {
				$('.scrollup').fadeOut();
			}
    });
    //section bar that appears in row-mode
    $('.filter-row-bar div').click(function(){
    	//if not active
    	if(!$(this).is('.active')){
    		//make active and remove active from siblings
  			$(this).addClass('active').siblings().removeClass('active').data('order','ASC');
    	}else if($(this).is('.active.reverse')){
    		//if reverse, make it normal
    		$(this).removeClass('reverse').data('order','ASC');
    	}else{
    		//if not reverse, make it reverse
    		$(this).addClass('reverse').data('order','DESC');
    	}
    	//set sort options back to alphabetical
    	$("#sort").val('1');
    	//set field to sort plus order
    	$('#sort-filter-field').val($(this).data('sort')+" "+$(this).data('order'));
    	//query data
    	filter_query();
			ga('send', 'event', 'Sort', $(this).data('sort'), $(this).data('order'));
    });
    //filter option list
    $('#sort option').each(function () {
	    $(this).data('original', $(this).text());
		});
		$('#sort').change(function () {
			//for each option make them their shorter version
		    $('option', this).each(function () {
		        $(this).text($(this).data('original'));
		    });
		    //replace long version with short version
		    $(this).find('option:selected').text($(this).find('option:selected').attr('value'));
		    //if sort active
		    if ($('.filter-row-bar section').hasClass('active')){
			    $('.filter-row-bar section').removeClass('active');
				}
				//set sort field to select sort and order
    		$('#sort-filter-field').val($("#sort option:selected").data('sort')+" "+$("#sort option:selected").data('order'));
	    	//query data
				ga('send', 'event', 'Sort', $("#sort option:selected").data('sort'), $("#sort option:selected").data('order'));
        filter_query();
		});

		//all checkboxes are inactive by default
		$('.query-filter').addClass('inactive');
		//except the "all" checkboxes at the top of each list
		$('.query-filter.all').attr('checked',true).removeClass('inactive').addClass('active');
		//select all alphabet
		$('.query-filter.link[id=]').addClass('active');
		//resets filters, query is blank
		$('#reset').click(function(){
			//all filters back to unchecked.
			$('.query-filter').attr('checked',false).addClass('inactive');
			//filters for "all" are checked
			$('.query-filter.all').attr('checked',true).removeClass('inactive').addClass('active');
			//trigger resets for sliders
			$('#math-slider-bar, #language-slider-bar').trigger('reset');
			//remove link default
			$('#search-filter-field').val('');
			$('#alpha-filter-field').val('');
			$('#alpha-current').html('A - Z');
			$('.query-filter[name=alpha]').removeClass('active');
			$('.query-filter.link[id=]').addClass('active');
			//if degree row isn't selected or reverse, click row-degree for default.
			if($('.degree-block').length != total){
				if($('.row-degree').hasClass('reverse') || (!$('.row-degree').hasClass('active'))){
					$('.row-degree').click();
				}else{
					filter_query();
				}
			}
			return false;
		});
		//filter options
		$('.query-filter').on('click',function(e){

			var attr = $(this).attr('name'),
				data = $(this).data('attr'),
				val = $(this).val();

			if($(this).data('name') == 'alpha'){
				e.preventDefault();
				val = $(this).attr('id') != "" ? $(this).attr('id') : "";
				attr = "alpha";
				$('#alpha-filter-field').val(val);
				$(this).addClass('active').removeClass('inactive');
				$(this).siblings().removeClass('active').addClass('inactive');
				//if a not equal, reset, otherwise equal a
				$('#alpha-current').html(($(this).attr('id') === '' ? "A - Z" : $(this).attr('id')));
			}

			switch(attr){
				case "college[]":
				case "holland[]":
				case "mymf[]":
					val = (val != "" ? val + " - " + data : "ALL");
				break;
				//allow only one premajor to be checked at a time, unless all
				case "premajor":
					val = val != "" ? val : "ALL";
					$('.query-prem').not(this).attr('checked', false);
				break;
				//allow only one campus to be checked at a time, unless all
				case "location":
					val = val != "" ? val + " - " + $(this).next('label').text() : "ALL";
					$('.query-campus').not(this).attr('checked', false);
				break;
			}

			if(val != "")
				ga('send', 'event', 'Filter', attr, val);

			//all checkbox
			if($(this).hasClass('all')){
				$(this).attr('checked',':checked');
				$(this).parent().parent().find('.query-filter').not('.all').removeAttr('checked');
			}else{
				$(this).parent().parent().find('.all.query-filter').removeAttr('checked');
			}

			filter_query();

		});
		//if college is set, filter
		
		// Namespace:
		var ClassSliders = ClassSliders || {};
		ClassSliders.init = function () {

			//language
			$('#language-slider-bar').slider({
				min: 1,
				max: 3,
				animate: "normal",
				range:true,
				values: [1,3],
				slide: function( event, ui ) {
					none = 'NONE',
					two = '2SEM',
					four = '4SEM',
					lff = $('#lang-filter-field');

					switch (ui.values[0]) {
						case 1: //show none
							switch (ui.values[1]) {
								case 1: lff.val(none); break;
								case 2:	lff.val(none+","+two); break;
								case 3: lff.val(""); break;
							}
						break;
						case 2: //2 sem
							switch (ui.values[1]) {
								case 2: lff.val(two); break;
								case 3: lff.val(two+","+four); break;
							}
						break;
						case 3:  //4 sem
							switch (ui.values[1]) {
								case 3: lff.val(four); break;
							}
						break;
					}

					filter_query();

					if(lff.val() != "")
						ga('send', 'event', 'Filter', 'Language Required', lff.val());
					//manage the range label values
					var slideVals = ["None","2 Semesters","4 Semesters"];
					if (ui.values[0] == ui.values[1]) {
					  	$( "#language-range" ).val( slideVals[ui.values[ 0 ] - 1] );
					}
					else {
						$( "#language-range" ).val( slideVals[ui.values[ 0 ] - 1] + " - " + slideVals[ui.values[ 1 ] - 1] );
					}
				}
			}).bind({
				'reset': function () {
					//resets the slider handles to the poles
					$(this).slider("values", 0, 1).slider("values", 1, 3);
					$('#lang-filter-field').val('');
					//update the range label above the slider
					var slideVals = ["None","2 Semesters", "4 Semesters"];
					$( "#language-range" ).val( slideVals[0] + " - " + slideVals[2] );
					return false;
				}
			});

			var slideVals = ["None","2 Semesters","4 Semesters"];
			var lowRange = slideVals[$( "#language-slider-bar" ).slider( "values", 0 ) - 1];
			var hiRange = slideVals[$( "#language-slider-bar" ).slider( "values", 1 ) - 1];
			$( "#language-range" ).val( lowRange + " - " + hiRange);

			$('#math-slider-bar').slider({
				min: 1,
				max: 3,
				animate: "normal",
				range:true,
				values: [1,3],
				slide: function( event, ui ) {
					gen = 'GEN',
					mod = 'MOD',
					sub = 'SUB',
					mff = $('#math-filter-field');
					switch (ui.values[0]) {
						case 1: //show gen mod and sub on the low side
							switch (ui.values[1]) {
								case 1: mff.val(gen); break;
								case 2: mff.val(gen+","+mod); break;
								case 3: mff.val(""); break;
							}
						break;
						case 2: //gen low side
							switch (ui.values[1]) {
								case 2: mff.val(mod); break;
								case 3: mff.val(mod+","+sub); break;
							}
						break;
						case 3: //mod low side
							switch (ui.values[1]) {
								case 3: mff.val(sub); break;
							}
						break;

					}
					if(mff.val() != "")
						ga('send', 'event', 'Filter', 'Math Required', mff.val());

					filter_query();

					//manage the range label values
					var slideVals = ["General","Moderate","Substantial"];
					if (ui.values[0] == ui.values[1]) {
					  	$( "#math-range" ).val( slideVals[ui.values[ 0 ] - 1] );
					}
					else {
						$( "#math-range" ).val( slideVals[ui.values[ 0 ] - 1] + " - " + slideVals[ui.values[ 1 ] - 1] );
					}
				}
			}).bind({
				'reset': function () {
					//resets the slider handles to the poles
					$(this).slider("values", 0, 1).slider("values", 1, 3);
					$('#math-filter-field').val("");
					//update the range label above the slider
					var slideVals = ["General","Moderate", "Substantial"];
					$( "#math-range" ).val( slideVals[0] + " - " + slideVals[2] );
					return false;
				}
			});

			var slideVals = ["General","Moderate", "Substantial"];
			var lowRange = slideVals[$( "#math-slider-bar" ).slider( "values", 0 ) - 1];
			var hiRange = slideVals[$( "#math-slider-bar" ).slider( "values", 1 ) - 1];
			$( "#math-range" ).val( lowRange + " - " + hiRange);


		};
		$(document).ready(ClassSliders.init);
   </script>
   <a class="scrollup" href="#">
    Scroll To Top
   </a>
  </div>
  <div id="overlay-start">
   <div id="overlay-message-start">
   </div>
  </div>
