                 <script>
                  var _tz = timezoneJS.timezone;
      _tz.loadingScheme = _tz.loadingSchemes.MANUAL_LOAD;
      _tz.loadZoneJSONData('/sites/all/modules/custom/properties_nav/js/america.new_york.json', true);
      var boston_time = new timezoneJS.Date(new Date().getTime(), 'America/New_York');
      var hour = (boston_time.getHours() % 12 || 12);
      var minutes = boston_time.getMinutes() < 10 ? '0'+boston_time.getMinutes() : boston_time.getMinutes();
      var ampm = boston_time.getHours() >= 12 ? 'PM' : 'AM';
      var timezone = boston_time.getTimezoneAbbreviation();
      jQuery('#boston-time').text(hour + ':' + minutes + ' ' + ampm + ' ' + timezone);
                 </script>
