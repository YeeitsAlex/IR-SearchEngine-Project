    <div class="zone-wrapper zone-header-wrapper clearfix" id="zone-header-wrapper" style="position: relative;">
     <div id="header-tile">
     </div>
     <div class="zone zone-header clearfix container-12" id="zone-header">
      <div id="banner-container">
       <a href="http://asc.osu.edu/" id="ascbanner">
        College of Arts and Sciences
       </a>
      </div>
      <div class="grid-6 region region-header-first" id="region-header-first">
       <div class="region-inner region-header-first-inner">
        <div class="block block-views block-front-block-1 block-views-front-block-1 odd block-without-title" id="block-views-front-block-1">
         <div class="block-inner clearfix">
          <div class="content clearfix">
           <div class="view view-front view-id-front view-display-id-block_1 view-dom-id-52154c9b70a79fc1edbac070402adc52">
            <div class="view-content">
             <div class="views-row views-row-1 views-row-odd views-row-first views-row-last">
              <article about="/content/front-page" class="node node-front-page node-published node-not-promoted node-not-sticky self-posted author- odd clearfix" id="node-front-page-1" typeof="sioc:Item foaf:Document">
               <header>
                <h2 class="node-title">
                 <a href="/content/front-page" title="Front Page">
                  Front Page
                 </a>
                </h2>
               </header>
               <span class="rdf-meta element-hidden" content="Front Page" property="dc:title">
               </span>
               <div class="content clearfix">
                <div class="galleryformatter galleryview galleryformatter-greenarrows">
                 <div class="gallery-slides" style="width: 460px; height: 262px;">
                  <div class="gallery-frame">
                   <ul>
                    <li class="gallery-slide" id="slide-0-field_slideshow-1">
                     <a href="/ics" title="">
                      <img alt="Institute for Chinese Studies Graduate Forum" height="262" src="https://easc.osu.edu/sites/easc.osu.edu/files/styles/front_short/public/GradForum-cropped2.jpg?itok=8SeaxbIl" title="Institute for Chinese Studies Graduate Forum" typeof="foaf:Image" width="460"/>
                     </a>
