    jQuery(function ($, undefined) {
    //Menus
    $("#MenuV .Vertical > ul").BindMENU({ MenuStyle: "DefaultMenu", Orientation: "Vertical" });
    $("#MenuH .Horizontal > ul, ul#CMSListMenuPrimary").BindMENU({ MenuStyle: "DefaultMenu", Orientation: "Horizontal" });
  });
   </script>
   <script>
    $( ".CMSListMenuUL a" ).attr("aria-label",function(){return ($(this).text() + " " + $(this).closest('ul').siblings('a').text());});
  //$( ".main-menu a" ).append(function(){return (" "+$(this).closest('ul').siblings('a').text());});
  //$('.main-menu a').keydown(function(event){ if(event.keyCode==9){ eval($(this).attr('href'));}});
  $(document).ready(function(){
    $('a').on('keydown', function(event){ if(event.keyCode==37){ $(this).next('ul').toggle().next('li').focus()}});
    $('a').on('hover', function(event){ $(this).next('ul').append($('<style>.'+classname+' { display:block; }</style>'))});
    $('a').on('keydown', function(event){ if(event.keyCode==38){ $(this).next('ul').toggle().next('li').focus()}});
    $('a').on('keydown', function(event){ if(event.keyCode==39){ $(this).next('ul').toggle().next('li').focus()}});
    $('a').on('keydown', function(event){ if(event.keyCode==40){ $(this).next('ul').toggle().next('li').focus()}});
    $('a').on('keydown', function(event){ if(event.keyCode==9){ $(this).parents('li').siblings('li').children('ul').hide()}});
    $('a').click(function () {  
      $(".active").removeClass("active");  
      $(this).addClass("active");
    }); 
  });
   </script>
   <script>
    $("a[href^='#']")
    .click(function(evt){
      var j = $(evt.currentTarget);
      var anchorTarget = j.attr("href");
      $("body")
        .scrollTo(anchorTarget, 500, {
          onAfter:function() {
            window.location.hash = anchorTarget.substr(1);
            $(anchorTarget).attr("tabindex",-1).focus();
          }
        });
      
      evt.preventDefault();
    });
   </script>
   <script type="text/javascript">
    //<![CDATA[

WebForm_InitCallback();
theForm.oldSubmit = theForm.submit;
theForm.submit = WebForm_SaveScrollPositionSubmit;

theForm.oldOnSubmit = theForm.onsubmit;
theForm.onsubmit = WebForm_SaveScrollPositionOnSubmit;
//]]>
   </script>
  </form>
