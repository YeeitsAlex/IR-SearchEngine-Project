        $("#div-search-container").on("keypress", function (e) {
            if (e.keyCode == 13) {
                personsearch();
            }
            return true;
        });
        $("#img-person-search-button").on("click", function () {
            personsearch();
            return true;
        });
        
    });

    function personsearch() {
        document.getElementById("ctl00_ContentMain_rptMain_ctl00_ctl00_hdnSearch").value = "true"
        document.forms[0].submit();
        return true;
    }
