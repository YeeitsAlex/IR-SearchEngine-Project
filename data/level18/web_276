function myResize() {
	if ((browType=="Netscape")&& theBrowser.hasDHTML)
		{
		self.location.reload(false);
		self.frames[0].location.reload(false);
		self.frames[1].location.reload(false);
		self.frames[2].location.reload(false);
		self.frames[3].location.reload(false);
		self.frames[4].location.reload(false);
	 	}
	}
function menusubmit(as_menu,Target,ProcessState,p1,p2,p3,p4,p5,p6,p7) {
	if (window.frames[2].document.forms[0]) {
		if (as_menu != "billing_info") loadingMsg();
		myform = window.frames[2].document.forms[0];
		if (as_menu == "logout") Target="_top";
		myform.target = (Target) ? Target : "wustext";

		//myform.as_session_id.value = strSessionID;
		myform.as_transaction_id.value = strTransID;
		myform.as_process_state.value = ProcessState;
		myform.as_menu_selection.value = as_menu;
		myform.as_p1.value = p1;
		myform.as_p2.value = p2;
		myform.as_p3.value = p3;
		myform.as_p4.value = p4;
		myform.as_p5.value = p5;
		myform.as_p6.value = p6;
		myform.as_p7.value = p7;
		
		//if (theMenu.selectedEntry >= 0) setHelpPage(theMenu.entry[theMenu.selectedEntry].text);
		
		myform.submit();
	}

	if (as_menu == "logout") {
		document.clear();
		};
	};

function rnd() {
		rnd.today=new Date();
		rnd.seed=rnd.today.getTime();
        rnd.seed = (rnd.seed*9301+49297) % 233280;
        return rnd.seed/(233280.0);
	}

function rand(number) {
        return Math.ceil(rnd()*number);
	}

function menusubasp(url)
	{
	loadingMsg();
	//if (theMenu.selectedEntry >= 0) setHelpPage(theMenu.entry[theMenu.selectedEntry].text);

//	wustext = window.frames[5].document;
//	wustext.location = url;
	rExp = /[\?\&]r=/;
	if (url.search(rExp) < 0) {
		rExp = /\?/;
		if (url.search(rExp) < 0) {
			newurl = url + '?r=' + rnd()
			}
		else {
			newurl = url + '&r=' + rnd()
			};
		}
	else {
		newurl = url;
		}
	wustext = window.frames[5];
	wustext.location.replace(newurl);
	
	}
	
function loadingMsg()
	{
	/*
	if (((browType == 'IE') || (browType == 'Netscape')) && (browPlatform == 'MacPPC')) { }
	else {
		//wustext = window.frames[5].document;
		//wustext.location.replace("/blank.htm");
		
		wustext = window.frames[5];
//		alert(wustext + '\n' + wustext.src);
		wustext.location.replace("blank.htm");
		
		}
		*/
	showLoading(true);
	}
// ***** New window close function 2000-05-11 jwj *****
function closePlan() {
	top.wustext.WUClosePopUps();
	if (winSearch) winSearch.close();
	window.close();
}

//  *************** Get the Initial Page for a plan
function getInitialPage(selectedNodeID) {
	submitplannerform('text','get_initial_page',selectedNodeID,'','','','','')
}

function getinternalrecord(studentid) {
	menusubmit('student_printout','wustext','get_student_record','',studentid)
}

function popupHelp(url) {
	window.open("/help/" + url, "WebSTACHelp", "directories=no,location=no,menubar=yes,resizable=yes,scrollbars=yes,status=yes,toolbar=no,left=10,top=10,screenx=10,screeny=10,width=700,height=570");
	}
function popupWikiHelp(target) {
	// pass in empty string to point to the main help page
	// or pass in file path to point to a specific help page to the "target" parameter	
	var helpWindow;
	helpWindow = window.open("http://aiswiki.wustl.edu/webstachelp" + target, "WebSTACHelp", "directories=no,location=no,menubar=yes,resizable=yes,scrollbars=yes,status=yes,toolbar=no,left=10,top=10,screenx=10,screeny=10");
	helpWindow.focus();
	}
function CourseListing(url) {
	window.open(url, "CourseListing", "directories=no,location=yes,menubar=yes,resizable=yes,scrollbars=yes,status=yes,toolbar=yes,left=10,top=10,screenx=10,screeny=10,width=1024,height=768");
	}
function popupNew(url,name) {
	window.open(url, name, "directories=yes,location=yes,menubar=yes,resizable=yes,scrollbars=yes,status=yes,toolbar=yes,left=10,top=10,screenx=10,screeny=10");
	}	
function showLoading(showit) {
	top.frames[3].showLoading(showit)  
	}
function setHelpPage(targetPage) {
	top.frames[3].setHelpPage(targetPage)
	}
window.defaultStatus = "";
