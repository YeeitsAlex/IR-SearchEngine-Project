                <script type="text/javascript">
                 function enterKey(evt) {
        	var evt = (evt) ? evt : event
        	var charCode = (evt.which) ? evt.which : evt.keyCode
        	if (charCode == 13) {
        		searchPrimo()
        	}
        } 
        function searchPrimo() {
        	var query = document.getElementById("primoQueryTemp").value;
          var query_clean = query.replace(/,/g, " ");
                document.getElementById("primoQuery").value = "any,contains," + query_clean;
                document.forms["searchForm"].submit();
        }
        function searchPrimoEvent(event) {
        	if (event && event.which == 13) {
        		document.getElementById("primoQuery").value = "any,contains," + 
        			document.getElementById("primoQueryTemp").value;
              document.forms["searchForm"].submit();
        	}
        	else
        		return true;
        }
                </script>
