   <section id="main" role="main">
    <div class="row clearfix">
     <div id="page-title">
      <h2 class="title">
       Title IX
      </h2>
     </div>
     <div id="content-lead">
      <div class="region region-content-lead block-count1 clearfix">
       <div class="block block-nodeblock block-region-weight-1 odd" id="block-nodeblock-30">
        <div class="content block-body clearfix">
         <div about="/content/titleix-carousel" class="node node-gt-ct-carousel" id="node-30" typeof="sioc:Item foaf:Document">
          <div class="content node-body clearfix">
           <div class="gtct-carousel__slides-wrapper">
            <span class="rdf-meta element-hidden" content="TitleIX - Carousel" property="dc:title">
            </span>
            <div id="carousel-30">
             <ul class="slides">
              <li class="slide">
               <div about="/content/titleix-pic2-girl-group" class="node node-gt-ct-carousel-slide clearfix" id="node-29" typeof="sioc:Item foaf:Document">
                <div class="gtct-carousel-slide__bg-image bg-color-gt-search-gray" style="background-image: url('http://titleix.gatech.edu/sites/default/files/styles/slide_overlay_image/public/uploads/images/carousel_slides/bg/titleix-pic2_0.jpg?itok=DMTwC0ar');">
                 <div class="content node-body clearfix gtct-carousel-slide__overlay-image title-left">
                  <div class="gtct-carousel-slide__title-wrapper title-left">
                   <span class="rdf-meta element-hidden" content="TitleIX-Pic2-Girl Group" property="dc:title">
                   </span>
                   <div class="gtct-carousel-slide__title-graphic">
                    <a href="">
                     <img alt="TitleIX Picture of Female Students Talking" src="http://titleix.gatech.edu/sites/default/files/styles/slide_overlay_image/public/uploads/images/carousel_slides/title_graphics/transparent_1.png?itok=QjOkrAP3"/>
                    </a>
                    <div class="gtct-carousel-slide__title-static hidden">
                     <h4 class="gtct-carousel-slide__title-text">
                      <a href="">
                       TitleIX Picture of Female Students Talking
                      </a>
                     </h4>
                     <a class="gtct-carousel-slide__jumplink" href="">
