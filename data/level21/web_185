     <section class="block block-views clearfix" id="block-views-6c71730b7f059963e11a1ebc51a70bbb">
      <div class="view view-short-front-page-image-rotator view-id-short_front_page_image_rotator view-display-id-block front-rotator view-dom-id-d4ce8da811848e0f91cb3bcf03afefbe">
