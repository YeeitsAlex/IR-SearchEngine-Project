                   <div class="region-inner clearfix">
                    <section class="block block-boxes block-boxes-os_sv_list_box" delta="os_news_latest" id="block-boxes-os-news-latest" module="boxes">
                     <div class="block-inner clearfix">
                      <h2 class="block-title" ng-non-bindable="">
                       Upcoming Events
                      </h2>
                      <div class="block-content content" ng-non-bindable="">
                       <div class="boxes-box" id="boxes-box-os_news_latest">
                        <div class="boxes-box-content">
                         <div class="os-sv-list os_sv_list_box sv-list-event sv-list-view-sidebar_teaser sv-list-layout-list sv-grid-3" data-delta="os_news_latest" data-page="0" id="box-os_news_latest-page">
                          <article class="node node-event article event-start clearfix" id="node-1156205" role="article" target="_top">
                           <div class="event-start">
                            <span class="event-year">
                             2019
                            </span>
