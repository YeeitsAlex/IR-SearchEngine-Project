   var allNewsCategories = document.querySelectorAll('div');
	var flag = 0;


    for (var i = 0; i < allNewsCategories.length; i++) {
        if (allNewsCategories[i].classList.contains('et_pb_slide')) {
			flag = 1;
			break;
        }
    }
	if (flag == 0) {
			document.getElementById("main-header").style.backgroundColor = "#000";
		}
