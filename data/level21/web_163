  <script type="text/javascript">
   $(document).ready(function () {
            $(".imgResponsive").each(function () {
                var item = $(this);
                var parWidth = item.parent().width();
                var tmp = item.attr("src");
                if (parWidth > 600) {
                    item.attr("src", tmp.slice(0, -3) + "850");
                }
                else if (parWidth > 500) {
                    item.attr("src", tmp.slice(0, -3) + "600");
                }
                else if (parWidth > 400) {
                    item.attr("src", tmp.slice(0, -3) + "500");
                }
            });
        });
  </script>
