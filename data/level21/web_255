  <script>
   jQuery("#athleticsSelector").click(function () {
    jQuery("#tablepress-1-scroll-wrapper").show();
    jQuery("#tablepress-1-scroll-wrapper").siblings().hide();
});
jQuery("#admissionsSelector").click(function () {
    jQuery("#tablepress-2-scroll-wrapper").show();
    jQuery("#tablepress-2-scroll-wrapper").siblings().hide();
});
jQuery("#collegesSelector").click(function () {
    jQuery("#tablepress-3-scroll-wrapper").show();
    jQuery("#tablepress-3-scroll-wrapper").siblings().hide();
});
jQuery("#departmentsSelector").click(function () {
    jQuery("#tablepress-4-scroll-wrapper").show();
    jQuery("#tablepress-4-scroll-wrapper").siblings().hide();
});
jQuery("#locationsSelector").click(function () {
    jQuery("#tablepress-5-scroll-wrapper").show();
    jQuery("#tablepress-5-scroll-wrapper").siblings().hide();
});
jQuery("#newsSelector").click(function () {
    jQuery("#tablepress-6-scroll-wrapper").show();
    jQuery("#tablepress-6-scroll-wrapper").siblings().hide();
});
jQuery("#lifeSelector").click(function () {
    jQuery("#tablepress-7-scroll-wrapper").show();
    jQuery("#tablepress-7-scroll-wrapper").siblings().hide();
});
jQuery("#testSelector").click(function () {
    jQuery("#tablepress-8-scroll-wrapper").show();
    jQuery("#tablepress-8-scroll-wrapper").siblings().hide();
});



(function($){
    var fixed_side = jQuery('#fixedSide');
	if ( 0 === fixed_side.length ) {
		return;
	}
	var fixed_section_top = fixed_side.parents('section').position().top;
	var fixed_section_width = fixed_side.width();
    $(document).on('scroll',function(){
        var top;
        var fixme = $('.fixme');
        top = $(document).scrollTop();
        if( top > ( fixed_section_top - 50 ) ){
            fixme.addClass('fixed');
            fixme.css('width', fixed_section_width + 'px');
        } else {
            fixme.removeClass('fixed');
            fixme.css('width','');
        }
    });
}(jQuery));
  </script>
