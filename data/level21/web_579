  <script>
   $(document).ready(function() {
      var $carousel = $('.carousel');
      $carousel.carousel();
      $carousel.on('slid', function() {
        var to_slide = $('.carousel-inner .item.active').attr('id');
        $('.carousel-indicators').children().removeClass('active');
        $('.carousel-indicators [data-slide-to=' + to_slide + ']').addClass('active');
      });
      $(document).on('keydown', function(e) {
        if (e.which == 37) {
          $carousel.carousel('prev');
        } else if (e.which == 39) {
          $carousel.carousel('next');
        }
      });
    });

    var titleSearch = new Bloodhound({
      datumTokenizer: function(d) {return Bloodhound.tokenizers.obj.whitespace(d.title);},
      queryTokenizer: Bloodhound.tokenizers.whitespace,
      limit: 30,
      // prefetch: {
      //   url: '/data/scope/books',
      // },
      remote: {
        url: '?get=search&term=%QUERY',
        wildcard : '%QUERY',
        filter: function(response) {
          if( response.hits > 1){
          return response.result;
        } else {
          var result= {};
          return result;
        }
      }
    }
    });

    titleSearch.initialize();

    $('#inputsearch').typeahead({
      'highlight': true,
      'hint': false,
      'minLength': 3
    }, {
      name: 'books',
      displayKey: 'title',
      source: titleSearch.ttAdapter(),
      templates: {
        header: '<div class="suggestion-inner"><h4> Suggested Title Results... </h4></div>',
        suggestion: function(item) {
          var title = item.title;
          if (item.year > 0) {
            title += ' (' + item.year + ')';
          }

          var cover = '//images.nap.edu/images/';
          var coll_id = item.path.split('=').pop();
          if (coll_id > 0 ) {
            cover += 'collections/' + coll_id + '.png';
          } else {
            cover += 'cover.php?id=' + item.record_id + '&type=tinycov';
          }

          template = '<div class="suggestion-inner" id="' + title + '"><div class="img-container img-circle"><img src="' + cover + '"></div><p>' + title + '</p></div>'

          return template;
        }
      }
    }).on('typeahead:selected', function(event, item) {
      var url;
      if (item.record_id === '0000') {
        url = item.path;
      } else {
        url = 'https://www.nap.edu/catalog.php?record_id=' + item.record_id;
      }
      // trackEvent('homepage', 'autocomplete', item.title);
      // trackEvent('homepage', 'search', $('#inputsearch').typeahead('val'));
      window.location = url;
      ttvalue = $('#inputsearch').typeahead('val');
      tttitle = item.title;
      window.dataLayer = window.dataLayer || [];dataLayer.push({ 
        'searchTitle'   : tttitle,
        'event'         : 'searchTypeahead',
      });

    });
    $('.btn-searchhome').appendTo('.twitter-typeahead');
  </script>
  <script>
   $('.recommendations').on('click', 'a', function(e) {
    var rowTitle = 'Recommended for You';
    var position = ($(this).closest('.rfy-tile').index() + 1).toString();
    var element  = $(this).data('alias');
    var title    = $(this).data('title');
    dlPush(rowTitle, position, element, title);

  });

  $('.book-row .book-tile a').on('click', function(e) {
    var rowTitle = $(this).closest('.book-row').data('title');
    var position = ($(this).closest('.book-tile').index()).toString();
    var element  = $(this).data('alias');
    var title    = $(this).data('title');
    dlPush(rowTitle, position, element, title);
  });

  $('.sc-row .sc-tile').on('click', 'a, .sc-outer, .sc-inner h4, .sc-inner .sc-desc', function(e) {
    var rowTitle = $(this).closest('.sc-row').data('title');
    var position = ($(this).closest('.sc-tile').index()).toString();
    var element  = $(this).data('alias');
    var title    = $(this).closest('.sc-tile').find('a').data('title');
    dlPush(rowTitle, position, element, title);

  });

  $('.mm-row .mm-tile a').on('click', function(e) {
    var rowTitle = $(this).closest('.mm-row').data('title');
    var position = ($(this).closest('.mm-tile').index()).toString();
    var element  = $(this).data('alias');
    var title    = $(this).data('title');
    dlPush(rowTitle, position, element, title);
  });


  $('#dl-menu').dlmenu({
    animationClasses: {
      classin:  'dl-animate-in-5',
      classout: 'dl-animate-out-5'
    }
  });

  $('.truncate').dotdotdot({
    watch: 'window',
    lastCharacter: {
      remove: [' ', ',', ';', '.', '!', '?']
    }
  });

  $(document).on('click', '.sc-inner, .sc-outer, .rfy', function() {
    window.location = $(this).find('a').attr('href');
  });

  $('.col-sc').hover(function() {
    $(this).find('.sc-inner').filter(':not(:animated)').animate({ top: '0px' });
  }, function () {
    $(this).find('.sc-inner').animate({ top: '190px' });
  });

  $('.close, .overlay').click(function() {
    $('#bbt-sidebar').animate({ width: 'toggle' });
    $('.overlay').fadeOut();
  });

  $('#subscribe-form').on('submit', function(event) {
    if ($('#email').val().match(/@/)) {
      $('#subscriber-key').val(email);
    } else {
      event.preventDefault();
      $('#email')
        .val('')
        .attr('placeholder', 'The email address you entered is invalid.')
        .focus()
        .closest('.input-group')
        .addClass('has-error');
      setTimeout(function() {
        $('#email')
          .attr('placeholder', 'Sign up for e-mail updates')
          .closest('.input-group')
          .removeClass('has-error');
      }, 5000);
    }
  });
  function dlPush(rowTitle, position, element, title, event){
    window.dataLayer = window.dataLayer || [];dataLayer.push({ 
        'rowTitle'  : rowTitle,
        'position'  : position,
        'element'   : element,
        'title'     : title,
        'rowpage'   : 'homepage',
        'event'     : 'rowSelect',
      });
  }
  </script>
