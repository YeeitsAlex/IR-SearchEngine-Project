  <script>
   if (typeof $.placeholder === 'function') {
    $('input[placeholder]').placeholder();    
  }

  $('#bbt_Btn').on('click', function() { 
    $('.bbt-wrapper').slideToggle("slow");
  });

  $('#mobile_menuBtn').on('click', function() {
    $('.mobilemenu').slideToggle('slow');
    $('.mobilesearch, .mobileuser').slideUp();
  });
  
  

  // Simplification of Barebones Lightbox Template
  // by Kyle Schaeffer - kyleschaeffer.com
  function lightbox(content) {
    if ($('#lightbox').size() === 0) {
      $('<div id="lightbox"/><div id="lightbox-shadow"/>')
        .appendTo('body');
    }
    $('#lightbox').empty().append(content);
    $('#lightbox, #lightbox-shadow').fadeIn();
  }
  function closeLightbox() {
    $('#lightbox, #lightbox-shadow').fadeOut(function() {
      $('lightbox').empty();
    });
    return false;
  }

  // Wrapper for all the places that call GB_showImage
  // in their onclick attribute
  function GB_showImage(title, image) {
    lightbox('<img src="' + image + '"/>');
    $('#lightbox, #lightbox-shadow').on('click', closeLightbox);
    return false;
  }
  
  //drop down menu in top nav
  
 
	$(function () {
		$('nav#sitelinks li ul').hide().removeClass('fallback');
		$('nav#sitelinks li').hover(function () {
			$('ul', this).stop().slideToggle(200);
		});
	});
	

  // Save to MyNAP handler
  $('a.saveit').one('click', function(e) {
    e.preventDefault();
    $.getJSON('/napsave.php', {
        type: 'shelf', 
        r: $(this).data('recordid')
      }, function(response) {
        $(e.target)
          .toggleClass('saveit saveit_active')
          .attr('href', '/mynap.php')
          .text('Added to Bookshelf');
    });
  });

  // Back To Top control
  $(function() {
    var offset = 220;
    var duration = 500;
    $(window).scroll(function() {
      if ($(this).scrollTop() > offset) {
        $('.back-to-top').fadeIn(duration);
      } else {
        $('.back-to-top').fadeOut(duration);
      }
    });
    $('.back-to-top').on('click', function(event) {
      event.preventDefault();
      $('html, body').animate({scrollTop: 0}, duration);
      return false;
    });
  });
  </script>
  <script src="/includes/js/vendor/bootstrap/3.1.1/bootstrap.min.js">
  </script>
