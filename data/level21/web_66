         <!--front panel regions beg-->
         <div class="at-panel gpanel panel-display content clearfix" id="content-panels">
          <div class="region region-content-top">
           <div class="region-inner clearfix">
            <div class="block block-boxes block-boxes-os_slideshow_box no-title" delta="1389218539" id="block-boxes-1389218539" module="boxes">
             <div class="block-inner clearfix">
              <div class="block-content content" ng-non-bindable="">
               <div class="boxes-box" id="boxes-box-1389218539">
                <div class="boxes-box-content">
                 <div id="layout_16_9_side">
                  <div class="slide">
                   <ul class="rslides">
                    <li>
                     <a href="https://dmca.harvard.edu/pages/copyright-policy">
                      <img alt="" class="image-style-os-slideshow-169-820" height="461" src="//static.hwpi.harvard.edu/files/styles/os_slideshow_16%3A9_820/public/dmca/files/dmca.png?m=1515603698&amp;itok=ZVVj4H-J" width="820"/>
                     </a>
                     <div class="caption slide-copy">
                      <h2>
                       <a href="https://dmca.harvard.edu/pages/copyright-policy">
                        Harvard's copyright policy
                       </a>
                      </h2>
                      <p>
                       <a href="https://dmca.harvard.edu/pages/copyright-policy">
                        Learn how Harvard complies with the Digital Millenium Copyright Act
                       </a>
                      </p>
                     </div>
                    </li>
                   </ul>
                   <!--
<ul id="rslides-controls">
</ul>
-->
                  </div>
                 </div>
                </div>
