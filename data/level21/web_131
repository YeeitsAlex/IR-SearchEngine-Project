          <section id="main-content">
           <header class="clearfix" id="main-content-header">
            <h1 id="page-title">
             SBS Home
            </h1>
           </header>
           <div id="content">
            <div class="block block-quicktabs no-title odd first last block-count-10 block-region-content block-front-page" id="block-quicktabs-front-page">
             <div class="quicktabs-wrapper quicktabs-style-nostyle" id="quicktabs-front_page">
              <div class="item-list item-list-q">
               <ul class="quicktabs-tabs quicktabs-style-nostyle">
                <li class="active even first">
                 <a class="quicktabs-tab quicktabs-tab-block quicktabs-tab-block-panels-mini-delta-front-page-about active" href="/welcome-social-behavioral-sciences?qt-front_page=0#qt-front_page" id="quicktabs-tab-front_page-0">
