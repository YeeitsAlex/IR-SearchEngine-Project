      <div class="col-sm-12 page-header clearfix">
       <h1>
        Land Resources and Environmental Sciences
       </h1>
      </div>
     </div>
     <div class="row">
      <div class="col-sm-12 main-banner">
       <div class="image-carousel">
        <ul data-slick='{"slide": "li","arrows": true,"dots": false,"autoplay": false}'>
         <li id="slide_d421a439-4fb0-4aac-9191-13ab6cd4ddad.png">
          <!--false-->
