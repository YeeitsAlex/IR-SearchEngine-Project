<!-- sidebar right bottom navigation script-->
<script type="text/javascript">
 var $ = jQuery;
$('.itc-bottom-sidebar-container .box-content-nav ul ul').addClass('hidden');
$(document).ready(function () {
$('.parent-nav').click(function (e) {
e.stopPropagation();
$(this).find('ul').first().toggleClass('hidden');
});
var set = 'url(/wp-content/uploads/2017/09/down-arrow.png)', 
unset = 'url(/wp-content/uploads/2017/09/menu-arrow.png)';
$('.itc-bottom-sidebar-container .box-content-nav ul > li').on("click",function (e) {
e.preventDefault();
$image = $(this);
$image.toggleClass("selected");
$image.css('listStyleImage', $image.hasClass("selected")?set:unset);
});	
$('.other-amount').bind('focus blur', function() {
$(this).toggleClass('green');
$("#control_06").prop("checked", true);
});
$("#myITCFrameButton").click(function(){
var iframe = $("#myITCFrame");
iframe.attr("src", iframe.data("src")); 
});
$('.itc-no-follow a').attr('rel','nofollow');
$('.ai1ec-gmap-link').attr('rel','nofollow');
$('.ai1ec-tooltip-trigger').attr('rel','nofollow');
});
</script>
<script type="text/javascript">
 function submit_alumni_fee(){
ajax1();
}
function ajax1(){
$.post( '/e-commerce/alumni-association-fee/', { 'quantity' : 1 , 'add-to-cart' :  3319, 'product_id' : 3319 });
//alert('Donations are added to your cart');
$(document).ajaxStop(function(){
window.location.replace("/checkout");
});
}
</script>
