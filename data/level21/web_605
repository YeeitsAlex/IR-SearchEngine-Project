<script type="text/javascript">
 ( function($){
//STEP 1: Remove link of first child of a list with sub menu and attach ID
$("#menu-primary .menu-item-has-children > a").attr("href","javascript:void(0);");
//STEP 2: Add collapsible function
$("#menu-primary .menu-item-has-children > a").click(function(){
var display =  $( this ).parent().find(".sub-menu").first().css("display");
if( display === "none" ){
$( this ).parent().find(".sub-menu").first().css("display","block");
$( this ).addClass("itc-expanded");
}else{
$( this ).parent().find(".sub-menu").first().css("display","none");
$( this ).removeClass("itc-expanded");
}
});
//STEP 3: Expand the menu that has the class .current_page_item
$(".menu-primary-container .current_page_item").parents("ul").css("display","block");
$(".current_page_item").parents("ul").each( function(i,e){
$(this).prev("a").first().addClass("itc-expanded"); 
});
})(jQuery);
</script>
