			var $params = {};
			
		
		$params['type'] = 'news';
		$params['version'] = 2;
		$params['region'] = 'maincontent';$params['style'] = 'default';$params['group[]'] = '';$params['tag[]'] = Array(8,1);$params['custom_tag'] = 0;$params['count'] = 5;$params['pagination'] = 0;$params['search'] = 0;$params['filter_type'] = '';$params['filter_label'] = '';$params['sticky[]'] = '18114';$params['include[]'] = '';$params['exclude[]'] = Array(18232,18219,18265);$params['only[]'] = '';
