  <script>
   $(function(){
      Array.prototype.getItemByParam = function(paramPair) {
        var key = Object.keys(paramPair)[0];
        return this.find(function(item){return item[key]});
      }

      $.views.tags("wvutoday_photo_large_square", function(val) {
        if (val.photos.length > 0){
          if (val.photos.getItemByParam({tags: []}).tags.indexOf("wvutoday") > -1){
            return val.photos.getItemByParam({tags: []}).versions.large_square
          } else {
            return val.photos[0].versions.large_square
          }
        } else {
          return "http://brand.wvu.edu/files/a3a97086-0590-4558-b70c-9ffe56cb43ac/500x500?cb=1422226465"
        }
      });

      $.views.tags("wvutoday_photo_square", function(val) {
        if (val.photos.length > 0){
          if (val.photos.getItemByParam({tags: []}).tags.indexOf("wvutoday") > -1){
            return val.photos.getItemByParam({tags: []}).versions.square
          } else {
            return val.photos[0].versions.square
          }
        } else {
          return "http://brand.wvu.edu/files/a3a97086-0590-4558-b70c-9ffe56cb43ac/500x500?cb=1422226465"
        }
      });

      $.ajax({
        url: 'https://experts.wvu.edu/featured.json',
        dataType: 'json'
      }).done(function(data){
        var template;
        $.each(data.experts, function(i,expert){
          if (i < 3){
            template = $.templates('#wvutoday-experts-template');
            $('.wvu-featurettes__featurettes').append(template.render(expert));
          }
        });
        template = $.templates('#wvutoday-experts-topic-template');

        $('.wvutoday-find-an-expert__topic').append(template.render(data));

      });
    });
  </script>
