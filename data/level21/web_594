  <script>
   $(document).ready(function(){
   var menulag;
   var menuOpen = 0;
//   $( '#menu >li').hover(function () {
//   	$(this).find('ul.submenu').show();
//   },function () {
//   	$(this).find('ul.submenu').hide();
//   })
	$( '#menu > li' ).focusin(function() {
		if(menuOpen == 1  && $( document.activeElement ).parent().parent(".submenu").length) {
			clearTimeout(menulag);
		}
		$(this).find("ul.submenu").show();
	});
	$( '#menu > li' ).focusout(function() {
		target = $(this);
		menulag = setTimeout(function(){target.find("ul.submenu").hide();menuOpen=0;}, 250);
		menuOpen = 1;
  	});
});
  </script>
