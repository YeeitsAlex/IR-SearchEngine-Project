     <section class="cover" id="centers">
      <!--one Column HTML--------
::::::::::::::::::::::::::::::::-->
      <div class="container-fluid no-pad fill">
       <div class="row no-margin col-row">
        <div class="cols col-1 col-xs-12">
         <a class="col-img" href="#" style="background:url(https://haslam.utk.edu/sites/default/files/ncgc-3_0.jpg) #58595b no-repeat center center ; -webkit-background-size: cover; -moz-background-size: cover; background-size: cover; -o-background-size: cover; ">
         </a>
         <div class="column-content">
          <div class="content-bg">
           <h1>
            <a href="/neelcgc/speaker-series">
             Speaker
             <span>
              Series
             </span>
            </a>
           </h1>
           <div class="col-desc">
            <p>
             The Neel Corporate Governance Speaker Series was founded in 2003 with three speakers from regional law firms. Since then it has brought academics, politicians, regulators, industry executives, board members and other experts in corporate governance from around the country.
            </p>
            <a class="btn btn-default" href="/neelcgc/speaker-series">
             Learn More
            </a>
           </div>
          </div>
         </div>
        </div>
       </div>
       <!--end row-->
      </div>
      <!--end container-->
      <!--::::::::::::::::::::::::::::::::
----------- End One Column----->
     </section>
     <!--end colums section-->
