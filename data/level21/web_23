            <section class="block block-boxes block-boxes-os_boxes_columns cards icons" delta="1524759265" id="block-boxes-1524759265" module="boxes">
             <div class="block-inner clearfix">
              <h2 class="block-title" ng-non-bindable="">
               Browse Resources By
              </h2>
              <div class="block-content content">
               <div class="boxes-box" id="boxes-box-1524759265">
                <div class="boxes-box-content">
                 <!-- Three column 3x33 Gpanel -->
                 <div class="at-panel gpanel panel-display three-3x33 clearfix">
                  <div class="region region-three-33-first">
                   <div class="region-inner clearfix">
                    <div class="block block-boxes block-boxes-os_boxes_html no-title" delta="1524759141" id="block-boxes-1524759141" module="boxes">
                     <div class="block-inner clearfix">
                      <div class="block-content content" ng-non-bindable="">
                       <div class="boxes-box" id="boxes-box-1524759141">
                        <div class="boxes-box-content">
