  <script>
   $(document).ready(function(){
		
	loadevents(0);
	
	$(window).bind('load',function(){
	$("#loadingpage").hide();
	});
	
	
	$('#datepicker').on('change',function(event){
	updateLoad();
	});
        
	$('#closeblock').click(function(){
            $("#calfilter").hide();
            $("#addeventsdiv").hide();
            $("#loadingpage").show();
	});
        
	$('#today').click(function(){
	nonformpost();
	$("#datepicker").datepicker("destroy")
	$("#datepicker").datepicker({gotoCurrent:true,nextText:'',prevText:''});
	updateLoad();
	   $("#filterinlay").change(function() {
                
			var src = $(this).val();
                        alert(src);
			//window.location = "http://calendar.msu.montana.edu/dev" + src;
		});
	});
});
 

function loadevents(offset) {
			
		year = '2019';
		month = '03';
		day = '10';
			$('#addeventsdiv').load('includes/inlay.php',{'newday': day, 'newmonth': month, 'newyear': year,'offset': offset});

	}


// On click or default (today) load, update load jquery to new date.
function updateLoad() { 
	day = $('#datepicker').datepicker('getDate').getDate();
	month = $('#datepicker').datepicker('getDate').getMonth() + 1;
	if (month < 10) month = "0"+month;
	year = $('#datepicker').datepicker('getDate').getFullYear();
	var fullDate = year + "-" + month + "-" + day;
	fullDate = fullDate + " 00:00:00";

			$('#addeventsdiv').load('includes/inlay.php',{'newday': day, 'newmonth': month, 'newyear': year});	
			//alert('updateLoadcomplete');
}
  </script>
 </body>
</html>
