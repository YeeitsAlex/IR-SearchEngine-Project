  // <![CDATA[
$(function() {
  $( "div.accordion" ).accordion({
        active: true,             // false = no accordians start open
        collapsible: true,        // false = one accordian must be open at all times
        heightStyle: "content",   // content = so all panels are not the same size
        navigation: true,         // needed for opening accoridans via url
        header: "h3",              // element holding the accoridan (for opening via url)
		changestart: function (event, ui) {
        	ui.oldContent.accordion("activate", false);
	    }  });
