               <div class="boxes-box" id="boxes-box-1414681197">
                <div class="boxes-box-content">
                 <div class="accordion">
                  <h3>
                   Level 1 Data Types
                  </h3>
                  <div class="accordion-panel">
                   <div class="block block-boxes block-boxes-os_boxes_html no-title" delta="1414681066" id="block-boxes-1414681066" module="boxes">
                    <div class="block-inner clearfix">
                     <div class="block-content content" ng-non-bindable="">
                      <div class="boxes-box" id="boxes-box-1414681066">
                       <div class="boxes-box-content">
                        <p>
                         <a href="https://policy.security.harvard.edu/it-security-2/level-1">
                          <img alt="Directory Information Icon" class="media-element file-default file-os-files-small" height="31" src="//static.hwpi.harvard.edu/files/styles/os_files_small/public/it-security-2/files/directoryinfo_0.png?m=1515603855&amp;itok=CGZg6fSW" style="float: left;" title="Directory Information" width="34"/>
                         </a>
                         <a href="https://policy.security.harvard.edu/it-security-2/level-1">
                         </a>
                        </p>
                        <p>
                         <a href="https://policy.security.harvard.edu/it-security-2/level-1">
                          Directory Information
                         </a>
                        </p>
                       </div>
                      </div>
                     </div>
                    </div>
                   </div>
                  </div>
                 </div>
                </div>
               </div>
              </div>
             </div>
