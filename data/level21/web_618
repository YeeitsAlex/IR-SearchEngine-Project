       <div class="content block-body clearfix">
        <p>
         <!-- Start of LiveChat (www.livechatinc.com) code -->
         <script type="text/javascript">
          var __lc = {};
__lc.license = 6655631;
(function() {
  var lc = document.createElement('script'); lc.type = 'text/javascript'; lc.async = true;
  lc.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'cdn.livechatinc.com/tracking.js';
  var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(lc, s);
})();
         </script>
         <!-- End of LiveChat code -->
         <!-- Start LiveChat Status Modifications -->
         <script>
          var LC_API = LC_API || {};
var agentsAvailable = false;
LC_API.on_after_load = function()
{
	if (LC_API.agents_are_available())
	{
        agentsAvailable = true;
        console.log("Agents Available to Chat");
	} else {
        agentsAvailable = false;
		console.log("No Agents Available to Chat");
        document.getElementById("chatButton").innerHTML = "Live Chat Offline";
    }
};

function openChat() {
    if(agentsAvailable) {
        LC_API.open_chat_window();
        ga('set', 'metric1', '1');
        return false;
    } else {
        ga('set', 'metric2', '1');
        if(confirm("No agents are available to chat at this time. Press 'OK' to open a help request, or 'No' to cancel.")) {
            window.location.href = "/live-chat-offline";
        }
        return false;
    }
}
         </script>
         <!-- End LiveChat Status Modifications -->
        </p>
