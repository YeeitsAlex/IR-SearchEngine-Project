  <script>
   (function($){
    jQuery.fn.outerHTML = function() {
      return jQuery('<div />').append(this.eq(0).clone()).html();
    };
    // News header
    jQuery("body.blog header.main-header, body.post-template-default header.main-header, body.category header.main-header").after('<section class="row single gutter header"><div style="" class="column one "><div class="flexwrap"><h1 class="rule"><span class="h-sm">News and Events</span> News</h1></div></div></section>');
    // News next/prev button
    jQuery(".next.page-numbers").html("NEXT ");
    jQuery(".prev.page-numbers").html("  PREV");
    jQuery("a[rel=prev],a[rel=next]").each(function(){
      var html = jQuery(this).outerHTML();  
      jQuery(this).parent().html(html);
    });
    // Cougar head above footer
    jQuery(".footersection").before('<section class="row single pad-top pad-bottom cougarhead"><div style="" class="column one "><pre><img class="size-full wp-image-7884 aligncenter" src="https://s3.wp.wsu.edu/uploads/sites/1368/2017/03/ico-cougarhead-trans-blk.png" alt="Cougar Head" width="50" height="50" data-pin-nopin="true"></pre></div></section>');
    // Adjust bg  for cougarhead color
    if(jQuery("section.cougarhead").prev().hasClass("bglhtgry"))
        jQuery("section.cougarhead").addClass("bglhtgry");
    if(jQuery("section.cougarhead").prev().hasClass("section-wrapper-has-background"))
        jQuery("section.cougarhead").remove();
    
    jQuery.loadScript = function (url, callback) {
        jQuery.ajax({
            url: url,
            dataType: 'script',
            success: callback,
            async: true
        });
    }
    function getUrlVars()
    {
        var vars = [], hash;
        var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
        for(var i = 0; i < hashes.length; i++)
        {
            hash = hashes[i].split('=');
            vars.push(hash[0]);
            vars[hash[0]] = hash[1];
        }
        return vars;
    }
    
	function display_posts() {
		var results = window.provost_feed_data;

		$('.page-id-2 .column.two').append('<div id="feed" class="guttered json feed"></div>');

		var i = 0;
		$.each( results, function( index, value ) {
			if(index < 4 ) {
	            	var html = '';

	                html += '<article><header>'
	                + '<h2 class="article-title">'
	                + '<a href="'
	                + value.link
	                + '">'
	                + value.title
	                + '</a>'
	                + '</h2>';

					var date = $.datepicker.formatDate( 'MM d, yy', new Date(value.date) );

	                html += '<div class="article-meta">'
                    + '<time>'+date+'</time>'
                    + '<cite>'+value.author_name+'</time>'
	                + '</div>'
	                + '</header>';

	                html += '<div class="article-tease">'
	                + value.excerpt
                    + '<a href="'
	                + value.link
	                + '">» More</a>'
	                + '</div>';
	                + '</article>';

	            $('#feed').append(html);
            }
		});

		$('#feed').append('<a href="/posts" class="more-posts">More news...</a>');

		$('.strategy').css('min-height', $('main').height());
		$(window).on('resize', function() {
			$('.strategy').css('min-height', $('main').height());
		});
	}

    if ( $('.page-id-2').length > 0 ) { display_posts(); }

	$(document).ready(function(){
        $(".foldout .unfold" ).on( 'click', function() {
	        $(this).parent(".foldout").siblings(".foldout").removeClass("unfolded");
	        $(this).parent(".foldout").toggleClass("unfolded");
		});

		$('body[class*="faq"] dt,body[class*="faq"] dd').on( 'click', function() {
	        $(this).parent("dl").toggleClass("unfolded");
		});

		$(".article-thumbnail").each(function (){
			$(this).prependTo($(this).parents("article"));
		});
    });
}(jQuery));
