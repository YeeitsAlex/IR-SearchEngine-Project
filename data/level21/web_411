            <section class="block block-boxes block-boxes-os_boxes_html" delta="1389715057" id="block-boxes-1389715057" module="boxes">
             <div class="block-inner clearfix">
              <h2 class="block-title" ng-non-bindable="">
               Hours of Operation
              </h2>
              <div class="block-content content" ng-non-bindable="">
               <div class="boxes-box" id="boxes-box-1389715057">
                <div class="boxes-box-content">
                 <p>
                  <strong>
                   Coffee Bar Hours:
                  </strong>
                 </p>
                 <p>
                 </p>
                 <p>
                  Monday: 9am-2pm
                 </p>
                 <p>
                  Tuesday: 9am-2pm
                 </p>
                 <p>
                  Wednesday 9am-2pm
                 </p>
                 <p>
                  Thursday: 9am-2pm
                 </p>
                 <p>
                 </p>
                 <p>
                 </p>
                 <p>
                  <strong>
                   Pub Hours:
                  </strong>
                 </p>
                 <p>
                  Wednesday 6pm-12am
                 </p>
                 <p>
                  Thursday 6pm-12am
                 </p>
                 <p>
                  Friday 6pm-1am
                 </p>
                 <p>
                  Saturday 8pm-1am
                 </p>
                 <p>
                  *contact
                  <a href="mailto:events.cqh@gmail.com">
                   events.cqh@gmail.com
                  </a>
                  for private function requests that fall outside of the above listed operating hours*
                 </p>
                 <p>
                  <strong>
                  </strong>
                 </p>
