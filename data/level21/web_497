                    <section class="block block-boxes block-boxes-os_sv_list_box" delta="1493748396" id="block-boxes-1493748396" module="boxes">
                     <div class="block-inner clearfix">
                      <h2 class="block-title" ng-non-bindable="">
                       Featured Items
                      </h2>
                      <div class="block-content content" ng-non-bindable="">
                       <div class="boxes-box" id="boxes-box-1493748396">
                        <div class="boxes-box-content">
                         <div class="os-sv-list os_sv_list_box sv-list-news sv-list-view-teaser sv-list-layout-list sv-grid-3" data-delta="1493748396" data-page="0" id="box-1493748396-page">
                          <article class="node node-news node-teaser article modified-in-os_news_process_node clearfix" id="node-1147020" role="article" target="_top">
                           <div class="field field-name-field-photo field-type-image field-label-hidden view-mode-teaser">
