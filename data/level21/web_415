             jQuery.ajax({
    type: "GET",
    url: "/cns_custom_code/utmsi_current_cond/index.php",
    dataType:'HTML', 
    success: function(response){
        jQuery(".cond_hide_me").hide();
        jQuery("#conditions").append(response);
        jQuery(".cond_count_time").hide();
    }
});
            </script>
            <script type="text/javascript">
             var count=10;
var counter=setInterval(timer, 1000); //1000 will  run it every 1 second
function timer()
{
  count=count-1;
  if (count <= 0)
  {
     clearInterval(counter);
     return;
  }
  
   jQuery(".cond_count_time").html(count);
}
            </script>
