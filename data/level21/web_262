  <script>
   (function ($) {
	'use strict';
	function display_posts(feed, slug, element, title, count) {

		if (element == 'main') {
			$('main').append(
				'<section class="row margin-right gutter feed json shortcode">'
				+ '<div id="' + slug + '" class="column one ' + slug + '">'
				+ '<h2>' + title + '</h2>'
				+ '</div>'
				+ '</section>'
			);
		} else {
			$('main ' + element ).append(
				'<div id="' + slug + '" class="' + slug + '">'
				+ '<h2>' + title + '</h2>'
				+ '</div>'
			);
		}

		var i = 0;

		$.each( feed, function( index, value ) {
			if(index<count){
				var html = '';

				html += '<header>'
				+ '<h3 class="article-title">'
				+ '<a href="'
				+ value.link
				+ '">'
				+ value.title
				+ '</a>'
				+ '</h3>';

				//var date = $.datepicker.formatDate( 'MM d, yy', value.date );

				html += '<div class="article-meta">'
				//+ '<time>'+value.date+'</time>'
				+ '</div>'
				+ '</header>';

				$('#'+slug).append(html);
			}
		});
	}
	
	if ( $('body.home').length !== 0 ) {
		display_posts(in_the_news,'in-the-news','.in-the-news-section .column.two','<a href="everett.wsu.edu/news/">Latest News</a>','3');
	}

}(jQuery));
  </script>
