<script>
 var tag = document.createElement('script');
tag.src = "https://www.youtube.com/iframe_api";
var firstScriptTag = document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

    (function($) {
        $(document).ready(function() {
            $( "li iframe" ).each( function( index ) {
                $(this).attr('id','iframe' + index);
            });
        });
    })(jQuery);



      // 3. This function creates an <iframe> (and YouTube player)
      //    after the API code downloads.
      var player;
      function onYouTubeIframeAPIReady() {
        player = new YT.Player('iframe0', {
          events: {
            'onReady': onPlayerReady,
            'onStateChange': onPlayerStateChange
          }
        });
      }

      // 4. The API will call this function when the video player is ready.
      function onPlayerReady(event) {
       return false;
      }

      // 5. The API calls this function when the player's state changes.
      //    The function indicates that when playing a video (state=1),
      //    the player should play for six seconds and then stop.
      var done = false;
      function onPlayerStateChange(event) {
        (function($) {
            if (event.data == 1 || event.data == 3) {
                Drupal.flexsliderViewsSlideshow.active['#'+$('.flexslider_views_slideshow_main').attr('id')].pause();
                Drupal.flexsliderViewsSlideshow.active['#'+$('.flexslider_views_slideshow_main').attr('id')].manualPause = true;
                done = false;
                console.log('Event:' + event.data + ' PAUSE SLIDESHOW');
            }
            else if (event.data == 2 || event.data == 5) {
                if(!done)
                {
                   //Drupal.flexsliderViewsSlideshow.active['#'+$('.flexslider_views_slideshow_main').attr('id')].pause();
                   //Drupal.flexsliderViewsSlideshow.active['#'+$('.flexslider_views_slideshow_main').attr('id')].manualPause = true;
                   Drupal.flexsliderViewsSlideshow.active['#'+$('.flexslider_views_slideshow_main').attr('id')].play();
                   //Drupal.flexsliderViewsSlideshow.active['#'+$('.flexslider_views_slideshow_main').attr('id')].manualPause = false;
                   Drupal.flexsliderViewsSlideshow.active['#'+$('.flexslider_views_slideshow_main').attr('id')].getTarget("next");     
                   console.log('Event:' + event.data + ' PLAY SLIDESHOW');
                }
            }else{
                console.log('Unknown Event: ' + event.data);
            }
        })(jQuery);
        return false;
      }
</script>
