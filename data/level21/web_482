        <div class="content-inner">
         <a name="main-content">
         </a>
         <!--front panel regions beg-->
         <div class="at-panel gpanel panel-display content clearfix" id="content-panels">
          <div class="region region-content-top">
           <div class="region-inner clearfix">
            <div class="block block-boxes block-boxes-os_boxes_columns no-title" delta="1415907267" id="block-boxes-1415907267" module="boxes">
             <div class="block-inner clearfix">
              <div class="block-content content">
               <div class="boxes-box" id="boxes-box-1415907267">
                <div class="boxes-box-content">
                 <!-- Two column 33-66 -->
                 <div class="at-panel gpanel panel-display two-33-66 clearfix">
                  <div class="region region-two-33-66-first">
                   <div class="region-inner clearfix">
                    <div class="block block-boxes block-boxes-os_boxes_html no-title" delta="1445270395" id="block-boxes-1445270395" module="boxes">
                     <div class="block-inner clearfix">
                      <div class="block-content content" ng-non-bindable="">
                       <div class="boxes-box" id="boxes-box-1445270395">
                        <div class="boxes-box-content">
                         <p>
                          <span style="font-size:large">
                           <strong>
                            Welcome.
                           </strong>
                          </span>
                          <br/>
                          The Office of Career Services is dedicated to educating, advising, and connecting students to opportunities for summer and post-graduation in order to foster their intellectual, social, and personal transformations. OCS serves students of
                          <strong>
                           Harvard College, Graduate School of Arts &amp; Sciences, School of Engineering and Applied Sciences, and Harvard Extension School degree programs.
                           <a href="about">
                            Learn more
                           </a>
                           .
                          </strong>
                         </p>
                        </div>
                       </div>
                      </div>
                     </div>
                    </div>
                   </div>
                  </div>
