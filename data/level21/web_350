{        
        jQuery.ajax({
                    type : 'POST',
                    dataType : 'json',
                    url : "https://stat.utexas.edu/index.php?option=com_jevents&ttoption=com_jevents&typeaheadtask=gwejson&file=fetchlatestevents&path=module&folder=mod_jevents_latest&token=b21ebdef5cf693a281256e282e113652",
