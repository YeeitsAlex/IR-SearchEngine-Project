                    <section class="block block-boxes block-boxes-os_sv_list_box" delta="1406661420" id="block-boxes-1406661420" module="boxes">
                     <div class="block-inner clearfix">
                      <h2 class="block-title" ng-non-bindable="">
                       Upcoming Events
                      </h2>
                      <div class="block-content content" ng-non-bindable="">
                       <div class="boxes-box" id="boxes-box-1406661420">
                        <div class="boxes-box-content">
                         <div class="os-sv-list os_sv_list_box sv-list-event sv-list-view-sidebar_teaser sv-list-layout-list sv-grid-3" data-delta="1406661420" data-page="0" id="box-1406661420-page">
                          <article class="node node-event article event-start clearfix" id="node-1315930" role="article" target="_top">
                           <div class="event-start">
                            <span class="event-year">
                             2019
                            </span>
                            <span class="event-start-month">
                             Mar
                            </span>
                            <span class="event-start-day">
                             21
                            </span>
                           </div>
                           <div class="event-content">
                            <header class="node-header">
                             <h1 class="node-title" ng-non-bindable="" rel="nofollow">
                              <a href="https://uraf.harvard.edu/event/houghteling-memorial-scholarship-application-deadline-12noon" rel="bookmark" target="_top" title="On Thu Mar 21">
                               Houghteling Memorial Scholarship Application Deadline (12noon)
                              </a>
                             </h1>
                            </header>
                            <div class="node-content" ng-non-bindable="">
                             <div class="field field-name-field-date field-type-datetime field-label-hidden view-mode-sidebar_teaser">
                              <div class="field-items">
                               <div class="field-item even">
                                <span class="date-display-single">
                                 (All day)
                                </span>
                                <p>
                                </p>
                               </div>
                              </div>
