            <section class="block block-boxes block-boxes-os_boxes_columns innercolumn grid" delta="1496865568" id="block-boxes-1496865568" module="boxes">
             <div class="block-inner clearfix">
              <h2 class="block-title" ng-non-bindable="">
               Move Spotlight
              </h2>
              <div class="block-content content">
               <div class="boxes-box" id="boxes-box-1496865568">
                <div class="boxes-box-content">
                 <!-- Four column Gpanel -->
