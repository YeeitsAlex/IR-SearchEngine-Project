         <header id="main-content-header">
          <a name="main-content">
          </a>
          <h1 id="page-title" ng-non-bindable="">
           Center for Public Interest Careers
          </h1>
         </header>
         <!--front panel regions beg-->
         <div class="at-panel gpanel panel-display content clearfix" id="content-panels">
          <div class="region region-content-top">
           <div class="region-inner clearfix">
            <div class="block block-boxes block-boxes-os_slideshow_box no-title" delta="1477505420" id="block-boxes-1477505420" module="boxes">
             <div class="block-inner clearfix">
              <div class="block-content content" ng-non-bindable="">
               <div class="boxes-box" id="boxes-box-1477505420">
                <div class="boxes-box-content">
                 <div id="layout_16_9_overlay">
                  <div class="slide">
                   <ul class="rslides">
                    <li>
                     <a href="https://news.harvard.edu/gazette/story/newsplus/phillips-brooks-house-to-host-public-service-recruiting-day/">
                      <img alt="PSR 2017" class="image-style-os-slideshow-169-820" height="461" src="//static.hwpi.harvard.edu/files/styles/os_slideshow_16%3A9_820/public/servicetosociety/files/psr_2017_sarah_angell_mo_kim_gina_schwartz.jpg?m=1535052526&amp;itok=KMzejSDV" title="PSR 2017" width="820"/>
                     </a>
