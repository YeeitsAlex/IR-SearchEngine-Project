  <script>
   // Creates the tracker and sends a pageview as normal
	// since the `userId` value is not yet known.
	(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
	(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
	m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
	})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
	  
	    var thisUserID = document.getElementById('gaUserID').value;
	    var thisQueryString = document.getElementById('gaQS').value;
	    var thisPage = document.location.pathname + '?' + thisQueryString;
	    ga('create', 'UA-103819828-1', 'auto', {userId: thisUserID});
	    ga('set', 'userId', thisUserID);
	    ga('set', 'page', thisPage);
	    ga('send', 'pageview', thisPage);
  </script>
