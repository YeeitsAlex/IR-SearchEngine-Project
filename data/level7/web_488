      <script type="text/javascript">
       var $j = jQuery.noConflict();	
$j(document).ready(function() {
	$j( "#username" ).focus();
	
	// validate calendar form on keyup and submit
	$j("#loginForm").validate({
		rules: {
			username: {
				required: true
			},
			password: {
				required: true
			}
		},
		messages: {
			username: {
				required: "Please enter Username"
			},
			password: {
				required: "Please enter Password"
			}
		},
		submitHandler: function(form) {	
			$j("#formMessageB").html("").removeClass("alert").removeClass("alert-info").removeClass("alert-danger");
			
			var regex = new RegExp('[,/\\##+<>;"=()*|&]');
			var key = $j("#username").val();
			if (regex.test(key)) {
				$j("#formMessageB").html("Login Error. Try Again.").addClass("alert alert-danger");
				return false;
			}
					
			var sessiontxt = "";
			sessiontxt += "<p>Browser CodeName: " + navigator.appCodeName + "</p>";
			sessiontxt += "<p>Browser Name: " + navigator.appName + "</p>";
			sessiontxt += "<p>Browser Version: " + navigator.appVersion + "</p>";
			sessiontxt += "<p>Cookies Enabled: " + navigator.cookieEnabled + "</p>";
			sessiontxt += "<p>Browser Language: " + navigator.language + "</p>";
			sessiontxt += "<p>Browser Online: " + navigator.onLine + "</p>";
			sessiontxt += "<p>Platform: " + navigator.platform + "</p>";
			sessiontxt += "<p>User-agent header: " + navigator.userAgent + "</p>";
			sessiontxt += "<p>Java Enabled: " + navigator.javaEnabled() + "</p>";
			sessiontxt += "<p>Total width/height: " + screen.width + "*" + screen.height + "</p>";
			sessiontxt += "<p>Available width/height: " + screen.availWidth + "*" + screen.availHeight + "</p>";
			sessiontxt += "<p>Color depth: " + screen.colorDepth + "</p>";
			sessiontxt += "<p>Color resolution: " + screen.pixelDepth + "</p>";
			sessiontxt += "<p>Font smoothing enabled: " + screen.fontSmoothingEnabled + "</p>";
			
			submitdata = new FormData(form);
			submitdata.append('userSession', sessiontxt);
			
//			$j("#formMessageB").html('<i class="fa fa-spinner fa-pulse fa-fw fa-lg"></i>&nbsp;Logging in..').addClass("alert alert-info");
			$j("#loginBtn").val('Logging in..').addClass("disabled");
			$j.ajax({
				url: '/index.cfm?p=user.login',
				type:'POST',
				data: submitdata, 
				processData: false,
				contentType: false,	
				async: true, 
				dataType: 'json'
			}).then(function(result){
				console.log(result);
				if (result.SUCCESS == 1) {
					// redirect user to homepage or current page
					window.location.href = result.REDIRECTURL;
				}
				else {
					if (result.ERRORMESSAGE != "") {
						$j("#formMessageB").html(result.ERRORMESSAGE).addClass("alert alert-danger");	
					}
					$j("#loginBtn").val('login').removeClass("disabled");
				}
			}, function(reason){
				console.log("Error in processing your request.", reason);
				$j("#formMessageB").html("Error in processing your request. Please try again later.").addClass("alert alert-danger");
				$j("#loginBtn").val('login').removeClass("disabled");
			});
			return false; 
		}	
	});

	$j(".openFB").fancybox({
		maxWidth	: 500,
		maxHeight	: 650,
		fitToView	: true,
		width		: '70%',
		height		: '70%',
		autoSize	: false,
		closeClick	: false,
		openEffect	: 'none',
		closeEffect	: 'none',		
		helpers		: {
			title	: { type : 'inside' }
		}
	});
	
	$j(".forgotPassword").click(function(){
		$j.fancybox({
			'width'				: "100%",
			'maxWidth'			: 600,
			'height'			: "90%",
			'fitToView' 		: false,
			'autoSize'			: false,
			'autoScale'			: false,
			'transitionIn'		: 'elastic',
			'transitionOut'		: 'fade',
			'type'				: 'iframe',
			'href'				: "/index.cfm?p=main.requestPassword&username="+$j("#username").val()+"&nolayout",
			'overlayOpacity'	: 0.6
		});
		return false;
	})
	
});
      </script>
