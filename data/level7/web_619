   <script>
    $(window).ready(function () {
        //  		$('.nav-pills, .nav-tabs').tabdrop();

        $('#selectSearch').on('change', function () {
            switch ($(this).val()) {
                case 'all':
                    $('#termCode').hide();
                    $('#units').hide();
                    $('#geCategory').hide();
                    $('#subjectArea').show();
                    $('#btnAdvancedSearch').show();
                    break;
                case 'ge':
                    $('#subjectArea').hide();
                    $('#termCode').hide();
                    $('#units').hide();
                    $('#geCategory').show();
                    break;
                case 'writing-2':
                    $('#subjectArea').hide();
                    $('#geCategory').hide();
                    $('#units').hide();
                    //('#termCode').show();
                    break;
                case 'units':
                    $('#subjectArea').hide();
                    $('#termCode').hide();
                    $('#geCategory').hide();
                    $('#units').show();
                    break;
            }
        });
        $('#btnSearchClasses').click(function (event) {
            $('.classes-list').show();
            $('html, body').animate({
                scrollTop: $('.classes-list').offset().top
            }, 1000);
            event.preventDefault();
        });
        $(function () {
			try {
				$('[data-toggle="popover"]').popover();
			} catch(err) {}
        });

		try {
			$('#toolbarAffix').affix({
				offset: {
					top: function () {
						return (this.top = $("#toolbarAffix").offset().top - $('#toolbarAffix').outerHeight(true))
					},
					bottom: function () {
						return (this.bottom = $('#global-footer').outerHeight(true))
					}
				}
			})
		} catch(err) {}

        $(document).ready(function () {
            $("span[class='breadcrumb']").click(function () {

                var text = $("span[class='breadcrumb']").text();
                window.location = "/" + text.replace(/\W+/g, "-") + "/overview/";
            });
        });

(function() {
if(window.location.pathname == '/Calendars/Annual-Academic-Calendar') {
    try {
    var li = document.querySelector('.nav-tabs li');
    li.classList.add('active');
    var a = li.getElementsByTagName('a')[0];
    a.setAttribute('aria-expanded', 'true');
    var tab = document.querySelector(a.getAttribute('href'));
    tab.classList.add('in');
    tab.classList.add('active');
    } catch(err) {};
}
})();

(function() {
if(window.location.pathname == '/Archives/Academic-Calendar-Archive') {
    try {
    var ul = document.querySelector('.tab-pane.fade.in.active ul');
    var li = document.createElement('li');
    li.classList.add('media');
    li.classList.add('category-list-item');
    li.innerHTML = '<div class="media-body"><h3 class="media-heading"><a href="/Archives/Academic-Calendar-Archive/Academic-Calendar-2017-18">2017-18 Academic Calendar</a></h3></div>';
    ul.insertBefore(li, ul.childNodes[0]);  
    } catch(err) {};
}
})();



    });
