(function($) {
  Drupal.behaviors.opportunities = {
    attach: function(context, settings) {
      
      $('.form-item-field-term-job-location-und .form-checkbox').each(function(){
        var locationValue = $(this).val()
        if($(this).is(':checked') && locationValue < 40) {
          $('.form-item-field-term-job-location-und .form-checkbox').each(function() {
            if($(this).prop('value') > locationValue && $(this).prop('value') < 40) {
              $(this).prop('checked', true)
              $(this).prop('disabled', 'disabled');
            }
          })
        }
      })
      
      $('.form-item-field-term-job-location-und .form-checkbox').change(function(){
        var locationValue = $(this).val()
        if(locationValue < 40) {
          if($(this).is(':checked')) {
            // Deal with california use-cases
            $('.form-item-field-term-job-location-und .form-checkbox').each(function() {
              if($(this).prop('value') > locationValue && $(this).prop('value') < 40) {
                $(this).prop('disabled', false);
                $(this).prop('checked', true)
                $(this).prop('disabled', 'disabled');
              }
            })
          }
          else {
            $('.form-item-field-term-job-location-und .form-checkbox').each(function() {
              if($(this).prop('value') <= parseInt(locationValue)+6) {
                $(this).prop('disabled', false);
              }
            })
          }
        }
      })
      $('#job-node-form').submit(function(e) {
        $('.form-item-field-term-job-location-und .form-checkbox').each(function() {
          $(this).prop('disabled', false);
        })
      })
      // Page is loaded, add the javascript you want
    }
  }
})(jQuery)
