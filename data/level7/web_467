          <div class="wpb_wrapper">
           <div class="wpb_text_column wpb_content_element ">
            <div class="wpb_wrapper">
             <h2 style="text-align: center;">
              Western Center for Food Safety
             </h2>
            </div>
           </div>
           <div class="vc_separator wpb_content_element vc_separator_align_center vc_sep_width_30 vc_sep_border_width_2 vc_sep_pos_align_center vc_separator_no_text vc_custom_1501259608517 vc_custom_1501259608517">
