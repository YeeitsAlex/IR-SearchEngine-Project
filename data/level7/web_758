 <script>
  $(window).ready(adjustForPinnedNavHeight);
$(window).resize(adjustForPinnedNavHeight);

function adjustForPinnedNavHeight() {
  var headerHeight = $(".navbar-fixed-top").outerHeight();
  var appMenuHeight = 0;
  var appMenu = $(".app-header-navigation");
  if (appMenu) {
    appMenuHeight = appMenu.outerHeight();
  }
  $(".app-wrapper").css("padding-top", headerHeight + appMenuHeight);
}
 </script>
