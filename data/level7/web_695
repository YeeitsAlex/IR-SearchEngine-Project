   <script type="text/javascript">
    $(document).ready(function () { 

	var isTransClosed = true;

	$("#tabimg_translate, #closeTranslate").click(function() {
		if(isTransClosed == true)
		{
		  $('#translate_tab').animate({width:200});		 
		  $("#translate_div").fadeIn();
		  isTransClosed = false;
		}
		else 
		{
			$("#translate_div").fadeOut();
		  	$('#translate_tab').css('width','0px');
		  	isTransClosed = true;
		}
	});	
	
});  

function GTUrlTranslate(sel) {
	var sToLang = sel.value;  
	var sFromLang = "en";
	var sThisPage = document.location.href;
	if(sThisPage.indexOf('https://edit')>-1)
	{
		sThisPage = sThisPage.replace('https://edit','https://');
	}
	
	document.location.href= "http://translate.google.com/translate?hl=&sl="+sFromLang +"&tl="+sToLang +"&u=" + sThisPage + "&sandbox=0";
}
