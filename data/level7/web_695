            <script language="javascript" type="text/javascript">
             function Global_ctl01DoSearch() {
		if (typeof encodeURIComponent != "function") {
			alert('Your web browser does not support this function. Please upgrade to the latest version of your current browser.');
			return;
		}
				
		var searchPhrase = $("#Global_ctl01QuickSearch").promptBox("getValue");
		if (searchPhrase.length == 0) {
			alert('You must enter a search phrase.');
			return;
		}
	    
		searchPhrase = global.cleanInputString(searchPhrase, " ");

        document.location.href = '/Search/SearchResults.pg?SearchPhrase=' + encodeURIComponent(searchPhrase) + '&SearchType=text&SearchOperator=And';
	}

	function Global_ctl01KeyCheck(e) {
	    if ((window.event && e.keyCode == 13) || (e.which && e.which == 13)) {
			Global_ctl01DoSearch();
			return false;
	    }
	    
	    return true;
	}

	$(function() {
        $("#Global_ctl01QuickSearch").promptBox(
            "init", 
            { promptText: 'Search for...' });			 
	});
            </script>
