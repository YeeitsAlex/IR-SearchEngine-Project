    <script type="text/javascript">
     /* ************************************************************************************************ 
			The below shows the appropriate Section Title Header DIV above based on the subdomain 
			string by changing the CSS from "display:none" to "display:block".
			Sticking to standard JavaScript to avoid potential JS library conflicts 
			with vendors using this header template.
			The conditionals below also disable the stylesheet that allows for a responsive design. 
			The responsive stylesheet will be dubdomains on subdomains that are not providing a responsive experience 
		************************************************************************************************ */
			
			var urlParts = location.hostname.split('.');
			var urlWhole = window.location.href.toLowerCase();
			var subdomain = urlParts[0].toLowerCase();
			if(subdomain == 'doctors')
			{
				if(urlWhole.indexOf('.edu/clinicians') >= 0)
				{
					document.getElementById("FindAClinicianHeader").style.display = 'block';
					document.getElementById("searchdrop").style.visibility= 'hidden';
				}
				else
				{
					document.getElementById("FindADoctorHeader").style.display = 'block';
					document.getElementById("searchdrop").style.visibility= 'hidden';
				}
			}
			if(subdomain == 'clinicians' || subdomain == 'dev')
			{
				document.getElementById("FindAClinicianHeader").style.display = 'block';
				document.getElementById("searchdrop").style.visibility= 'hidden';
			}
			
			if(subdomain == 'providers' )
			{
				document.getElementById("FindADoctorHeader").style.display = 'block';
				document.getElementById("searchdrop").style.visibility= 'hidden';
			}


			if(subdomain == 'myhealth')
			{
				document.getElementById("MyHealthHeader").style.display = 'block';
			}
						
			if(subdomain == 'go')
			{
				document.getElementById("ADXHeader").style.display = 'block';
		
			}

			if(subdomain == 'assets' || subdomain == 'editassets')
			{
				document.getElementById("AssetsHeader").style.display = 'block';
				document.getElementById("searchdrop").style.visibility= 'hidden';
	
	
			}
			
			function DisableResponsive()
			{
				var sheetToBeRemoved = document.getElementById('responsiveStylsheet');
				var sheetParent = sheetToBeRemoved.parentNode;
				sheetParent.removeChild(sheetToBeRemoved);
				document.getElementById('mobile-menu').styles.display = 'none';
				document.getElementById('sidr').styles.display = 'none';
			}
    </script>
    <script type="text/javascript">
     $(document).ready(function () { 
			
