   <script type="text/javascript">
    function GTUrlTranslate(sel) {
	var sToLang = sel.value;  
	var sFromLang = "en";
	var sThisPage = document.location.href;
	if(sThisPage.indexOf('https://edit')>-1)
	{
		sThisPage = sThisPage.replace('https://edit','https://');
	}
	
	document.location.href= "http://translate.google.com/translate?hl=&sl="+sFromLang +"&tl="+sToLang +"&u=" + sThisPage + "&sandbox=0";
}
   </script>
   <script type="text/javascript">
    /*<![CDATA[*/
(function() {

var sThisPage = document.location.href;
	if(sThisPage.indexOf('myhealth.ucsd.edu') < 0)
	{
	var sz = document.createElement('script'); sz.type = 'text/javascript'; sz.async = true;
	sz.src = '//us2.siteimprove.com/js/siteanalyze_14686.js';
	var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(sz, s);	
	}

})();
/*]]>*/
   </script>
