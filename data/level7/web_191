					jQuery('#slider').css({ display : 'block' });
					jQuery("#slider").slides({
						preload: true,
						preloadImage: 'http://urap.berkeley.edu/sites/all/themes/bowtie/images/slider/loading.gif',
						play: 0,
						width: 960,
						pause: 4500,
						slideSpeed: 1000,
						generatePagination: true,
						hoverPause: true,
						autoHeight: true
					});	
				}
      })(jQuery);
	  
//--><!]]>
</script>

