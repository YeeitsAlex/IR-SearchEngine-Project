  <script type="text/javascript">
   (function() {
        /** CONFIGURATION START **/
        var _sf_async_config = window._sf_async_config = (window._sf_async_config || {});

        // start get sections
        var categoriesList = [{
          "category-arts_culture": "Arts & Culture"
        }, {
          "category-campuscommunity": "Campus & Community"
        }, {
          "category-opinion": "Opinion"
        }, {
          "category-people": "People"
        }, {
          "category-research": "Research"
        }, {
          "category-web-general": "Web General"
        }]
        var categoriesArray = [];

        categoriesList.forEach(function(item) {
          Object.keys(item).forEach(function(key) {
                if(jQuery('article').hasClass(key)){
                    categoriesArray.push(item[key]);
                }
          });
        });
        // end get sections

        // start get authors
        var queryNames = document.querySelectorAll('.author-name');
        var nameArray = [];

        for (i = 0; i < queryNames.length; i++) {
            var nameSanitized = queryNames[i].textContent;
            nameSanitized= nameSanitized.replace(/,/g , " ");
            nameArray.push(nameSanitized);
        }
        // end get authors

        // convert categoriesArray and nameArray to strings with no commas or double spaces
        var categoriesString = categoriesArray.toString();
        var nameString = nameArray.toString();

        // regex to replace double spaces
        categoriesString = categoriesString.replace(/  +/g, ' ');
        nameString = nameString.replace(/  +/g, ' ');
        // end conversion

        // populate variables only if post
        if(jQuery('body').is('[class*="postid-"]')){
          _sf_async_config.sections = categoriesString;
          if(jQuery('.author-name').length == 0){
            _sf_async_config.authors = 'No Byline';
          } else {
            _sf_async_config.authors = nameString;
          }
        }
        /** CONFIGURATION END **/
        function loadChartbeat() {
            var e = document.createElement('script');
            var n = document.getElementsByTagName('script')[0];
            e.type = 'text/javascript';
            e.async = true;
            e.src = '//static.chartbeat.com/js/chartbeat.js';
            n.parentNode.insertBefore(e, n);
        }
        loadChartbeat();
      })();


      document.addEventListener('DOMContentLoaded', function() {
          console.log('sections: ' + _sf_async_config.sections);
          console.log('authors: ' + _sf_async_config.authors);
      });
  </script>
