  <script type="text/javascript">
   var LOAD_MIX_PANEL = function (source) {

        var script = document.createElement("script");
        script.type = "text/javascript";
        script.async = true;
        script.src = source;
        script.onload = function () {

            console.log("mixpanel loaded: " + script.src); //debug
        };

        document.getElementsByTagName("body")[0].appendChild(script);
    };
  </script>
  <script id="email-history-entry" type="text/template">
   <!--
we need inline css here because this will be going out in an email,
and there will be no reliable css links.
we keep the class names only to reference the divs the inline styles
pull from
-->
<td style="font-size:11px; text-align:left; padding:10px; vertical-align:top; overflow:hidden; word-wrap:break-word;">

	<%= date %>
</td>
<td style="font-size:11px; text-align:left; padding:10px; vertical-align:top; overflow:hidden;">

    <p style="font-size:11px;">

    	<b>Query&nbsp;:&nbsp;</b>
    	<%- history["query"] %>
    </p>
    <p style="font-size:11px;">

    	<b>Date Range&nbsp;:&nbsp;</b>
    	<%- dateRange %>
    </p>
    <p style="font-size:11px;">

    	<b>Hidden Items&nbsp;:&nbsp;</b>
    	<%- hiddenItems %>
    </p>
    <p style="font-size:11px;">

    	<b>Collections Searched&nbsp;:&nbsp;</b>
    	<%- collectionsSearched %>
    </p>
    <p style="font-size:11px;">

    	<b>Filters Applied&nbsp;:&nbsp;</b>
    	<%- facets %>
    </p>
</td>
<td style="font-size:11px; text-align:left; padding:10px; vertical-align:top; overflow:hidden;  word-wrap:break-word;">

    <a href="<%- URL %>">

    	<%- window.location.protocol + "//" + window.location.host + window.location.pathname + URL.substr(1) %>
    </a>
</td>

<td style="font-size:11px; text-align:left; padding:10px; vertical-align:top; overflow:hidden;">

    <%- history["resultCount"].toLocaleString().split(".")[0] %>
</td>

