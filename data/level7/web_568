                        break;
                    case "drug":
            
                        print("<div class=\"result-bottom-row-item drug-color\"><b>Drug Industry Query</b></div>");
                        break;
                    case "chemical":

                        print("<div class=\"result-bottom-row-item chemical-color\"><b>Chemical Industry Query</b></div>");
                        break;
