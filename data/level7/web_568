   <div id="open-search-text-container">

    <div id="open-search-text-render">
    </div>
    <textarea id="open-search-text-area"></textarea>
</div>

<div class="query-builder-bottom-row" style="margin-top:10px; margin-bottom:10px; padding-right:15px;">

