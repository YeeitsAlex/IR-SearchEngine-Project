  <script id="query-row" type="text/template">
   <%
    var boolLi = "";
    for(var key in bools){

        boolLi += "<li data-code=\""+ bools[key] +"\">";
        boolLi += bools[key];
        boolLi += "</li>";
    }

    var fieldLi = "";
    var fields = METADATA["field"];
    for(var a = 0; a < fields.length; ++a){

        if(fields[a] in METADATA["allFields"]){

            if(fields[a] in METADATA["subField"]){
                
                fieldLi += "<li class=\"dropdown-submenu\" data-read=\""+ METADATA["allFields"][fields[a]][0] +"\" data-code=\""+ fields[a] +"\">";

                fieldLi +=      "<div>";
                fieldLi +=          CAPS(METADATA["allFields"][fields[a]][0]);
                fieldLi +=          "<div class=\"glyphicon glyphicon-chevron-right\" style=\"float:right;\"></div>";
                fieldLi +=      "</div>";
                fieldLi +=      "<ul class=\"dropdown-menu query-row-field-subdropdown\">";

                var subField = METADATA["subField"][fields[a]];
                for(var b = 0; b < subField.length; ++b){

                    var field = subField[b];
                    if(field.length > 13){
    
                        field = field.substring(0, 13) +"...";
                    }

                    if(field == "Other"){

                        fieldLi += "<li data-code=\""+ fields[a] +"\" data-read=\""+ METADATA["allFields"][fields[a]][0] +"\" title=\""+ subField[b] +"\">"+ field +"</li>";
                    }
                    else{

                        var val = subField[b]
                        if(subField[b].indexOf(" ") > -1){

                            val = "&quot;"+ subField[b] +"&quot;"
                        }
                        fieldLi += "<li data-code=\""+ fields[a] +"\" data-read=\""+ METADATA["allFields"][fields[a]][0] +"\" data-val=\""+ val +"\" title=\""+ subField[b] +"\">"+ field +"</li>";
                    }    
                }

                fieldLi +=      "</ul>";   
            }
            else{

                fieldLi += "<li data-code=\""+ fields[a] +"\" data-read=\""+ METADATA["allFields"][fields[a]][0] +"\">";
                fieldLi +=      METADATA["allFields"][fields[a]][0];
            }

            fieldLi += "</li>";
        }
    }
%>

<!-- QUERY BOX ROW -->
<% if(bool != null){ 

    if(order == 2){
%>

<div class="query-row-bool-container" id="query-row-first-bool">

<% } else { %>

<div class="query-row-bool-container">

<% } %>
    <button class="btn query-row-btn dropdown-toggle query-row-bool-toggle" type="button" id="query-row-bool-button-<%- cid %>" data-toggle="dropdown">
        
        <b><%- bool %></b>&nbsp;<span class="query-row-caret"></span>
    </button>
    <ul class="dropdown-menu query-row-bool-dropdown" role="menu" aria-labelledby="query-row-bool-<%- cid %>">
        
        <% print(boolLi); %>            
    </ul>
</div>
<% } else { %>

<div class="query-row-bool-container" style="visibility:hidden;">
</div>
<% } %>

<div class="query-row-field-container" <% if(bool == null){ %>  id="query-builder-first-field" <% } %> >

    <button class="btn query-row-btn dropdown-toggle query-row-field-toggle" type="button" id="query-row-field-button-<%- cid %>" data-toggle="dropdown">
        
        <!-- button label will be set here -->
    </button>
    <ul class="dropdown-menu multi-level query-row-field-dropdown" role="menu" aria-labelledby="query-row-field-<%- cid %>" style="margin-left:0px;">

        <% print(fieldLi); %>
    </ul>
</div>

<div class="query-text-container">

    <input class="query-text-input" id="query-text-input-<%- cid %>" tabindex="<%- tabIndex %>" value="<%- text %>"/>
</div>
  </script>
