  <script type="text/javascript">
   $('#mainMenu').on('focusout', function () {
            var $elem = $(this);
            setTimeout(function () {
                var hasFocus = !!($elem.find(':focus').length > 0);
                if (!hasFocus) {
                    toggleMenu('mainMenu', 'close', 1200);
                }
            }, 10);
        });
        $('#favoritesPanel').on('focusout', function () {
            var $elem = $(this);
            setTimeout(function () {
                var hasFocus = !!($elem.find(':focus').length > 0);
                if (!hasFocus) {
                    toggleMenu('favoritesPanel', 'close', 1200);
                }
            }, 10);
        });
        $('#routesMenu').on('focusout', function () {
            var $elem = $(this);
            setTimeout(function () {
                var hasFocus = !!($elem.find(':focus').length > 0);
                if (!hasFocus) {
                    toggleMenu('routesMenu', 'close', 1200);
                }
            }, 10);
        });
  </script>
