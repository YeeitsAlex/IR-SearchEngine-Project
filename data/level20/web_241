          <!-- Month / Year Headers -->
          <h2 class="tribe-events-list-separator-month">
           <span>
            March 2019
           </span>
          </h2>
          <!-- Event  -->
          <div class="type-tribe_events post-3916 tribe-clearfix tribe-events-venue-1194 tribe-events-first" id="post-3916">
           <!-- Event Title -->
           <h3 class="tribe-events-list-event-title">
            <a class="tribe-event-url" href="https://gacoast.uga.edu/event/marine-organism-sampling-cruise/" rel="bookmark" title="Winter Science Series: Fish Sampling Cruise">
             Winter Science Series: Fish Sampling Cruise
            </a>
           </h3>
           <!-- Event Meta -->
           <div class="tribe-events-event-meta">
            <div class="author location">
             <!-- Schedule & Recurrence Details -->
             <div class="tribe-event-schedule-details">
              <span class="tribe-event-date-start">
               March 12, 9:00 am
              </span>
              -
              <span class="tribe-event-time">
               12:00 pm
              </span>
             </div>
             <!-- Venue Display Info -->
             <div class="tribe-events-venue-details">
              <a href="https://gacoast.uga.edu/venue/uga-marine-education-center-and-aquarium/" title="UGA Marine Education Center And Aquarium">
               UGA Marine Education Center And Aquarium
              </a>
              ,
              <span class="tribe-address">
               <span class="tribe-street-address">
                30 Ocean Science Circle
               </span>
               <span class="tribe-locality">
                Savannah
               </span>
               <span class="tribe-delimiter">
                ,
               </span>
               <abbr class="tribe-region tribe-events-abbr" title="Georgia">
                GA
               </abbr>
               <span class="tribe-postal-code">
                31411
               </span>
               <span class="tribe-country-name">
                United States
               </span>
              </span>
              <a class="tribe-events-gmap" href="https://www.google.com/maps/search/?api=1&amp;query=31.9876209%2C-81.0237742" target="_blank" title="Click to view a Google Map">
               + Google Map
              </a>
             </div>
             <!-- .tribe-events-venue-details -->
            </div>
           </div>
           <!-- .tribe-events-event-meta -->
           <!-- Event Cost -->
           <div class="tribe-events-event-cost">
            <span class="ticket-cost">
             $42
            </span>
           </div>
           <!-- Event Image -->
           <!-- Event Content -->
           <div class="tribe-events-list-event-description tribe-events-content description entry-summary">
            <p>
