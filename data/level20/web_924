{        
        jQuery.ajax({
                    type : 'POST',
                    dataType : 'json',
                    url : "https://cns.utexas.edu/index.php?option=com_jevents&ttoption=com_jevents&typeaheadtask=gwejson&file=fetchlatestevents&path=module&folder=mod_jevents_latest&token=10e66209f75360afef3c4811eb751d3d",
