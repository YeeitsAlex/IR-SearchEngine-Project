    <section class="prepare">
     <h2>
      Prepare for your interview
     </h2>
     <ul class="clearfix">
      <li class="interviewday">
       <div>
        <img height="158" onerror="this.src='https://mdadmissions.wustl.edu/wp-content/themes/mdadmissions/_/img/interviewday.png';this.onerror=null;" src="https://mdadmissions.wustl.edu/wp-content/themes/mdadmissions/_/img/interviewday.svg" width="151"/>
        <a class="button" href="/how-to-apply/selection-process/the-interview-what-to-expect" onclick="__gaTracker('send', 'event', 'prepare-for-interview', '/how-to-apply/selection-process/the-interview-what-to-expect', 'Interview Day');">
         Interview Day
        </a>
       </div>
      </li>
      <li class="interviewerbios">
       <div>
        <img height="108" onerror="this.src='https://mdadmissions.wustl.edu/wp-content/themes/mdadmissions/_/img/interviewerbios.png';this.onerror=null;" src="https://mdadmissions.wustl.edu/wp-content/themes/mdadmissions/_/img/interviewerbios.svg" width="155"/>
