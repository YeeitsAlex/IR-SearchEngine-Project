         <script type="text/javascript">
          jQuery(window).load(function() {

    	function vidSize() {
    		if (jQuery(window).width() > 1199) {
	    		jQuery('#front_slides ul').prepend('<li><div class="rounded nobottom"><iframe id="front_vid_left_full" src="https://player.vimeo.com/video/304151999" width="585" height="360" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe></div></li>');
	    	} else if (jQuery(window).width() < 1200 && jQuery(window).width() > 979) {
	    		jQuery('#front_slides ul').prepend('<li><div class="rounded nobottom"><iframe id="front_vid_left_small" src="https://player.vimeo.com/video/304155646" width="470" height="360" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe></div></li>');
	    	} else if (jQuery(window).width() < 980 && jQuery(window).width() > 767) {
	    		jQuery('#front_slides ul').prepend('<li><div class="rounded nobottom"><iframe id="front_vid_top" src="https://player.vimeo.com/video/304151999" width="724" height="446" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe></div></li>');
	    	} else if (jQuery(window).width() < 768) {
	    		var iframeW = jQuery('#front_mission').width() + 40;
	    		var iframeH = iframeW / 1.625;
	    		// alert('h = '+iframeH+', w = '+iframeW);
	    		jQuery('#front_slides ul').prepend('<li><div class="rounded nobottom"><iframe id="front_vid_top_flex" src="https://player.vimeo.com/video/304151999" width="' + iframeW + '" height="' + iframeH + '" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe></div></li>');
	    	}
    	}

  		// jQuery(window).resize(function() {
		//   	vidSize();
		// });

    	vidSize();

    	jQuery('#front_slides ul').addClass('slides');
    	jQuery('#front_slides img').wrap('<div class="rounded nobottom">');
    	jQuery('#front_slides_wrap li:nth-child(2)').addClass('second');
    	jQuery('#front_slides_wrap li:nth-child(3)').addClass('third');
    	jQuery('#front_slides_wrap li:nth-child(4)').addClass('fourth');
    	jQuery('#front_slides_wrap li:nth-child(5)').addClass('fifth');

  		jQuery('.flexslider').flexslider({
    		slideshowSpeed: 20000,
    		slideshow: false, // prevent auto start slides
    		animationSpeed: 1000,
    		randomize: false,    		pauseOnHover: true
  		});

  		jQuery('#front_slides_wrap').css('display','block');

  		jQuery('#front_slides img').each(function() {
  			var imgTtl = jQuery(this).attr('title');
  			var imgAlt = jQuery(this).attr('alt');
  			jQuery(this).after('<div class="img_cap"><h2>'+imgTtl+'</h2><p>'+imgAlt+'</p></div>');
  			jQuery(this).removeAttr('title');
			jQuery(this).attr('alt', imgTtl + ' - ' + imgAlt);
  		});

  		jQuery('#front_slides img.adaptive').hover(
  			function() {
  				jQuery('.img_cap').stop(true, true).fadeIn();
  			},
  			function() {
  				jQuery('.img_cap').stop(true, true).fadeOut();
  			}
  		);

  		jQuery('.front_path_wrap .adaptive-image').each(function(){
			var imgPath = jQuery(this).attr('src');
			jQuery(this).parent().css('background-image','url("'+imgPath+'")');

			var imgTtl = jQuery(this).attr('title');
			var imgAlt = jQuery(this).attr('alt');

			jQuery(this).attr('title', imgTtl + ' - ' + imgAlt);
			jQuery(this).attr('alt', imgTtl + ' - ' + imgAlt);
		});


		var imgCapA = '';

    	jQuery('ul.slides li').each(function(){
    		if (jQuery('.img_cap a', this).length > 0) {
    			imgCapA = jQuery('.img_cap a', this).attr('href');
    			jQuery('img', this).attr('onclick', 'window.location = \'' + imgCapA + '\'').css('cursor', 'pointer');
    		}
    	});

  		
	});
         </script>
         <div class="highlightbox span6 contenttable" id="front_mission">
          <h2 class="top10">
           <i class="icon-light-up">
           </i>
           <p>
            <em>
             Same Veritas,
             <strong>
              More Lux.
             </strong>
            </em>
           </p>
          </h2>
          <div class="clear">
          </div>
          <p>
           Yale Summer Session provides academically rigorous and innovative undergraduate programs and courses to Yale and visiting students across platforms and around the world. Students come to Yale Summer Session to take Yale College courses in New Haven, online, or abroad or to hone their craft at the Yale Writers’ Workshop or in the Summer Drama Program. International students can also study law, business, or English in one of our certificate programs.
          </p>
          <p>
           <a href="/node/43">
            <span class="btn btn-primary btn-normal mr">
             Learn More
             <i class="icon-right-open-1">
             </i>
            </span>
           </a>
          </p>
         </div>
