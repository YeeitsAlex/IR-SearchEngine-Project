			// -- End tracking script -- //

			// ------- SCRIPT TO TRACK FILES AS VIRTUAL PAGEVIEWS ------- //
			// UPDATED 8/25/15
			$(document).ready(function() {
				var currentDomain = window.location.host;
				$("a").click(function() {
					//First, make sure the clicked link has an href in order to avoid undefined exceptions
					if ($(this).attr('href') != undefined) {
						//Get link target and text
						var targetHref = $(this).attr('href');
						//Trim any '../'s off target href
						if (targetHref.substr(0, 3) == "../") {
							var currentHref = window.location.href.substr(window.location.href.indexOf("//") + 1);
							var count = (targetHref.match(/\.\.\//g) || []).length;
							var pathArray = currentHref.split("/");
							var pathAppend = "";
							for (var i = 0; i < pathArray.length - count - 1; i++) {
								pathAppend += pathArray[i] + "/";
							};
							targetHref = pathAppend + targetHref.substr(count * 3);
						};
						var targetTitle = $(this).text();
						//A regular expression of the filetypes we're looking for
						var filetypes = new RegExp("(?:pdf|doc|ppt|xls|zip|mov|avi|mpg|mp3|m4a|mp4)x?(?:(?:#|\\?|&).*)?$");
						//Set destination variable to internal or external based on link target
						var destination = (targetHref.indexOf(currentDomain) != -1 || targetHref.indexOf("http") == -1) ? "internal" : "external";
						//Category variable to be passed to Google Analytics is set to either "Downloads" or "External Link"
						var category = (targetHref.toLowerCase().search(filetypes) != -1) ? "Downloads" : "External Link";
						//If it's an internal file download, we send that to Google as a pageview
						//Else, we send a tracking event to google, categorized as either "Downloads" or "External Link"
						if (destination == "internal" && category == "Downloads") {
							//Trim the domain off the href sent to Google in order for it to be formatted correctly
							targetHref = targetHref.replace(window.location.host, "");
							ga('send', 'pageview', {
								'page': targetHref,
								'title': targetTitle
							});
						} else if (destination == "external") {
							ga('send', {
								'hitType': 'event',
								'eventCategory': category,
								'eventAction': 'Outbound link click - with referral page',
								'eventLabel': window.location.href + ' --> ' + targetHref
							});
							ga('send', {
								'hitType': 'event',
								'eventCategory': category,
								'eventAction': 'Outbound link click',
								'eventLabel': targetHref
							});
						};
					};
				});

				// Temporary removal of image map on header - GB 9/16/16
				$("#main-logo").removeAttr("usemap");
				$("#main-logo").wrap("<a href='http://www.caes.uga.edu' alt='College of Agricultural and Environmental Sciences (CAES)'></a>");
			});
		}
</script>

