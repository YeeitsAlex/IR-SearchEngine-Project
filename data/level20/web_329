  <script type="text/javascript">
   (function() {
    "use strict";
    var searchForm, searchField, searchInputString;
    var host = window.top.location.hostname;

    if (host.indexOf("proxy.multitranstms.com") > -1 || host.indexOf("global.si.edu") > -1 ) {
        appendScript();
    }
    document.addEventListener("readystatechange", function() {
        if (document.readyState === "complete") {
            emptyContainer();
            recallDomloadedEvent();
            fixFontIssueOnArabic();

            searchForm = document.querySelector("#google-appliance-block-form"),
            searchField = document.querySelector("#google-appliance-block-form .form-item-search-keys input"); 
            if ((searchForm !== undefined) && (searchField !== undefined)){
                addSearchFormOverride();
                overrideInfoLinks();
            }
        }
    });
    function appendScript() {
        document.head.appendChild(createScript());
    }
    function createScript() {
        var script = document.createElement("script");
        script.src = "proxy.multitranstms.com/js/webtranslate/languageDropdown.js?code=8ha151k1";
        return script;
    }
    function emptyContainer() {
        var selectorContainer = document.querySelector(".customSelectParent");
        if (!selectorContainer) return;
        while (selectorContainer.hasChildNodes()) {
            selectorContainer.removeChild(selectorContainer.lastChild);
        }
    }
    function recallDomloadedEvent() {
        var DOMContentLoaded_event = document.createEvent("Event");
        DOMContentLoaded_event.initEvent("DOMContentLoaded", true, true);
        window.document.dispatchEvent(DOMContentLoaded_event);
    }
    function fixFontIssueOnArabic() {
        if (document.querySelector("html").getAttribute("lang") !== "ar-SA") return;
        var allNodes = document.querySelectorAll("*");
        for (var i = 0; i < allNodes.length; i++) {
            var font = window.getComputedStyle(allNodes[i], null).getPropertyValue("font-family").replace(/\"/g, "");
            if (font === "Montserrat") {
                allNodes[i].style.fontFamily = "Arial";
            }
        }
    }
    function addSearchFormOverride() {
        searchField.addEventListener("input", function (){
            setSearchFormAction();
        });
    }
    function setSearchFormAction() {
        searchInputString = searchField.value;
        searchForm.setAttribute("action", "/sitesearch/" + encodeURI(searchInputString));
    }
    function overrideInfoLinks (){
        if (location.pathname.search("/sitesearch") === -1) { return; };
        var searchResults = document.querySelectorAll(".search-results.google-appliance-results li .search-info.google-appliance-info a");
        for (var i = 0; i < searchResults.length; i++) {
            var link = searchResults[i];
            link.textContent = link.getAttribute("href");
        }
    }
})();

document.addEventListener("DOMContentLoaded", DFSAdjustNumber, false);
function DFSAdjustNumber() {
 if(location.pathname === "/our-impact" && (document.querySelector("html").getAttribute("lang") === "fr-FR")) {
  var selector = "#block-iknow-core-impact-slideshow div div div.slideshow.clearfix div:nth-child(4) div div div.left div.stat.title";
  if(selector && $(selector)[0].innerHTML === "30,000") {
    $(selector)[0].innerHTML = $(selector)[0].innerHTML.replace(",", "&nbsp;");
  }
 }
}
  </script>
 </head>
