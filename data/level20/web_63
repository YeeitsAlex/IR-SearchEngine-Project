	var dynamic_styles_ids = (["ajax-5b606caf6c4b3","ajax-5b606caf6e6e3","ajax-5b606caf70637","ajax-5b606caf7230d","ajax-5b606caf75341","ajax-5b606caf7bd20","ajax-5b606caf8256a","ajax-5b606caf87f36","ajax-5b606caf9001c","ajax-5b606caf90551","ajax-5b606caf9b2ef","ajax-5b606caf8d90d"] != null) ? ["ajax-5b606caf6c4b3","ajax-5b606caf6e6e3","ajax-5b606caf70637","ajax-5b606caf7230d","ajax-5b606caf75341","ajax-5b606caf7bd20","ajax-5b606caf8256a","ajax-5b606caf87f36","ajax-5b606caf9001c","ajax-5b606caf90551","ajax-5b606caf9b2ef","ajax-5b606caf8d90d"] : [];

	var styleTag = document.createElement('style'),
		head = document.getElementsByTagName('head')[0];

	styleTag.type = 'text/css';
	styleTag.setAttribute('data-ajax', '');
	styleTag.innerHTML = dynamic_styles;
	head.appendChild(styleTag);


	$('.mk-dynamic-styles').each(function() {
		$(this).remove();
	});

	function ajaxStylesInjector() {
		$('.mk-dynamic-styles').each(function() {
			var $this = $(this),
				id = $this.attr('id'),
				commentedStyles = $this.html();
				styles = commentedStyles
						 .replace('<!--', '')
						 .replace('-->', '');

			if(dynamic_styles_ids.indexOf(id) === -1) {
				$('style[data-ajax]').append(styles);
				$this.remove();
			}

			dynamic_styles_ids.push(id);
		});
	};
  </script>
