    <div class="row clearfix">
     <div id="page-title">
      <h2 class="title">
       Home Page
      </h2>
     </div>
     <div class="no-sidebars" id="content">
      <div class="region region-content block-count1 clearfix">
       <div class="block block-system block-region-weight-1 odd" id="block-system-main">
        <div class="content block-body clearfix">
         <div about="/content/home-page" class="node node-horizontal-landing-page node-promoted node-full clearfix" id="node-24" typeof="sioc:Item foaf:Document">
          <span class="rdf-meta element-hidden" content="Home Page" property="dc:title">
          </span>
          <div class="content">
           <div class="block-row block-row-odd block-count-3">
            <div class="row-content clearfix">
             <div class="block block-nodeblock related-info-block third-width block-region-weight-1 odd nodeblock-gt-ct-super-block" id="block-nodeblock-12361">
              <div class="content block-body clearfix">
               <div about="/content/q-chats-1" class="node node-super-block clearfix sb-skin-gold sb-teaser-false sb-image-left" id="node-12361" typeof="sioc:Item foaf:Document">
                <h4 class="super-block__title">
                 <a class="super-block__title-link" href="http://lgbtqia.gatech.edu/q-chats-lgbtqia-discussion-groups">
                  Q Chats
                 </a>
                </h4>
                <span class="rdf-meta element-hidden" content="Q Chats " property="dc:title">
                </span>
                <div class="content node-body clearfix ">
                 <div class="super-block__image">
                  <div class="super-block__image-wrapper">
                   <a href="http://lgbtqia.gatech.edu/q-chats-lgbtqia-discussion-groups">
                    <img alt="" height="420" src="http://lgbtqia.gatech.edu/sites/default/files/uploads/images/superblock_images/q_chats_web_0.png" typeof="foaf:Image" width="630"/>
                   </a>
                   <div class="super-block__jump-link clearfix">
                    <a href="http://lgbtqia.gatech.edu/q-chats-lgbtqia-discussion-groups">
                     Student Discussion Groups
                    </a>
                   </div>
                  </div>
                 </div>
                 <div class="super-block__lower">
                 </div>
                </div>
               </div>
              </div>
             </div>
             <div class="block block-nodeblock related-info-block block-region-weight-1 even nodeblock-gt-ct-super-block" id="block-nodeblock-12393">
              <div class="content block-body clearfix">
               <div about="/content/lavender-awards" class="node node-super-block clearfix sb-skin-gold sb-teaser-false sb-image-left" id="node-12393" typeof="sioc:Item foaf:Document">
                <h4 class="super-block__title">
                 <a class="super-block__title-link" href="http://lgbtqia.gatech.edu/lavender-awards">
                  Lavender Awards
                 </a>
                </h4>
                <span class="rdf-meta element-hidden" content="Lavender Awards" property="dc:title">
                </span>
