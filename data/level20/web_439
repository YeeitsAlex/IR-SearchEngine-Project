   <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js" type="text/javascript">
   </script>
   <style type="text/css">
    #menubar { position:absolute; top:0px; z-index: 4; margin: 0 auto; width:100%; text-align:center; height:27px; margin-left:-23px; }
            #menubar_gold { position:absolute; top:0px; z-index: 3; margin: 0 auto; width:100%; text-align:center; height:27px; margin-left:-23px; }
   </style>
   <script type="text/javascript">
    var currentPath = "/Communications/Annual Giving/index";
        	currentPath = currentPath.substring(0, currentPath.lastIndexOf("/"));
		var highlightSuffix = "_gold";
		var menubarId = "#menubar";
		var highlightedId = "#menubar_gold";
		
	 	$(document).ready(function() {
						
			$(menubarId+' img').each(function()
			{	
							
				var imgname = $(this).attr('src');
				var imgExt = imgname.substring(imgname.lastIndexOf('.'));
				var imgnameGold = imgname.substring(0, imgname.lastIndexOf('.'));
				imgnameGold = imgnameGold+highlightSuffix+imgExt;
				var urlPath = $(this).parent().attr('href');
				var imgClass = $(this).attr('class');
				
				$(highlightedId).append('<img src="'+imgnameGold+'" />');
			
				
				if (imgClass == 'current')
				{
					$(this).css("opacity", "0");
				}
				else
				{
					$(this).mouseover(function()
					{
						$(this).css("opacity", "0");
					});
			
					$(this).mouseout(function()
					{
						$(this).css("opacity", "1");
					});
				}
		
			});
		});
   </script>
