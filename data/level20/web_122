   var commmitBoxHeight = $("#comittment").height();
		$("#events").height(commmitBoxHeight).css("overflow-y","scroll");
		
		var maxHeight = -1;

		$('.frontpage_col').each(function() {
				maxHeight = maxHeight > $(this).height() ? maxHeight : $(this).height();
		});

		$('.frontpage_col').each(function() {
				$(this).height(maxHeight + 44);
		});
