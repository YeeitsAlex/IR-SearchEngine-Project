   <script>
    // fires same as $(document).ready
  jQuery(window).load(function() {
    $('#m_header-toggler').ccpeMobileNav();

    placeInfo();
    
    // NAVIGATION TAB FINDER
	$("ul.nav > li").each(function(){
		var top = $(this);
		
		$(this).find("a").each(function(){
			if($(this).attr('href') == window.location.pathname)
			{
				$(this).attr('class','current');
				top.children("a:first-child").attr('class','current');
				return true;
			}
			
		});
	});
	//END TAB FINDER
  });
  
 // .ccpeMobileNav is the script used to toggle the menu navigation for mobile devices
  $.fn.ccpeMobileNav = function() {
    
	  $('#sup-navigation_mobile').hide();
	  $('#header_mobile nav').hide();
	
	  
	var self = $(this),
    myParent = self.parents('#header_mobile'),
    animationSpeed = 800,
    //heightOpen = 460; // pre engagement / research
    heightOpen = 440;
    function openMenu() {
    	 $('#sup-navigation_mobile').show();
   	  	$('#header_mobile nav').show();
    	
    	// .stop() && .animate() the super-navigation in top section of header.
      $('#sup-navigation_mobile')
        .stop()
        .animate({
          height: 120
        }, animationSpeed);
      // .stop() && .animate() the main-navigation in header.
      $('#header_mobile nav')
        .stop()
        .animate({
          height: 238
        }, animationSpeed);
      // .stop() && .animate() the header section as a whole.
      $('#header_mobile')
        .stop()
        .animate({
          height: heightOpen
        }, animationSpeed, function() {
          myParent.addClass('open');
        });
    }
    function closeMenu() {
    	
    	
    	
    	$('#sup-navigation_mobile, #header_mobile nav')
        .stop()
        .animate({
          height: 0
        }, animationSpeed);
      $('#header_mobile')
        .stop()
        .animate({
          height: 83
        }, animationSpeed, function() {
          myParent.removeClass('open');
        });
    }
    self.on('click.fndtn touchstart.fndtn', function() {
      if(myParent.hasClass('open')) {
        closeMenu();
      } else {
        openMenu();
      }
    });
  };
   </script>
