   if ( typeof soliloquy_slider === 'undefined' || false === soliloquy_slider ) {soliloquy_slider = {};}var soliloquy_width_696 = jQuery('#soliloquy-container-696').width() < 960 ? 960 : jQuery('#soliloquy-container-696').width();jQuery('#soliloquy-container-696').css('height', Math.round(soliloquy_width_696/(960/300))).fadeTo(300, 1);jQuery(document).ready(function($){var soliloquy_container_696 = $('#soliloquy-container-696'),soliloquy_696 = $('#soliloquy-696'),soliloquy_holder_696 = $('#soliloquy-696').find('.soliloquy-preload');if ( 0 !== soliloquy_holder_696.length ) {var soliloquy_src_attr = 'data-soliloquy-src';soliloquy_holder_696.each(function() {var soliloquy_src = $(this).attr(soliloquy_src_attr);if ( typeof soliloquy_src === 'undefined' || false === soliloquy_src ) {return;}var soliloquy_image = new Image();soliloquy_image.src = soliloquy_src;$(this).attr('src', soliloquy_src).removeAttr(soliloquy_src_attr);});}soliloquy_slider['696'] = soliloquy_696.soliloquy({slideSelector: '.soliloquy-item',speed: 400,pause: 5000,auto: 1,keyboard: 1,useCSS: 1,startSlide: 0,autoHover: 0,autoDelay: 0,adaptiveHeight: 1,adaptiveHeightSpeed: 400,infiniteLoop: 1,mode: 'fade',pager: 1,controls: 1,autoControls: 0,autoControlsCombine: 0,nextText: '',prevText: '',startText: '',stopText: '',onSliderLoad: function(currentIndex){soliloquy_container_696.find('.soliloquy-active-slide').removeClass('soliloquy-active-slide').attr('aria-hidden','true');soliloquy_container_696.css({'height':'auto','background-image':'none'});if ( soliloquy_container_696.find('.soliloquy-slider li').length > 1 ) {soliloquy_container_696.find('.soliloquy-controls').fadeTo(300, 1);} else {soliloquy_container_696.find('.soliloquy-controls').addClass('soliloquy-hide');}soliloquy_696.find('.soliloquy-item:not(.soliloquy-clone):eq(' + currentIndex + ')').addClass('soliloquy-active-slide').attr('aria-hidden','false');$('.soliloquy-clone .soliloquy-video-holder').remove();soliloquy_container_696.find('.soliloquy-controls-direction').attr('aria-label','carousel buttons').attr('aria-controls', 'soliloquy-container-696');soliloquy_container_696.find('.soliloquy-controls-direction a.soliloquy-prev').attr('aria-label','previous');soliloquy_container_696.find('.soliloquy-controls-direction a.soliloquy-next').attr('aria-label','next');$(window).trigger('resize');soliloquy_container_696.parent().attr('data-soliloquy-loaded', 1);},onSlideBefore: function(element, oldIndex, newIndex, oldelm ){soliloquy_container_696.find('.soliloquy-active-slide').removeClass('soliloquy-active-slide').attr('aria-hidden','true');$(element).addClass('soliloquy-active-slide').attr('aria-hidden','false');},onSlideAfter: function(element, oldIndex, newIndex){}});});
