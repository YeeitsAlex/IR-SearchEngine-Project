  <script language="javascript" type="text/javascript">
   $(document).ready(function() {

    $("[name=access_key_pretty]").on('input', function() {
		if (!$("[name=access_key_submit]").prop('disabled')) {
    		$("[name=access_key_submit]").attr("style","background-color:#AB0520;");
    	} else {
			//$("[name=access_key_submit]").attr("style","background-color:white;");
    	}
    });


    $("[name=customer_name]").attr("style", "width:318px;");

    $("[name=id]").change(function() {
  		$("[name=submit]").attr("style","background-color:#AB0520;");
    });

      // Callback for service statuses/alerts
      var processStatuses = function(data) {

        if (data.alerts.length > 0) {
          $('#uits_alerts').append('<div class="uits-alerts"></div>');
          $('#uits_alerts .uits-alerts').append('<h3 style="font-size: 16pt;text-align: center; margin-top: 10px; margin-bottom: 10px;">UITS Alerts</h3>');
          $('#uits_alerts .uits-alerts').append('<ul class="alerts" style="padding-left: 0px;"></ul>');
          $.each(data.alerts, function(i,item) {
            $('#uits_alerts ul.alerts').append('<li style="list-style-position: inside;"><a target="_blank" style="text-align: center;white-space: nowrap; font-size: 13pt; display: block; float: none; width: inherit;margin-bottom: 10px;" href="' + item.link + '">' + item.title + '</a></li>');
          });
        }

        $('#service_status').append('<div class="uits-service-statuses"></div>');
        $('#service_status .uits-service-statuses').append('<h1 style="text-align: center;" class="sectionTitle">Systems Status</h1>');
        $('#service_status .uits-service-statuses').append('<ul class="feedlist services-statuses"></ul>');
        //$(".services-statuses").load("files/pullStatuses.php");

        $.each(data.service_statuses, function(i,item) {
          $('#service_status ul.services-statuses').append('<li style="font-size: 16px;" class="' + item.status + '"> <b>' + item.title + '</b></li>');
        });
      }

      // Asynchronously get service status information and shove it into a div
      $.ajax({
        url: "https://webservice.oscr.arizona.edu/services/service_status?format=jsonp",
        dataType: "jsonp",
        success: processStatuses,
        error:  function(xhr, status, errorThrown) {
                  console.log(errorThrown+'\n'+status+'\n'+xhr.statusText);
                }
      });

    });
  </script>
