  <script>
   function snapDialog(url){
	$("#main").css("margin-right", "300px");
	$("#snap_div").css("height", "100%").css("right", "0px").css("position", "fixed").css("width", "300px").css("top", "0px").show();
	$("#snap_ifr").height($("#snap_div").height());
	$("#snap_ifr").attr("src", url);
	$("#snap_ifr").data("eb-type", "snap");
	g_snapDivVisible = true;
}

function closeSnapDialog(){
	try
	{
		if($("#snap_div").is(":visible")){
			$("#snap_div").toggle("slow");
			g_snapDivVisible = false;
			setTimeout("resetSnapDialog()", 3000);	
			if($("#snap_ifr").data("eb-type") == "snap"){
				$("#snap_ifr").data("eb-type", "");
				$("#main").css("margin-right", "0px");
			}
		}
	}
	catch(err){}
}

function resetSnapDialog(){
	if(g_snapDivVisible == false){
		var snap_ifr = document.getElementById("snap_ifr");
		snap_ifr.setAttribute("src", "../html/blank.html");
	}
}
  </script>
