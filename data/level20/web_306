  <script>
   $(document).ready(function() {
	$( "#faq_list" ).draggable(
	  {
		cursor: 'move',
		start: function() {
			$(this).css({transform: "none", top: "15%", left:"90%"});
		}
	 });
	$("#faq_ask_box").hide();
	$('#widget-grid-faq').jarvisWidgets({
		onFullscreen: function () {
			if($("#faq_body").data("full") != "true"){
				$("#faq_body").css("max-height", "initial");
				$("#faq_body").data("full", "true");
			}else{
				$("#faq_body").css("max-height", "250px");
				$("#faq_body").data("full", "false");
			}
		}
	});

	$("#faq_wid_body").on("click", "a", function(){
		$(this).attr("target", "_blank");
	});
});

function ebFaqTogglePost(){
	$("#faq_ask_box").toggle();
}
  </script>
