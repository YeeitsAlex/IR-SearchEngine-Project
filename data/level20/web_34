     <div class="content-area" id="primary">
      <div class="site-header" id="sitetitle">
       <h2 class="department">
        <a href="https://ne.utk.edu/" rel="home" title="Department of Nuclear Engineering">
         Department of Nuclear Engineering
        </a>
       </h2>
      </div>
      <div class="entry-thumbnail-slider">
      </div>
      <div class="site-content wide home-margin" id="content" role="main">
       <article class="post-6 page type-page status-publish hentry" id="post-6">
        <header class="entry-header">
        </header>
        <!-- .entry-header -->
        <div class="entry-content reg">
         <div class="soliloquy-outer-container" data-soliloquy-loaded="0">
          <div aria-live="polite" class="soliloquy-container soliloquy-transition-fade soliloquy-fade soliloquy-arrows-active soliloquy-default-slider soliloquy-theme-classic" id="soliloquy-container-564" style="max-width:0px;max-height:0px;margin:0 auto 20px;max-width:100%;max-height:none;">
           <ul class="soliloquy-slider soliloquy-slides soliloquy-wrap soliloquy-clear" id="soliloquy-564">
            <li aria-hidden="true" class="soliloquy-item soliloquy-item-1 soliloquy-id-201 soliloquy-image-slide" draggable="false" style="list-style:none;">
             <img alt="Nuclear Research Reactor Laboratory Abroad Program" class="soliloquy-image soliloquy-image-1" data-soliloquy-src-mobile="https://ne.utk.edu/wp-content/uploads/sites/19/2016/08/ne6-720x486_c.jpg" id="soliloquy-image-201" src="https://ne.utk.edu/wp-content/uploads/sites/19/2016/08/ne6.jpg"/>
