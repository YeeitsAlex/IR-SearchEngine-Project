    <div class="container" id="main-content-container-fixed">
     <div class="row" id="main-content-row">
      <!-- LEFT PAGE CONTENT -->
      <div class="col-xs-12 col-sm-12 col-md-9 col-lg-9" id="left-page-content-div">
       <!-- *** CONTENT MOVED WITHIN LEFT SIDE FOR SECTIONPG *** -->
       <!-- *** *** *** *** *** *** *** *** *** *** *** *** *** *** *** -->
