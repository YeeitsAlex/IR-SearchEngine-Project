	function switchSkin(skinName) {   
		$("#" + skinName).addClass("selected")   
						   .siblings().removeClass("selected");
		$("#cssfile").attr("href", null);
		$("#cssfile").attr("href", "/Portals/3/Skins/Web3.0/css/layout/" + skinName + ".css"); 
