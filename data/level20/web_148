   <script>
    // From http://stackoverflow.com/questions/7244246/generate-an-rfc-3339-timestamp-similar-to-google-tasks-api
function ISODateString(d){
 function pad(n){return n<10 ? '0'+n : n}
 return d.getUTCFullYear()+'-'
      + pad(d.getUTCMonth()+1)+'-'
      + pad(d.getUTCDate())+'T'
      + pad(d.getUTCHours())+':'
      + pad(d.getUTCMinutes())+':'
      + pad(d.getUTCSeconds())+'Z'}
   </script>
   <script>
    var cal_id =  "gkq16kt2b5qia3e75i2bd9m2bo@group.calendar.google.com";
		var calendar_widget = new Calendar("calendar_content");

		// Callback after the calendar api has been loaded (see load)
		make_call_request = function () {
				// Arguments for our query
				var arguments = {
						"calendarId": cal_id,
						"timeMin": ISODateString(new Date())
						// "timeMax": ISODateString( function () {
						// 		var one_week = new Date();
						// 		one_week.setDate(one_week.getDate() + 14);
						// 		return one_week;
						// }() ) // Only put a weeks worth of events

				};
				// Create the request, exicute it, and deal with the callback
				var request = gapi.client.calendar.events.list( arguments );
				request.execute( function (response) {
						var list = response.items;
						calendar_widget.add_events(list);
						calendar_widget.render();
				});
		}

		// Callback after google has loaded it's api
		load = function () {
				gapi.client.setApiKey('AIzaSyC4Odj0gjZW4Lh8bWkvp2d-67ky-VKsx1I');
				gapi.client.load('calendar', 'v3', make_call_request);
		}
   </script>
