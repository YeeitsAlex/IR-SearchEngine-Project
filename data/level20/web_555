     <div id="page-title">
      <h2 class="title">
       Home
      </h2>
     </div>
     <div class="no-sidebars" id="content">
      <div class="region region-content block-count1 clearfix">
       <div class="block block-system block-region-weight-1 odd" id="block-system-main">
        <div class="content block-body clearfix">
         <div about="/home" class="node node-horizontal-landing-page node-full clearfix" id="node-1" typeof="sioc:Item foaf:Document">
          <span class="rdf-meta element-hidden" content="Home" property="dc:title">
          </span>
          <div class="content">
           <div class="body-row">
            <div class="row-content clearfix">
             <p>
              <a href="http://pts.gatech.edu/passio-go-gts-new-official-bus-tracker">
               <img alt="" src="http://pts.gatech.edu/sites/default/files/images/passio_web_banner.png" style="width: 1200px; height: 446px;"/>
              </a>
             </p>
             <p style='margin-bottom: 0px; font-stretch: normal; font-size: 17px; line-height: normal; font-family: ".SF UI Text"; color: rgb(69, 69, 69);'>
              <span style="color:#0000CD;">
               <span style="font-size:16px;">
                <span style="font-family:arial,helvetica,sans-serif;">
                 <strong>
                  <span style="font-weight: bold;">
                   ATTENTION:  D
                  </span>
                 </strong>
                </span>
               </span>
              </span>
              <strong style="font-family: arial, helvetica, sans-serif; font-size: 16px; color: rgb(0, 0, 205); letter-spacing: 0.025em;">
               ue to construction and road closures, the Tech Trolley stop at Publix – Spring Street is closed until further notice. Please use the stop at Midtown MARTA to access the Trolley on both inbound and outbound travels. We apologize for the inconvenience this may cause.
              </strong>
             </p>
            </div>
           </div>
