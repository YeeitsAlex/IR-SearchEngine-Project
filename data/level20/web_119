   <section id="main" role="main">
    <div class="row clearfix">
     <div id="page-title">
      <h2 class="title">
       Achieving Inclusive Excellence
      </h2>
     </div>
     <div id="content-lead">
      <div class="region region-content-lead block-count1 clearfix">
       <div class="block block-views block-region-weight-1 odd" id="block-views-nodequeue-1-block">
        <div class="content block-body clearfix">
         <div class="view view-nodequeue-1 view-id-nodequeue_1 view-display-id-block view-dom-id-8f5d256cc203b857c6f9cff7b1635046">
          <div class="view-content">
           <div class="flexslider optionset-photo-fader" id="flexslider-1">
            <ul class="slides">
             <li>
