  var redirectUrl = 'https:\x2F\x2Fwww.google.com\x2Faccounts\x2FAccountChooser?hd=g.harvard.edu\x26continue=https%3A%2F%2Fcalendar.google.com%2Fcalendar%2Frender\x26followup=https%3A%2F%2Fcalendar.google.com%2Fcalendar%2Frender\x26osid=1\x26service=cl';
