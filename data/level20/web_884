          <div class="views-field views-field-field-news-date-published">
           <div class="field-content">
            <h6>
             <i aria-hidden="true" class="fa fa-calendar">
             </i>
             <span class="date-display-single">
              February 5, 2019
             </span>
             <br/>
            </h6>
           </div>
