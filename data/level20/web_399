   window.onload = function() {
  var redirectUrl = 'https:\x2F\x2Fwww.google.com\x2Faccounts\x2FAccountChooser?hd=g.harvard.edu\x26continue=https%3A%2F%2Fdocs.google.com%2Fa%2Fg.harvard.edu%2F\x26followup=https%3A%2F%2Fdocs.google.com%2Fa%2Fg.harvard.edu%2F\x26osid=1';
  var domain = 'g.harvard.edu';
  var hash = window.location.hash;
  if (hash) {
  var match = hash.match(/[#&]Email=([^&]+)/);
  if (match) {
  redirectUrl += "&Email=" + match[1] + "@" + domain;
  }
  }
  window.location.replace(redirectUrl);
};
