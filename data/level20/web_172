  <script type="text/javascript">
   /*global $ */
$('.squareThree > div').append('<span class="plusSign" tabindex="0">+</span>');
$(window).on('resize', function () {
  'use strict';
  var columnHeightThree = $('.squareThree').width(),
    footerHeight = $('footer').height();
  
  $('.footerPad').css('padding-bottom', footerHeight + 100);
  
  if (columnHeightThree < 350) {
    $('.squareThree').css({'height': columnHeightThree, 'font-size' : '.74rem', 'line-height' : '1.2'});
  } else {
    $('.squareThree').css({'height': columnHeightThree, 'font-size' : 'inherit', 'line-height' : 'inherit'});
  }
}).resize();
  </script>
  <script type="text/javascript">
   /*global $ */
$(function () {
  'use strict';
  $('.squareThree > div').click(function (e) {
    var offset = $(this).offset(),
      w = $(this).width(),
      h = $(this).height(),
      relativeX = (e.pageX - offset.left),
      relativeY = (e.pageY - offset.top);

    if (relativeX > 0.9 * w && relativeY < 0.1 * h) {
      $(this).toggleClass('hoverEffect');
    }
  });
});

$('.plusSign').focus(function () {
  $(this).parent('.squareThree > div').addClass('hoverEffect');
})
$('.plusSign').focusout(function () {
  $(this).parent('.squareThree > div').removeClass('hoverEffect');
})
  </script>
