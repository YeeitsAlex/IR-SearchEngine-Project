      <style>
       .tie_advanced .hours-select-type.open { background: #f0f0f0;}
	.tie_advanced .hours-select {position: relative;}
	.tie_advanced .hours-select-type li {padding: .8rem 5%; padding: 12px 5%; display: none; width: 90%; color: #007a95;;  margin: 0;}
	.tie_advanced .hours-select-type li:first-child, .hours-select-type li.plus {display: block;}
	.tie_advanced .hours-select-type li:hover {cursor:pointer; background: #febc11 !important;}
	.tie_advanced .hours-select-type li.active {font-weight: 400;}
	.tie_advanced .hours-times li {padding: .8rem 5%; padding: 9px 5%; display: block; width: 90% !important; color: #000;}
	.tie_advanced .hours-times li:hover {background: #e5e5e5 !important;}
	.tie_advanced .hours-times span.hours-left {display: inline-block; width: 55%; vertical-align: top;}
	.tie_advanced .hours-times span.hours-right {display: inline-block; text-align: right; width: 45%; vertical-align: top;}
	.tie_advanced .plus_wrapper {position: absolute; top: 1rem; right: 5%; z-index: 1;}
	.plus_wrapper:hover + .hours-select-type.open li:first-child {background: #febc11;}
	.plus_wrapper:hover + .hours-select-type.open li {background: #f0f0f0;}
	.plus_wrapper:hover + .hours-select-type li {background: #febc11;}
	.tie_advanced .hours-times span.hours-left {display: block; width: 100%; font-weight: 400;}
	.tie_advanced .hours-times span.hours-right {display: block; width: 100%; text-align: left;}
      </style>
      <script language="javascript" type="text/javascript">
       $( document ).ready(function() {
		loadHours("mt");
		$(".hours-select-type #mt").addClass('active');
		$(".tie_advanced .plus_wrapper").click(function(){
			var ul = $(this).next();
			var sel;
			if(ul.hasClass("open")){
				sel = "li:first-child";	
			}else{
				sel = "li:visible";
			}
			$(this).next(".hours-select-type").children(sel).click();
		});

		$(".hours-select-type li").click(function(){
			var ul = $(this).parent("ul");
			var siblings = $(this).siblings('li');
			var id = $(this).attr("id");
			var plus = $(this).parent().prev(".plus_wrapper");
			var speed  = "fast";
			var index = 0;
		
			if ($(".hours-select-type li").hasClass('active')) {
				$(".hours-select-type li").removeClass('active');
			}
		
			// Open
			if(siblings.css('display') == "none"){
				ul.addClass("open");
				siblings.slideDown(speed);
				plus.addClass("minus");
				$(this).addClass('active');
			}
			
			// Close
			else{
				loadHours(id);
				ul.removeClass("open");
				siblings.slideUp(speed);
				plus.removeClass("minus");
				$(this).addClass('active');
			}	
		});
	
		// Load hours in
		function loadHours(id){	
			$.post("docs/hours/hours.php", {setNum: id}, function(data) {
				$(".hours-content ul li").remove();
				$(".hours-content ul").append(data);
			})
			.fail(function() {
				$(".hours-content ul li").remove();
				$(".hours-content ul").append("<li>Error loading. Please try again later.</li>");
			});
		}
	});
      </script>
