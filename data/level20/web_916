    <section aria-label="page header" id="hero">
     <div class="hero-container">
      <img alt="" class="d-none d-sm-block d-md-block d-lg-block d-xl-block img-fluid img-stretch" src="https://lgbtq.ecu.edu/wp-content/pv-uploads/sites/643/2017/12/LGBT-Banner-2000x500.jpg"/>
      <img alt="" class="d-sm-none d-md-none d-lg-none d-xl-none img-fluid" src="https://lgbtq.ecu.edu/wp-content/pv-uploads/sites/643/2017/12/LGBT-Banner-2000x500-768x384.jpg"/>
