  <script type="text/javascript">
   function googleTranslateElementInit() {
        new google.translate.TranslateElement({
            pageLanguage: 'en',
            autoDisplay: false,
            layout: google.translate.TranslateElement.InlineLayout.SIMPLE
        }, 'google_translate_element');

        function changeGoogleStyles() {
            if(jQuery('.goog-te-menu-frame').contents().find('.goog-te-menu2').length) {
                var maxWidth = '96%';

                if(jQuery(window).width() <= 480) {
                    var maxWidth = '367px';
                }
                else if(jQuery(window).width() < 375) {
                    var maxWidth = '312px';
                }

                jQuery('.goog-te-menu-frame').contents().find('.goog-te-menu2').css({
                    'max-width': maxWidth,
                    'overflow':'auto',
                    'box-sizing':'border-box',
                    'height':'auto',
                    '-webkit-overflow-scrolling': 'touch'
                }).find('a div').each(function() {
                    this.style.setProperty('background-color', 'transparent', 'important');

                    jQuery(this).find('span.text').css({
                        'color': '#a51c30',
                        'font-family': '"ProximaNova-Regular", sans-serif'
                    });
                });
            } else {
                setTimeout(changeGoogleStyles, 50);
            }
        }
        changeGoogleStyles();

        jQuery(window).resize(function() {
            changeGoogleStyles();
        })
    }
  </script>
