  <script type="text/javascript">
   $(".toplevel").click(function () {
		if ($(this).hasClass("hover")) {
			$(".toplevel").removeClass("hover");
		} else {
			$(".toplevel").removeClass("hover");
			$(this).toggleClass("hover");
		}
	});
  </script>
