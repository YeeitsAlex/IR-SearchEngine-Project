  <script type="text/javascript">
   function keepAliveImage() {
		if (document.getElementById('keepAliveImage')) {
			document.getElementById('keepAliveImage').src = '/assets/images/alive.png?x=' + Math.random();
		}
	}
	
	window.setInterval("keepAliveImage()", 30000);
  </script>
