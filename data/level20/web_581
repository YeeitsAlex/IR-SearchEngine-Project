    suggestion: Handlebars.compile('<div class="searchListItem"><a href="https://www.coop.utk.edu/?p={{ID}}" class="searchLink" aria-label="{{post_title}}"><span class="suggestion-head">{{post_title}}</span></a></div>')
  }
}).on('typeahead:selected', function(event, selection) {
  url = "https://www.coop.utk.edu/" + "?p=" + (selection.ID);
  window.location.href = url;
});
  </script>
