   <script>
    // ensures `$` is aliased for jQuery
(function ($) {
  // Home page banner
  var images = ['home-01.jpg', 'home-02.jpg', 'home-03.jpg', 'home-04.jpg', 'home-05.jpg', 'home-06.jpg', 'home-07.jpg'];
  $('.WhyATM').css({'background-image': 'url(/App_Themes/OGAPS/images/HomeBanner/' + images[Math.floor(Math.random() * images.length)] + ')'});
  
  // on document ready
  $(function () {
    // Site navigation
    $('#MenuV .Vertical > ul').BindMENU({
      MenuStyle: 'DefaultMenu',
      Orientation: 'Vertical',
      Event: 'hover',
      Speed: 'fast'
    });
    
    // Remove unfilled table rows
    $.each($('.table tr td:first-child'), function (i, v) {
      var $v = $(v);
      if (!$v.html().length) {
        $v.parent().remove();
      }
    });
    
    /* This function handles the Bootstrap bug of collapsed accordion panels not getting
     * the "collapsed" class added to them after being opened then closed again.
     */
    $('.accordion').on('hide', function (e) {
      $('.accordion-toggle', $(e.target).parent()).addClass('collapsed');
    })
  
    // Carousel
    $('.carousel').carousel({
      interval: false //5000
    });
    
    // Tooltips
    $('*[data-toggle=tooltip]').tooltip();
    
    // Popovers
    $('*[data-toggle=popover]').popover({
      trigger: 'hover'
    });
  });
})(jQuery);
   </script>
   <script type="text/javascript">
    //<![CDATA[

WebForm_InitCallback();
theForm.oldSubmit = theForm.submit;
