      <div class="s-lc-box-container s-lc-color-bars-border s-lc-box-borderless s-lc-box-type-1 s-lc-box-position-1" id="s-lc-box-31667-container">
       <div class="s-lc-box s-lc-box-html" id="s-lc-box-31667">
        <div class="s-lc-box-title s-lc-color-bars s-lc-color-bars-border">
         <h2>
         </h2>
        </div>
        <div class="s-lc-box-content clearfix">
         <div class="s-lc-content-evts s-lc-content" id="s-lc-content-evts-38888">
          <form action="#" method="get" onsubmit="return runSearch_38888();" role="search">
           <label class="sr-only" for="esq_38888">
            Search
           </label>
           <div class="input-group">
            <input aria-label="Search for event..." aria-required="true" autocomplete="off" class="form-control" id="esq_38888" name="q" placeholder="Search for event..." type="text"/>
            <span class="input-group-btn">
             <button class="btn btn-info" id="esqb_38888" type="submit">
              Search
             </button>
            </span>
           </div>
          </form>
          <div class="margin-top-med s-lc-search_res" id="search_res_38888">
          </div>
          <script>
           var template = "<div class=\"media s-lc-c-evt\">\n    <div class=\"media-body\"><h3 class=\"media-heading\"><a href=\"{{url}}\">{{title}}<\/a><\/h3>\n        <div class=\"s-lc-c-evt-des\">{{{shortdesc}}}\n            {{#shortdesc_was_truncated}}\n            &nbsp;<a href=\"{{url}}\">More<\/a><br>\n            {{\/shortdesc_was_truncated}}\n        <\/div>\n        <dl class=\"dl-horizontal\">\n            <dt>Date:<\/dt>\n            <dd>{{date}}\n                {{#future_dates.0}}\n                    <i class=\"fa fa-calendar pad-left-med\" aria-hidden=\"true\"><\/i>\n                    <a href=\"#\" onclick=\"jQuery('#future_{{id}}').toggle(); return false;\"> Show more dates<\/a>\n                    <p id=\"future_{{id}}\" style=\"display:none\">\n                        {{#future_dates}}\n                        <a href=\"\/event\/{{event_id}}\">{{start}}<\/a><br>\n                        {{\/future_dates}}\n                    <\/p>\n                {{\/future_dates.0}}\n            <\/dd>\n\n            <dt>Time:<\/dt>\n            <dd>{{start}}{{#end}} - {{end}}{{\/end}}<\/dd>\n\n            <dt>Location:<\/dt>\n            <dd>{{#location}}{{location}}{{#campus}}, {{\/campus}}{{\/location}}{{#campus}}{{campus}}{{\/campus}}<\/dd>\n\n            {{#registration_cost}}\n                <dt>Event Cost:<\/dt>\n                <dd>{{registration_cost}}<\/dd>\n            {{\/registration_cost}}\n\n            {{#audiences.0}}\n            <dt>Audience:<\/dt>\n            <dd>{{#audiences}}\n                <span class=\"s-lc-event-category-color\" style=\"background-color:{{color}}\">&nbsp;<\/span>\n                <span class=\"s-lc-event-category-link\">\n                    <a href=\"{{calendar_url}}&t=d&cal%5B%5D={{calendar_id}}&audience%5B%5D={{id}}\">{{name}}<\/a>\n                <\/span> &nbsp;\n                {{\/audiences}}\n            <\/dd>\n            {{\/audiences.0}}\n\n            {{#categories_arr.0}}\n            <dt>Categories:<\/dt>\n            <dd>{{#categories_arr}}\n                <span class=\"s-lc-event-category-color\" style=\"background-color:{{color}}\">&nbsp;<\/span>\n                <span class=\"s-lc-event-category-link\">\n                    <a href=\"{{calendar_url}}&t=d&d=0000-00-00&cal%5B%5D={{calendar_id}}&ct%5B%5D={{cat_id}}\">{{name}}<\/a>\n                <\/span> &nbsp;\n                {{\/categories_arr}}\n            <\/dd>\n            {{\/categories_arr.0}}\n\n            <dt><\/dt>\n            <dd>\n                {{#seats}}\n                <dt><\/dt>\n                <dd><a href=\"{{url}}\" class=\"btn btn-success btn-sm margin-top-med\">Register! {{#seatsleft}} - {{seatsleft}} seat{{#seatsleft_multiple}}s{{\/seatsleft_multiple}} left{{\/seatsleft}}<\/a><\/dd>\n                {{\/seats}}\n\n                {{#registration_msg}}\n                <dt><\/dt>\n                <dd><a href=\"{{url}}\" class=\"btn {{class}} btn-sm margin-top-med\">{{msg}}<\/a><\/dd>\n                {{\/registration_msg}}\n            <\/dd>\n        <\/dl>\n    <\/div>\n    {{#featured_image}}\n    <div class=\"media-right\"><a href=\"{{url}}\"> <img class=\"media-object img-thumbnail s-lc-c-evt-fi\" src=\"{{featured_image}}\" alt=\"{{title}}\"> <\/a><\/div>\n    {{\/featured_image}}\n<\/div>";
	
						jQuery("#esqb_38888").click(function() {
							runSearch_38888();
							return false;
						});
						jQuery("#esq_38888").keyup(function(e){
							if(e.keyCode == 13){
								runSearch_38888();
								return false;
							}
						});
						function runSearch_38888(){
							var q = jQuery("#esq_38888").val();
							var sr = jQuery("#search_res_38888");
							
							if(q.length < 2) {
								sr.html('').removeClass('s-lc-search_results');
								errorAlert("Search string must be at least 2 characters.");
								return false;
							}

							sr.html('<p class="text-center"><br><i class="fa fa-spinner fa-spin fa-3x" aria-hidden="true"></i></p>');

							$.ajax({
								url: "/process_search.php?site_id=14288&cal_id=8498&sType=1&perpage=25&q="+encodeURIComponent(q)+"&callback=?",
								dataType: "jsonp",
								success: function(searchData){
									
									var output = "";
									if(searchData.status == 0){
										output = "<p>"+searchData.error+"</p>";
									}
									if(searchData.status == 1){
										
										var results = searchData.results.length;
										if(results < 1){
											output = "<p class=\"s-lc-evt-search-no\">No results found!...</p>";
										}else{
											//Loop over each result
											for(var i=0;i<results;i++){
												output += Mustache.render(template, searchData.results[i]);
											}
										}

									}
									sr.html(output).addClass('s-lc-search_results');
								}
							});
							return false;
						}
          </script>
