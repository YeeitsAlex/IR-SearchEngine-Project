     <script type="text/javascript">
      jQuery(document).ready(function(){	
	var hashId = false;
	jQuery(function(){
		if (window.location.hash) 
		{
		  jQuery('.accordion').children('div').children('h3').each(function(i){
			var href = $(this).children('a').attr('href').slice(1).toLowerCase();
			if (href === window.location.hash.slice(1)) {
			  hashId = i;
			}
		  });
		  jQuery('.accordionClosed').children('div').children('h3').each(function(i){
			var href = $(this).children('a').attr('href').slice(1).toLowerCase();
			if (href === window.location.hash.slice(1)) {
			  hashId = i;
			}
		  });
		}
	});
	
	jQuery(function(){
      jQuery(".accordion").accordion({ 
	  header: "h3", 
	  autoHeight: false, 
	  heightStyle: "content", 
	  active:hashId
	  });	
	});
	
	jQuery(function(){
      jQuery(".accordionClosed").accordion({ 
	  header: "h3", 
	  autoHeight: false, 
	  heightStyle: "content", 
	  collapsible: true,
	  active:hashId  
	  });	
	});
	
   jQuery(function(){
      jQuery("#dialog").dialog({ autoOpen: false, show: "slide", hide: "slide", modal: true, width: 600, dialogClass: 'dropShadow' }); });
   jQuery(function(){
      jQuery(function(){ jQuery(".opener").click(function() { jQuery( "#dialog" ).dialog( "open" ); return false; }); }); });
   jQuery(function(){
	jQuery('#jQueryExpand').click(function(){
	  jQuery('.accordion .ui-accordion-content').not('.ui-accordion-content-active').show();
	  jQuery('.accordionClosed .ui-accordion-content').not('.ui-accordion-content-active').show();
    });
   });
   jQuery(function(){
	jQuery('#jQueryCollapse').click(function(){
	  jQuery('.accordion .ui-accordion-content').not('.ui-accordion-content-active').hide();
	  jQuery('.accordionClosed .ui-accordion-content').not('.ui-accordion-content-active').hide();
    });
   });
});
     </script>
