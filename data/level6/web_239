           $(document).ready(function(){
			
			
			$('iframe').each(function() {
				var url = $(this).attr("src");
				if (url.toLowerCase().indexOf("youtube") != -1) {
					if ($(this).attr("src").indexOf("?") > 0) {
						$(this).attr({
						"src" : url + "&wmode=transparent",
						"wmode" : "Opaque"});
					}
					else {
						$(this).attr({
						"src" : url + "?wmode=transparent",
						"wmode" : "Opaque"});
					}
				} 
			});

		
			
