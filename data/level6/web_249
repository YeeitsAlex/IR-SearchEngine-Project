          <script language="JavaScript" type="text/javascript">
           var help
    function helpwin(url) {
        var loc="https://ucanr.edu/seek/anrdirectoryinfo.cfm?index="+url;
        if(help && !help.closed) {
            help.close();
        }
        help = window.open(loc,"Help","height=700,width=650,menubar,resizable,scrollbars");
    }
