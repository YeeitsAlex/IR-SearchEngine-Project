        <style type="text/css">
         #qp {width:200px; position:fixed; left:-200px; top:0; bottom:0; z-index:999; transition:1s; background-color:#3BAAE3;}
#qp-close {border:none; background:#3BAAE3; color: white; display:block; width:100%; text-align:right; font-size: 1.5em;}
#qp-need-help {z-index: 998; position:fixed; left: -200px; top:50%; transform:translate(0,-50%);border-top-right-radius:6px; border-bottom-right-radius:6px;background:#1966A2; color: white;transition: 2s;   }
#qp-need-help:focus { left:0; }
#qwidget-button {background:#1966A2; color: white; font-weight:bold; margin: 12px;outline:none;border:none;border:5px solid transparent; }
#qwidget-button:focus { outline: 2px solid rgb(94, 158, 214);outline-offset: 2px;  }
#qwidget-button-hide { background:#1966A2;outline:none;border:none; }
#qwidget-button-hide:focus { outline: 2px solid rgb(94, 158, 214);outline-offset: 2px; }
#qp-need-help-mobile { z-index: 998; position:fixed;left:0; bottom: -200px;border-top-right-radius:6px; border-bottom-right-radius:6px;background:#1966A2;color: white;display:none; transition: 2s;  }
#qwidget-button-mobile { border:none; background:#1966A2;  margin: 12px 12px 12px 0; }
#close-qwidget {display:none;}
#close-need-help:hover { cursor:pointer; }
#close-need-help-mobile { cursor:pointer; }
@media only screen and (max-width: 768px) {
#qp-need-help {display:none;}
#qp-need-help-mobile {display:inline-block;}
#close-qwidget {display:inline;}
}
        </style>
        <script>
         $(document).ready(function(){
// determine which page
var pop_up_page = "";
var page_url = window.location.pathname;
switch(page_url) {
case '/langson':
pop_up_page = "langson";
break;

case '/ayala':
pop_up_page = "ayala";
break;

case '/how-do-i':
pop_up_page = "how-do-i";
break;

default:
pop_up_page = "index";
}
// check if no cookie has been set. if none, pop up the qwidget
if (typeof window.sessionStorage != undefined) {
sessionvar = "UCILibMessage"+pop_up_page;
if (!(sessionStorage.getItem(sessionvar))) {
sessionStorage.setItem(sessionvar, true);
setTimeout(function(){
$('#qp-need-help').css('left','0');
}, 15000);
setTimeout(function(){
$('#qp-need-help-mobile').css('bottom','0');
}, 15000);
}
}
$('#qwidget-button-hide').click(function(event) {
event.preventDefault();
$('#qp-need-help').css('transition','2s');
$('#qp-need-help').css('left','-200');
});

$('#close-need-help-mobile').click(function(event) {
event.preventDefault();
$('#qp-need-help-mobile').css('bottom','-200');
});

$('#close-qwidget').click(function(event) {
	$('#qwidget-button').css('left', '-200px');
	event.preventDefault();
	$('#qwidget-button-mobile').css('bottom', '-200px');
	event.preventDefault();
});

$('#qp-close').click(function() {
	$('#qp').css('left', '-200px');
});

$('#qwidget-button').focus(function() {
$('#qp-need-help').css('left','0');
$('#qp-need-help').css('transition','none');
});

});
        </script>
