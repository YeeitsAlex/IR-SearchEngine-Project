      function redirect() {
    var searchitem = document.getElementById("q");
    var searchurl = "https://uci.primo.exlibrisgroup.com/discovery/search?query=any,contains," + searchitem.value + "&tab=Everything&search_scope=MyInst_and_CI&vid=01CDL_IRV_INST:UCI&offset=0";
    window.open(searchurl,"");
    }
