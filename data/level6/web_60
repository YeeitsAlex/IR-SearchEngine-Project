<script>
 function loadCEPNews() {
  var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4 && this.status == 200) {
      document.getElementById("CEPNews").innerHTML =
      this.responseText;
    }
  };
  xhttp.open("GET", "http://cep2.ucmerced.edu/index_AJAX_CEPNews.cfm?r=Math.random()", true);
  xhttp.send();
}
</script>
