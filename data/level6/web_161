  <script>
   <!--//--><![CDATA[// ><!--

// Add target to certain slider links.
// .views-field-field-spotlight-link = display:none set in style-sheet.
var spotlight_links = document.getElementsByClassName('spotlight-link'); //class added on content type.
for(i=0; i<spotlight_links.length; i++) {
	if(spotlight_links[i].getAttribute("target") == "_blank") {
		if(spotlight_links[i].parentNode.parentNode.parentNode.classList.contains("flexslider-views-slideshow-main-frame-row")) {
			spotlight_links[i].parentNode.parentNode.parentNode.getElementsByClassName("spotlight-title-link")[0].target="_blank";
			//spotlight_links[i].parentNode.parentNode.parentNode.getElementsByClassName("spotlight-img-link")[0].target="_blank";
		}
	}
}
// Add style 
var enhancedSpotlights=document.getElementsByClassName("enhanced-spotlight");
for(i=0; i<enhancedSpotlights.length; i++) {
	enhancedSpotlights[i].parentNode.parentNode.classList.add("enhanced-spotlight");
}
function spotlightDropper(event,spotlightArray) {
	if(document.body.contains(document.getElementById("views_slideshow_controls_text_pause_news-block_1_1"))) {
		if(!event.target.parentNode.parentNode.getElementsByClassName("spotlightDrop")[0].classList.contains("spotlightDropped")) {
			document.getElementById("views_slideshow_controls_text_pause_news-block_1_1").getElementsByTagName("a")[0].click();
		}
	}
	if(event.target.parentNode.parentNode.getElementsByClassName("spotlightDrop")[0].classList.contains("spotlightDropped")) {
		event.target.parentNode.parentNode.getElementsByClassName("spotlightDrop")[0].classList.remove("spotlightDropped");
		for(i=0; i<spotlightArray.length; i++) {
			event.target.parentNode.parentNode.getElementsByClassName("spotlightDrop")[0].classList.remove(spotlightArray[i]);
		}
		event.target.classList.remove("spotlightDropped");
	}
	else {
		event.target.parentNode.parentNode.getElementsByClassName("spotlightDrop")[0].classList.add("spotlightDropped");
		for(i=0; i<spotlightArray.length; i++) {
			event.target.parentNode.parentNode.getElementsByClassName("spotlightDrop")[0].classList.add(spotlightArray[i]);
		}
		event.target.classList.add("spotlightDropped");
	}
}

//--><!]]>
  </script>
