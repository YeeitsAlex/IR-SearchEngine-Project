setTimeout(function(){ eventTitleHacks(); }, 200);
function eventTitleHacks() {
	// Handle alternate titles
	var event_alternate_titles = document.getElementsByClassName('event-alternate-title');
	for(i=0; i<event_alternate_titles.length; i++) {
		event_alternate_titles[i].parentNode.parentNode.getElementsByClassName('event-title-link')[0].innerHTML = event_alternate_titles[i].innerHTML;
	}
}

setTimeout(function(){ manageEventHacks(); }, 210);
function manageEventHacks() {
	//Handle events with a particular title
	var event_titles = document.getElementsByClassName('event-title-link');
	for(i=0; i<event_titles.length; i++) {
		switch(event_titles[i].innerHTML) {
			case "ChangeThisTitlesDateTimeLocationField":
				event_titles[i].target="_self";
				event_titles[i].parentNode.parentNode.parentNode.getElementsByClassName('views-label-field-date')[0].innerHTML = '<span style="color:#f26d04">NEW DATE: ';
				event_titles[i].parentNode.parentNode.parentNode.getElementsByTagName('time')[0].innerHTML = "March 7th, 8th, 9th & 15th";
				event_titles[i].parentNode.parentNode.parentNode.getElementsByTagName('time')[1].innerHTML = "Noon to 1:00 pm";
				event_titles[i].parentNode.parentNode.parentNode.getElementsByTagName('time')[1].parentNode.parentNode.style.display = "none";
				event_titles[i].parentNode.parentNode.parentNode.getElementsByClassName('views-field-field-location')[0].getElementsByClassName('field-content')[0].innerHTML = 'Laurel Heights, Mission Bay, ZSFG, Parnassus';
				break;
			case "Medical Care of Vulnerable and Underserved Populations 2019":
				event_titles[i].parentNode.parentNode.parentNode.getElementsByTagName('time')[0].innerHTML = "Feb. 28th - Mar. 2nd";
				break;
			case "Women's Career Advancement Symposium ":
				event_titles[i].parentNode.parentNode.parentNode.getElementsByTagName('time')[0].innerHTML = "March 6th & 7th 2019";
				event_titles[i].parentNode.parentNode.parentNode.getElementsByTagName('time')[1].innerHTML = "<em>See the schedule.";
				break;
			case "Addressing Bullying and Abusive Conduct":
				event_titles[i].parentNode.parentNode.parentNode.getElementsByTagName('time')[0].innerHTML = "March 7th, 8th, 9th & 15th";
				event_titles[i].parentNode.parentNode.parentNode.getElementsByTagName('time')[1].innerHTML = "Noon to 1:00 pm";
				event_titles[i].parentNode.parentNode.parentNode.getElementsByClassName('views-field-field-location')[0].getElementsByClassName('field-content')[0].innerHTML = 'Laurel Heights, Mission Bay, ZSFG, Parnassus';
				break;
			case " 18th Annual Developmental Disabilities Update":
				event_titles[i].parentNode.parentNode.parentNode.getElementsByTagName('time')[0].innerHTML = "March 14th & 15th 2019";
				event_titles[i].parentNode.parentNode.parentNode.getElementsByTagName('time')[1].innerHTML = "<em>See the schedule.";
				break;
			default:
				break;
		} 
	}
}

// Expand/collapse event list
setTimeout(function(){ manageEventRows(); }, 220);
function manageEventRows() {
	var initialEventsList=7;
	var eventRows=document.getElementsByClassName("view-id-events")[0].getElementsByClassName("views-row");
	if (eventRows.length > initialEventsList) {
		document.getElementsByClassName("more-events-link")[0].classList.remove("more-events-link-hidden");
		if(document.getElementById("expand-events-list").classList.contains("events-shown")) {
			for(i=0; i<eventRows.length; i++) {
				if(i>=initialEventsList) {
					eventRows[i].style.display="none";
				}
				if(i==initialEventsList-1) {
					eventRows[i].classList.add("events-expanded");
				}
			}
			document.getElementById("expand-events-list").classList.remove("events-shown");
			document.getElementById("expand-events-list").classList.add("events-hidden");
			document.getElementById("expand-events-list").innerHTML="more »";
		}
		else {
			for(i=0; i<eventRows.length; i++) {
				if(i>=initialEventsList) {
					eventRows[i].style.display="block";
				}
				if(i==initialEventsList-1) {
					eventRows[i].classList.remove("events-expanded");
				}
			}
			document.getElementById("expand-events-list").classList.add("events-shown");
			document.getElementById("expand-events-list").classList.remove("events-hidden");
			document.getElementById("expand-events-list").innerHTML="« less";
		}
	}
	else {
		document.getElementsByClassName("more-events-link")[0].classList.add("more-events-link-hidden");
		document.getElementsByClassName("view-id-events")[0].getElementsByClassName("views-row-last")[0].classList.add("more-events-link-hidden");
	}
}

//--><!]]>
    </script>
