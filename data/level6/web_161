    <script>
     <!--//--><![CDATA[// ><!--

//document.getElementById("sizer").innerHTML='<span style="font-size:1em">' + window.innerWidth + ' x '  + window.innerHeight + ' x '  + window.devicePixelRatio + ' load';
document.getElementsByTagName("HTML")[0].setAttribute("lang", "en");
document.getElementById("mobile-link").getElementsByTagName("SPAN")[0].innerHTML="Mobile main menu";
theParent = document.getElementsByClassName("header-region");
theKid = document.getElementsByClassName("block-search");
theKidClone = theKid[0].cloneNode(true);
theKidClone.classList.add("block-search-clone");
theKidClone.classList.remove("block-search");
theKidClone.getElementsByTagName("LABEL")[0].parentNode.removeChild(theKidClone.getElementsByTagName("LABEL")[0]);
theKidClone.getElementsByTagName("H2")[0].parentNode.removeChild(theKidClone.getElementsByTagName("H2")[0]);
theKid[0].parentNode.removeChild(theKid[0]);
theSibling = document.getElementsByClassName("block-nice-menus");
complaintLink = document.createElement("A");
complaintLink.setAttribute("id", "complaintLink");
complaintLink.setAttribute("class", "complaint-link");
complaintLink.setAttribute("href", "http://ophd.ucsf.edu/complaint-resolution");
complaintLink.setAttribute("target", "_blank");
complaintLink.innerHTML="File a Discrimination/Harassment Complaint";
theLogo = document.getElementsByClassName("siteinfo");
theLogo = theLogo[0].getElementsByTagName("IMG");
windowSize()
window.addEventListener("resize",windowSize);
function windowSize() {
	if(window.innerWidth < 815) {
		theParent[0].insertBefore(theKidClone, theSibling[0]);
		//theParent[0].insertBefore(complaintLink, theSibling[0]);
		theLogo[0].src="/sites/diversity.ucsf.edu/files/mobile_logo_white_bg.png";
	}
	else {
		theParent[0].parentNode.insertBefore(theKidClone, theParent[0]);
		//theParent[0].parentNode.insertBefore(complaintLink, theKidClone);
		theLogo[0].src="/sites/diversity.ucsf.edu/files/identity-logo.png";
	}
}

theBody = document.getElementsByTagName("BODY");
clickBody();
function clickBody() {
	theBody[0].addEventListener("click", function(e) {
		closeMenu(e,"doc");
	});
	theBody[0].addEventListener("touchstart", function(e) {
		//closeMenu(e,"doc");
	});

}

niceMenuLIs=document.getElementById("nice-menu-1").children;
for(i=0; i<niceMenuLIs.length; i++) {
	niceMenuLIs[i].addEventListener("click", function(e) {
		e.stopPropagation();
	});
	nmliChildren=niceMenuLIs[i].children;
	// https://css-tricks.com/dangers-stopping-event-propagation/
	for(j=0; j<nmliChildren.length; j++) {
		if(nmliChildren[j].tagName=="A") {
			innerHTML=nmliChildren[j].innerHTML;
if(innerHTML!="Donate") {
			nmliChildren[j].classList.add("no-click")
}
else {
			nmliChildren[j].parentNode.classList.add("menuparent")
}
			//innerHTML=nmliChildren[j].innerHTML;
			clonedLink=nmliChildren[j].cloneNode();
			clonedLink.innerHTML=innerHTML;
			//clonedLink.addEventListener("click", function(e) { e.preventDefault(); });
			niceMenuLIs[i].replaceChild(clonedLink, nmliChildren[j]);
		}
		if(nmliChildren[j].tagName=="UL") {
			nmliChildren[j].id="menu-child-" + i;
			nmliChildren[j].className="menu-child";
		}
		
	}
}
function closeMenu(e,thingy) {
	if(document.getElementsByClassName("down")[0]) {
		openMenuParent = document.getElementsByClassName("down");
		openMenuParent[0].classList.add("up");
		openMenuParent[0].classList.remove("down");
	}
}

var odoMondoMenuImages = document.getElementsByClassName("field-name-field-odo-mondo-menu-image")[0].getElementsByTagName("IMG");
var odoMondoMenuContents = document.getElementsByClassName("field-name-field-odo-mondo-menu-content")[0].getElementsByClassName("field-item");
var odoMondoMenuPadding = document.getElementsByClassName("field-name-field-odo-mondo-menu-padding")[0].getElementsByClassName("field-item");

var head = document.head;
var style = document.createElement('style');
style.type = 'text/css';
head.appendChild(style);
var css = '@media screen and (min-width:815px) {';

for(i=0; i<odoMondoMenuImages.length; i++) {
css += 'ul.nice-menu-down li.menuparent > ul#menu-child-' + i + '::before {';
css += 'background-image:url("' + odoMondoMenuImages[i].src + '");';
css += 'content:"' + odoMondoMenuContents[i].innerHTML + '";';
css += 'padding-top:' + odoMondoMenuPadding[i].innerHTML + '; padding-bottom:' + odoMondoMenuPadding[i].innerHTML + ';';
css += '}';
}
css += '}';
style.appendChild(document.createTextNode(css));

if(document.getElementsByClassName("field-name-field-header-image").length > 0) {
headerImg=document.getElementsByClassName("field-name-field-header-image")[0].getElementsByTagName("IMG")[0];
headerImgSrc="url(" + headerImg.src +")";
headerImgField=document.getElementsByClassName("field-name-field-header-image");
headerImgField[0].style.backgroundImage = headerImgSrc;
headerImgLink=document.getElementsByClassName("header-image-link");
headerImgField[0].appendChild(headerImgLink[0]);
}

//--><!]]>
    </script>
   </div>
  </div>
 </div>
