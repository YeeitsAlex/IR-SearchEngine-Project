      <h1 class="widget-title">
       Our Location
      </h1>
      <div id="sidebar-map" style="width: 270px; height: 260px;">
      </div>
      <script src="//maps.googleapis.com/maps/api/js?key=AIzaSyAYbR7nTJfkAcXTbt7EWa-0Pvu0vERl-A4&amp;sensor=false" type="text/javascript">
      </script>
      <script type="text/javascript">
       function initialize() {
							var mapOptions = {
							  zoomControl: true,
							  center: new google.maps.LatLng(44.9568426,-89.6315644),
							  mapTypeId: google.maps.MapTypeId.ROADMAP,
							  streetViewControl: false,
							  disableDefaultUI: true,
							  zoom: 15
	 						};

							var map = new google.maps.Map(document.getElementById('sidebar-map'),
								mapOptions);
					
							var MyPlace = new google.maps.LatLng(44.9568426,-89.6315644);
					
							var marker = new google.maps.Marker({
								position: MyPlace,
								url: 'https://marathon.extension.wisc.edu/office-map-directions/',
								title: 'Office Map and Directions',
								map: map
							});
					
							google.maps.event.addListener(marker, 'click', function() {
							  window.location.href = marker.url;
							});

						  }
						  google.maps.event.addDomListener(window, 'load', initialize);
      </script>
      <p>
       212 River Drive, Suite 3
       <br/>
       Wausau, WI 54403-5476
      </p>
