		slideshow.easing = "swing";
		slideshow.alwaysauto = true;
		slideshow.autoheight = false;
		slideshow.autoheight_max = false;
		slideshow.imgSpeed = 20;
		slideshow.navOpacity = 25;
		slideshow.navHover = 75;
		slideshow.letterbox = "#000000";
		slideshow.linkclass = "linkhover";
		slideshow.imagesid = "images";
		slideshow.info = "information";
		slideshow.infoonhover = 0;
		slideshow.infoSpeed = 10;
