							var mapOptions = {
							  zoomControl: true,
							  center: new google.maps.LatLng(43.426223,-88.175681),
							  mapTypeId: google.maps.MapTypeId.ROADMAP,
							  streetViewControl: false,
							  disableDefaultUI: true,
							  zoom: 16
	 						};

							var map = new google.maps.Map(document.getElementById('sidebar-map'),
								mapOptions);
					
							var MyPlace = new google.maps.LatLng(43.426223,-88.175681);
					
							var marker = new google.maps.Marker({
								position: MyPlace,
								url: 'https://washington.extension.wisc.edu/map/',
								title: 'Office Map and Directions',
								map: map
							});
					
							google.maps.event.addListener(marker, 'click', function() {
							  window.location.href = marker.url;
							});

						  }
						  google.maps.event.addDomListener(window, 'load', initialize);
