<!--<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js"></script>-->
<script>
 NORMALIZATION_TOKENS = "()|?! \t\n\r\".,";
sessionId = "2e325039-4304-11e9-bfe7-9bae0dbda2c9";
interfaceId = "2";
var lastWordSubmitted;
$(document).ready(function() {
    $("#question").autocomplete({
        source: function (request, response) {
            var inputString = request.term;
            var tokens = inputString.split(/[()|?! \t\n\r\".,]/);
            var newString = tokens[tokens.length-1].replace(lastWordSubmitted,"");
            if ((newString.length > 3) ||
                (NORMALIZATION_TOKENS.indexOf(inputString.charAt(inputString.length-1))!=-1 && NORMALIZATION_TOKENS.indexOf(inputString.charAt(inputString.length-2))==-1) )
                {
                    lastWordSubmitted=tokens[tokens.length-1];
                    $.getJSON("https://cornell.intelliresponse.com/IntelliSuggest?callback=?", {term: inputString, SESSIONID: sessionId, interfaceID: interfaceId}, function(data) {
                        response(data);
                    });
                }
        },
select: function (event, ui) {
      responseId = ui.item.secret;
      $("#responseId").val(responseId);
      // 8 for inquiries that came from intellisuggest
      $("#typeId").val('8');
      // clear out the question asked before submit so that we post only the specific responseId
      $("#question").val('');
      setTimeout(function() {
        // Option 1: redirect and behave as a GET
        window.location.replace("index.php?responseId=" + responseId + "&typeId=8");

        // Option 2: submit the form
        //$("#ask-submit").click();
        }, 500);
      //setTimeout(getResponse('+1+', '+2e325039-4304-11e9-bfe7-9bae0dbda2c9+', '+[8]+', '+[0]+', '+[typeId]+'), 1000);
      return false;
    },

    delay:200,
    minLength:2,
    focus: function() {
      return false;
    }
}).data("autocomplete")._renderItem = function( ol, item ) {
  return $( "<li></li>" )
  .data( "item.autocomplete", item )
  .append( "<a>"+ item.label + "</a>" )
  .appendTo( ol );
};
});
</script>

