  <script>
   window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());
        gtag('config', 'UA-36954622-1');
  </script>
  <script>
   $(document).ready(function(){
            //click action
            $(".btn-menu").click(function(){
                if( $("#menu-state").val() == 'open'){
                    $(".body").css("left", "0px");
                    $(".body").css("position", "relative");

                    $(".body").css("-webkit-transition", "all .5s ease-in-out");
                    $(".body").css("-moz-transition", "all .5s ease-in-out");
                    $(".body").css("-o-transition", "all .5s ease-in-out");
                    $(".body").css("transition", "all .5s ease-in-out");

                    $(".mobile_menu").delay(400).fadeOut();
                    $("#menu-state").val('closed');
                }else{
                    $(".body").css("position", "relative");
                    $(".body").css("-webkit-transition", "all .5s ease-in-out");
                    $(".body").css("-moz-transition", "all .5s ease-in-out");
                    $(".body").css("-o-transition", "all .5s ease-in-out");
                    $(".body").css("transition", "all .5s ease-in-out");
                    $(".mobile_menu").show();
                    $( ".body" ).animate({ "left": "-=250px" }, "fast" );
                    $("#menu-state").val('open');

                }

                //resize mobile menu to match the .body height
                $(".mobile_menu").css("height", $(document).height() + "px");

            });

            //resize action
            $(window).resize(function(){
                if( $(window).width() > 768){
                    //automatically close menu on resize on large browsers
                    $("#menu-state").val('closed');
                    $(".body").css("left", "0px");
                }else{
                    //resize mobile menu to match the .body height
                    //$(".mobile_menu").css("height", $(document).height() + "px");
                }
            });

//fix mobile caption
            $(document).ready(function(){
                if( $(window).width() <= 768){
                    var mypadding = 28;
                    var myheight = $(".heading_secondary h1").height();

                    myheight = myheight - mypadding;

                    $('.heading_secondary h1').css( 'top', '-'+ myheight + 'px' );

                }


                $(window).resize(function(){
                    if( $(window).width() <= 768){
                        var mypadding = 28;
                        var myheight = $(".heading_secondary h1").height();

                        myheight = myheight - mypadding;

                        $('.heading_secondary h1').css( 'top', '-'+ myheight + 'px' );

                    }
                });
            });



            //mobile menu accordion
            $('#mobile_menu').dcAccordion({
                eventType: 'click', // Event for activating menu - options are "click" or "hover"
                autoClose: true, // If set to "true" only one sub-menu open at any time
                saveState: false,/*saveState: true,*/
                disableLink: false,
                speed: 'fast',
                showCount: false, //count
                autoExpand: true,// If set to "true" all sub-menus of parent tags with class 'classExpand' will expand on page load
                cookie	: 'dcjq-accordion-1',
                classExpand	 : 'dcjq-current-parent'
            });
        });



        //carousel

        function EventListInit(carousel) {
            jQuery('.jcarousel-control a').bind('click', function() {
                carousel.scroll(jQuery.jcarousel.intval(jQuery(this).text()));
                return false;
            });

            jQuery('.jcarousel-scroll select').bind('change', function() {
                carousel.options.scroll = jQuery.jcarousel.intval(this.options[this.selectedIndex].value);
                return false;
            });

            jQuery('#upcoming-event-next').bind('click', function() {
                carousel.next();
                return false;
            });

            jQuery('#upcoming-event-prev').bind('click', function() {
                carousel.prev();
                return false;
            });
        };
        jQuery(document).ready(function() {
            jQuery('#upcoming-events-list').jcarousel({
                scroll: 1,
                initCallback: EventListInit,
                buttonNextHTML: null,
                buttonPrevHTML: null
            });
        });
  </script>
