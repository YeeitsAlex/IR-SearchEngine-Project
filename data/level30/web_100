   <script type="text/javascript">
    // 
		$(function(){
		
	var filtered = $('table.articleTable').filterTable({
	placeholder: 'Search articles',
	callback: function (term, table) {
		if (term === '') {
			table.find('tbody').hide();
			$('#instruction').show();
		} else {
			table.find('tbody').show();
			$('#instruction').hide();
		}
		
		($.debounce( 1500, function() {
					setHash(window.searchbox.val());
					if(table.find('tr:visible').length == 0 && window.searchbox.val().length > 1) {
						$('#askit_form:hidden').fadeIn();
					} else {
						$('#askit_form').hide();
					}
				} ))();
		table.find('tr').order(true, function(el){ var $el = $(el); return [$el.data('computed'), $el.text()]; }, function(){doStripe(table); table.find('tr:visible').each(function(){/*console.log($(this).data())*/}).removeData()}, window.searchbox.val() != 'all');
		
		$(".clearFilterTable")
			.addClass('btn-primary')
			.removeAttr("disabled");
	},
	label:''
	});

	window.searchbox = $(".filter-table input");
		/*.on('blur change', function() {
			setHash(window.searchbox.val());
		});
*/
	
	$(".filter-table")
		// .after($('<button style="margin-left:1em;" class="showall btn btn-default btn-sm">Show all</button>').on('click', function(){setQuery('all');}))
		.after($('<button class="clearFilterTable btn btn-sm btn-default" style="margin-bottom:10px;">Show all</button>').on("click", function() {
			setQuery('all');
			$( this ).removeClass('btn-primary').attr( "disabled", "disabled");
			window.searchbox.focus();
		}));


	filtered.find('tbody').hide();


	filtered.on('sorted', function () {
		/*doStripe(filtered);*/
	});

	$('.filters a').on('click', function(evt){
		evt.preventDefault();
		setQuery($(this).data('query'));
	});

	// do this LAST
	$(window).on('hashchange', function(evt){
		if (window.searchbox.val() != window.location.hash.replace('#search/', '')) {
			setQuery(decodeURIComponent(window.location.hash.replace('#search/', '')));
		}
		
	}).trigger('hashchange');
	window.searchbox.focus();
});

function setHash (query) {
	if (encodeURIComponent(window.searchbox.val()) == window.location.hash.replace('#search/', '')) {
		return;
	}
	if (query) {
		window.location.hash = 'search/' + encodeURIComponent(query);
	} else {
		removeHash();
	}
}
function setQuery(query) {
	if (!window.searchbox) {
		window.searchbox = $(".filter-table input");
	}
	window.searchbox.val(decodeURIComponent(query)).trigger("search");
}


function doStripe(table) {
	table.find('tr:visible').removeClass('striped').filter(':even').addClass('striped');
}

function removeHash () { 
    var scrollV, scrollH, loc = window.location;
    if ("pushState" in history)
        history.pushState("", document.title, loc.pathname + loc.search);
    else {
        // Prevent scrolling by storing the page's current scroll offset
        scrollV = document.body.scrollTop;
        scrollH = document.body.scrollLeft;

        loc.hash = "";

        // Restore the scroll offset, should be flicker free
        document.body.scrollTop = scrollV;
        document.body.scrollLeft = scrollH;
    }
}

// track searches in GA

function trackTAsearches(t) {
    function e(t, e, n) {
        ga(o + ".send", "event", {
            eventCategory: t,
            eventAction: e,
            eventLabel: n,
			transport: 'beacon',
            hitCallback: function() {}
        })
    }
    var n = window.location.hostname
      , i = n.replace(/-/g, "")
      , o = i.substring(0, i.indexOf("."));
    o = o + "_" + siteNameCounter,
    siteNameCounter += 1,
    ga("create", t, "auto", {
        name: o
    }),
    $(document).ready(function() {
        var t = window.location.href;
        $("a").on("click", function(evt) {
            $(this).parents("table").length ? e( "articleSearch", $('.filter-table input').val() , $(this).text() + "_" + this.href ) : e('articleSearch', "abandoned", $(this).text() + "_" + this.href);
			return true;
        })
    })
}

trackTAsearches('UA-18311705-2'); // ITS
trackTAsearches('UA-75143705-33') // UREL-new


