    <script type="text/javascript">
     var isMobile = false;
			
            // Bb supplied javascript function
            function loadLoginPage() {
                if (top != self) {
                    top.location.replace(self.location.href);
                }
                if (document.forms.login.user_id != undefined) {
                    document.forms.login.user_id.focus();
                }
                setTimeout("triggerScreenreaderAlert()", 500);
            }

            function triggerScreenreaderAlert() {
                if (document.getElementById('loginErrorMessage')) {
                    $('loginErrorMessage').update(
                            $('loginErrorMessage').innerHTML);
                }
            }

            // 
            /* function to process user action */
            function process(what) {
                if (what == 'netId') {
                    // mobile 'Cornell NetID' btn clicked
                    loginByNetId();
                } else if (what == 'CWID' ) {
                    // mobile 'Weill ID' btn clicked
                    loginByCWID();
                } else if (what == 'other' || what == 'blackboardId') {
                    // mobile 'Guest' btn or 'Blackboard ID' btn clicked
                    loginGuest();
                } else if (what == 'continue') {
                    // 'Continue' btn clicked
                    selectLoginOption();
                } else if (what == 'bbCancel') {
                    // bb login 'Cancel' btn clicked
					cancelLogin();					
                } else if (what == 'cwidLogin') {
                    // CWID login 'Login' btn clicked
                    submitCWIDLogin();
                } else if (what == 'bbLogin') {
                    // bb login 'Login' btn clicked
                    submitGuestLogin();
                } else {
                    alert("Error: unrecognized process parameter: " + what + " Please contact acadtech@cornell.edu");
                }
            }

            /* function to determine which login action to perform based on radio 
               btn selection - applies to desktop only */
            function selectLoginOption() {
                var fm = $('loginOptionsForm');
                var loginOption = "";
                for (i = 0; i < fm.loginOption.length; i++) {
                    if (fm.loginOption[i].checked) {
                        loginOption = fm.loginOption[i].value;
                        break;
                    }
                }

                if (loginOption == "netId") {
                    loginByNetId();
				} else if (loginOption == "CWID") {
                    loginByCWID();
                } else if (loginOption == "blackboardId") {
                    loginGuest();
                } else {
                    // this scenario should not occur
                    alert("Error: Login option selection failed. Please contact acadtech@cornell.edu wtih this error message.");
                }
 
                return false;
            }

            /* function to login using netId */
            function loginByNetId() {
               var target = ""
               try {
                  target = getHrefForShib();
               }  catch (ex) {
               }               
                if (target.length > 0) {
                    window.location.href = target;
                } else {
                    alert("Error: There was a problem logging you into Blackboard using your Cornell NetID. Please contact acadtech@cornell.edu");
                }
            }
			
            /* function to login using CWID */
            function loginByCWID() {
			    ensureIsCWIDLogin();
				document.forms.login.onsubmit = submitCWIDLogin;
				displayCWIDLoginMsg();
			    showBlackboardLogin();
            }
			
			function displayCWIDLoginMsg() {
				$('cwidMsg').style.display = "block";
			}
			
            /* Build flag for CWID login as hidden form field document fragment */
            /* input class='' type="hidden" name="isCWIDLogin" value="isCWIDLogin" */
            function buildIsCWIDLoginEl() {
                var docFrag = document.createDocumentFragment();
                var newField = document.createElement("input");
                newField.setAttribute("type", "hidden");
                newField.setAttribute("name", "isCWIDLogin");
                newField.setAttribute("value", "isCWIDLogin");
                newField.setAttribute("id", "isCWIDLogin");
                docFrag.appendChild(newField);
                return docFrag;
            }

            /* Take documentFragment of a CWID Login hidden form field and insert */
            function insertIsCWIDLoginEl(frag) {
                var parentEl = document.getElementById("loginFormFields").parentNode;
				var submitEl = document.getElementById("loginFormFields");
				var insertedEl;
				
				insertedEl = parentEl.appendChild(frag);

                return insertedEl;
            }

            /* Ensure CWID Login hidden form field is created only if it doesn't exist */
            function ensureIsCWIDLogin() {
                var isCWIDLogin = document.getElementById("isCWIDLogin");
                var docFrag = null;
                if (!isCWIDLogin) {
                    docFrag = buildIsCWIDLoginEl();
                    isCWIDLogin = insertIsCWIDLoginEl(docFrag);
                } else if (isCWIDLogin.value != "isCWIDLogin")
					isCWIDLogin.value = "isCWIDLogin";
            }

            /* function to submit the CWID login form */
            function submitCWIDLogin() {
               transformCWIDtoBbUser();
               submitBlackboardLogin();
            }

            /* function to submit the Guest login form */
            function submitGuestLogin() {
               submitBlackboardLogin();
            }			
			
            /* function to submit the blackboard login form */
            function submitBlackboardLogin() {
                var fm = $('bbLoginForm');
                if (validate_form(fm)) {
                    fm.submit();
                }
            }

            /* function to display the blackboard login form */
            function showBlackboardLogin() {
                $('loginOptionsMobile').style.display = "none";
                ensureCancelButton();
                $('loginBox').style.display = "block";

                return false;
            }
			
            /* function to display the blackboard login form */
            function loginGuest() {
                showBlackboardLogin();
				$('guestMsg').style.display = "block";

                return false;
            }

            /* function to cancel (hide) the CWID login */
            function cancelLogin() {
			    var iscwidlogin = $('isCWIDLogin');
					
				if ((document.forms.login.isCWIDLogin != undefined) && (iscwidlogin.value == "isCWIDLogin")){	
				    cancelCWIDLogin();
				} else {
				    cancelGuestLogin();
				}

                return false;
            }				
			
            /* function to cancel (hide) the CWID login form */
            function cancelCWIDLogin() {
                var iscwidloginEl = document.getElementById("isCWIDLogin")
			    var parentEl = document.getElementById("isCWIDLogin").parentNode;
				if ((typeof iscwidloginEl !== 'undefined') && (typeof parentEl !== 'undefined')) { 
				    parentEl.removeChild(iscwidloginEl);
				}

				$("cwidMsg").style.display = "none";
				cancelBlackboardLogin();

                return false;
            }
			
            /* function to cancel (hide) the Guest login form */
            function cancelGuestLogin() {
				$("guestMsg").style.display = "none";
				cancelBlackboardLogin();

                return false;
            }	

            /* function to cancel (hide) the blackboard login form */
            function cancelBlackboardLogin() {
                $('loginOptionsMobile').style.display = "block";
				
                $("loginBox").style.display = "none";

                return false;
            }
			
			/* function to convert a CWID to a Blackboard userid for Cornell Weill user */
			function transformCWIDtoBbUser() {
				var frm_username = document.forms.login.user_id.getValue();
				var cwid_ext = "@wcmc";
				
				if ((typeof frm_username !== 'undefined') && (frm_username.indexOf(cwid_ext) == -1)) {
					document.forms.login.user_id.setValue(frm_username + cwid_ext);
				} 		
			}	

            /* function to create cookie */
            function createCookie(name, value, days) {
                var expires = "";
                if (days) {
                    var date = new Date();
                    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                    var expires = "; expires=" + date.toGMTString();
                }
                document.cookie = name + "=" + value + expires + "; path=/";
            }

            /* function to read cookie */
            function readCookie(name) {
                var nameEq = name + "=";
                var ca = document.cookie.split(';');
                for (var i = 0; i < ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0) == ' ') {
                        c = c.substring(1, c.length);
                    }
                    if (c.indexOf(nameEq) == 0) {
                        return c.substring(nameEq.length, c.length);
                    }
                }
                return null;
            }

            /* function to erase cookie */
            function clearCookie(name) {
                createCookie(name, "", -1);
            }

            /* function to check if user hit enter key */
            function checkEnter(evt) {
                // TODO check this for mobile 
                var evt = (evt) ? evt : ((event) ? event : null);
                if (evt.keyCode == 13) {
                    if (isMobile) {
                    } else {
                        if ($('loginOptions')
                                && ($('loginOptions').style.display == "block" || $('loginOptions').style.display == "")) {
                            selectLoginOption();
						// Add an else if option that differentiates the Bb loginBox dialog (guest login) from CWID loginBox dialog
                        } else if ($('loginBox')
                                && ($('loginBox').style.display == "block") 
								&& ( $('isCWIDLogin') != null ) && ($('isCWIDLogin').value == "isCWIDLogin")){
                            submitCWIDLogin();							
                        } else if ($('loginBox')
                                && $('loginBox').style.display == "block") {
                            submitBlackboardLogin();
                        } else if (shibLogout) {
                            // window.location.href = "";
                            return false;
                        } else {
                            return false;
                        }
                    }
                }
            }
			
            /* Build cancel button as document fragment */
            /* input class='' type="button" name="cancel" value="Cancel"*/
            function buildCancelButton() {
                var docFrag = document.createDocumentFragment();
                var newBtn = document.createElement("input");
                newBtn.setAttribute("class", "btn");
                newBtn.setAttribute("type", "button");
                newBtn.setAttribute("name", "cancel");
                newBtn.setAttribute("value", "Cancel");
                newBtn.setAttribute("id", "loginCancelBtn");
                newBtn.setAttribute("onclick", "process('bbCancel')");
                docFrag.appendChild(newBtn);
                return docFrag;
            }

            /* Take documentFragment of a cancel button and insert */
            function insertCancelButton(frag) {
                var ul = document.getElementById("loginFormList");
                var inputEls = ul.getElementsByTagName("input");
                var i, inputType, inputName, submitEl, parentEl;
                var insertedEl;

                for (i = 0; i < inputEls.length; i++) {
                    inputType = inputEls[i].getAttribute("type");
                    inputName = inputEls[i].getAttribute("name");
                    if (inputType == "submit" && inputName == "login") {
                        submitEl = inputEls[i];
                        parentEl = submitEl.parentNode;
                        insertedEl = parentEl.insertBefore(frag, submitEl);
                        break;
                    }
                }
                return insertedEl;
            }

            /* Ensure cancel button is created only if it doesn't exist */
            function ensureCancelButton() {
                var cancelBtn = document.getElementById("loginCancelBtn");
                var docFrag = null;
                if (!cancelBtn) {
                    docFrag = buildCancelButton();
                    cancelBtn = insertCancelButton(docFrag);
                }

            }
			
            /* Retrieve the value of the href used to redirect to Shibboleth server */
            function getHrefForShib() {
                var ul = document.getElementById("loginRedirectProviderList");
                var aEls = ul.getElementsByTagName("a");
                var i, hrefVal;
                var anchorEl;
                var prefixToCheck = "/webapps/bb-auth-provider-shibboleth-";
                var prefixLen = prefixToCheck.length;
                anchorEl = aEls[0];
                hrefVal = anchorEl.getAttribute("href");
                if ((hrefVal) && (prefixToCheck != hrefVal.substr(0, prefixLen))) {
                      hrefVal = "";
                      alert("Error: Unable to get shibboleth authentication URL. Please contact acadtech@cornell.edu with this error message.");
                } 
                return hrefVal;
            }				
			
            document.onkeypress = checkEnter;
    </script>
