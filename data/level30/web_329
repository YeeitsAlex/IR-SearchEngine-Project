              <script>
               if (typeof(EventSearchBarInit) == 'undefined') {
        window["EventSearchBarInit"] = [{
            id: "sidebar-search",
            searchPath: "/event-search",
            searchMode: "sidebar-search",
            idkPath: "/search-idk"
        }];
    } else {
        EventSearchBarInit.push({
            id: "sidebar-search",
            searchPath: "/event-search",
            searchMode: "sidebar-search",
            idkPath: "/search-idk"
        });
    }
jQuery(document).ready(function(){
var newvariable= '0m';
if(jQuery("#datefrom").val()=="" && jQuery("#dateto").val()==""){
	newvariable= '0m';
}
else{
	newvariable= new Date(parseInt(jQuery("#dateto").val().split("/")[2]), parseInt(jQuery("#dateto").val().split("/")[0])-1, parseInt(jQuery("#dateto").val().split("/")[1]));
}
	if(jQuery("#datepicker").length){	
		jQuery("#datepicker").datepicker({
			minDate: 0,
			onChangeMonthYear: function(year, month, widget) {
				setTimeout(function(){
					if(jQuery("#datefrom").val()!="" && jQuery("#dateto").val()!=""){
								jQuery("#datepicker a").removeClass("ui-state-active");
								highlightmaincalendar(jQuery("#datefrom").val().split("/")[2]+"-"+jQuery("#datefrom").val().split("/")[0]+"-"+jQuery("#datefrom").val().split("/")[1], jQuery("#dateto").val().split("/")[2]+"-"+jQuery("#dateto").val().split("/")[0]+"-"+jQuery("#dateto").val().split("/")[1]);
					}
				},0);
			},
		   defaultDate: newvariable,
		   onSelect: function (dateText, inst) {
				var date = jQuery(this).val();
				if(jQuery("#datefrom").val()==""){
					jQuery("#datefrom").val(date);
				}
				else{
					if(dateText<jQuery("#dateto").val() && jQuery("#dateto").val()!=""){
						jQuery("#datefrom").val(date);
						jQuery("#dateto").val(date);
					}
					else{
					jQuery("#dateto").val(date);
						jQuery("#datepicker a").removeClass("ui-state-active");
						setTimeout(function(){
							highlightmaincalendar(jQuery("#datefrom").val().split("/")[2]+"-"+jQuery("#datefrom").val().split("/")[0]+"-"+jQuery("#datefrom").val().split("/")[1], jQuery("#dateto").val().split("/")[2]+"-"+jQuery("#dateto").val().split("/")[0]+"-"+jQuery("#dateto").val().split("/")[1]);
						},0);
					}
				}
			}
		});
		jQuery("#datepicker a").removeClass("ui-state-active");
		jQuery("#datefrom").datepicker({
			minDate: 0,	
			beforeShow: function (textbox, instance) {
			   var txtBoxOffset = jQuery(this).offset();
			   var top = txtBoxOffset.top;
			   var left = txtBoxOffset.left;
			   var textBoxWidth = jQuery(this).outerWidth();
					   setTimeout(function () {
						   instance.dpDiv.css({
							   top: top+43, 
					   });
				   }, 0);
			},		
			onSelect: function(dateText, inst){
				var date = jQuery(this).datepicker('getDate'),
				day  = date.getDate(),  
				month = date.getMonth(),              
				year =  date.getFullYear();
				jQuery("#datepicker a").removeClass("ui-state-active");
				jQuery("#datepicker.hasDatepicker .ui-datepicker-calendar tr td[data-month='"+month+"'][data-year='"+year+"'] a:contains('"+day+"')").filter(function () { return jQuery(this).text() == day; }).addClass("ui-state-active");
		}
		});	
		jQuery("#dateto").datepicker({
			minDate: 0,
			beforeShow: function (textbox, instance) {
			   var txtBoxOffset = jQuery(this).offset();
			   var top = txtBoxOffset.top;
			   var left = txtBoxOffset.left;
			   var textBoxWidth = jQuery(this).outerWidth();
					   setTimeout(function () {
						   instance.dpDiv.css({
							   top: top+42,
					   });
				   }, 0);
			},
			onSelect: function(dateText, inst){			
				if(dateText<jQuery("#datefrom").val()){
					alert("Please select date greater or equal to "+ jQuery("#datefrom").val());
					jQuery("#dateto").val("");
				}
				else if(jQuery("#datefrom").val()==""){
					alert("Please select from date first");
					jQuery("#dateto").val("");
				}
				else{
					jQuery("#datepicker a").removeClass("ui-state-active");
					highlightmaincalendar(jQuery("#datefrom").val().split("/")[2]+"-"+jQuery("#datefrom").val().split("/")[0].toString()+"-"+jQuery("#datefrom").val().split("/")[1], dateText.split("/")[2]+"-"+dateText.split("/")[0].toString()+"-"+dateText.split("/")[1]);
				}
			}
		});	
	}
	if(jQuery("#datefrom").val()!="" && jQuery("#dateto").val()!=""){
			setTimeout(function(){
				jQuery("#datepicker a").removeClass("ui-state-active");
				highlightmaincalendar(jQuery("#datefrom").val().split("/")[2]+"-"+jQuery("#datefrom").val().split("/")[0]+"-"+jQuery("#datefrom").val().split("/")[1], jQuery("#dateto").val().split("/")[2]+"-"+jQuery("#dateto").val().split("/")[0]+"-"+jQuery("#dateto").val().split("/")[1]);
			},500);
		}
		mobileconversion("#mobile-container","#sidebar-search",992);
});

function highlightmaincalendar(startDate, endDate){
		var listDate = [];		
		var dateMove = new Date(startDate);
		var strDate = startDate;
		while (strDate < endDate){
		  var strDate = dateMove.toISOString().slice(0,10);
		  listDate.push(strDate);
		  dateMove.setDate(dateMove.getDate()+1);
		};
	 for(var i=0;i<listDate.length;i++){
		var day = listDate[i].split("-")[2],  
			month = parseInt(listDate[i].split("-")[1])-1,
			year =  listDate[i].split("-")[0];
			day = day.replace(/^0+/, '');
		 jQuery("#datepicker.hasDatepicker .ui-datepicker-calendar tr td[data-month='"+month+"'][data-year='"+year+"'] a:contains('"+day+"')").filter(function () { return jQuery(this).text() == day; }).addClass("ui-state-active");
	 }
}


function mobileconversion(mobile_ele,move_sele,width) {
  //mobile ele is the element the move sele needs to move to. Width is the window width that needs to respond to
  //this allows an element to move to a certain div on mobile and move back on breakpoint

  if(jQuery(move_sele).length==1 && jQuery(mobile_ele).length==1){
    origin_parent=jQuery(move_sele).parent()[0];
    //init moved on mobile if its desktop moved it should be 0
    
    moved=0;
;
    if(jQuery(window).width() < width) {
        temp=jQuery(move_sele).detach();
        jQuery(mobile_ele).prepend(temp);
        moved=1;
    }
    jQuery(window).resize(function(){
            if (jQuery(this).width()<width) {
        //do something if this is hasnt been moved to mobile
        if (moved==0) {
          temp=jQuery(move_sele).detach();
          jQuery(mobile_ele).prepend(temp);
          moved=1;

        }
      }
      else {
        if (moved==1) {
          temp=jQuery(move_sele).detach();
          jQuery(origin_parent).prepend(temp);
          moved=0;
        }
      }
    });/* 
      if (jQuery(this).width()<width) {
        //do something if this is hasnt been moved to mobile
        if (moved==0) {
          temp=jQuery(move_sele).detach();
          jQuery(mobile_ele).prepend(temp);
          moved=1;

        }
      }
      else {
        if (moved==1) {
          temp=jQuery(move_sele).detach();
          jQuery(origin_parent).prepend(temp);
          moved=0;
        }
      }*/
    };
  }
              </script>
             </div>
