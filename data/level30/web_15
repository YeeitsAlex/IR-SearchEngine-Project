   //To play hidden audio/video content in colorbox
jQuery(".emuseum-colorbox-inline").colorbox({
width: "auto",
height:"auto",
maxWidth:"100%",
maxHeight:"100%",
inline: true,
href: function() {
var elementID = $(this).attr('id');
return '#hidden-' + elementID;
}
});
  </script>
  <script type="text/javascript">
   var $ = jQuery;
Tapestry.onDOMLoaded(function() {
Tapestry.init({"formEventManager":[{"formId":"searchForm","validate":{"submit":false,"blur":false}},{"formId":"viewForm","validate":{"submit":false,"blur":false}}]});
});
  </script>
