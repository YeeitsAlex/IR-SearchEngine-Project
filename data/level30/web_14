    function setHeight(){
  var windowWidth = $(window).width();
  if(windowWidth > 959){
    var mainHeight = $('#main-body').innerHeight();
    $('#secondary, #secondary-nav').innerHeight(mainHeight);
  } else{
    $('#secondary, #secondary-nav').innerHeight('auto');
  }
}

$(window).resize( function(){
 setHeight();
} );


