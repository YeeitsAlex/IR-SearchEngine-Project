   jQuery(function($){
$('#number_slideshow').on('shown', function(e){
$('.looper-nav > li', this)
.removeClass('active')
.eq(e.relatedIndex)
.addClass('active');
});
});
  </script>
  <script>
   jQuery(function($){
$('#photo_cycle_container').on('shown', function(e){
$('.looper-nav > li', this)
.removeClass('active')
.eq(e.relatedIndex)
.addClass('active');
});
});
  </script>
