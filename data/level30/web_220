  </script>
  <script src="/commonspot/javascript/lightbox/window_ref.js" type="text/javascript">
  </script>
  <script src="/commonspot/pagemode/always-include-common.js" type="text/javascript">
  </script>
  <script>
   $(function(){
$(".menuButton").click(function(){
$("#mobileMenu").toggleClass("open");
$("#mobileSearch").removeClass("open");
return false;
});
$("#mobileMenu ul li").has("ul").each(function() {
$(this).children("a:first-of-type").addClass("isParent");
});
$("#mobileMenu ul li a.isParent").click(function() {
$("#mobileMenu ul li").not($(this).parent()).removeClass("open");
$(this).parent().toggleClass("open");
if ( $(this).parent().hasClass("showSubmenuPreview") ) {
$(this).parent().removeClass("showSubmenuPreview");
$(this).parent().children(".submenuViewAll").css("display","none");
}
return false;
});
$(".showSubmenuPreview").each(function() {
$(this).append("<a href='#' class='submenuViewAll'>View all Quick Links</a>");
});
$(".submenuViewAll").click(function() {
$(this).parent().addClass("open");
$(this).parent().removeClass("showSubmenuPreview");
$(this).css("display","none");
});
$(".searchButton").click(function() {
$("#mobileSearch").toggleClass("open");
$("#mobileMenu").removeClass("open");
return false;
});
$(".showSecondaryMenu").click(function(){
$(this).hide();
$(".hideSecondaryMenu").show();
$("#nav_primary").addClass("open");
return false;
});
$(".hideSecondaryMenu").click(function(){
$(this).hide();
$(".showSecondaryMenu").show();
$("#nav_primary").removeClass("open");
return false;
});
});
  </script>
  <!-- end (1) PrimaryResources -->
