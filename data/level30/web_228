    <div class="container">
     <div class="row">
      <h2 id="news-and-events">
       News and Events
      </h2>
      <script type="text/javascript">
       // 
$(function (){
$.ajax({
url: '//campusdata.uark.edu/apiv2/articles/articlesCommon?$orderby=publishDate+desc&$filter=Commons/any(c:+c/id+eq+1)&$top=3',
data: {},
contentType: "application/json; charset=utf-8",
dataType: 'jsonp',
//jsonpCallback: 'Articles',
async: "true",
cache: "true",
success: function (articles) {
dataDisplay = "";
$.each(articles, function (key, a) {
var briefText = "", image = "";
var date = new Date(a.publishDate);
date = date.toDateString();
if (a.brief != null && a.brief != "") { briefText = a.brief;  briefText = briefText.replace(/(<p>|<\/p>)/g, "");}
if (a.image != null && a.image != "") {
image = a.image;
} else { image = "PMUA_Logo-web.jpg"; }
dataDisplay = "<div class='col-md-4 col-sm-12'><div class='media'>" +
"<div class='media-body'>" +
"<h4 class='media-heading'><a href='http://news.uark.edu/articles/" + a.id + "'>" + a.headline + "</a></h4>" + briefText + 
"</div></div><br>";

$("#newsArticles").append(dataDisplay);
});
}
});

});
//
      </script>
      <div id="newsArticles">
      </div>
