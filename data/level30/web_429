    <div id="featuresRow">
     <div id="featuresArea">
      <div class="region region-todayshours">
       <div class="views-element-container block block-views block-views-blocktodays-hours-todays-hours-block clearfix" id="block-views-block-todays-hours-todays-hours-block">
        <h2 class="block-title">
         Today's Hours
        </h2>
        <div class="form-group">
         <div class="view view-todays-hours view-id-todays_hours view-display-id-todays_hours_block js-view-dom-id-8551f6b355192b8ddf1ce94b98f848c483f6bbb9451ffd27ba22070aa09ea809">
          <div class="view-content">
           <div class="hours-index views-row">
            <h4>
             Main Building:
             <br/>
             <b>
              10:00 AM - 4:00 PM
             </b>
            </h4>
            <h4>
             Pharmacy:
             <br/>
             <b>
              10:00 AM - 4:00 PM
             </b>
            </h4>
           </div>
          </div>
          <div class="view-footer">
           <br/>
           <ul>
            <li>
             <strong>
              <a href="/get-care/hours">
               Hours for Specific Services
              </a>
             </strong>
            </li>
