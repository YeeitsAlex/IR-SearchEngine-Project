   <section aria-label="Homepage Sub-Footer" id="subfooterRow">
    <div id="subfooterArea">
     <div class="region region-subfooter">
      <div class="views-element-container block block-views block-views-blockhome-page-subfooter-block-1 clearfix" id="block-views-block-home-page-subfooter-block-1">
       <div class="form-group">
        <div class="cuhsubfooter view view-home-page-subfooter view-id-home_page_subfooter view-display-id-block_1 js-view-dom-id-3ded4fa3476a1c41cd4bc31da3a1e10a460b6ec0f1573cd6221ef34e6cef9586">
         <div class="view-content">
          <div class="views-view-grid horizontal cols-3 clearfix">
           <div>
            <div class="views-col col-1">
             <div class="views-field views-field-field-link-icon">
              <div class="field-content">
               <img alt="how do I?" class="img-responsive" height="38" src="/sites/health/files/2016-08/icon_how.png" typeof="foaf:Image" width="38"/>
              </div>
             </div>
             <div class="views-field views-field-title">
              <span class="field-content">
               How do I...
              </span>
             </div>
             <div class="views-field views-field-body">
