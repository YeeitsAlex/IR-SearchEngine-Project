                 <section class="cu_events-block_4 paragraph paragraph--type--cu-views-display paragraph--view-mode--default anchored" id="views-display-5802">
                  <div class="views-conditional-container container">
                   <div class="field field--name-field-cu-view-content field--type-viewfield field--label-hidden">
                    <div class="viewfield-wrapper">
                     <div class="viewfield">
                      <div class="view view-cu-events view-id-cu_events view-display-id-block_4 js-view-dom-id-888da33fa2d4bcde72f414c2f9005db15830c806b4a8b0d0ba4d7f89e14f9269">
                       <div class="view-content">
                        <div class="row">
                         <div class="views-row col-sm-6 col-md-4 col-lg-4">
                          <article about="/events/memorial-day" class="cu-event teaser clearfix row events" role="article">
                           <div class="date-wrapper col-xs-3 col-sm-3 col-md-3 col-lg-2 text-center">
                            <div class="date-range-wrapper">
                             <div class="date">
                              <a href="/events/memorial-day" rel="bookmark">
                               <span class="month">
                                May
                               </span>
                               <span class="day">
                                27
                               </span>
                               <abbr class="weekday" title="Monday">
                                Mon
                               </abbr>
                              </a>
                             </div>
                            </div>
                           </div>
