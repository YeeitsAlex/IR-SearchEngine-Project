         <script type="text/javascript">
          (function($){
    $(document).ready(function(){
      var box = $('#video-lightbox');
      $('.close', box).click(function(){
        $('iframe', box).attr('src', '');
        box.hide();
      });
      $('a[href]').each(function(i, e){
        var match = $(e).attr('href').match(/^https:\/\/youtu\.be\/([^\/]+)/);
        if (match){
          $(e).click(function(ev){
            ev.preventDefault();
            var $video_id = match[1];
            $.ajax({
              url: 'https://www.googleapis.com/youtube/v3/videos?id=' + $video_id + '&key=AIzaSyBb4DAmY88xmkhmD1FTRcG58l2E1FpKflY&part=snippet'
            }).done(function(data){
              $('iframe', box).attr('src', 'https://www.youtube.com/embed/' + $video_id + '?showinfo=0');
              $('.video-title', box).text(data.items[0].snippet.title);
              $('.video-description', box).text(data.items[0].snippet.description);
              box.focus().show();
            });
          });
        }
      });
    })
  })(jQuery)
