     <div class="span4">
      <div class="section-block widget widget-news color-grey">
       <script type="text/javascript">
        $(function () {
                                $.ajax({
                                    url: 'https://campusdata.uark.edu/apiv2/articles?$orderby=publishDate+desc&$top=3&$filter=%28Schools/any%28s1:+s1/id+eq+1%29+or+Topics/any%28t1:+t1/id+eq+3%29%29',
                                    data: {},
                                    dataType: 'jsonp',
                                    cache: 'true',
                                    success: function (articles) {
                                        $.each(articles, function (key, a) {
											var image = '';
											if(a.image != null && a.image != "") {
												image = a.image;
											} else {
												image = 'PMUA_Logo-web.jpg';
											}
                                            var pubDate = $.datepicker.formatDate('MM dd, yy', new Date(a.publishDate));
                                            var dateTime = $.datepicker.formatDate('yy-mm-dd', new Date(a.publishDate));

                                            $('#newsList').append('<li><a class="news-image" href="https://campusdata.uark.edu/resources/images/articles/' + image + '"><img src="https://campusdata.uark.edu/resources/images/articles/' + image + '" alt=""></a><h4><a href="/news-and-events/news-detail.php?id=' + a.id + '">' + a.headline + '</a></h4></li>');

                                        });
                                    }
                                });

                            });
       </script>
