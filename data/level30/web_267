  <!-- Optional JavaScript -->
  <!-- jQuery first, then Popper.js, then Bootstrap JS -->
  <script src="https://blog.cps.edu/wp-content/themes/cps_blog/node_modules/jquery/dist/jquery.min.js">
  </script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js">
  </script>
  <script src="https://blog.cps.edu/wp-content/themes/cps_blog/node_modules/bootstrap/dist/js/bootstrap.min.js">
  </script>
  <!-- Angular JS
<script src="https://blog.cps.edu/wp-content/themes/cps_blog/js/angular.min.js"></script>-->
  <script>
   function openMobileNav() {
        $('.navCOntent').slideToggle();
    }
    $('.navCOntent .nav-item .nav-link').click(function () {
        $('.mobileNavOpener').removeClass('All Students School District Sports').addClass($(this).text());
        $('.mobileNavOpener .btnText').text($(this).text());
        $('.navCOntent').slideUp();
    });

    $(document).ready(function () {
        $(window).scroll(function () {
            var totalScroll = $(window).scrollTop();
            if (totalScroll > 100) {
                $('#top-link-block').removeClass('d-none');
            } else {
                $('#top-link-block').addClass('d-none');
            }
        })


        $("#top-link-block").click(function () {
            $("html, body").animate({scrollTop: 0}, "slow");
            return false;
        });

        msieversion();
        $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
            msieversion();
        });

        $('#emma-firstname').addClass('form-control w-100');
        $('#emma-main-input').addClass('form-control w-100');
        $('.emma-form-row > div > input[type="submit"]').addClass('btn btn-primary text-uppercase');
        elemenitedCategoryArray = ['Top Stories', 'zz_Feature1', 'zz_Feature2', 'zz_Feature3', 'zz_Feature4', 'zz_PubOnCPSedu_Feature', 'zz_PubOnCPSedu_News_1', 'zz_PubOnCPSedu_News_2', 'zz_PubOnCPSedu_News_3', 'zz_PubOnCPSedu_News_4'];
        $('.wpp-list .card .catTime .categories').each(function () {
            $categoryArray = $(this).text().split(', ');
            for (i = 0; i < $categoryArray.length; i++) {
                if ($.inArray($categoryArray[i], elemenitedCategoryArray) == -1) {
                    $(this).html($categoryArray[i]);
                    switch ($categoryArray[i]) {
                        case 'Students':
                            $(this).prev().addClass('bg-yellow');
                            break;

                        case 'Schools':
                            $(this).prev().addClass('bg-cyan');
                            break;

                        case 'District Initiatives':
                            $(this).prev().addClass('bg-yellowgreen');
                            break;

                        case 'Sports':
                            $(this).prev().addClass('bg-orange');
                            break;

                        default:
                            $(this).prev().addClass('bg-success');
                            break;
                    }
                    return 0;
                }
            }
        })
    })

    function msieversion() {

        var ua = window.navigator.userAgent;
        var msie = ua.indexOf("MSIE ");

        if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./))  // If Internet Explorer, return version number
        {
            $('.card img').each(function () {
                var thisHeight = $(this).height();
                if (thisHeight != 0) {
                    $(this).css('height', thisHeight);
                }
            })
        }

        return false;
    }
  </script>
