            <section class="cu_events-block_2 paragraph paragraph--type--cu-views-display paragraph--view-mode--default anchored" id="views-display-64">
             <div class="views-conditional-container container">
              <h2 class="block-title">
               UPCOMING WORKSHOPS
              </h2>
              <div class="field field--name-field-cu-view-content field--type-viewfield field--label-hidden">
               <div class="viewfield-wrapper">
                <div class="viewfield">
                 <div class="view view-cu-events view-id-cu_events view-display-id-block_2 js-view-dom-id-a7748a3926575a849f20333577082b4d97f956b02d49e80cfedf1f33273c1666">
                  <div class="view-content">
                   <div class="row">
                    <div class="views-row col-sm-6 col-md-4 col-lg-4">
                     <article about="/events/eat-5-sustainable-bites" class="cu-event teaser clearfix row events" data-history-node-id="349" role="article">
                      <div class="date-wrapper col-xs-3 col-sm-3 col-md-3 col-lg-2 text-center">
                       <div class="date-range-wrapper">
                        <div class="date">
                         <a href="/events/eat-5-sustainable-bites" rel="bookmark">
                          <span class="month">
                           Mar
                          </span>
                          <span class="day">
                           12
                          </span>
                          <abbr class="weekday" title="Tuesday">
                           Tue
                          </abbr>
                         </a>
                        </div>
                       </div>
                      </div>
                      <div class="event-details col-xs-9 col-sm-9 col-md-9 col-lg-10">
                       <h3>
                        <a href="/events/eat-5-sustainable-bites" rel="bookmark">
                         <span>
                          EAT 5 Sustainable Bites
                         </span>
                        </a>
                       </h3>
                       <div class="event-location-name">
                        <div class="field field--name-field-cu-location field--type-string field--label-hidden field--item">
                         Studebaker Building, 622 W. 132 St., New York, NY 10027	367
                        </div>
                       </div>
                       <div class="event-time">
                        1:00 PM - 1:45 PM
                       </div>
                      </div>
                     </article>
                    </div>
                    <div class="views-row col-sm-6 col-md-4 col-lg-4">
                     <article about="/events/identity-theft-protecting-your-money-cons-frauds-and-id-theft" class="cu-event teaser clearfix row events" data-history-node-id="375" role="article">
                      <div class="date-wrapper col-xs-3 col-sm-3 col-md-3 col-lg-2 text-center">
                       <div class="date-range-wrapper">
