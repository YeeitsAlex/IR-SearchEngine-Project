  <!-- JRespond: switch functions on desktop / mobile -->
  <script src="/scripts/jRespond.min.js" type="text/javascript">
  </script>
  <script>
   var jRes = jRespond([
          {
            label: 'mobile',
            enter: 0,
            exit: 480
          },{
            label: 'tablet',
            enter: 481,
            exit: 1165
          },{
            label: 'desktop',
            enter: 1166,
            exit: 10000
          }
        ]);

        // register enter and exit functions for multiple breakpoints and functions
        jRes.addFunc([
          {
            breakpoint: ['desktop','tablet'],
            enter: function() {
              // Organize cover flow
              featuredPhotos.sortCoverFlow();
              
							// Fix events column on scroll
							$(window).load(function(){
							  
							  calcEventsPosition();

							  $('#events').fixTo('#inbound', {
							      top: eventsPosition
							  });
							
							});

							// Reposition events on window resize
							$(window).resize(function(){
								
								calcEventsPosition();
					      
					      // Reset events to new position
								$('#events').fixTo('setOptions', {
								    top: eventsPosition
								});
							});

              // Load pipeline with default num elements (5)
              if(!ie6) {
                fetchPipeline();
              }
            },
            exit: function() {

              // Stop feature animation
              featuredPhotos.trigger('destroy');

              // Remove caption colors
              featuredPhotos.children().removeClass('green purple orange');
            }
          },{
            breakpoint: 'desktop',
            enter: function() {

              // switch feature animation to cover flow - desktop dimensions
              featuredPhotos.coverFlow();
          
            },
            exit: function() {
            }
          },{
            breakpoint: 'tablet',
            enter: function() {

              // Call feature animation again for tablet size - tablet dimensions
              featuredPhotos.coverFlow();
          
            },
            exit: function() {
            }
          },{
            breakpoint: 'mobile',
            enter: function() {
              (function(jQuery, undefined) {

                // Load pipeline
                // ***** Change this to include more pipeline items! ********
                fetchMobilePipeline();

                $(document).ready(function(){

                  // switch feature animation to mobile version
                  $('.img_slides_wrap').imgSwiper();

                  // Link up entire feature caption area
                  linkFeatureCaption();

                  // add swiper for events
                  $('.event_slides_wrap').swiper();
                  
                  // once pipeline has finished loading
                  $(window).on('pipelinefinished', function(){

                      // add swiper for pipeline lists
                      $('.pipeline_slides_wrap').swiper();
                  });
                });

                /*Truncate text once dom is ready, once everything has loaded, and when window is resized*/
                $(document).ready(function(){ 
                  $(".feature-caption").dotdotdot();

                  $(".homepage_events_mobile .lw_events_title").dotdotdot();

                  $(".homepage_events_mobile .lw_events_place").dotdotdot();

                  $(".pipeline-item .caption").dotdotdot();
                });

                $(window).on("load resize",function(e){ 
                  $(".feature-caption").dotdotdot();

                  $(".homepage_events_mobile .lw_events_title").dotdotdot();

                  $(".homepage_events_mobile .lw_events_place").dotdotdot();

                  $(".pipeline-item .caption").dotdotdot();
                });

              })(jQuery);
            },
            exit: function() {

              // Unlink entire feature caption area
              unlinkFeatureCaption();

              /*Remove text truncation*/ 
              $(".feature-caption").trigger("destroy");

              $(".homepage_events_mobile .lw_events_title").trigger("destroy");

              $(".homepage_events_mobile .lw_events_place").trigger("destroy");

              $(".pipeline-item .caption").trigger("destroy");

            }
          }
        ]);
  </script>
