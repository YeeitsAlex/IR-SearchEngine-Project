	});
	$('.moving:not(.moving1)').hide();
	var pos = 1;
	var count = $('.moving').length;
	timeout();
	
	function timeout() {	
		setTimeout(function(){
			$('.moving' + pos).fadeOut(400, function() {
				pos++;
				if (pos == count+1) {
					pos = 1;
				}
				$('.moving' + pos).fadeIn();			
			});
			timeout();
		}, 3000);
	}
});
  </script>
