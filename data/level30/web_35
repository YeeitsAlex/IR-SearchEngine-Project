        <script type="text/javascript">
         var allTestimonials = document.querySelectorAll('div.blue.force-full div.wrapper.blue');
  var circlesDiv = document.querySelector('div.circles');
  var currentNum = 0;
  var nextNum = 0;

  function showTestimonial(testimonialNum)
  {
    nextNum = (testimonialNum == 'next') ? ++currentNum : testimonialNum;
    nextNum = (nextNum > (allTestimonials.length - 1)) ? 0 : nextNum;

    circlesDiv.innerHTML = '';

    for(var i = 0; i < allTestimonials.length; i++)
    {
      allTestimonials[i].setAttribute('style', 'display:' + ((i == nextNum) ? 'block !important' : 'none'));
      circlesDiv.innerHTML += '<a href="javascript:void(0);" onclick="clearInterval(testimonialInterval); showTestimonial(' + i + ');"' + ((i == nextNum) ? ' class="on"' : '') + '>&nbsp;</a>';
    }

    currentNum = nextNum;
  }

  // Initialize the circles
  showTestimonial(0);

  // Automatically flip
  var testimonialInterval = setInterval(function() { showTestimonial('next'); }, 8000);
        </script>
       </div>
      </div>
     </div>
    </div>
