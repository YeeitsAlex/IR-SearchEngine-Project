         <script src="/_resources/js/date_fns.min.js">
         </script>
         <script>
          //
	$(function () {
        $.ajax({
            url: "https://campusdata.uark.edu/apiv2/articles/SearchArticle?$orderby=publishDate+desc&$top=2&$filter=(Topics/any(t49:+t49/id+eq+49))",
            data: {},
            dataType: 'jsonp',
            cache: 'true',
            success: function (articles) {
                dataDisplay = "";
                $.each(articles, function (key, a) {
                    var briefText = "", image = "";
                    var date = dateFns.format(a.publishDate, 'dddd, MMMM D, YYYY');
					var url = "news/news_detail.php?id=" + a.id;
					var flag = "";
                    if (a.brief != null && a.brief != "") { briefText = a.brief;  briefText = briefText.replace(/(<p>|<\/p>)/g, "");}
                    if (a.image != null && a.image != "") {
                        image = 'https://campusdata.uark.edu/resources/images/articles/' + a.image;
                    } else { image = "/_resources/images/cio-it-services-logo-shadow.png"; }
//console.log(a);
					if(a.contacts && otherDepartment(a.contacts)) {
						url = "https://news.uark.edu/articles/" + a.id;
						flag = "<p><span class='label label-default'>Submitted by " + otherDepartment(a.contacts) + "</span></p>";
					}
                    dataDisplay = "<li><div class='media-top'>" +
                                        "<a rel='nofollow' class='thumb' href='" + url + "'>" +
                                            "<img class='img-responsive' src='" + image + "?w=550' alt='' /></a>" +
                                    "</div>" +
                                    "<div class=''>" +
                                        "<p class='media-heading'><a rel='nofollow'  href='" + url + "'>" + a.headline + "</a></p>" +
                                        "<p><i>" + date + "</i></p>" +
										flag +
                                        "<p>" + briefText + "</p>" +
                                    "</div></li>";
                    $("#itsNewsHome").append(dataDisplay);
                });
            }
        });
    });
function otherDepartment(coll){
	var otherDept = false;
	var uits = [
		"University Information Technology Services",
		"Information Technology Services",
		"IT Services",
		"UITS"
	];
	$.each(coll, function(index, value){
		if(uits.indexOf(value.department) == -1) otherDept = value.department;
	});
	return otherDept;
}
//
         </script>
