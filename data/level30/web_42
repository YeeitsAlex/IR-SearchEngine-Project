      <script id="statusTemplate" type="text/template">
       <div class="status">
<p class="head">Network Health</p>
<p class="notice" style="width:75%;"><a style="display:inline-block; width:100%; color: #fff; font-weight:bold;" href="http://itstatus.uark.edu/network/">{0}%</a></p>
																																		<div class="progress" style="width:75%">
  <div class="progress-bar progress-bar-success" style="width: {0}%;"></div>
  <div class="progress-bar progress-bar-danger" style="width: {1}%;"></div>
</div>
</div></div>
<p class="head">Service Health</p>
<ul class="list-unstyled">{2}</ul>
      </script>
      <script id="serviceTemplate" type="text/template">
       <li title="{0}">{1} {2}</li>
      </script>
      <script>
       String.prototype.format = function() {
  var args = arguments;
  return this.replace(/{(\d+)}/g, function(match, number) {
    return typeof args[number] != 'undefined'
      ? args[number]
      : match
    ;
  });
};
	var req = $.getJSON('https://itstatus.uark.edu/api/summary/?callback=?');
	req.done(function( data ) {
		var widgetTemplate = $('#statusTemplate').html();
		var serviceTemplate = $('#serviceTemplate').html();
		var widget;
		var services = '';
		$.each(
			data.monitors.detail,
			function(index, value) {
				var html = serviceTemplate.format(
					value.name + ' ' + value.message,
					keyToIcon(value.status),
					value.name,
					value.link
					);
				services += html;
			}
			);
		widget = widgetTemplate.format(
			Math.floor(data.network.health),
			100 - Math.floor(data.network.health),
			services
		);
		$('#detail').html(widget);
		$('#alertCharm').html(data.alert).slideDown();
		$('#loading').hide();

	});
function keyToIcon(key) {
	switch(key)
	{
	case 'up':
		return '<i class="fa fa-check"></i>';
		break;
	case 'down':
		return '<i class="fa fa-exclamation-triangle"></i>';
		break;
	case 'up':
		return '<i class="fa fa-exclamation-triangle medium"></i>';
		break;
	default:
	case 'up':
		return '<i class="fa fa-question-circle"></i>';
		break;
	}
}
      </script>
