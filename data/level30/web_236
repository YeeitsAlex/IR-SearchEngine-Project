    <script>
     var ready = function ( fn ) {

        // Verify function is passed
        if ( typeof fn !== 'function' ) return;

        // If document is already loaded, run method
        if ( document.readyState === 'complete'  )
          return fn();

        // Otherwise, wait until document is loaded
        document.addEventListener( 'DOMContentLoaded', fn, false );

      }

      ready(function(){

        var heroVideo = document.getElementById('o-home-hero-video'),
            heroVideoSource = heroVideo.querySelectorAll("source"),
            heroStill = document.getElementById('home-background-image'),
            windowWidth = window.innerWidth,
            hd_src = 'https://player.vimeo.com/external/265470380.hd.mp4?s=ddf5b1b5197bb8bf01cad920cee767af75cba222&profile_id=175',
            ld_src = 'https://player.vimeo.com/external/265470380.sd.mp4?s=360b0fa203c6f639d9ee06f591f678c8afa06a52&profile_id=165',
            hls_src = 'https://player.vimeo.com/external/265470380.m3u8?s=d9d230bc3ef3444c124a1cc64819def615be608b',
            supportsHLS = heroVideo.canPlayType('application/x-mpegURL; codecs="avc1.42E01E"'),
            siteTitle = document.getElementById('site-title'),
            siteSubtitle = document.getElementById('site-subtitle'),
            chaptersButton = document.getElementById('chapters-button'),
            video_src;

        heroStill.classList.remove('reveal');

        if (supportsHLS && supportsHLS === 'probably' && hls_src !== null) {
          video_src = hls_src;
          heroVideoSource[0].setAttribute('type', 'application/x-mpegURL');
        } else {
          if (windowWidth > 800) {
            video_src = hd_src;
          } else {
            video_src = ld_src;
          }
        }

        heroVideoSource[0].setAttribute('src', video_src);
        heroVideo.load();
        setTimeout(function() {
          heroVideo.classList.add('reveal');
          siteTitle.classList.add('reveal');
          siteSubtitle.classList.add('reveal');
          chaptersButton.classList.add('reveal');
        }, 200);

        // const src = heroStill.getAttribute('data-src');
        // const srcset = heroStill.getAttribute('data-srcset');

        // if (src) {
        //   heroStill.src = src;
        // }
        // if (srcset) {
        //   heroStill.srcset = srcset;
        // }

        setTimeout(function() {
          if (heroVideo.readyState == 0) {
            heroVideo.classList.remove('reveal');
            heroStill.classList.add('reveal');
            heroStill.style.zIndex = "2";
          }
        }, 3000);

        // object-fit polyfill
        objectFitVideos(heroVideo);

      });
    </script>
