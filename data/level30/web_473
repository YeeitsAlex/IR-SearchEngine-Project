<script>
 $('.sesqui-marquee-video').bind('inview', function(event, isInView) {
		 if (isInView) {
			// element is now visible in the viewport
			this.play();  
			//console.log('visible');
		 } else {
			// element has gone out of viewport
			this.pause();
			//console.log('not visible');
		 }
		});
</script>

