   <h1 class="off-screen">
    Cornell University
   </h1>
   <div class="marquee-mask with-slideshow" role="marquee">
    <!--[PANO_SHIM]-->
    <div class="marquee">
     <span class="marquee-item gradientAdjustTop50p gradientAdjustBottom25p" data-click-action="pano | content" data-click-label="1|7F7EB696-B3B1-F08D-E7AE13815E2708DC|36681816-FA7C-2F65-D44AD0F15CADF4DD|Ripple effect" data-counter="1" id="panobg-7F7EB696-B3B1-F08D-E7AE13815E2708DC" style="position: absolute; left: 0px; top: 0px; display: block; " title="© University Photography">
      <!--[PANO_IMAGE_START]-->
      <div class="marquee-img" data-alt="Ripple effect" data-class="marquee-img" data-src="//media.univcomm.cornell.edu/photos/{width}/7F98CD94-91C5-801A-A700FB44C4B1CD79.jpg" id="pano-7F7EB696-B3B1-F08D-E7AE13815E2708DC">
      </div>
