  <script>
   $(function () {
  //http://stackoverflow.com/questions/31569518/how-to-detect-facebook-in-app-browser
  var isFacebookBrowser = function () {
    var ua = navigator.userAgent || navigator.vendor || window.opera;
    return (ua.indexOf("FBAN") > -1) || (ua.indexOf("FBAV") > -1);
  };

  if (!localStorage.showBrowserNotice) {
    if (
      ($.browser.name === 'msie' && $.browser.versionNumber > 9) ||
      ($.browser.name === 'chrome' && $.browser.versionNumber > 30) ||
      ($.browser.name === 'firefox' && $.browser.versionNumber > 30) ||
      ($.browser.name === 'safari' && $.browser.versionNumber > 5) ||
      isFacebookBrowser()
    ) {
      localStorage.setItem('showBrowserNotice', 'false');
    } else {
      $('.browser-upgrade-notice').html(app.template('shared/browser_upgrade_notice'));
    }
  }

  $('.browser-upgrade-notice-dismiss').click(function () {
    $('.browser-upgrade-notice').html('');
    localStorage.setItem('showBrowserNotice', 'false');
  });
});
  </script>
