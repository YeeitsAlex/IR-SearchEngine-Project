  <div class="contain-1440 home-search" id="library-search-dropdown">
   <div class="contain-1120">
    <div class="section-top">
     <h3>
      Start Your Research
     </h3>
     <p class="subhead">
      Use our catalogs, tools and expertise
     </p>
    </div>
    <div class="search-column">
     <span>
      Search
     </span>
     <select id="multi-search" name="type">
      <option value="nusearch">
       NUsearch
      </option>
      <option value="books">
       Books &amp; Multimedia
      </option>
      <option value="articles">
       Articles
      </option>
      <option value="e-journals">
       E-Journals
      </option>
      <option value="databases">
       Databases
      </option>
      <option value="worldcat">
       Worldcat
      </option>
      <option value="course-reserves">
       Course Reserves
      </option>
      <option value="web">
       Library Website
      </option>
     </select>
    </div>
    <div class="multi-search-form" id="nusearch" style="display:none;">
     <form action="https://search.library.northwestern.edu/primo-explore/search" enctype="application/x-www-form-urlencoded; charset=utf-8" method="get" name="libsearch">
      <div class="by-column">
       <span>
        by
       </span>
       <select class="field" name="field">
        <option value="any">
         Keyword
        </option>
        <option value="title">
         Title
        </option>
        <option value="creator">
         Author/Creator
        </option>
        <option value="sub">
         Subject
        </option>
       </select>
      </div>
      <div class="for-column">
       <span>
        for
       </span>
       <input class="searchbox" maxlength="256" name="query" placeholder="Ex: Cleopatra" size="20" title="search" type="text" value=""/>
       <input name="institution" type="hidden" value="01NWU"/>
       <input name="vid" type="hidden" value="NULVNEW"/>
       <input name="search_scope" type="hidden" value="NWU"/>
       <input name="tab" type="hidden" value="default_tab"/>
       <input name="mode" type="hidden" value="Basic"/>
       <input name="displayMode" type="hidden" value="full"/>
       <input name="bulkSize" type="hidden" value="10"/>
       <input name="highlight" type="hidden" value="true"/>
       <input name="dum" type="hidden" value="true"/>
       <input id="primoQuery" name="query" type="hidden"/>
       <input name="displayField" type="hidden" value="all"/>
       <input name="pcAvailabiltyMode" type="hidden" value="true"/>
       <button type="submit">
        <span class="hide-label">
         Search
        </span>
       </button>
       <div class="advanced-search">
        <a href="https://search.library.northwestern.edu/primo-explore/search?vid=NULVNEW&amp;mode=advanced&amp;sortby=rank">
         Advanced Search
        </a>
       </div>
      </div>
     </form>
    </div>
    <div class="multi-search-form" id="books" style="display:none;">
     <form action="https://search.library.northwestern.edu/primo-explore/search" enctype="application/x-www-form-urlencoded; charset=utf-8" method="get" name="libsearch">
      <div class="by-column">
       <span>
        by
       </span>
       <select class="field" name="field">
        <option value="any">
         Keyword
        </option>
        <option value="title">
         Title
        </option>
        <option value="creator">
         Author/Creator
        </option>
        <option value="sub">
         Subject
