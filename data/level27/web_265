   <section class="gray">
    <div class="container">
     <div class="row">
      <!--news-->
      <div class="col-xs-12">
       <h2 id="news">
        News
       </h2>
       <script type="text/javascript">
        // 
$(function (){
$.ajax({
            url: "https://campusdata.uark.edu/apiv2/articles/SearchArticle?$orderby=publishDate+desc&$top=3&$filter=((Topics/any(t29:+t29/id+eq+29)))",
data: {},
contentType: "application/json; charset=utf-8",
dataType: 'jsonp',
//jsonpCallback: 'Articles',
async: "true",
cache: "true",
success: function (articles) {
dataDisplay = "";
$.each(articles, function (key, a) {
var briefText = "", image = "";
var date = new Date(a.publishDate);
date = date.toDateString();
if (a.brief != null && a.brief != "") { briefText = a.brief;  briefText = briefText.replace(/(<p>|<\/p>)/g, "");}
if (a.image != null && a.image != "") {
image = a.image;
} else { image = "PMUA_Logo-web.jpg"; }
dataDisplay = "<div class='col-sm-4 col-xs-12'>" +
"<div class='news-image'><a href='https://news.uark.edu/articles/" + a.id + "'><img width='100%' alt='" + a.headline + "' class='img-rounded' src='//campusdata.uark.edu/resources/images/articles/" + image + "?width=600' /></a></div>" +

"<div>" +
"<h4><a href='https://news.uark.edu/articles/" + a.id + "'>" + a.headline + "</a></h4>" +
"<p>" + briefText + "</p></div>"+ 
"</div>";

$("#newsArticles").append(dataDisplay);
});
}
});

});
//
       </script>
