      (function($){
		$(document).ready(function(){
			if (!skrollr.get()) {
				$('body').attr('id', 'skrollr-body');
				s = skrollr.init({
					forceHeight: false,
					keyframe: function(element, name, direction) {
						var canvas = $(element);
						if (canvas.hasClass('campaign-progress-canvas') && canvas.hasClass('skrollable-after')) 
							canvas.removeClass('do');
					}
				});
			}
		});
	})(jQuery);
     </script>
