        <h2 id="sectionHeading">
         Announcements
         <a href="http://research.uark.edu/_resources/feeds/postannouncement.xml" target="new" title="Subscribe to VPRED Announcements (RSS Feeds)">
          <i aria-hidden="true" class="fa fa-rss-square" style="color: #ff6600;">
          </i>
         </a>
        </h2>
        <script type="text/javascript">
         // 
          $(function ()
            {
                $.ajax({
                    type: 'GET',
                    url: 'proxy.php',
data: { requrl: 'https://research.uark.edu/_resources/feeds/postannouncement.xml' },
                    success: function (xml)
                    {
                        $('#announceNotice').empty();
                        $(xml).find("item").slice(0, 10).each(function ()
                        {
                            var title = $(this).find("title").text();
                            var linkUrl = $(this).find("link").text();
                            var brief = $(this).find("description, content").text();
                            var url = $(this).find("enclosure").attr('url');
                            var $div = $('<div></div>').addClass('span4');
                            var $detaildiv = $('<div></div>').addClass('split-wide');

                            $('<h4></h4>').append($('<a></a>').attr('href', linkUrl).text(title)).appendTo($detaildiv);
							$('<p style="padding-bottom:10px; font-size: 98%;"></p>').html(brief).appendTo($detaildiv);
                            $div.append($detaildiv);

                            $div.appendTo("#announceNotice");
                        });
                    }
                });
            });
		//
        </script>
        <div id="announceNotice">
        </div>
        <p>
        </p>
        <h2 id="sectionHeading">
         Research &amp; Innovation News
        </h2>
        <script type="text/javascript">
         // 
          $(function ()
            {
                $.ajax({
                    type: 'GET',
                    url: 'proxy.php',
                    data: { requrl: 'https://news.uark.edu/rssfeeds/feed?Type=RSS&amp;FbyT=1&amp;top=7-25-' },
                    success: function (xml)
                    {
                        $('#newsArticles').empty();
                        $(xml).find("item").slice(0, 10).each(function ()
                        {
                            var title = $(this).find("title").text();
	//var description = $(this).find("description").text().substr(0, 200) + '...';
                            var linkUrl = $(this).find("link").text();
                            var brief = $(this).find("a10\\:content, content").text();
                            var url = $(this).find("enclosure").attr('url');
                            var $div = $('<div></div>').addClass('span4');
	// var $imgdiv = $('<div class="split-narrow"></div>').append('<a href="' + url + '"><img src="' + url + '" style="padding-right:10px; float:left; width:90px; height:90px;" /></a>');
                            var $detaildiv = $('<div></div>').addClass('split-wide');

                            $('<h5 style="padding-bottom:13px;"></h5>').append($('<a></a>').attr('href', linkUrl).attr('target', 'new').text(title)).appendTo($detaildiv);
							//$('<p style="padding-bottom:10px;"></p>').html(brief).appendTo($detaildiv);
	//$div.append($imgdiv); 
                            $div.append($detaildiv);

                            $div.appendTo("#newsArticles");
                        });
                    }
                });

            });
         
        
//
        </script>
        <div id="newsArticles">
        </div>
