  <script type="text/javascript">
   function setupTinyMCE() {
            tinymce.init({
                selector: ".input-richtext",
                theme: "modern",
                browser_spellcheck: true,
                plugins: [
                    "advlist autolink link image lists charmap preview hr anchor pagebreak",
                    "searchreplace wordcount visualblocks visualchars code fullscreen insertdatetime media nonbreaking",
                    "save table contextmenu directionality template paste textcolor"
                ],
                toolbar: "insertfile undo redo | styleselect | bold italic | alignleft aligncenter alignright alignjustify | bullist numlist outdent indent | link image | preview media fullpage | forecolor backcolor | spellchecker"
            });

            tinymce.init({
                selector: ".input-richtext-readonly",
                theme: "modern",
                readonly: true,
                toolbar: ""
            });
        }

        function applyMasks() {
            $(".input-phone").mask("(999)999-9999");
            $(".input-zipcode").mask("00000");
            $(".input-money").mask("#,##0.00", { reverse: true, maxlength: false }).focusin(function () {
                $(this).before("<label class='input-money-dollar'>$</label>")
            }).focusout(function () {
                $(this).prev(".input-money-dollar").remove();
            });
            $(".input-number").mask("#", { maxlength: false });
            $(".input-spinner").spinner();

        }

        $(function () {
            setupTinyMCE();
        });

        $(document).ready(function () {
            //setupTinyMCE();
            applyMasks();
        });

        var prm = Sys.WebForms.PageRequestManager.getInstance();

        prm.add_endRequest(function () {
            setupTinyMCE();
            applyMasks();
        });
  </script>
