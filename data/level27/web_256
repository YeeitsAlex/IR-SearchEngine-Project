					#slide-out-tab-tab {
						color: #666666;
						background-color: #ffffff;
						border-top: 5px solid #ffffff;
						border-right: 5px solid #ffffff;
						border-left: 5px solid #ffffff;
					}
					#slide-out-tab-content {
						color: #666666;
						background-color: #ffffff;
						border-top: 5px solid #ffffff;
						border-right: 5px solid #ffffff;
						border-left: 5px solid #ffffff;
						width: 400px;
					}
       </style>
