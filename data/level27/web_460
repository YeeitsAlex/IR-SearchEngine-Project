   <script>
    $(document).ready(function(){
$('#calendar_new').datepicker({
		inline: true,
		firstDay: 1,
		showOtherMonths: true,
		dayNamesMin: ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat']
	});
$("#mobile-menu-container #mobile-right #home, .desktop #"+currentPage).css("display", "block");
	
$("#mobile-menu").click(function(){
	$("#mobile-menu-container").slideToggle();
	});
	
$("#mobile-search").click(function(){
	$("#mobile-search-container").slideToggle();
	});
	
});

$('.jsomevents').click(function(){
 $('html, body').animate({
           'scrollTop':   $('#events').offset().top
         }, 800);
        return false;
    });
	
$('.jsomcmc').click(function(){
 $('html, body').animate({
           'scrollTop':   $('#cmc').offset().top
         }, 800);
        return false;
    });
	
$('.jsomannounce').click(function(){
 $('html, body').animate({
           'scrollTop':   $('#announce').offset().top
         }, 800);
        return false;
    });

$('#back-top').click(function(){
        $('html, body').animate({scrollTop : 0},800);
        return false;
    });
   </script>
