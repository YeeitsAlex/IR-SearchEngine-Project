     <div class="column medium-8 large-9">
      <section class="main-content">
       <div class="region region-content-top-left">
        <div class="edit-links">
        </div>
        <div class="block block-fieldblock fieldblock_field_utexas_promo_list" id="block-fieldblock-node-standard-page-default-field-utexas-promo-list">
         <div class="content row">
          <div class="field field_utexas_promo_list">
           <div class="column two-column">
            <div class="utexas-promo-list-wrapper">
             <div class="promo-list-item-wrapper">
              <div class="promo-list-li columns small-12 large-6 end">
               <div class="utexas-promo-field utexas-promo-list">
                <img alt="Longhorn Band commemorative sculpture" class="promo-image" src="https://registrar.utexas.edu/sites/default/files/styles/utexas_promo_list_image/public/Longhorn%20Band%20commemorative%20sculpture-cropped.JPG?itok=jd8N1dH3" title="Longhorn Band commemorative sculpture" typeof="foaf:Image">
