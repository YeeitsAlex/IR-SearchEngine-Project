       <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.1.0/moment.min.js" type="text/javascript">
       </script>
       <script type="text/javascript">
        //  
$(function ()
{
$.ajax({
type: 'GET',
url: '/_resources/php/proxy.php',
data: { requrl: "https://uark.givepulse.com/group/rss2/6748.xml" },
success: function (data)
{
var months = ["JAN", "FEB", "MAR", "APR", "MAY", "JUN", "JULY", "AUG", "SEPT", "OCT", "NOV", "DEC"]; // Store month names in array
var days = ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"];
var list_data = '';
var $xml = $(data).find('item:lt(3)');    // specify number of items to be pulled
console.log($xml);
$xml.each(function () {
var $this = $(this);
var headline = $this.find('title').text().replace(/ *\([^)]*\) */g, "");
//var description = $this.find('description').text().replace(/<!\[CDATA\[.*?\]\]>/g, '');
//description = $(description).text();
var unparsedDesc = $this.find('description').text();
// see if string has CDATA or not
if (unparsedDesc.indexOf("CDATA") >= 0)
{ 
var description = $(this).find('description').text().replace(/<!\[CDATA\[.*?\]\]>/g, ''); 
}
else
{ 
var description = $(this).find('description').text(); 
description = description.replace(/<[^>]+>/ig,"");
} 
var briefText = "";
briefText = description.substring(0, 149);
briefText += "...";
var pubDate = (new Date($this.find('pubDate').text())).toDateString();
var link = $this.find('link').text();
var date = new Date($this.find('pubDate').text()).getDate();
var time = moment($this.find('pubDate').text()).format("h:mm A");
var month = months[new Date($this.find('pubDate').text()).getMonth()];
var day = days[new Date($this.find('pubDate').text()).getDay()];
// parse description to find img and get the src attribute												
var description2 = $(this).find('description').text();  
var temp_container = $("<div></div>");
temp_container.html(description2);
var image = temp_container.find("img:first").attr("src");
if(image == undefined) { image = '//campusdata.uark.edu/resources/images/articles/PMUA_Logo-web.jpg'; }

if(time=='12:00 AM'){ time='All Day';}

/* includes image and date/time 
list_data += "<div class='media'>" +
"<a href='" + link + "'><img class='img-responsive pull-left padded-image' src='" + image + "' style='max-width: 82px; alt='" + headline + "' /></a>" +
"<h4 style='margin-top: 0;'><a href='" + link + "'>" + headline + "</a></h4>" +
"<div class='dispDT'>" + month + " " + date + " " + time  + "</div>" + briefText + "</div>";
*/

list_data += "<div class='media'>" +
"<h4 style='margin-top: 0;'><a href='" + link + "'>" + headline + "</a></h4>"  + briefText + "</div>";

});

$(list_data).appendTo("#blogPosts");
}
});
});
//
       </script>
       <div id="blogPosts">
