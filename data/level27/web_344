        </div>
        <div class="col-sm-4 col-md-4 split-list">
         <h2 id="events">
          Events
         </h2>
         <!-- Dynamically Switching Events for Honors College Home Page -->
         <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.1.0/moment.min.js" type="text/javascript">
          // 

//
         </script>
         <style>
          .rss-message { padding-left: 20px; }
         </style>
         <script type="text/javascript">
          // 
$(function () {
	function initRSSDisplay(whichFeed) {
		
		var rssURL = '';
		var message = '';

		// first see which feed needs to be called
		if(whichFeed == 1){

			// honorscollege events
			rssURL = "https://calendars.uark.edu/RSSSyndicator.aspx?type=N&number=5&department=233&sortorder=ASC";
			message = null;
		} else if(whichFeed == 2){
			
			// uark events
			rssURL = "https://calendars.uark.edu/RSSSyndicator.aspx?type=N&category=32-0&numdays=7&binary=Y&rssid=25&sortorder=ASC";
			//message = '<div class="row"><p class="rss-message">There are no upcoming Honors College events at this time, but here are some UARK events.</p></div>';
			
		}

		$.ajax({
			type: 'GET',
			url: '/_resources/php/proxy.php',
			// below data pulls honorscollege events
			data: { requrl: rssURL },
			cache: 'true',
			success: function (data)
			{
				
				var months = ["JAN", "FEB", "MAR", "APR", "MAY", "JUN", "JULY", "AUG", "SEPT", "OCT", "NOV", "DEC"];
				var days = ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"];
				var list_data = '';

				var $xml = $(data).find('item:lt(3)');
				$xml.each(function () { 
					var $this = $(this); 
					//var headline = this.title.replace(/ *\([^)]*\) */g, "");
					var headline = $this.find('title').text(); 
					var link = $this.find('link').text(); 
					var publishedDate = $this.find('pubDate').text();
					var content = $this.find('pubDate').text();

					var d = new Date();
					var date = new Date(publishedDate).getDate();
					var time = moment(publishedDate).format("h:mm A");
					var month = months[new Date(publishedDate).getMonth()];
					var day = days[new Date(publishedDate).getDay()];

					var year = new Date(publishedDate).getFullYear();   

					list_data += '<div class="row"><div class="col-sm-3 col-md-3"><h4>' + month + ' ' + date + '</h4></div><div class="col-sm-9 col-md-9"><p><a href="'+ link +'">' + headline + '</a><br/>' + time + '</p></div></div>';

				});

				// set 100 ms timer and see if list_data is empty, if there are no honors college events, call the UARK Newswire feed

				setTimeout(function(){
					if(list_data == ''){ 
						initRSSDisplay(2);	
					}
				}, 100);
				
				$(message).appendTo("#calendar");
				$(list_data).appendTo("#calendar");
			}
			
		});

	}  // end initRSSDisplay

// call the funtion with honors college events firstly
initRSSDisplay(1);

});
//
         </script>
         <div id="calendar">
         </div>
