         <style>
          section .quick-links {
									margin-top: 0 !important;
									}
         </style>
         <script type="text/javascript">
          // 
$(function () {
$.ajax({
url: '//campusdata.uark.edu/apiv2/articles?$orderby=publishDate+desc&$top=3&$filter=((Schools/any(s14:+s14/id+eq+14)))',
data: {},
contentType: "application/json; charset=utf-8",
dataType: 'jsonp',
//jsonpCallback: 'Articles',
async: "true",
cache: "false",
success: function (articles) {
dataDisplay = "";
$.each(articles, function (key, a) {
var briefText = "", image = "";
if (a.brief != null && a.brief != "") { briefText = a.brief.substr(0, 100) + '...';  briefText = briefText.replace(/(<p>|<\/p>)/g, "");}
if (a.image != null && a.image != "") {
image = a.image;
} else { image = "PMUA_Logo-web.jpg"; }
dataDisplay = "<div class='media'>" +
"<img class='img-responsive pull-left' src='//campusdata.uark.edu/resources/images/articles/" + image + "?w=100' alt='' /></a>" +
"<h4 style='margin-top: 0;'><a href='/news/news-detail.php?id=" + a.id + "'>" + a.headline + "</a></h4>" +
"<p>" + briefText + "</p>"+ 
"</div>";
$("#newsArticles").append(dataDisplay);
});
}
});

});
//
         </script>
         <h2 id="news">
          News
         </h2>
         <div id="newsArticles">
         </div>
         <ul class="nav pull-right clearfix">
          <li>
