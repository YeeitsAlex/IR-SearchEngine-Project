   <script>
    $('.radar-toggle').on('click', function()
{
    $(this).toggleClass('clicked');
    if ( $(this).hasClass('clicked') )
    {
        $(this).text('To view the original image, click here');
    }
    else
    {
        $(this).text('In Case This Image is UNAVAILABLE, use this Image');
    }
    
    $('.radar').each(function()
    {
        $(this).toggleClass('show');
    });
})

setInterval(function()
{
    $('.radar').each(function()
    {
        $this = $(this);
        var src = $this.data('src');
        var new_src = src + '?' + new Date().getTime();
        $this.attr('src', new_src);
    });
}, 1000*60*10);

setInterval(function()
{
    $('.blocks li').each(function()
    {
        $this = $(this);
        var src = $this.find('a').data('src');
        var new_src = src + '?' + new Date().getTime();
        $this.find('a').attr('href', new_src);
        $this.find('img').attr('src', new_src);
    });
}, 1000*60*10);
   </script>
