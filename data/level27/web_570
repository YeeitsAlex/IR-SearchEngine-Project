  <script type="text/javascript">
   jQuery(document).ready(function($){
            var resizeTimer, scrollTimer, topHeader = $( "#top-header" ), mainHeader = $("#main-header");

            function setHeaderSize(){

								$('#page-container').removeClass('tr-resizing');

                if($('body').width()<=980 || mainHeader.hasClass('et-fixed-header')){return}

                if(topHeader.is(':visible')) {
                    var totalHeight = topHeader.innerHeight() + mainHeader.innerHeight();

                } else {
                    var totalHeight = mainHeader.innerHeight();
                }

                mainHeader.attr("data-height-onload", mainHeader.innerHeight() );
                $("#page-container").css("padding-top",totalHeight);
            }

            $(window).resize(function(){

								if( $('body').width() >= 981 || mainHeader.hasClass('et-fixed-header') == false) {
									$('#page-container').addClass('tr-resizing');
								}
                clearTimeout(resizeTimer);
                resizeTimer = setTimeout(function() { setHeaderSize(); }, 500);

            });

            $(window).scroll(function(){
                clearTimeout(scrollTimer);
                scrollTimer = setTimeout(function() { setHeaderSize(); }, 500);
            });
        });
  </script>
