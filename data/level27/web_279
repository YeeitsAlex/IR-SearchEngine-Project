   <script type="text/javascript">
    $(document).ready(function () {
		/*https://stackoverflow.com/questions/30293784/smooth-scroll-to-anchor-after-loading-new-page*/
		// to scroll to anchor on a separate page
		if ( window.location.hash ) scroll(0,0);
		// void some browsers issue
		setTimeout( function() { scroll(0,0); }, 10);
		if(window.location.hash) {
			// smooth scroll to the anchor id
			$('html, body').animate({
				scrollTop: $(window.location.hash).offset().top + 'px'
			}, 1000, 'swing');
		}
	
		document.getElementById("1-imgfocus-image").style.visibility = "visible";
		var imageslist = document.getElementsByTagName("A");
		var elementslist = Array.prototype.slice.call( imageslist, 0 );
		for(var i = elementslist.length - 1; i >= 0 ; i--) {
			if (elementslist[i].id.indexOf('-imgfocus') <= -1) {
				elementslist.splice(i, 1);
			}
		}
		for(var i = elementslist.length - 1; i >= 0 ; i--) {
			element = elementslist[i];
			element.addEventListener('click', function() {
				for(var i = 0; i < elementslist.length; i++){
					imgElementId = elementslist[i].id + '-image';
					document.getElementById(imgElementId).style.visibility = "hidden";
				}
				var imgElementId = this.id + '-image';
				document.getElementById(imgElementId).style.visibility = "visible";
			}, false);
		}
    });

	/*https://stackoverflow.com/questions/19012495/smooth-scroll-to-div-id-jquery*/
	$(document).on('click', 'a[href^="#"]', function(e) {
		// target element id
		var id = $(this).attr('href');

		// target element
		var $id = $(id);
		if ($id.length === 0) {
			return;
		}

		// prevent standard hash navigation (avoid blinking in IE)
		e.preventDefault();

		// top position relative to the document
		var pos = $id.offset().top;

		// animated top scrolling
		$('body, html').animate({scrollTop: pos}, 1000);
	});
	
	/*https://codepen.io/anon/pen/BpKnx*/
	if ($('.scrollToTop')[0]){
		$('.scrollToTop').click(function(){
			$('html,body').animate({ scrollTop: 0 }, 1000);
		});
	}
   </script>
