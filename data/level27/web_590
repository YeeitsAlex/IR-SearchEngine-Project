@media (min-width: 768px) and (max-width: 991px) {
  blockquote:before,
  blockquote:after {
    margin-bottom: 5px;
  }
}
blockquote:after {
  margin-top: 15px;
  margin-bottom: 0;
}
@media (min-width: 768px) and (max-width: 991px) {
  blockquote:after {
    margin-top: 5px;
  }
}
@media (min-width: 992px) {
  blockquote[style="text-align: right;"] {
    float: right;
  }
}
@media (min-width: 768px) {
  blockquote[style="text-align: right;"] {
    margin-right: -35%;
  }
}
@media (min-width: 992px) {
  blockquote[style="text-align: left;"] {
    float: left;
  }
}
@media (min-width: 768px) {
  blockquote[style="text-align: left;"] {
    margin-left: -35%;
  }
}
  </style>
 </head>
