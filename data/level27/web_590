  <script type="text/javascript">
   jQuery(document).ready(function () {
	jQuery('a[href*="https://uwlax.photoshelter.com/gallery/"]').each(function () {
		if (jQuery(this).text().toLowerCase().indexOf("gallery: ") == 0) {
			var photoAlbum = jQuery(this).attr("href").match(/\/G00(.*)\//g).toString().replace(/\//g, "");

			jQuery(this).after('<h3>' + jQuery(this).text() + '</h3><div class="photo-album-embed" data-ps-embed-type="slideshow" data-ps-embed-gid="' + photoAlbum + '"></div>');
			jQuery(this).remove();
		}
	});
	jQuery.getScript('https://uwlax.photoshelter.com/js/psEmbed.js', function () {
		_psEmbed("https://uwlax.photoshelter.com");
	});

jQuery('#story-rating-panel').hide();
      jQuery('.wpcf7-starratingawesome input').on("change",function(){
            jQuery('#story-rating-panel').show();
});

jQuery('blockquote').each(function(){
     jQuery(this).css("text-align", jQuery(this).find(">p").css("text-align") );
jQuery(this).find(">p").css("text-align","")
});

jQuery('.single-post #primary .wp-post-image').wrap('<a href="'+jQuery('#primary .wp-post-image').attr("src")+'"></a>');



jQuery('#text-11 .textwidget').append('<table id="footer-current-publications-output" style="width:100%; border:none;"><tbody><tr>\
		<td style="border:none; width: 50%; padding-left:5%; padding-right:2%;" id="footer-current-publication-0"></td>\
		<td style="border:none; width: 50%; padding-right:5%; padding-left:2%;" id="footer-current-publication-1"></td>\
	</tr></tbody></table>');
jQuery('#text-11 ul').hide().find("li").each(function( index ){
	var self = jQuery(this);
	var selfIndex = index;
	jQuery.getJSON(
		//Get the ISSUU oembed data, this lets us add the document's ID to the download link to use when clicked.
		'https://issuu.com/oembed?url=' + jQuery(this).text() + '&format=json',
		function(data) {
			//You could add a thumbnail of the magazine as well:
			jQuery('#footer-current-publication-'+selfIndex).html('<a href="'+self.text()+'"><img class="thumbnail" src="'+data.thumbnail_url.replace("_medium","_large")+'" width="100%" alt=""></a>');
						
		}
	);
});



});
  </script>
