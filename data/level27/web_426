    <section class="col-sm-12">
     <a id="main-content">
     </a>
     <div class="region region-content">
      <section class="block block-views clearfix" id="block-views-cluster-term-view-block-2">
       <div class="view view-cluster-term-view view-id-cluster_term_view view-display-id-block_2 program-clusters tabbed-view view-dom-id-5f1b420bb3ea67cf744da3188dbe9212">
        <div class="view-content">
         <div class="views-bootstrap-tab-plugin-style" id="views-bootstrap-tab-1">
          <ul class="nav nav-tabs ">
           <li class="active">
            <a data-toggle="tab" href="#tab-1-0">
             Biomedical Sciences
            </a>
           </li>
           <li class="">
            <a data-toggle="tab" href="#tab-1-1">
             Darwinian Sciences
            </a>
           </li>
           <li class="">
            <a data-toggle="tab" href="#tab-1-2">
             Independent Programs
            </a>
           </li>
           <li class="">
            <a data-toggle="tab" href="#tab-1-3">
             Molecular Biosciences
            </a>
           </li>
           <li class="">
            <a data-toggle="tab" href="#tab-1-4">
             Neuroscience
            </a>
           </li>
          </ul>
          <div class="tab-content">
           <div class="tab-pane active" id="tab-1-0">
            <div class="views-field views-field-view">
             <span class="field-content">
              <div class="view view-program-clusters view-id-program_clusters view-display-id-block_6 cluster-wide-view-content row view-dom-id-1388e48632b07a39394f6b0576b23800">
               <div class="view-content">
                <div class="views-row views-row-1 views-row-odd views-row-first col-md-3 col-sm-4">
                 <div class="views-field views-field-title">
                  <strong class="field-content">
                   <div class="item ">
                    <span class="image">
                     <a href="/programs/cancer-biology">
                      <img alt="An image of a cell from research in a cancer biology lab" class="img-responsive" height="175" src="https://biosciences.uchicago.edu/sites/default/files/styles/scale_crop_270_175/public/images/bannerimage/cancer%20bio%203%20copy.jpg?itok=bOJ6bowy" title="An image of a cell from research in a cancer biology lab" typeof="foaf:Image" width="270"/>
                     </a>
                    </span>
                    <div class="text">
                     <span class="title">
                      <a href="/programs/cancer-biology">
                       Cancer Biology
                      </a>
                     </span>
                     <span class="body">
                      Exploring novel scientific mechanisms that explain the etiology, progression and treatment of cancer as a disease
                     </span>
                    </div>
                   </div>
                  </strong>
                 </div>
                </div>
