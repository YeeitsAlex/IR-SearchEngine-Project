        <div class="view-content">
         <div class="views-bootstrap-accordion-plugin-style panel-group" id="views-bootstrap-accordion-1">
          <div class="panel panel-default">
           <div class="panel-heading">
            <h4 class="panel-title">
             <a class="accordion-toggle" data-parent="#views-bootstrap-accordion-1" data-toggle="collapse" href="#collapse0">
              Biomedical Sciences
             </a>
            </h4>
