                           <script>
                            fieldCounter = 0;

    function makeSchoolpicker(fieldname, accType) {
        fieldname = fieldname.replace(" ", "");
        fieldname = 'tfa_' + fieldname;
        $("#" + fieldname).attr("name", "lookupField" + fieldCounter); //Rename old field
        $("#" + fieldname).attr("id", "lookupField" + fieldCounter);
        document.getElementById("lookupField" + fieldCounter).value = "";

        // Even though it won't do anything, we need to set this on the fake field to avoid javascript errors
        var con = document.getElementById("lookupField" + fieldCounter).getAttribute('data-condition');
        $("#" + fieldname + "-D").append('<input id="' + fieldname + '" name="' + fieldname + '" type="text" style="display:none;" data-condition="' + con + '">');

        var respLookup = new Array();
        $("#lookupField" + fieldCounter).autocomplete({

            source: function(request, response) {
                $.ajax({
                    //url: "https://unh.secure.force.com/admission/targetx_eventsb__schoolPicker",
                    url: "https://wapi.unh.edu/api/SfSchools/listSchools",
                    type: "GET",
                    dataType: "json",
                    data: {
                        nl: "yes",
                        otl: "Account",
                        value: request.term,
                        type: accType
                    },
                    success: function(data) {
                        var respJSON = data.schools;
                        for (var key in respJSON) {
                            var row = respJSON[key];
                            respLookup[row.value] = row.id;
                        }
                        response(respJSON);
                    }
                });
            },
            select: function(event, ui) {
                $("#" + fieldname).val(respLookup[ui.item.label]);
            }
        });
        $("#lookupField" + fieldCounter).blur(function() {
            if ($(this).parent().parent().children('input').val() == '') {
                $(this).val('');
            }
        });




        // Validate that the hidden field actually got a value
        wFORMS.behaviors.validation.messages['clickedSchoolpicker' + fieldCounter] = "Please select a value from the dropdown";

        wFORMS.behaviors.validation.rules['clickedSchoolpicker' + fieldCounter] = {
            selector: "#" + fieldname,
            check: function(element) {
                if (document.getElementById(fieldname + "-D").className.indexOf("required") > 0) {
                    var c = document.getElementById(fieldname);
                    if (element.value == '' || element.value == null) {
                        return false;
                    }
                }
                return true;
            }
        };
        fieldCounter++;
    }
                           </script>
                           <script type="text/javascript">
                            document.domain = 'explore.unh.edu';
    //$("body").addClass("default wFormWebPage");
                           </script>
