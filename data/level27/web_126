  <!-- assignment app grader scripts -->
  <script type="text/javascript">
   $(document).ready(function() {
        $("#btnUWCSSO").click(function() {
            var href = $("#btnUWCSSO").attr('href');
            var locstring = location.search;
            if (locstring.search('target') > 0) {
                $("#btnUWCSSO").attr('href', href + locstring);
            }
        });
        $("#btnUWCD2L").click(function() {
            var href = $("#btnUWCD2L").attr('href');
            var locstring = location.search;
            if (locstring.search('target') > 0) {
                $("#btnUWCD2L").attr('href', href + locstring);
            }
        });
        $("#btnFlexD2L").click(function() {
            var href = $("#btnFlexD2L").attr('href');
            var locstring = location.search;
            if (locstring.search('target') > 0) {
                $("#btnFlexD2L").attr('href', href + locstring);
            }
        });
    });
  </script>
