     <!-- desktop section -->
     <div class="opportunity-menu-wrapper desktop">
      <div class="opportunity-menu">
       <ul class="js-opportunity-menu">
        <li class=" active">
         <span>
          Driving Innovation
         </span>
        </li>
        <li class=" ">
         <span>
