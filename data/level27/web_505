   <link href="/css/totopbutton.css" rel="stylesheet" type="text/css"/>
   <div class="hidden-xs hidden-sm" id="toTopContainer">
    <button class="learnMore-btn" href="#" id="toTopButton" onclick="toTopOfPage()" title="Back to Top">
     ➤
    </button>
   </div>
   <script>
    //The scroll up animation when clicking the button
    function toTopOfPage() {
        $("html,body").animate({ scrollTop: 0 }, "slow");
    }

    
    $(window).scroll(function () {
        var height = $(window).scrollTop();
        var footerOffset = $('#footer').offset().top;
        var viewportHeight = Math.max(document.documentElement.clientHeight, window.innerHeight || 0);
        //Adjusts when the button fades in and out
        if (height > viewportHeight) {
            $('#toTopButton').fadeIn();
            //Only makes the container if the height of the page is more than the viewportHeight and needs a toTop button
            $('#toTopContainer').css('height', $('#toTopButton').outerHeight().toString());
            $('#toTopContainer').css('margin-top', 10);
        } else {
            $('#toTopButton').fadeOut();
            $('#toTopContainer').css('height', 0);
            $('#toTopContainer').css('margin-top', 0);
        }
        //Adjusts when the button will stick itself back inside the container given to it (here it is right when the footer is revealed in the webpage)
        
        if (height + viewportHeight > footerOffset) {
            $('#toTopButton').css('position', 'relative');
            
        } else {
            $('#toTopButton').css('position', 'fixed');

        }
    });
   </script>
