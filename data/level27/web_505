    function logoSlider() {
        var n = $('#logoslider li').length;
        $('#logoslider').bxSlider({
            mode: 'horizontal',
            autoControls: false,
            maxSlides: 5,
            controls: false,
            slideWidth: 150,
            ticker: true,
            slideMargin: 10,
            speed: 5000 * n
        });
    };
