      <script>
       function menu() {
        //Sticks the topic menu to the top of the screen and then creates a box space-holder container where that topic menu used to be.
        $(window).bind('scroll', function () {
            if ($(window).scrollTop() > $('#menu-space-holder').offset().top) {
                $('#menu').addClass('menu_fixed');
                $('#menu-space-holder').addClass('edenMenu');
            } else {
                $('#menu').removeClass('menu_fixed');
                $('#menu-space-holder').removeClass('edenMenu');

            }
        });
    }
      </script>
