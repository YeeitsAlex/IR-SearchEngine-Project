    $(window).load(function () {
            $('#weather-api').weatherfeed(['72701']);
            $('#weather-api2').weatherfeed(['72701']);

            $('#email_subscribe').bootstrapValidator({
                feedbackIcons: {
                    valid: 'glyphicon glyphicon-ok',
                    invalid: 'glyphicon glyphicon-remove',
                    validating: 'glyphicon glyphicon-refresh'
                },
                live: 'enabled',
                submitButtons: 'button[id="subscribe-news"]',
                excluded: [':disabled'],
                fields: {
                    'subscribe.email': {
                        group: '.col-sm-12',
                        validators: {
                            notEmpty: {
                                message: 'Email address is required'
                            },
                            emailAddress: {
                                message: 'This is not a valid email address'
                            },
                            stringLength: {
                                max: 50,
                                message: 'The name must be less than 50 characters long'
                            }
                        }
                    }
                }
            });
        });
        
        $(document).ready(function () {
            var windowWidth = $(window).width();
            
            if (windowWidth <= 480) {
                $('.panel-collapse').removeClass('in');
                $('#headingOne a:first').on('click', function (e) {
                    
                    if ($('#headingTwo a').parents('.panel').children('.panel-collapse').hasClass('in')) {
                        $('#collapseTwo').addClass('collapse').removeClass('in');
                    }
                    if ($('#headingThree a').parents('.panel').children('.panel-collapse').hasClass('in')) {
                        $('#collapseThree').addClass('collapse').removeClass('in');
                    }
                    if ($('#headingFour a').parents('.panel').children('.panel-collapse').hasClass('in')) {
                        $('#collapseFour').addClass('collapse').removeClass('in');
                    }
                    $('#collapseOne').addClass('in');
                    e.preventDefault();
                    e.stopPropagation();
                   $('html, body').animate({
                        scrollTop: $("#accordionArticles").offset().top
                    }, 1500);
                });
                $('#headingTwo a:first').on('click', function (e) {
                    if ($('#headingOne a').parents('.panel').children('.panel-collapse').hasClass('in')) {
                        $('#collapseOne').addClass('collapse').removeClass('in');
                    }
                    if ($('#headingThree a').parents('.panel').children('.panel-collapse').hasClass('in')) {
                        $('#collapseThree').addClass('collapse').removeClass('in');
                    }
                    if ($('#headingFour a').parents('.panel').children('.panel-collapse').hasClass('in')) {
                        $('#collapseFour').addClass('collapse').removeClass('in');
                    }
                    $('#collapseTwo').addClass('in');
                    e.preventDefault();
                    e.stopPropagation();
                    $('html, body').animate({
                        scrollTop: $("#accordionFeatured").offset().top
                    }, 1500);
                });
                $('#headingThree a:first').on('click', function (e) {
                    if ($('#headingOne a').parents('.panel').children('.panel-collapse').hasClass('in')) {
                        $('#collapseOne').addClass('collapse').removeClass('in')
                    }
                    if ($('#headingTwo a').parents('.panel').children('.panel-collapse').hasClass('in')) {
                        $('#collapseTwo').addClass('collapse').removeClass('in');
                    }
                    if ($('#headingFour a').parents('.panel').children('.panel-collapse').hasClass('in')) {
                        $('#collapseFour').addClass('collapse').removeClass('in');
                    }
                    $('#collapseThree').addClass('in');
                    e.preventDefault();
                    e.stopPropagation();
                    $('html, body').animate({
                        scrollTop: $("#accordionEvents").offset().top
                    }, 1500);

                });
                $('#headingFour a:first').on('click', function (e) {
                    if ($('#headingOne a').parents('.panel').children('.panel-collapse').hasClass('in')) {
                        $('#collapseOne').addClass('collapse').removeClass('in')
                    }
                    if ($('#headingTwo a').parents('.panel').children('.panel-collapse').hasClass('in')) {
                        $('#collapseTwo').addClass('collapse').removeClass('in');
                    }
                    if ($('#headingThree a').parents('.panel').children('.panel-collapse').hasClass('in')) {
                        $('#collapseThree').addClass('collapse').removeClass('in');
                    }
                    $('#collapseFour').addClass('in');
                    $('html, body').animate({
                        scrollTop: $("#accordionHistory").offset().top
                    }, 1500);
                });
                $('#weather-api2').show();
                $('#weather-api').hide();
            } else {
                $('#weather-api').show();
                $('#weather-api2').hide();
                
                $('.panel-heading a:first').on('click', function (e) {
                    $('.panel-collapse').removeClass('collapse').addClass('in');
                    e.preventDefault();
                    e.stopPropagation();
                });
            }
            
            

            $("#s-news").on("click", function (e) {
                e.preventDefault();
                if ($('#hWebsite').val() !== '' && $('#subscribe_email').val() == '') {
                    return false;
                } else {
                    $.ajax({
                        type: "post",
                        url: "/subscription",
                        data: $("#email_subscribe").serialize(),
                        success: function (data) {
                            if (data.success) {
                                alert("Please go to your email account and confirm your subscription");
                                $("#subscribe_email").val('');
                            } else {
                                $("#subscribe_email").val('');
                                alert('Email address already exits in the system');
                            }
                        },
                        error: function (XMLHttpRequest, textStatus, errorThrown) {
                            alert('Email Address is required!!');
                        }
                    });
                }
                return false;
            });
        });

        var w = $(window).width();

        $(window).on('resize', function () {
            var wWidth = $(window).width();
            if (wWidth <= 751) {
                $('#weather-api2').show();
                $('#weather-api').hide();
            } else { $('#weather-api').show(); $('#weather-api2').hide(); }

            if (wWidth <= 480) {
                if (w != wWidth){ location.reload(); }
            } else {
                $('.panel-collapse').removeClass('collapse').addClass('in');
                $('.panel-heading a:first').on('click', function (e) {
                    $('.panel-collapse').removeClass('collapse').addClass('in');
                    e.preventDefault();
                    e.stopPropagation();
                });
            }
        });
   </script>
