<script id="x-custom-js">
 jQuery(document).ready(function() {
  jQuery(window).scroll(scrollFunction);
  jQuery(window).resize(resizeFunction);
  resizeFunction();

 jQuery('.x-nav-wrap.mobile').hide();
 console.log('test');
  jQuery(document).on('click', '.x-btn-navbar', function() {
        var target = jQuery(this).attr('data-target');

        jQuery(''+target+'').slideToggle(500);
    
    });
});

jQuery(window).load(function() {
  resizeFunction();
});

function scrollFunction() {  
  if(jQuery(window).scrollTop() >= 36) {
    jQuery(".x-navbar-wrap").addClass("fixed-navbar");
  }
  else {
    jQuery(".x-navbar-wrap").removeClass("fixed-navbar");
  }
}

function resizeFunction() {
  var subject_overview_height = 0;
  
	jQuery(".page-highlight-element.large").css({
    "height": "initial"
  });  
  
  jQuery(".page-highlight-element.large").each(function() {
    if(jQuery(this).outerHeight() > subject_overview_height) {
      subject_overview_height = jQuery(this).outerHeight();
    }
  });
  
  jQuery(".page-highlight-element.large").css({
    "height": subject_overview_height + "px"
  });
  
  jQuery(".faculty-highlight-element").each(function() {
    if(jQuery(this).parent().outerWidth() >= 700) {
      jQuery(this).addClass("horizontal");
    }
    else {
     	jQuery(this).removeClass("horizontal");
    }
  });
  
  jQuery(".student-highlight-element").each(function() {
    if(jQuery(this).parent().outerWidth() >= 700) {
      jQuery(this).addClass("horizontal");
    }
    else {
     	jQuery(this).removeClass("horizontal");
    }
  });
}
</script>

