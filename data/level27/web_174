<script>
 /*// Persist desktop search status
  var searchState = window.localStorage.getItem("state");
  if(searchState == null) {
    window.localStorage.setItem('state', 0);
  } else if (searchState == 1) {
    expand();
  }*/

  // If the text box in desktop nav bar search has data, expand the search box
  if ($('#cseq').val() != "") {
    expand();
  }

  $('#desktop-search-button').on('click', expand);


window.setInterval(function() {

  var current = new Date();
  var expiry  = new Date("January 25, 2019 11:00:00")
  var expiry2 = new Date("January 25, 2019 7:00:00")

  if(current.getTime()<expiry.getTime()){
    $('#one').show();
  }
  
  if(current.getTime()>expiry.getTime()){
    $('#two').show();
    $('#one').hide();/*change this back to hide */
  }
  
  if(current.getTime()>expiry2.getTime()){
    $('#two').hide();
    $('#one').show();
  }
 

}, 1000);

$('#one').show();
</script>
<div id="two" style="display:none">
