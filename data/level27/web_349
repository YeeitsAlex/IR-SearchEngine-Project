		});		
		
		setTopNavSelect();
		
		setTimeout("scrolltextup(1000)", 3000);
		
	});

	function hideForMobile()
	{
/*
		if ($(window).width() < 767) {
			$('#GlobalLinks').hide();
			$('#quicklinktitle').hide();
			$('.addrdiv').hide();
			$('.mbladdrdiv').show();
		} else {
			$('#GlobalLinks').show();
			$('#quicklinktitle').show();
			$('.addrdiv').show();
			$('.mbladdrdiv').hide();
			$("#GlobalLinks").attr("style","@media print{display:none;}");
		}
*/
		if ($(window).width() < 500) {
			$('#cdcfootertitle').text( "CDC" );
		} else {
			$('#cdcfootertitle').text( "Career Development Center" );
		}
	}
	
	function setTopNavSelect()
	{
		var curUrl = document.location.href;
		
		$('#mainnav').children('li').each(function(){
			var liUrl = $(this).find('a').attr('href');
			if ( (curUrl == "http://career.uark.edu/cdc/") && (liUrl == "/cdc/index.asp") ) 
				$(this).find('a').addClass("current");
			else if (curUrl.indexOf(liUrl) !== -1)
				//$(this).find('a').addClass("current");
			else
				//$(this).find('a').removeClass("current");
		});
	}
  </script>
