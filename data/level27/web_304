   <div class="current">
    <section>
     <h2 id="school-of-law-news">
      School of Law News
     </h2>
    </section>
    <div class="container">
     <div class="row">
      <div class="col-md-8" id="newsStories">
       <section>
        <h3 id="school-of-law-news" style="margin-top: 0 !important; padding-top: 0 !important;">
         School of Law
         <strong>
          NEWS
         </strong>
        </h3>
        <div id="news">
         <ul class="list-unstyled" id="newsArchive">
         </ul>
        </div>
        <script type="text/javascript">
         // 
    $(function () {
        $.ajax({
            url: "https://campusdata.uark.edu/apiv2/articles/SearchArticle?$orderby=publishDate+desc&$top=5&$filter=((Schools/any(s9:+s9/id+eq+9)))",
            data: {},
            dataType: 'jsonp',
            cache: 'true',
            success: function (articles) {
                dataDisplay = "";
                $.each(articles, function (key, a) {
                    var briefText = "", image = "";
                    var date = new Date(a.publishDate);
                    date = date.toDateString();
                    if (a.brief != null && a.brief != "") { briefText = a.brief;  briefText = briefText.replace(/(<p>|<\/p>)/g, "");}
                    if (a.image != null && a.image != "") {
                        image = a.image;
                    } else { image = "PMUA_Logo-web.jpg"; }
                    dataDisplay = "<li class='media'><div class='media-left'>" +
                                        "<a class='thumb' href='https://campusdata.uark.edu/resources/images/articles/" + image + "'>" +
                                            "<img class='media-object' src='https://campusdata.uark.edu/resources/images/articles/" + image + "' alt='' width='80' /></a>" +
                                    "</div>" +
                                    "<div class='media-body'>" +
                                        "<p class='media-heading'><a href='http://news.uark.edu/articles/" + a.id + "'>" + a.headline + "</a></p>" +
                                        "<p><i>" + date + "</i></p>" +
                                        "<p>" + briefText + "</p>" +
                                    "</div></li>";
                    $("#newsArchive").append(dataDisplay);
                });
            }
        });
    });
//
        </script>
       </section>
       <!-- inside news, no class or id -->
       <p class="">
        <a href="http://news.uark.edu/Articles/Archive/?school=school-of-law">
         more news →
        </a>
       </p>
      </div>
