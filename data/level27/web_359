        // SmartMenus jQuery init -->   
        $(function () {
            $('#main-menu').smartmenus({
                subMenusSubOffsetX: 1,
                subMenusSubOffsetY: -8
            });
        });
    
       $(document).ready(function () {

           $.urlParam = function (name) {
               var results = new RegExp('[\?&]' + name + '=([^&#]*)').exec(window.location.href);
               if (results == null) {
                   return null;
               }
               else {
                   return results[1] || 0;
               }
           };

           var accordion_index = parseInt($.urlParam('heading'));

           if (!accordion_index) {
               accordion_index = 0;
           }

           $(function () {
               $("#accordion").accordion({
                   collapsible: true,
                   active: accordion_index
               });
           });
                      
       });
  </script>
