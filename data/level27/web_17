            var jqxhr = $.ajax("https://calendar.uwgb.edu/RSSFeeds.aspx?data=P5WRNEbs5SVzVdy3sp3sN5Way%2bY8qqUJN0ItRs07jJD2aBoFVqmHsY9nbOk%2fgKwA")
                .done(function(data) {
                    $(data).find("item").each(function() {
                        if (itemCount <= maxItems) {
                            var el = $(this);
                            //console.log("------------------------");
                            //console.log("title      : " + el.find("title").text());
                            //console.log("link       : " + el.find("link").text());
                            //console.log("description: " + el.find("description").text());

                            var date;
                            
