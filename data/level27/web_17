    <script type="text/javascript">
     function googleTranslateElementInit() {
            new google.translate.TranslateElement({
                pageLanguage: 'en',
                includedLanguages: 'en,ar,zh-CN,hmn,ja,ko,ms,so,es',
                layout: google.translate.TranslateElement.InlineLayout.SIMPLE,
                gaTrack: true,
                gaId: 'UA-3904098-1'
            }, 'google_translate_element');
        }
    </script>
    <script src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit" type="text/javascript">
    </script>
    <!-- Translate Dropdown Functionality -->
    <script type="text/javascript">
     $('.translation-links a').click(function() {
            $("#translateMenu").dropdown("toggle");
            var lang = $(this).data("lang");
            var $frame = $('.goog-te-menu-frame:first');
            $frame.contents().find('.goog-te-menu2-item span.text:contains('+lang+')').get(0).click();
            if (lang != "English") {
                $frame.contents().find('.goog-te-menu2-item span.text:contains('+lang+')').get(0).click();
            }
        });
    </script>
   </div>
   <script>
    $(document).ready(function () {

    //when an item in mobile nav is clicked, close any other item in the nav that may already be open
    $('#main-nav-full li a').click(function() {
        if (!isMobile()) {
            switch ($(this).attr('id')) {
                case "nav-about":
                    window.location.href = "/sheboygan/about";
                    break;
                case "nav-campus-life":
                    window.location.href = "/sheboygan/campus-life";
                    break;
                case "nav-academics":
                    window.location.href = "/sheboygan/academics";
                    break;
                case "nav-apply-pay-register":
                    window.location.href = "/sheboygan/apply-pay-register";
                    break;
                case "nav-community-outreach":
                    window.location.href = "/sheboygan/community-outreach";
                    break;
                case "nav-continuing-ed":
                    window.location.href = "/sheboygan/athletics-recreation/overview/";
                    break;
            }
            //end switch
        } else {
            switch ($(this).attr('id')) {
                case "nav-about":
                    event.preventDefault();
                    break;
                case "nav-campus-life":
                    event.preventDefault();
                    break;
                case "nav-academics":
                    event.preventDefault();
                    break;
                case "nav-apply-pay-register":
                    event.preventDefault();
                    break;
                case "nav-community-outreach":
                    event.preventDefault();
                    break;
                case "nav-continuing-ed":
                    event.preventDefault();
                    break;
            }
            //end switch
        }
        //end if
    });

	$('.flexslider').flexslider({
    	animation: "slide",
    	randomize: true,
    	pauseOnHover: true,
    	controlNav: false,
    	directionNav: false,
    	nextText: "",
    	prevText: ""
  	});

   function socialHomeOver(myElement) {
        $(myElement).animate({
            marginLeft: '-35px'
        }, 100);
    }

    function socialHomeOut(myElement) {
        $(myElement).animate({
            marginLeft: '0px'
        }, 100);
    }

    //Begin make these navigate on click
    $('#social-home-facebook').click(function () {
        window.open(
            'https://www.facebook.com/uwgreenbay',
            '_blank'
        );
    });

    $('#social-home-twitter').click(function () {
        window.open(
            'https://www.twitter.com/uwgb',
            '_blank'
        );
    });

    /*
        $('#social-home-youtube').click(function () {
            window.open(
                'https://www.youtube.com/user/uwgb',
                '_blank'
            );
        });
    */

    $('#social-home-instagram').click(function () {
        window.open(
            'https://instagram.com/uwgb/',
            '_blank'
        );
    });
    //End make these navigate on click

    //Begin class switch for hover event, slide effect
    $(".social-home li").hover(function () {

            switch ($(this).attr('id')) {
            case 'social-home-facebook':
                socialHomeOver(this);
                break;
            case 'social-home-twitter':
                socialHomeOver(this);
                break;
                /*
            case 'social-home-youtube':
                socialHomeOver(this);
                break;
                */
            case 'social-home-instagram':
                socialHomeOver(this);
                break;
            }
        },
        function () {

            switch ($(this).attr('id')) {
            case 'social-home-facebook':
                socialHomeOut(this);
                break;
            case 'social-home-twitter':
                socialHomeOut(this);
                break;
                /*
            case 'social-home-youtube':
                socialHomeOut(this);
                break;
                */
            case 'social-home-instagram':
                socialHomeOut(this);
                break;
            }
        });
    //End class switch for hover event, slide effect
});
   </script>
