    // URL of feed to be parsed
    var feed = "https://calendar.uwgb.edu/RSSFeeds.aspx?data=P5WRNEbs5SVzVdy3sp3sN5Way%2bY8qqUJN0ItRs07jJD2aBoFVqmHsY9nbOk%2fgKwA";

