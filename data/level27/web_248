     <script>
      $(function() {

    $("#submitsearch").on("click", function(event) {
        event.preventDefault();
        if ($("#s").val().length > 0) {
            var baseURL = "http://www.uwgb.edu/search/index.asp?q=";
            var term = encodeURI($('#s').val().trim());

            window.open(baseURL + term);
        }
        //end if
    });

      $("#t").keypress("click", function(event) {
        if (event.which == 13) {

                    event.preventDefault();

                  if ($("#t").val().length > 0) {
            var baseURL = "http://www.uwgb.edu/search/index.asp?q=";
            var term = encodeURI($('#t').val().trim());

            window.open(baseURL + term);
        }
        //end if

  }


    });

});
     </script>
