	document.forms["primosearch"].onsubmit = function(event) { window.location = "https://uwi-primoalma-prod.hosted.exlibrisgroup.com/primo-explore/search?query=any,contains," + document.forms["primosearch"].primoQueryTemp.value + "&institution=WW&vid=WW&mode=Basic&displayMode=full&displayField=all&sortby=rank&lang=en_US&search_scope=" + document.forms["primosearch"].search_scope.value; return false; }
}
$(document).ready(function(a, b) {
    $('a[href*="' + a + '"]').each(function() {
        $(this).attr('href', $(this).attr('href').replace(a, b));
    });
	$('form[action*="' + a + '"]').each(function() {
        $(this).attr('action', $(this).attr('action').replace(a, b));
    });
}('uw-primo.hosted.exlibrisgroup.com', 'uwi-primoalma-prod.hosted.exlibrisgroup.com'));
