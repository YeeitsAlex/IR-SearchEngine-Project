                var visitorCountryCode = '';
        function getUrlVars() {
            let vars = {};
            let parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
                vars[key] = value;
            });
            return vars;
        }
        function geoip(json) {
            const query_param_identifier = 'visitorCountryCode=';
            const qpi_location_in_url = window.location.search.indexOf(query_param_identifier);
            if (qpi_location_in_url > -1) {
                const query_param_visitor_country_code = window.location.search.substr(qpi_location_in_url + query_param_identifier.length, 2);
                visitorCountryCode = query_param_visitor_country_code;
            } 
            else {
                visitorCountryCode = json.country_code;
            }
            if (
                (['CN', 'HK', 'MO'].indexOf(visitorCountryCode) > -1) && 
                (
                    typeof(getUrlVars().lang) === 'undefined' ||
                    getUrlVars().lang !== 'zh-hans'
                )
            ) {
                window.location.href = window.location.pathname + '?lang=zh-hans';
            }
        }
