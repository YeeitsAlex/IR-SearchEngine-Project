      <div class="smart_box palette-1" id="smartbox-id-7676">
       <div class="rev_slider_wrapper fullwidthbanner-container" data-source="gallery" id="rev_slider_10_1_wrapper" style="margin:0px auto;background:transparent;padding:0px;margin-top:0px;margin-bottom:0px;">
