  <script type="text/javascript">
   try{
if(document.getElementById('search_mini_form'))
{
var links = document.links;
for (i = 0; i < links.length; i++) {
if (links[i].href.search('/customer/account/login/') != -1) {
links[i].href = 'javascript:socialLogin.open();';
Event.observe(links[i], 'click', function () {
$('wsu_sso-login-social').style.display = "block";
if ($('sociallogin-other-a-popup')) $('sociallogin-other-a-popup').style.display = "block";
$('wsu_sso-popup').style.width="706px";
});
}
if (links[i].href.search('/wishlist/') != -1) {
links[i].href = 'javascript:socialLogin.open();';
Event.observe(links[i], 'click', function () {
$('wsu_sso-login-social').style.display = "block";
if ($('sociallogin-other-a-popup')) $('sociallogin-other-a-popup').style.display = "block";
$('wsu_sso-popup').style.width="706px";
});
}
if (links[i].href.search('/customer/account/') != -1) {
links[i].href = 'javascript:socialLogin.open();';
Event.observe(links[i], 'click', function () {
$('wsu_sso-login-social').style.display = "block";
if ($('sociallogin-other-a-popup')) $('sociallogin-other-a-popup').style.display = "block";
$('wsu_sso-popup').style.width="706px";
});
}
}
}
if(document.getElementById('product_comparison')){
var links = document.links;
for (i = 0; i < links.length; i++) {
if (links[i].href.search('/wishlist/') != -1) {
links[i].href = 'javascript:socialLogin.open();';
}
}
}
if(document.getElementById("checkout-step-login"))
{
$$('.col-2 .buttons-set').each(function(e) {
e.insert({bottom: '<div id="multilogin"> <button type="button" class="button" style="" onclick="javascript:socialLogin.open();" title="Social Login" name="headerboxLink1" id="headerboxLink1"><span><span>Social Login</span></span></button></div>'});
});
}
}catch(exception)
{ alert(exception);}
  </script>
  <script type="text/javascript">
   Event.observe(window, 'load', function () {
socialLogin = new Lightbox('wsu_sso-popup');
Event.observe('bg_fade', 'click', function () {
socialLogin.close();
});
});
Event.observe('sociallogin-close', 'click', function () {
socialLogin.close();
});
Event.observe(window, 'resize', function () {
socialLogin._centerWindow('wsu_sso-popup');
});
  </script>
  <script type="text/javascript">
   Event.observe(window, 'load', function() {
var options = {
email: document.getElementById('wsu_sso-sociallogin-popup-email').value,
pass : document.getElementById('wsu_sso-sociallogin-popup-pass').value,
login_url : "https://oc.store.wsu.edu/sociallogin/popup/login",
send_pass_url : "https://oc.store.wsu.edu/sociallogin/popup/sendPass",
create_url : "https://oc.store.wsu.edu/sociallogin/popup/createAcc",
};
Login = new LoginPopup(options);
});
  </script>
