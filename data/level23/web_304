     <script src="/javascripts/jquery-1.11.2.min.js" type="text/javascript">
      function showSiteNav()
     {
          $("#footer").addClass("expanded");
          $("#site-navigation h1").addClass("less");
          $("#site-navigation &gt; div").stop().slideDown(300, "easeOutCirc");
         
     }
     function hideSiteNav()
     {
          $("#footer").removeClass("expanded");
          $("#site-navigation h1").removeClass("less");
          $("#site-navigation &gt; div").stop().slideUp(300, "easeInCirc");

     }
$(document).ready(function(){

     if ( ! (navigator.userAgent.match(/webOS/i) ||
               navigator.userAgent.match(/Android/i))
        )
     {
          $("#site-navigation").addClass("siteNavFixed");
         
          if ( navigator.userAgent.match(/iPhone/i) ||
                navigator.userAgent.match(/iPad/i) ||
                navigator.userAgent.match(/iPod/i)
             )
          {
               $("#site-navigation").addClass("iOS");         
          }
          $("#site-navigation &gt; div").hide();
          $("#site-navigation h1").show();
          $("#site-navigation h1").click(function()
               {
                    $("#site-navigation").hoverIntent(null);
                    $("#footer").toggleClass("expanded");
                    $("#site-navigation h1").toggleClass("less");
                    $("#site-navigation &gt; div").stop().slideToggle(300, "easeOutCirc");
                    $("#site-navigation").hoverIntent({sensitivity:7, interval:200, over:showSiteNav, timeout:800, out:hideSiteNav});
               });
     }

    
    
     // Top main image change when small images mouseover
     $("#features li").mouseover(function()
          {
               if ( $(this).hasClass("selected") )
               {
                    return;    
               }
              
               var index = $("#features ul li").index($(this)) + 1;

               $("li.selected, li.selected span").removeClass("selected");
               $(this).addClass("selected").children("span").addClass("selected");
               $("#feature-select article.featured").stop(true,true).fadeOut(350).removeClass("featured");
               $("#feature-select article:nth-child(" + (index+1) + ")").fadeIn(350).addClass("featured");    
               $("#feature-caret").css("left",((index-1)*234)+82);  // 234=width of img, 82=initial position
        });
    
     //fancybox for videos
     $(".fancybox").fancybox({
          openEffect  : 'none',
          closeEffect : 'none',
          scrolling   : 'visible',
          type          : 'iframe',
          helpers :
               {
                    media : {
                         youtube : {
                              params : {
                                   wmode : 'window'
                              }
                         }
                    }
               },
          beforeLoad : function(){$("#site-navigation").addClass("fancybox");},
          beforeClose : function(){$("#site-navigation").removeClass("fancybox");}
     });

     $("#site-navigation").hoverIntent({sensitivity:7, interval:200, over:showSiteNav, timeout:800, out:hideSiteNav});

});
     </script>
