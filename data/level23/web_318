     <div class="imc-stat-numbers large-4 medium-4 small-12 columns">
      <h4>
       <h1 style="text-align: center; color: #002855; font-size: 5em; margin: 0;">
        20
       </h1>
      </h4>
      <h6>
       <h4 style="margin: 0px;">
        Students per course
       </h4>
      </h6>
     </div>
    </div>
   </div>
  </div>
