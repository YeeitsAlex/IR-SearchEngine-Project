(function($){
    $(".innerHidden").hide();
	$("#Section5").click(function(){
        $("#innerHidden5").animate( { "opacity": "toggle", "top":"250px"} , "slow" );
        if ($("#Toggle5").hasClass('shown')){
            $("#Toggle5").removeClass('shown').attr('src', "https://s3.wp.wsu.edu/uploads/sites/16/2016/04/closed_white.png" );
    	}
        else{
        	$("#Toggle5").addClass('shown');
            $("#Toggle5").attr('src', "https://s3.wp.wsu.edu/uploads/sites/16/2016/04/open_white.png" );	
        }
	});

