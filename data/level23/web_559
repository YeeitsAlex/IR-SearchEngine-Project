   (function($){
  $(document).ready(function(){
    if (($('body').hasClass('post-template-default')) && ($('body').hasClass('categorized-ccb-news'))){
    	if (($('body').hasClass('cob-palette-default')) && ($('body').hasClass('cob-palette-text-default'))){
    		$('body').removeClass('cob-palette-default cob-palette-text-default').addClass('cob-palette-wazzu-1 cob-palette-text-wazzu-1');
    	}
		$('section.row.side-right div.column.two').remove();
		$('section.row.side-right.gutter.pad-ends').removeClass('side-right').addClass('single');
	}
 	});
}(jQuery));

