  <!-- Custom JavaScript Editor -->
  <script>
   jQuery(document).ready(function () {
  jQuery.getScript('https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.0/jquery.easing.min.js');
  var currpage = document.location.pathname;
  switch (currpage) {
    case "/":
      jQuery.getScript('https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.17.1/moment.min.js', function () { // Load momentJS from CDN
        var myFormat = "MMMM D",
          myTZ = "-8", //Local timezone
          now = moment(),
          today = moment(now).utcOffset(myTZ).format(myFormat),
          tomorrow = moment(now).utcOffset(myTZ).add(1, "days").format(myFormat),
          expArticle = moment(now).subtract(6, "months"); //Set expiration period for articles
    
        jQuery( "span.content-item-event-date" ).each(function () { //Change calendar entries today and tomorrow from dates to relative titles
          var dateToCheck = jQuery(this).text();
          if (dateToCheck === today) { jQuery(this).html("Today").closest("li").addClass("today"); }
            else if (dateToCheck === tomorrow) { jQuery(this).html("Tomorrow"); }
        });

        jQuery( ".pinned span.content-item-byline-date" ).filter(function () { //Remove expired articles
          return moment(jQuery(this).text()).isBefore(expArticle);
        }).closest( "li" ).remove();
        removeDuplicateFeeds();
        jQuery( ".pinned ul:not(:has(li))" ).closest( ".pinned" ).remove(); //Remove pinned stories container if it contains no articles
      });
    break;
    case "/directory/":
      paginateScroll();
    break;
    default: break;
  }
});

function removeDuplicateFeeds() { //Remove repeated headlines from the secondary list
  var seen = {};
  jQuery('.wsuwp-content-syndicate-item').each(function () {
    var articleHead = jQuery(this).find('a:first').text();
    if (seen[articleHead]) { jQuery(this).closest('li').remove(); }
    else { seen[articleHead] = true; }
  });
}

function paginateScroll() {
  jQuery( ".dataTable" ).fadeOut(0, function () {
    jQuery('html, body').animate({
      scrollTop: jQuery(".dataTables_wrapper").offset().top}, 0, function() {
      jQuery( ".dataTable" ).fadeIn(1000, "easeOutCubic");
    });
  });
    jQuery(".paginate_button").unbind('click', paginateScroll);
    jQuery(".paginate_button").bind('click', paginateScroll);
}
  </script>
 </body>
</html>
