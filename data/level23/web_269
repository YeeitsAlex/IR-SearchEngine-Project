      <script type="text/javascript">
       function changeImg(imgNumber) {
	var myImages = ["/assets/bg02.jpg", "/assets/bg03.jpg"];
	var imgShown = document.getElementById('main-container').style.backgroundImage;
	var newImgNumber = Math.floor(Math.random() * myImages.length);
	document.getElementById('main-container').style.backgroundImage = 'url('+myImages[newImgNumber]+')';
	}
	window.onload=changeImg;
      </script>
      <script>
       //Homepage Feature
	$(window).bind("load resize", function() {
		var wvu = $('.wvu-header:visible').outerHeight(true);
	  var wvrhc = $('.wvrhc-header:visible').outerHeight(true);
	  var con = $('#content').outerHeight(true);
		var windowih = window.innerHeight;
		var pieces2 = wvu + wvrhc;
		var feature2 = windowih - pieces2;
		var topm = feature2 - con;
		$('#main-container').css('height', feature2);
		$('#main-container').css('max-height', 880);
		$('#main-container').css('min-height', con);
		$('#main-container > div:nth-child(2)').css('height', feature2);
		$('#main-container > div:nth-child(2)').css('max-height', 880);
		$('#main-container > div:nth-child(2)').css('min-height', con);
	});
      </script>
