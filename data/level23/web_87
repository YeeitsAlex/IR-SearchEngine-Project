		/*
		NexusConnectSDK.setCustomizationOptions({
			"themeUrl":"https://www.sdcryan.com/nexus/selfsupport/template-theme.css"
		}); 
		*/
		//The following code sets up the event handler for initiating chat
		$(document.body).prepend('<div id="nxlivehelp" class="nx-live-help"></div>');
		var container = $('#nxlivehelp');
		var liveHelpLink =  $('.openChat');
		liveHelpLink.append(container);
		container.dialog({
			modal: false,
			title: "",
			autoOpen: false,
			resizable: true,
			minWidth: 400,
			width: 400,
			minHeight: 340,
			height: 340,
			dialogClass: "nx-ui-dialog",
			open: function( event, ui ) {
				var dElm = $(this);
						
				//Optional context information - "Breadcrumb History"
				NexusConnectSDK.logAction("myActionName");				
				
				//Optional context information - "Other Context"				
				NexusConnectSDK.logContent("Source", "https://utsaoit.nexus.support.com");				

				NexusConnectSDK.showLiveHelp(dElm.get(0), {
					mode: "chat",					
					skill: "OITConnect Chat",
					remoteConnectionType: "remote"
				});
			},
			beforeClose: function(event, ui) {              
				//Warn of disconnect
			},
			close: function(event, ui) {              
				NexusConnectSDK.exitHelp();              
			}
		});
		liveHelpLink.off("click")
			.on("click", function showLiveHelp(e) {
				if (container.dialog("isOpen")) {
					container.dialog("close");
				} else {
						container.dialog("option", "position", { my: "right top", at: "left top", of: ".openChat" });
					container.dialog("open");
				}
				e.preventDefault();
				return false;
			});
		liveHelpLink.trigger('click');
  
    });
  </script>
  <!-- END: JS -->
  <!-- page scripts -->
 </body>
</html>
