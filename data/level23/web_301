        var h3s = $("h3.message").get().sort(function(){ 
            return Math.round(Math.random())-0.5;
        }).slice(0,1);
        $(h3s).appendTo(h3s[0].parentNode).show();
    });
     </script>
     <style type="text/css">
      .quote {
min-height: 120px;
margin-top: 20px !important;
margin-bottom: -200px !important;
border-bottom: solid 2px #ccc;
border-top: solid 2px #ccc;
padding: 10px 20px;
}
  .quote h3 {
text-align: center;
font-size: 2.25em;
/* text-shadow: 1px 1px 1px #686868; */
color: #124661;
font-family: "Calibri" !important;
font-weight: 200;
display: none;
}
     </style>
