        <script>
         window.jQuery || document.write('<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"><\/script>')    	
    	
	function add_q_main(){
		jQuery("#q-main-search").val(jQuery("#search-text-NAM #s").val());
	}
        </script>
        <form action="https://nam.edu/" id="searchform" method="get" onsubmit="add_q_main();">
         <div id="search-text-NAM">
