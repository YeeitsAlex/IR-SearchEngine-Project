    <script>
     function goHome(){
			jQuery('#mobileHome').addClass('open');
			window.location.href='/';
		}
		function toggleYMenu(){
			jQuery('#mobileMenu span').removeClass('open');
			jQuery('#mobileMenuContent>div').slideUp('300');
			jQuery('#ceMenu').toggle('slide').toggleClass('open');
			jQuery('#mobileYMenu').toggleClass('fa-chevron-right').toggleClass('fa-chevron-left');
		}
		function toggleMobileMenu(){
			if(jQuery('#ceMenu').hasClass('open')){
				jQuery('#ceMenu').removeClass('open').toggle('slide');
				jQuery('#mobileYMenu').toggleClass('fa-chevron-right').toggleClass('fa-chevron-left');
			}
			jQuery('#mobileMenu span:not(#mobileMenuToggle)').removeClass('open');
			jQuery('#mobileMenuToggle').toggleClass('open');
			jQuery('#mobileMenuContent>div:not(#mobileMainMenu)').slideUp('300');
			jQuery('#mobileMainMenu:visible').slideUp('300');
			jQuery('#mobileMainMenu:hidden').slideDown('300');
		}
		function toggleMobileSearch() {
			if(jQuery('#ceMenu').hasClass('open')){
				jQuery('#ceMenu').removeClass('open').toggle('slide');
				jQuery('#mobileYMenu').toggleClass('fa-chevron-right').toggleClass('fa-chevron-left');
			}
			jQuery('#mobileMenu span:not(#mobileSearchToggle)').removeClass('open');
			jQuery('#mobileSearchToggle').toggleClass('open');
			jQuery('#mobileMenuContent>div:not(#mobileSearch)').slideUp('300');
			jQuery('#mobileSearch:visible').slideUp('300');
			jQuery('#mobileSearchBox').show().focus();
			jQuery('#mobileSearch:hidden').slideDown('300');
		}
		function toggleMobileContact() {
			if(jQuery('#ceMenu').hasClass('open')){
				jQuery('#ceMenu').removeClass('open').toggle('slide');
				jQuery('#mobileYMenu').toggleClass('fa-chevron-right').toggleClass('fa-chevron-left');
			}
			jQuery('#mobileMenu span:not(#mobileContactToggle)').removeClass('open');
			jQuery('#mobileContactToggle').toggleClass('open');
			jQuery('#mobileMenuContent>div:not(#mobileContact)').slideUp('300');
			jQuery('#mobileContact:visible').slideUp('300');
			jQuery('#mobileContact:hidden').slideDown('300');
		}
		function hideSearch() {
			jQuery('#mobileSearch').hide();
		}		
		/*function runSearch(value){
			window.location.href='/search/google/' + encodeURIComponent(value);
		}*/
		hideSearch();
		jQuery('#mobileMainMenu').hide();
		jQuery('#mobileContact').hide();
		/*jQuery("#searchBox, #mobileSearchBox").keyup(function(event){
			if(event.keyCode == 13){
				runSearch(jQuery("#searchBox").val());
			}
		});*/
		jQuery(function(){
			jQuery('#ceHeader ul.menu > li > a').click(function(){
				var menuItem = jQuery(this).parent('li');
				if(menuItem.hasClass('expanded')){
					if(menuItem.hasClass('open')){
						menuItem.find('ul.menu').slideToggle(function(){
							menuItem.removeClass('open');
						});
					} else {
						menuItem.addClass('open').find('ul.menu').slideToggle();
					}
					return false;
				}
			});
			jQuery('.menu a').each(function(){
				var attr = jQuery(this).attr('href');
				if(typeof attr !== 'undefined' && attr !== false) {
					if(attr.indexOf('http://#') >= 0) {
						jQuery(this).click(function(){return false;});
					}
				}
			});
		});
    </script>
