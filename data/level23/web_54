<script type="application/javascript">
 jQuery(document).ready(function( $ ) {
			var originalURL = "";			var currentURL = window.location.hostname;
			console.log("originalURL: "+originalURL);
			console.log("currentURL: "+currentURL);
			
			if(originalURL.indexOf(currentURL) != -1) {
				console.log("match, leave it");
			} else {
				console.log ("different domain, replace it");
				$('p.wrap_login_logout a').attr('href', function(){
					//var getlink = this.href;
					//getlink = getlink.replace(currentURL,originalURL);
					//$('p.wrap_login_logout a').attr('href',getlink);
					
					
					console.log("fin change: "+this.href);
				});
				console.log("changed");
			}
		});
</script>
