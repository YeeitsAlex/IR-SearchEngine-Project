        }

        jQuery(function($) {

            //if an inner page does not have any content in the sub menu on the left then make the main content full width
            if ($('#secondary-nav .section-navigation').is(':empty') && $('#secondary-nav .quicklinks').length == 0) {
                $('#secondary-nav').remove();
            }

            secondaryNavHeight = $('aside.secondary-nav').innerHeight();
            contentSectionHeight = $('main.content-section').innerHeight();

            if (secondaryNavHeight > contentSectionHeight) {
                $('.content-section').css('height', secondaryNavHeight);
            }

            //polyfill for Android 2.3 to get click event working
            Modernizr.load({
                test: Modernizr.classlist,
                nope: 'Documents/m/morehouse-college/js/classList.min.js'
            });
        });
  </script>
