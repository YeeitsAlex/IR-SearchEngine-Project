			var originalURL = "";			var currentURL = window.location.hostname;
			console.log("originalURL: "+originalURL);
			console.log("currentURL: "+currentURL);
			
			if(originalURL.indexOf(currentURL) != -1) {
				console.log("match, leave it");
			} else {
				console.log ("different domain, replace it");
