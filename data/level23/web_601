     <article class="node node--type-tufts_landing_page node--view-mode-full clearfix node--promoted">
      <div>
       <div class="">
        <div class="block-region-top">
         <div class="block block-entity_field_node_field_header block-node-tufts_landing_page block-field_header">
          <div class="background-header-media" data-id="5">
          </div>
         </div>
        </div>
       </div>
       <div class="template-main-content">
        <div class="block-region-main">
         <div class="block-paragraph block block-block_content_72b50b40-525d-4032-9ea9-f3caab5a1499 block-content block-content--paragraph block-content-- paragraph">
          <div class=" same-height margin-top-20 ">
           <div class="paragraph paragraph--type--bp-columns paragraph--view-mode--default clearfix">
            <div class="paragraph__column">
             <div class="paragraph--type--bp-columns__2col same-height-content">
              <div class=" height-full">
               <div class="paragraph paragraph--type--element paragraph--view-mode--default clearfix height-full" id="">
                <div class="paragraph paragraph--type--tufts-paragraph-up-button paragraph--view-mode--default">
                 <a class="up-button display-block " href="/academics/undergraduate" style="">
