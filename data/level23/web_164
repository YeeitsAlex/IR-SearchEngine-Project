	});


 $(function() {
// run the currently selected effect
function runEffect() {
	var options = {};
	// run the effect
	$( ".caption" ).show( "blind", options, 500, callback );
	};

//callback function to bring a hidden box back
function callback() {
	};
	
// set effect from select menu value
$( "#buttonShow" ).click(function() {
	runEffect();
	return false;
	});

$( "#buttonHide" ).click(function() {
	$(".caption").hide();
	return false;
	});

$( ".caption" ).hide();

});
  </script>
