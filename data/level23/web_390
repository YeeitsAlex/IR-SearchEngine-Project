         <!--front panel regions beg-->
         <div class="at-panel gpanel panel-display content clearfix" id="content-panels">
          <div class="region region-content-top">
           <div class="region-inner clearfix">
            <div class="block block-boxes block-boxes-os_boxes_media no-title" delta="1538045178" id="block-boxes-1538045178" module="boxes">
             <div class="block-inner clearfix">
              <div class="block-content content">
               <div class="boxes-box" id="boxes-box-1538045178">
                <div class="boxes-box-content">
                 <div class="file file-html file-html-embed" id="file-15036">
                  <h2 class="element-invisible">
                   <a href="">
                    fe815e96b0a7b516c3ed46f13d209307
                   </a>
                  </h2>
                  <div class="content">
                   <div class="file-info">
                   </div>
                   <div class="field field-name-field-html-code field-type-text-long field-label-hidden view-mode-full">
                    <div class="field-items">
                     <div class="field-item even">
                      <div class="main-carousel" data-flickity='{ "imagesLoaded": true, "draggable": true, "pageDots": true, "prevNextButtons": false, "autoPlay": 4500, "wrapAround": true, "selectedAttraction": 0.005, "friction": 0.2 }' style="max-width:1200px; margin-left:auto; margin-right:auto;">
                       <div class="carousel-cell full-size">
                        <img alt="" class="carousel-cell-image" src="http://itatti.harvard.edu/sites/default/files/itatti/files/arcobaleno_banner.jpg"/>
                        <div class="flickity-caption">
                         <h2 class="caption-title">
                          <a href="/about" style="text-decoration:none;">
                           An intellectually vibrant setting
                          </a>
                         </h2>
                         <p class="caption-p">
                          Our location on the foothills of Florence encourages thoughtful and creative intellectual development
                         </p>
                        </div>
                       </div>
                       <div class="carousel-cell full-size">
                        <img alt="" class="carousel-cell-image" src="http://itatti.harvard.edu/sites/default/files/itatti/files/appointees18slide.jpg?m=1537189933"/>
                        <div class="flickity-caption">
                         <h2 class="caption-title">
                          <a href="/current-appointees" style="text-decoration:none;">
                           Meet our current appointees
                          </a>
                         </h2>
                         <p class="caption-p">
                          47 scholars from around the world will join us during academic year 2018/19
                         </p>
                        </div>
                       </div>
                       <div class="carousel-cell full-size">
                        <img alt="" class="carousel-cell-image" src="http://itatti.harvard.edu/sites/default/files/itatti/files/wkrrslide_01.jpg"/>
                        <div class="flickity-caption">
                         <h2 class="caption-title">
                          <a href="/berenson-library" style="text-decoration:none;">
                           World-leading Resources
                          </a>
                         </h2>
                         <p class="caption-p">
                          The Biblioteca Berenson plays a vital role in I Tatti's mission to advance scholarship in Italian Renaissance Studies
                         </p>
                        </div>
                       </div>
                       <div class="carousel-cell full-size">
                        <img alt="" class="carousel-cell-image" src="http://dev.itatti.harvard.edu/sites/default/files/itatti/files/dsc_6580.jpg"/>
                        <div class="flickity-caption">
                         <h2 class="caption-title">
                          <a href="/community" style="text-decoration:none;">
                           Scholarly Community
                          </a>
                         </h2>
                         <p class="caption-p">
                          I Tatti has welcomed over one thousand appointees from around the world working in the fields of Italian Renaissance art, history, literature, philosophy, history of science, and music
                         </p>
