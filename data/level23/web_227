  <script>
   $(window).load(function() {
function a11yFixes() {
    $("#livechat-eye-catcher-img > img:nth-child(1)").attr("alt","Let's talk. Click to activate the chat feature.");
    $("#livechat-eye-catcher-img").attr("alt","Let's talk. Click to activate the chat feature.");
    $("#livechat-eye-catcher a:first-of-type").attr("aria-label","Click to close the Let's Talk bubble.");
    $("#livechat-compact-container a#livechat-badge").attr("aria-label","Click to open the chat window.");
    $("#social-stream img.icon").each(function(){
      if(/youtube/i.test($(this).parent().attr("href"))) { $(this).attr("alt","I U Kokomo's Youtube channel.");}
      if(/flickr/i.test($(this).parent().attr("href"))) { $(this).attr("alt","I U Kokomo's Flicker account.");}
      if(/facebook/i.test($(this).parent().attr("href"))) { $(this).attr("alt","I U Kokomo's Facebook page.");}
      if(/twitter/i.test($(this).parent().attr("href"))) { $(this).attr("alt","I U Kokomo's Twitter feed.");}
    });
    console.log("hello");
    $("#social-stream span.section-thumb a img").each(function(){
      if(/twitter/i.test($(this).parent().attr("href"))) { $(this).attr("alt","I U Kokomo's Twitter feed.");}
      if(/facebook.+\/photos\//i.test($(this).parent().attr("href"))) { $(this).attr("alt","I U Kokomo's Facebook photos.");}
      if(/flickr/i.test($(this).parent().attr("href"))) { $(this).attr("alt","I U Kokomo's Flicker photos.");}
      if(/YouTube/i.test($(this).attr("alt"))) { $(this).attr("alt",'YouTube video for: '+$(this).parent().attr("title"));}
    });
  }
    a11yFixes();
    });
  </script>
