	function addTimberlakeTracker () {
		gaTimberlake('create', 'UA-64559130-1', 'auto', 'TimberlakeTracker');
    	gaTimberlake('set', 'anonymizeIp', true);
    	gaTimberlake('TimberlakeTracker.send', 'pageview');
	}

    function addClientTracking(){
        gaTimberlake('create', 'UA-47855177-1', 'auto', 'ClientTracker0');
gaTimberlake('ClientTracker0.send', 'pageview');

    }
    
