    <div class="widget wrapper widget-eventlist" id="siteline-panel-event-list-4247" style="">
     <div class="content">
      <div class="widget-instance widget-horizontal-event-list" id="horizontal-event-list">
       <div class="view view-vendini-event-list view-id-vendini_event_list view-display-id-H">
        <div class="view-content">
        </div>
       </div>
       <script type="text/template">
        <%
var showbillPath, startEventDate, endEventDate, eventDate;
_.each(eventsList.events, function(event) {


	showbillPath = '';
	if (event.showbillPath.length && showImage) {
		showbillPath = contentURL + event.showbillPath.substr(0, event.showbillPath.lastIndexOf('.')) + showbillSize + event.showbillPath.substr(event.showbillPath.lastIndexOf('.'));
	}
%>
<div class="clearfix event-single">
<%
	if (showbillPath) {
%>
	<div class="event-showbill" style="background-image: url('<%- showbillPath %>')" />
<%
	}
%>
	<div class="event-shim"></div>
	<div class="event-info">
		<div class="event-body">
			<div class="event-heading">
<%
				if (showVenue) {
%>
				<h4 class="event-venue"><%- event.venueName %></h4>
<%
				}

				if (showTitle) {
%>
				<h3><%- event.title %></h3>
<%
				}

				if (showDateRange) {
					startEventDate = VendiniDateFormatter.formatDate('MMM D, YYYY', event.firstPerformanceDate, event.timezone);
					endEventDate = VendiniDateFormatter.formatDate('MMM D, YYYY', event.lastPerformanceDate, event.timezone);

					if (startEventDate == endEventDate) {
						eventDate = startEventDate;
					}
					else{
						eventDate = startEventDate + ' - ' + endEventDate;
					}
%>
				<h4 class="event-date"><%- eventDate %></h4>
<%
				}
%>
			</div>
			<div class="event-showbill-mobile clearfix">
<%
				if (showbillPath) {
%>
				<div class="event-showbill" style="background-image: url('<%- showbillPath %>')" />
<%
				}
%>
				<div class="event-shim"></div>
				<div class="event-buttons">
<%
					if (moreInfo) {
%>
					<a class="button info" href="<%- baseURL %>event/<%- event.eventID %>"><span>info</span></a>
<%
					}
					if (event.shouldDisplayBuyButton) {
%>
					<a class="button tickets" target="_blank" href="<%- ticketLineURL %>?e=<%- event.eventID %>&t=tix"><span>tickets</span></a>
<%
					}
%>
				</div>
			</div>
			<p>
<%
				if (event.description.length && showDescription) {
%>
				<%- event.description %>
<%
				}
%>
			</p>
		</div>
		<div class="event-buttons">
<%
			if (moreInfo) {
%>
			<a class="button info" href="<%- baseURL %>event/<%- event.eventID %>"><span>info</span></a>
<%
			}
			if (event.shouldDisplayBuyButton) {
%>
			<a class="button tickets" target="_blank" href="<%- ticketLineURL %>?e=<%- event.eventID %>&t=tix"><span>tickets</span></a>
<%
			}
%>
		</div>
	</div>
</div>
<%
});
%>
       </script>
      </div>
