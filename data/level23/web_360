   <script>
    function commaSeparateNumber(val) {
    console.log(val);
    while (/(\d+)(\d{3})/.test(val.toString())){
      val = val.toString().replace(/(\d+)(\d{3})/, '$1'+','+'$2');
    }
    return val;
  }

  function commaSeparateNumber(val){
    while (/(\d+)(\d{3})/.test(val.toString())){
      val = val.toString().replace(/(\d+)(\d{3})/, '$1'+','+'$2');
    }
    return val;
  }

  function warn(el) {
    document.getElementById(el+'-warning').classList.toggle('hidden');
    document.getElementById(el+'-warning').classList.toggle('warning-bar');
    document.getElementById(el+'-field').classList.toggle('warning-field-section');
    document.getElementById(el+'-num').classList.toggle('warning-field-number');
  }

  function removeSetClasses(el) {
    document.getElementById(el+'-warning').classList.add('hidden');
    document.getElementById(el+'-warning').classList.remove('warning-bar');
    document.getElementById(el+'-field').classList.remove('warning-field-section');
    document.getElementById(el+'-num').classList.remove('warning-field-number');
  }

  document.getElementById('tuition-calculator-form').onsubmit = function(event) {
    event.preventDefault();
  }

  document.getElementById('tuition-calculator-submit').onclick = function(event) {

    removeSetClasses('enrollment');
    removeSetClasses('resident');
    removeSetClasses('study');

    document.getElementById('estimate-results-error').classList.add('hidden');
    document.getElementById('estimate-table').classList.add('hidden');

    var error = false;
    var enrollment = document.querySelector('input[name="enrollment-select"]:checked');
    var resident   = document.querySelector('input[name="resident-select"]:checked');
    var select     = document.querySelector('select[name="study-select"]');

    // check for missing vars
    if ((enrollment === null) || (enrollment.getAttribute('value') === '')) {
      error = true;
      warn('enrollment');
    } else {
      var enrollmentSelect = enrollment.getAttribute('value');
    }

    if ((resident === null) || (resident.getAttribute('value') === '')) {
      error = true;
      warn('resident');
    } else {
      var residentSelect = resident.getAttribute('value');
    }

    if ((select.options[select.selectedIndex] === undefined) || (select.options[select.selectedIndex].getAttribute('value') === '')) {
      error = true;
      warn('study');
    } else {
      var studySelect = select.options[select.selectedIndex].getAttribute('value');
      var studySelectT  = select.options[select.selectedIndex].innerHTML;
    }

    if (!error) {

      // set-up the query string
      var queryString = ''
      queryString += '&level='+enrollmentSelect;
      queryString += '&majorKey='+studySelect;
      queryString += '&residency='+residentSelect;
      queryString += '&campus=morgantown';
      queryString += '&semester=fallspring';
      queryString += '&year=2018';
      queryString += '&livingArrangement=oncampus';
      queryString += '&score=16';

      // create the script element
      var script = document.createElement('script');
      script.src = 'https://f09bec8ldd.execute-api.us-east-1.amazonaws.com/stable/tuition-calculator?callback=response'+queryString;
      document.body.appendChild(script);

      // test string
      // https://f09bec8ldd.execute-api.us-east-1.amazonaws.com/beta/tuition-calculator?callback=response&campus=morgantown&level=undergraduate&majorKey=3050bsme&semester=fallspring&year=2018&residency=resident&livingArrangement=oncampus&score=1300
      // https://f09bec8ldd.execute-api.us-east-1.amazonaws.com/beta/tuition-calculator?callback=response&campus=morgantown&level=undergraduate&majorKey=4550aud&semester=fallspring&year=2018&residency=resident&livingArrangement=oncampus&score=1300

    } else {

      window.scrollTo({ top: document.getElementById('estimate-form').offsetTop, behavior: 'smooth'});

    }

  }

  function response(data) {

    document.getElementById('fallspring-single-semester-hours').innerHTML = (data.queryOptions.level === 'undergraduate') ? '12' : '9';
    document.getElementById('fallspring-both-semester-hours').innerHTML   = (data.queryOptions.level === 'undergraduate') ? '24' : '18';

    var tuitionCost = data.tuitionAndFees;

    console.log(data);

    var uTuitionC = tuitionCost.universityTuitionPerCredit;
    var uFeesC    = tuitionCost.universityFeesPerCredit;
    var cTuitionC = tuitionCost.collegeTuitionPerCredit;
    var uTuition  = tuitionCost.universityTuitionPerSemester;
    var uFees     = tuitionCost.universityFeesPerSemester;
    var cTuition  = tuitionCost.collegeTuitionPerSemester;

    document.getElementById('utuition-credit').innerHTML = '$'+commaSeparateNumber(uTuitionC);
    document.getElementById('utuition-single').innerHTML = '$'+commaSeparateNumber(uTuition);
    document.getElementById('utuition-double').innerHTML = '$'+commaSeparateNumber(uTuition*2);
    document.getElementById('ufees-credit').innerHTML = '$'+commaSeparateNumber(uFeesC);
    document.getElementById('ufees-single').innerHTML = '$'+commaSeparateNumber(uFees);
    document.getElementById('ufees-double').innerHTML = '$'+commaSeparateNumber(uFees*2);
    document.getElementById('ctuition-credit').innerHTML = '$'+commaSeparateNumber(cTuitionC);
    document.getElementById('ctuition-single').innerHTML = '$'+commaSeparateNumber(cTuition);
    document.getElementById('ctuition-double').innerHTML = '$'+commaSeparateNumber(cTuition*2);

    var totalCredit = uTuitionC + uFeesC + cTuitionC;
    var totalSingle = uTuition + uFees + cTuition;

    // this isn't used by any current majors
    if ((data.queryOptions.level == 'graduate') && (tuitionCost.certificationFeesPerSemester !== undefined)) {

      var certFeesPH = tuitionCost.certificationFeesPerCredit;
      var certFeesPS = tuitionCost.certificationFeesPerSemester;

      document.getElementById('certfee-credit').innerHTML = '$'+commaSeparateNumber(certFeesPH);
      document.getElementById('certfee-single').innerHTML = '$'+commaSeparateNumber(certFeesPS);
      document.getElementById('certfee-double').innerHTML = '$'+commaSeparateNumber(certFeesPS*2);
      document.getElementById('graduate-certfees').classList.remove('hidden');
      totalCredit = totalCredit + certFeesPH;
      totalSingle = totalSingle + certFeesPS;

    }

    document.getElementById('total-credit').innerHTML = '$'+commaSeparateNumber(totalCredit);
    document.getElementById('total-single').innerHTML = '$'+commaSeparateNumber(totalSingle);
    document.getElementById('total-double').innerHTML = '$'+commaSeparateNumber(totalSingle*2);

    var majorTitle = data.major.title;
    majorTitle = (majorTitle === 'Multidisciplinary Studies') ? majorTitle+' ('+data.major.collegeSchool+')' : majorTitle;

    // Fall/Spring Semesters, Undergraduate Student, WV Resident, and School of Nursing
    document.getElementById('estimate-table-submissions').innerHTML = data.translations.semester+", "+data.translations.level+", "+data.translations.residency+", and "+majorTitle;

    document.getElementById('estimate-table').classList.remove('hidden');

    document.querySelectorAll('.frontpage-estimator__results').forEach(function(results) {
      results.classList.remove('hidden');
    });

    window.scrollTo({ top: document.getElementById('estimate-results').offsetTop, behavior: 'smooth'});

  }
   </script>
  </div>
  <!-- /.wrap__frontpage-estimator-results -->
