       <script>
        function loadmajors(data,type) {

          var scriptElement = document.getElementById('majors-'+type);

          var majorsList = document.createDocumentFragment();

          var el = document.createElement('option');
          el.textContent = 'Select a major';
          el.value = '';
          majorsList.appendChild(el);

          for (var i = 0; i < data.length; i++) {
            var opt = data[i];
            if ((opt.courseDeliveryOptions === undefined) || (opt.courseDeliveryOptions.indexOf('Online') === -1)) {
              var el = document.createElement('option');
              var majorTitle = opt.title;
              majorTitle = (majorTitle === 'Multidisciplinary Studies') ? majorTitle+' ('+opt.collegeOrSchool.title+')' : majorTitle;
              el.text = (type === 'undergraduate') ? majorTitle : majorTitle+' ('+opt.degreeDesignation.abbreviation+')';
              el.value = opt.majorKey;
              majorsList.appendChild(el);
            }
          }

          console.log(majorsList);

          majorsListCopy = majorsList.cloneNode(true);
          scriptElement.appendChild(majorsList);

          if (type === 'undergraduate') {
            var selectElement = document.getElementById('major-select');
            selectElement.appendChild(majorsListCopy);
          }

        }

        function loadmajorsu(data) {
          loadmajors(data,'undergraduate');
        }

        function loadmajorsg(data) {
          loadmajors(data,'graduate');
        }

        function fetchMajors() {

          // undergraduate majors
          var script = document.createElement('script');
          script.src = 'https://f09bec8ldd.execute-api.us-east-1.amazonaws.com/stable/programs?callback=loadmajorsu&sortby=title&campus=morgantown&type=major&level=undergraduate&projection=superthin';
          document.body.appendChild(script);

          // graduate majors
          var script = document.createElement('script');
          script.src = 'https://f09bec8ldd.execute-api.us-east-1.amazonaws.com/stable/programs?callback=loadmajorsg&sortby=title&campus=morgantown&type=major&level=graduate&projection=superthin';
          document.body.appendChild(script);

        }
        fetchMajors();

        function removeMajors(selectElement) {
          while (selectElement.firstChild) {
            selectElement.removeChild(selectElement.firstChild);
          }
        }

        document.getElementById('undergrad-select').onclick = function() {
          var selectElement = document.getElementById('major-select');
          var f = removeMajors(selectElement);
          for (var i = 0; i < document.getElementById('majors-undergraduate').childNodes.length; i++) {
            selectElement.appendChild(document.getElementById('majors-undergraduate').childNodes[i].cloneNode(true));
          }
        }

        document.getElementById('grad-select').onclick = function() {
          var selectElement = document.getElementById('major-select');
          var f = removeMajors(selectElement);
          for (var i = 0; i < document.getElementById('majors-graduate').childNodes.length; i++) {
            selectElement.appendChild(document.getElementById('majors-graduate').childNodes[i].cloneNode(true));
          }
        }
       </script>
      </div>
      <!-- END .field-section -->
      <!-- SUBMIT BUTTON -->
      <div>
       <input class="btn-submit" id="tuition-calculator-submit" name="saveForm" type="submit" value="Submit for Estimate"/>
      </div>
      <!-- END div/submit button -->
     </form>
    </section>
    <!-- END .estimate-form -->
