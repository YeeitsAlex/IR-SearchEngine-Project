        <section class="block block-block people-search block-15 block-block-15 odd" id="block-block-15">
         <div class="block-inner clearfix">
          <h2 class="block-title">
           Directory Search
          </h2>
          <div class="content clearfix">
           <script>
            function goSearch() {
  var page = document.getElementById('people-search-q').value;
  // window.location = "https://info.rpi.edu/people/search/" + page;
  if (page.length < 3) {
    alert("Directory searches must be at least 3 characters long.");
    document.getElementById('people-search-q').focus();
  } else {
    window.location = "https://info.rpi.edu/directory-search/" + page;
  }
  return false;
}
           </script>
           <form id="people-search" onsubmit="return goSearch();">
            <label for="people-search-q">
             Search by name, id, department
            </label>
            <input id="people-search-q" minlength="3" name="people-search-q" placeholder="Search by name, id, department" type="text"/>
            <input type="submit" value="&gt;"/>
           </form>
