           <script>
            // display alert.rpi.edu alerts
      if (alert_content != undefined && alert_content != "") {
         document.write('<div class="alertNotice">');
        document.write( "<a href=\"https://alert.rpi.edu\">" + alert_content + "</a>" );
        document.write('</div>');
      }

     // display public safety alerts
 function launchNoticeWindow(URL) {
        noticeWindow = window.open(URL, "noticeWindow", "width=650,height=650,scrollbars=yes,resizable=yes,alwaysRaised=yes,menubar=no,toolbar=no");
        window.noticeWindow.focus();
       }

      function addAlert(alertStr) {
        document.write('<div class="alertNotice">');
        document.write(alertStr);
        document.write('</div>');
      }
           </script>
