            <div class="views-slideshow-cycle-main-frame-row-item views-row views-row-0 views-row-odd">
             <div class="views-field views-field-nothing">
              <span class="field-content">
               <div class="col-xs-12 col-md-4">
                <div class="media">
                 <!-- scald=1556:mukurtu_scald_thumbnail_image {"link":"node\/29703"} -->
                 <a href="/digital-heritage/north-half-allotment-map">
                  <img alt="4WSUMASC0108.jpg" class="img-responsive" src="https://plateauportal.libraries.wsu.edu/system/files/styles/medium/private/atoms/image/4WSUMASC0108.jpg?itok=WxxmVfmE" title="4WSUMASC0108.jpg" typeof="foaf:Image"/>
                 </a>
                 <!-- END scald=1556 -->
