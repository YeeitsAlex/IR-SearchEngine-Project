           <!-- content group: width = grid_width - (sidebar_first_width + sidebar_last_width) -->
           <div class="content-group row nested grid16-12" id="content-group">
            <div class="content-group-inner inner" id="content-group-inner">
             <div class="heading_image_wrapper row nested " id="heading_image_wrapper">
              <div class="heading_image_wrapper-inner inner clearfix" id="heading_image_wrapper-inner">
