    var bbpage = Sys.WebForms.PageRequestManager.getInstance();
bbpage.add_pageLoaded(function(){

function addMobileClass(){
   var width = $(window).width();
   if (width < 768) {
            $('#wrapSearch').insertAfter($('#mobileMenu'));
            $('#wrapTitle').insertAfter($('#mobileMenu'));
            $('#wrapUtility').appendTo($('#wrapNav'));
            
   }else {
            $('#wrapUtility').insertAfter($('#logo'));
            $('#wrapSearch').insertAfter($('#wrapUtility'));
            $('#wrapTitle').insertAfter($('#wrapSearch'));
   }
};

addMobileClass();

$(window).resize(function() {
   addMobileClass();
});

$('#mobileMenu ').click(function(e){
    e.preventDefault();
    $(this).toggleClass('open');
    $('#wrapNav').toggleClass('open');
});

$('#wrapNav.open ul.menu li.parent ').click(function(e){
    e.preventDefault();
    $(this).toggleClass('open');
});

$('#wrapNav ul.menu li.parent > a').click(function(){
    $(this).parent().toggleClass('open'); 
});

if($('#wrapMedia img').length <= 0){
    $('#wrapMedia').addClass('hide')
};


});
   </script>
