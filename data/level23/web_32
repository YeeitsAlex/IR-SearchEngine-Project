    $('html,body').animate({
      scrollTop: $(".productView-description").offset().top
    }, 1000);
    $('.tab').removeClass('is-active');
    $('.tab-content').removeClass('is-active');
    $(".tab.reviews").addClass('is-active');
    $(".tab-content.reviews").addClass('is-active');
