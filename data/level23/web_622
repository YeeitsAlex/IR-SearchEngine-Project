   ( function( $, window ) {

	var tocs = $( ".toc" );

	var toc_position = function() {
		tocs.each( function() {
			var toc = $( this );
			var toc_box = this.getBoundingClientRect();
			var toc_list = toc.find( "> ul" );
            var offset = ( window.outerWidth < 989 ) ? 50 : ( $( "body" ).hasClass( "admin-bar" ) ) ? 32 : 0;
			
			if ( offset < toc_box.top ) {
				toc_list.removeAttr("style")
			} else {
                toc_list.css( {
                    "position": "fixed",
                    "top": offset,
                    "width": toc.width()
                } );
			}
		} );
	};
	
	$( window ).on( "scroll resize", function() {
		if ( tocs && 694 <= window.outerWidth ) {
			window.requestAnimationFrame( toc_position );
		}
	} );
	
}( jQuery, window ) );
