 i = 0;
	jQuery(".strategic-plan-icons").children(".sp-icon").each(function () {
		var delay = i * 500; 
		var working_div = jQuery(this);
		setTimeout(function() {
			working_div.removeClass("hidden").addClass("animated").addClass("zoomIn");
		}, delay); i++; })
