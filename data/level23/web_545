   // supports the hide/show FAQ stuff
        $(document).ready(function () {
            $('.faqanswer').hide();
            $('.faqitem').toggle(
                function () {
                    $(this).next('.faqanswer').show();
                    $(this).addClass('faqclose');
                },
                function () {
                    $(this).next('.faqanswer').hide();
                    $(this).removeClass('faqclose');
                }
                ); // end of toggle

        }); // end of ready
