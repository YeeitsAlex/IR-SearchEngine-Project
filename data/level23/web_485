  <!-- FlexSlider scripts - These go before the closing </body> tag -->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js">
  </script>
  <script src="https://code.wvu.edu/public/1438359736/vendor/flexslider/2.2/jquery.flexslider-min.js">
  </script>
  <script>
   $(window).load(function() {
		  // sets height of first homepage slider
		  var minH = 525;
		  function resizeSlider(){
			  var h;
			  if($( window ).width() < 430){
				   h = 387;
			  } else if($( window ).width() < 750){
				  h = 399;
			  } else if($( window ).width() < 960){
				  h = 291;
			  } else if($( window ).width() < 1150){
				  h = 305;
			  } else {
				  h = 265;
			  }
			  
			  h = $( window ).height()-h;
			  if(h < minH){
				  h = minH;
			  }
			  
			  $('.home-hero').css({"height":h});
		  }
		  // show/hide description in first slider
		  $( ".box-nav" ).click(function() {
			  $( ".box-desc" ).toggle();
		   });
	
		  // tiny helper function to add breakpoints to carousel
		  function getGridSize() {
			  return ($( window ).width() < 350) ? 1 :
					   ($( window ).width() < 600) ? 2 :
					   ($( window ).width() < 900) ? 3 : 4;
		  };
		  $('.flexslider').flexslider({
				// customize if needed
		  });
		  $('.flexslider-carousel').flexslider({
			 animation: "slide",
			 slideshow: false,
			  directionNav: false,
			  controlNav: false,
			  animationLoop: false,
			  itemWidth: 580,
			  itemMargin: 4,
			  start: function(slider){
				flexslider = slider;
				},
			  minItems: getGridSize(), // use function to pull in initial value
			  maxItems: getGridSize()
		  });
			
		  $('.prev, .next').on('click', function(){
				var href = $(this).attr('href');
				$('.flexslider-carousel').flexslider(href);
				return false;
		  }); 
		  // check grid size on resize event
		  $(window).bind('resize', function() { 
		    flexslider.vars.minItems;
		    gridSize = getGridSize();
			flexslider.vars.minItems = gridSize;
			flexslider.vars.maxItems = gridSize;
			setTimeout(function(){ 
				 var carousel = $('.flexslider-carousel').data('flexslider');
				carousel.resize(); 
		        resizeSlider();
			}, 1000);
			//carousel.resize(); // BUG !! too much recursion with the flexslider resize function call
		  }); 
		  resizeSlider();//resize on load

      });
  </script>
