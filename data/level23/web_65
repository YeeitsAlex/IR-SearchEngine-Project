     <a id="main-content">
     </a>
     <h1 class="title" id="page-title">
      Rensselaer News
     </h1>
     <div class="block block-system block-main block-system-main odd block-without-title" id="block-system-main">
      <div class="block-inner clearfix">
       <div class="content clearfix">
        <article about="/content/rensselaer-polytechnic-institute" class="node node-front-page node-promoted node-published node-not-sticky author-admin odd clearfix" id="node-front-page-142" typeof="sioc:Item foaf:Document">
         <span class="rdf-meta element-hidden" content="Rensselaer News" property="dc:title">
         </span>
         <span class="rdf-meta element-hidden" content="0" datatype="xsd:integer" property="sioc:num_replies">
