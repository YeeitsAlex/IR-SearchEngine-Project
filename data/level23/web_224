             <!-- [if !IE]><!-->
             <object data="http://www.photoshelter.com/swf/CSlideShow.swf?feedSRC=http%3A//tuftsphoto.photoshelter.com/gallery/Slideshow-Neighbors-Page-May-2012/G0000inVVCOyoT_k%3Ffeed%3Djson" height="480" type="application/x-shockwave-flash" width="640">
              <param name="wmode" value="opaque"/>
              <param name="allowScriptAccess" value="always"/>
