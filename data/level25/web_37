 <script>
  if(typeof campaign_meter==="undefined"){var campaign_meter=function(wrapper){this.wrap=wrapper;this.image_meter=wrapper.find('.cahnrs-campaign-meter-image-wrap');this.bar_meter=wrapper.find('.cahnrs-campaign-meter-bar-value');this.meter=wrapper.find('.cahnrs-campaign-meter-bar-value-label span');this.timer=false;var self=this;this.init=function(){self.wrap.addClass('active');self.bind_events();};this.bind_events=function(){self.do_meter();};this.do_meter=function(){if(!self.wrap.hasClass('rendered')){if(self.is_visible()){this.timer=false;var c_value=self.get_current_value();var e_value=self.get_end_value();setTimeout(function(){self.expand_meter(c_value,e_value);},500);}else{this.timer=setTimeout(function(){self.do_meter();},50);}}};this.expand_meter=function(c_value,e_value){var e_percent=self.get_percent(c_value,e_value);self.wrap.addClass('rendered');var speed=self.get_speed();self.image_meter.animate({height:e_percent+'%'},{duration:speed,queue:false});self.bar_meter.animate({height:e_percent+'%'},{duration:speed,queue:false,step:function(currentHeight){var c_percent=(currentHeight/e_percent);var m_value=(Math.round((c_value*c_percent)*10)/10); self.meter.html(m_value);}},);};this.get_percent=function(c_value,e_value){var e_percent=((c_value/e_value)*100);return e_percent;};this.get_current_value=function(){var value=self.wrap.data('value');return value;};this.get_speed=function(){var value=self.wrap.data('speed');return value;};this.get_end_value=function(){var value=self.wrap.data('targetvalue');return value;};this.is_visible=function(){var vis=false;var viewportHeight=jQuery(window).height();var meterOffset=self.wrap.offset().top;var documentPosition=jQuery(window).scrollTop();console.log(documentPosition+' '+meterOffset);var visibleArea=documentPosition+viewportHeight;if(visibleArea>meterOffset){vis=true;} console.log(vis);return vis;};this.init();}} jQuery(document).ready(function(){jQuery('.cahnrs-campaign-meter').not('.active').each(function(index){if(!jQuery(this).hasClass('active')){window['meter-'+index]=new campaign_meter(jQuery(this));}});});
 </script>
</p>
