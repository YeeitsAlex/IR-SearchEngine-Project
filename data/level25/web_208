      <section aria-label="Main Content">
       <script type="text/javascript">
        $(document).ready(function () {

        $("form").submit(function (event) {


            var emptyList = "";

            $("input:text").each(function () {

                var labelName = $('label[for="' + $(this).attr("id") + '"]').text().replace(":", "");

                if ($(this).val().length <= 0)
                {
                    emptyList += "- " + labelName + "is Required" + "\n\n";
                }

                else if (!$(this).valid())
                {
                    switch ($(this).attr("id"))
                    {
                        case "DOB":
                            emptyList += "- Date of Birth must be mm/dd/yyyy \n\n";
                            break;
                        case "ZipCode":
                            emptyList += "- Zip Code must be 5 digits \n\n";
                            break;
                        case "PhoneNumber":
                            emptyList += "- Phone Number must be 7 digits with no dashes \n\n"
                    }
                }
            });

            if (emptyList.length > 0) {
                alert("The following errors were found: \n\n" + emptyList);
                return false;
            }
        });
    });
       </script>
