   $(document).ready(function () {
		
            //hide right column if empty
            var testEmpty = $('#ctl01_divRightMenu').text();
			if($.trim(testEmpty) === "") {
				$('#ctl01_divRightMenu').remove();
			}
			
			//apply link's class to parent li
            $('a.orange').parent().addClass('orange');
            $('a.lime-green').parent().addClass('lime-green');
            $('a.burgundy').parent().addClass('burgundy');
            $('a.gold').parent().addClass('gold');
			$('a.blue').parent().addClass('blue');
			
            //hide first breadcrumb text
            $("div.breadcrumbs a:first-child").hide();
			
			//remove p tag containing .bigimg
			$('p .bigimg').each(function() {
				$(this).unwrap();
			});	
			
			//remove p tag containing a.gp_right
			$('p a.gp_right').each(function() {
				$(this).unwrap();
			});	
			
        });
