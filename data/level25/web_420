     <div id="HomeFeature">
      <script type="text/javascript">
       var featItems = new Array();
      </script>
      <script type="text/javascript">
       var featItem = new Array();
                featItem['title'] = " <a href="http://websurv.indiana.edu/nsse/registration/survey/2014/ " id="featItem_link"><span style="font-size:26px">  Registration for NSSE & FSSE 2014 <br />is now open!</span> </a>";
                featItem['content'] = " ";
                featItem['url'] = " ";
                featItem['imgfile'] = " ";
                featItem['imgcredit'] = " ";
                featItems.push(featItem);
      </script>
      <script type="text/javascript">
       var featItemN = 0;
    var featItemInterval = null;
    var fControl = "";
	
    function startFeatureRotation() {
        fControl = "<a href='javascript:;' onclick='stopFeatureRotation();'>[Stop]</a>";
        nextFeatItem();
        featItemInterval = setInterval("nextFeatItem();",15000);
    }
	
    function stopFeatureRotation() {
        fControl = "<a href='javascript:;' onclick='startFeatureRotation();'>[Restart]</a>";
        document.getElementById("fControl_div").innerHTML = fControl;
        clearInterval(featItemInterval);
    }
	
    function nextFeatItem(){
		
        featItemN++;
		
        if (featItemN >= featItems.length) {
            featItemN = 0;
        }
		
        var fid = "";
        fid += "<h2><a href=\"" + featItems[featItemN]['url'] + "\" id=\"featItem_link\">";
        fid += featItems[featItemN]['title'] + "</a></h2>";
        fid += "<p>";
        fid += "<a href=\"" + featItems[featItemN]['url'] + "\">" + featItems[featItemN]['content'] + "</a>";
        fid += "</p>";
		
		if (featItems.length > 1){
	        document.getElementById("fControl_div").innerHTML = fControl;
		}
        document.getElementById('featItem_div').innerHTML = fid;
//					document.getElementById('featItem_img').src = featItems[featItemN]['imgfile'];
//					document.getElementById('featItem_img').title = featItems[featItemN]['imgcredit'];
    }
      </script>
      <div class="item" id="hfItem_1" style="position:relative;">
