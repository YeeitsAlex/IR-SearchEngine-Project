      <h4 class="purple">
       FSSE in Action
      </h4>
      <link href="http://nsse.indiana.edu/rotation/style.css" rel="stylesheet" type="text/css"/>
      <script type="text/javascript">
       var rot_id = 5;
      </script>
      <script type="text/javascript">
       var imgpos = 0;
            var rImages;
            var rotInterval;
            function lir()
            {
                try
                {
                    rImages = loadImageRotation(rot_id);
                    var ravs = document.getElementsByClassName('rotNav');
                    for (var i=0;i<ravs.length;i++)
                    {
                        ravs[i].style.display = "inline-block";	
                    }
					// rotStart();
                    document.getElementById('rotNavControls').style.display = "block";
                }
                catch(er) {}
            }
            
            function rotStart()
            {
                rotInterval = setInterval("rotImage(rot_id,1,rImages);",5000);
            }
            function rotStop()
            {
                clearInterval(rotInterval);	
            }
            
            function loadImageRotation(rid)
            {
                var p = "action=getRotationJSON&rot_id="+rot_id;
                var aj = ajax("post","http://nsse.indiana.edu/rotation/ajax.cfm",p);
                var r = new Array();
                try { var r = eval("("+aj+")"); } catch(er){ }
                return r;
            }
            
            function rotImage(rid,n,a)
            {
                n = imgpos + n;
                if (n > (a.length-1)) { n = 0; }
                if (n < 0) { n = a.length - 1; }
                document.getElementById('rotation_'+rid+'_caption').innerHTML = a[n].caption;
                document.getElementById('rotation_'+rid+'_person').innerHTML = a[n].person;
				if (trim(a[n].image)!="")
				{
					document.getElementById('rotation_'+rid+'_image').style.display = "block";
	                document.getElementById('rotation_'+rid+'_image').src = trim(a[n].image);
	                document.getElementById('rotation_'+rid+'_image').alt = a[n].institution;
				}
				else
				{
					document.getElementById('rotation_'+rid+'_image').style.display = "none";
				}
                imgpos = n;
            }
            
            window.onload = lir;
            function rotImg(a,b,c)
            {
                rotStop();
                rotImage(a,b,c);
            }
      </script>
