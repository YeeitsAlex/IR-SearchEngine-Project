     <div class="row">
      <div class="col-md-6">
       <p>
        <h3 id="follow-the-ag-food-life-blog-">
         Follow the Ag Food Life Blog!
        </h3>
        <script type="text/javascript">
         // 
$(function () 
									{
										$.ajax({
											type: 'GET',
											url: '/_resources/php/proxy.php',
											data: { requrl: "https://agfoodlife.uark.edu/feed/" },
											cache: 'true',
											success: function (data) 
											{
												var months = ["JAN", "FEB", "MAR", "APR", "MAY", "JUN", "JULY", "AUG", "SEPT", "OCT", "NOV", "DEC"]; // Store month names in array
												var days = ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"];
												var list_data = '';
												var $xml = $(data).find('item:lt(1)');

												// set brief text length for if it is derived from content node text
												var briefTextLength = 300;
												$xml.each(function () {
													var $this = $(this);
													var headline = $this.find('title').text().replace(/ *\([^)]*\) */g, "");
													 
													
													
													var unparsedDesc = $this.find('description').text();
													// see if string has CDATA or not
													if (unparsedDesc.indexOf("CDATA") >= 0)
													{ 
														var description = $this.find('description').text().replace(/<!\[CDATA\[.*?\]\]>/g, ''); 
													}
													else
													{ 
														var description = $this.find('description').text(); 
														description = description.replace(/<[^>]+>/ig,"");
													} 
													var briefText = "";
													
													// trim briefText
													briefText = description.substring(0, briefTextLength);

													

													//if description node has nothing in it, make a description out of first 149 letters of content
													if(briefText.length == 0){
														var regExString = /(<([^>]+)>)/ig; //create reg ex and let it loop (g)
														
														// parse content:encoded to derive description 
														if($(this).find('content\\:encoded').text()){
															var contentString = $(this).find('content\\:encoded').html();
															contentString = $.trim(contentString.replace(regExString, ""));
															console.log("contentString in 1st is: " + contentString);
														}
														else
														{
															var contentString = $(this).find('encoded').html();
															contentString = $.trim(contentString.replace(regExString, ""));
															console.log("contentString in 2nd is: " + contentString);
														}
														briefText = contentString.substring(0, briefTextLength);

														

													}
													
													// if briefText was full length, most likely it was concatenated, so add ellipses 
													if(briefText.length == briefTextLength){
														briefText += "...";
													}
													

													var date = (new Date($this.find('pubDate').text())).toDateString();
													var link = $this.find('link').text();
													
													
													// parse content:encoded to find img and get the src attribute
													if($(this).find('content\\:encoded').text()){
														var description2 = $(this).find('content\\:encoded').text();
													}
													else
													{
														var description2 = $(this).find('encoded').text(); 
													}
													
													var temp_container = $("<div></div>");
													temp_container.html(description2);
													var image = temp_container.find("img:first").attr("src");
													if(image == undefined) { image = '//campusdata.uark.edu/resources/images/articles/PMUA_Logo-web.jpg'; }
													var temp_container = $("<div></div>");
													temp_container.html(description2);
													var image = temp_container.find("img:first").attr("src");
													if(image == undefined) { image = '//campusdata.uark.edu/resources/images/articles/PMUA_Logo-web.jpg'; }
													var imagesrc = "<a class='thumb' href='" + link + "'><img class='img-responsive media-object' src='" + image + "' width='100%' height='100%' alt='" + headline + "' /></a>";
														
													//var imagesrc = "<div class='media-left'><a class='thumb' href='" + link + "'><img class='img-responsive media-object' src='" + image + "' /></a></div>

													//console.log("imagesrc is: "  + imagesrc);

													var headlineLink = "<a class='thumb' href='" + link + "'>" + headline + "</a>";
													

													$("#agfoodlife-title").append(headlineLink);
													$("#agfoodlife-image").append(imagesrc);
													$("#agfoodlife-brief").append(briefText);
											});
										}
								});
						});
//
        </script>
       </p>
       <h3 id="agfoodlife-title">
       </h3>
       <div id="agfoodlife-image">
