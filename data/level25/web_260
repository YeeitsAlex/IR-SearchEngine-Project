      <div class="col-md-6">
       <h3 id="news">
        News
       </h3>
       <script type="text/javascript">
        // 
$(function () {
$.ajax({
url: "//campusdata.uark.edu/apiv2/articles/SearchArticle?$orderby=publishDate+desc&$top=3&$filter=((Schools/any(s2:+s2/id+eq+2))+or+(Topics/any(t2:+t2/id+eq+2)))",
data: {},
dataType: 'jsonp',
jsonpCallback: 'SearchArticle',
cache: 'true',
success: function (articles) {
dataDisplay = "";
$.each(articles, function (key, a) {
var briefText = "", image = "";
var date = new Date(a.publishDate);
date = date.toDateString();
if (a.brief != null && a.brief != "") { briefText = a.brief;  briefText = briefText.replace(/(<p>|<\/p>)/g, "");}
if (a.image != null && a.image != "") {
image = a.image;
} else { image = "PMUA_Logo-web.jpg"; }
dataDisplay = "<li class='media'><div class='media-left'>" +
"<a class='thumb' href='https://campusdata.uark.edu/resources/images/articles/" + image + "'>" +
"<img class='media-object' src='https://campusdata.uark.edu/resources/images/articles/" + image + "?w=70' alt='' /></a>" +
"</div>" +
"<div class='media-body'>" +
"<p class='media-heading'><a href='https://bumperscollege.uark.edu/newsdetail.php?id=" + a.id + "'>" + a.headline + "</a></p>" +
"<p><i>" + date + "</i></p>" +
"<p>" + briefText + "</p>" +
"</div></li>";
$("#newsArchive").append(dataDisplay);
});
}
});
});
//
       </script>
