   <script>
    window.jQuery || document.write("<script src='js/libs/jquery-1.12.2.min.js'>\x3C/script>");

	function setPageCookie(value) {
		if( value ) {
			document.cookie = "page=" + value;
		}
	}

	function getPageCookie() {
		return document.cookie.replace(/(?:(?:^|.*;\s*)page\s*\=\s*([^;]*).*$)|^.*$/, "$1");
	}

	function showAltLogin() {
		$('#login-options-container').addClass('visuallyhidden');
		$('#login-pwd-reset-container').addClass('visuallyhidden');
		$('#login-alt-container').removeClass('visuallyhidden');
		setPageCookie('alt');
	}

	function showDefaultLogin() {
		$('#login-options-container').removeClass('visuallyhidden');
		$('#login-alt-container').addClass('visuallyhidden');
		$('#login-pwd-reset-container').addClass('visuallyhidden');
		setPageCookie('default');
	}

	function showPwdRest() {
		$('#login-pwd-reset-container').removeClass('visuallyhidden');
		$('#login-alt-container').addClass('visuallyhidden');
	}

		function otherAccount () {
// Where the user navigated to
		var hash = window.location.hash; 
		hash = hash.toLowerCase();
		if (hash == "#login-alt-container" || hash == "#otheraccount" || hash == "#accountlogin") {
			showAltLogin();
		} else if (hash == "#passwordreset") {
			showPwdRest();
		} else {
			showDefaultLogin();
		}
	}

	$(window).bind('hashchange', function() {
		otherAccount();
	});



	$(document).ready(function() {
		if( getPageCookie() === 'alt' ) {
			showAltLogin();
		}

		otherAccount();
	});


	$('#js-options-login').on('click keypress', showDefaultLogin);
   </script>
