  <script>
   function startAnimations() {
    $("#news-rotate li").hide();
    function loop() {
      var $li = $("#news-rotate li:first-child").fadeIn(4000, function () {
        $li.fadeOut(8000, function () {
          $li.appendTo('#news-rotate');;
          loop()
        })
      });
    }
    loop()
  }
  startAnimations();

