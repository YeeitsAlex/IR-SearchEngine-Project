 function navCurrent() {
        if ($(window).width() > 979) {
            $("#nav-home > a").addClass("current current-dark"); //Apply .current-dark class highlighting to parent li's link (accordion; "-dark is to distinguish from other expanded accordions)
        }
    };
    $(window).on("resize", navCurrent);
    $(document).on("ready", navCurrent);
</script>

