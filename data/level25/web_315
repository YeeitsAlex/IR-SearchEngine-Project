     <section class="span4 section-list" role="complementary">
      <h2 class="page-header">
       News &amp; Media
      </h2>
      <div class="view view-news view-id-news view-display-id-block_news_media view-dom-id-c88c7a38455a6a290394de587e9b52b3">
       <div class="attachment attachment-before">
        <div class="view view-news view-id-news view-display-id-attachment_2">
         <div class="view-content">
          <div class="row">
           <div class="span4">
            <div class="section-teaser">
             <a href="/news/atlantic-there-really-really-isn%E2%80%99t-silver-bullet-climate-change-sam-ori-executive-director-epic">
              <img alt="" src="https://socialsciences.uchicago.edu/sites/default/files/styles/feature_teaser/public/news/news-header_56.jpg?itok=Fdb3d25W" typeof="foaf:Image"/>
