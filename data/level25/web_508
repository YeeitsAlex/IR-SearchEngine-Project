window.cookieconsent.utils.setCookie =
function(e, t, i, n, o, s) {
  var a = [e + "=" + t, "path=" + (o || "/")];
  if (t == 'allow') {
    var r = new Date;
     r.setDate(r.getDate() + (i || 365));
     a.push("expires=" + r.toUTCString());
  }
   n && a.push("domain=" + n), s && a.push("secure"), document.cookie = a.join(";");
}
});
