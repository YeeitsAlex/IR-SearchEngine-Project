       var system = {"name":"LibGuides"};
                    springSpace.publicObj = new springSpace.public.Public({
                        constant: {
                            PROCESSING: {
                                
                            },
                            CONTENT: {
                            }
                        },
                        system_name: system.name,
                        customer: {"name":"Princeton University"}
                    });
					springSpace.Session.CONST.ACTION_AUTH_SET_LG_SESSION_COOKIE = 750;
					jQuery(function() {
						springSpace.Session.getLGSessionStatus({
							url: "https://princeton.libapps.com/libapps/login_process.php",
							data: {
								action: 5,
								app_id: 1,
								domain: "libguides.princeton.edu",
								site_id: 77
							}
						});
					});
				jQuery(function() {
					setInterval(function() {
						springSpace.Session.getLGSessionStatus({
							url: "https://princeton.libapps.com/libapps/login_process.php",
							data: {
								action: 5,
								app_id: 1,
