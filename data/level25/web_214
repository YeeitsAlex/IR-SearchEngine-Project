    <section class="two-columns-right" id="container">
     <div id="content" role="main">
      <div class="post-6 page type-page status-publish hentry" id="post-6">
       Genomics
       <div class="entry-content">
        <!-- MasterSlider -->
        <div class="master-slider-parent msl ms-parent-id-2" id="P_MS5c84b82317e0e" style="max-width:1200px;">
         <!-- MasterSlider Main -->
         <div class="master-slider ms-skin-default" id="MS5c84b82317e0e">
          <div class="ms-slide" data-delay="3" data-fill-mode="fill">
           <img alt="" data-src="https://vpr.colostate.edu/genomics/wp-content/uploads/sites/33/2016/08/Genomics.jpg" src="https://vpr.colostate.edu/genomics/wp-content/plugins/master-slider/public/assets/css/blank.gif" title=""/>
           <div class="ms-thumb">
            <div class="ms-tab-context">
             <div class='"ms-tab-context"'>
             </div>
            </div>
           </div>
          </div>
         </div>
         <!-- END MasterSlider Main -->
        </div>
        <!-- END MasterSlider -->
        <script>
         (function ( $ ) {
			"use strict";

			$(function () {
				var masterslider_7e0e = new MasterSlider();

				// slider controls

				// slider setup
				masterslider_7e0e.setup("MS5c84b82317e0e", {
						width           : 1200,
						height          : 360,
						minHeight       : 0,
