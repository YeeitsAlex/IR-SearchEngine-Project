         <script type="text/javascript">
          $(document).ready(function() {

// properties
var props = {
  type: "extension-news-all",
  feed: "https://www.uaex.edu/media-resources/news/feeds/extension-news.xml",                                    
  numberToDisplay: 3
};                 
                      
                      
var params_to_send = { 
   type: props.type, feed: props.feed, numberToDisplay: props.numberToDisplay
};

$.ajax({
url : "/_resources/wnl/rss.ashx",	
data: params_to_send,
success: function(data){    
	$('#extension-news-all-httpswwwuaexedumediaresourcesnewsfeedsextensionnewsxml').before(data);
	$('#extension-news-all-httpswwwuaexedumediaresourcesnewsfeedsextensionnewsxml').remove();
	
}
});

});
         </script>
