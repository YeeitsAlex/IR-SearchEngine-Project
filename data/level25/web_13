            <section class="block block-boxes block-boxes-os_boxes_columns band greenbar" delta="1474575677" id="block-boxes-1474575677" module="boxes">
             <div class="block-inner clearfix">
              <h2 class="block-title" ng-non-bindable="">
               Events
              </h2>
              <div class="block-content content">
               <div class="boxes-box" id="boxes-box-1474575677">
                <div class="boxes-box-content">
                 <!-- Two column 66-33 -->
                 <div class="at-panel gpanel panel-display two-66-33 clearfix">
