    <div class="cpb-column one column-border" style="">
     <h2 class="cpb-subtitle">
      Events
     </h2>
     <div class="cpb-textblock cpb-item ">
      <div class="cahnrs-events-shortcode basic-display">
       <div class="cahnrs-event">
        <div class="cahnrs-event-link">
         <a href="http://csanr.wsu.edu/event/healthy-soils-healthy-region-workshop/" rel="bookmark">
          View Event Details
         </a>
        </div>
        <div class="cahnrs-event-calendar-details">
         <span class="cahnrs-day">
          12
         </span>
         <span class="cahnrs-month">
          Mar
         </span>
        </div>
        <div class="cahnrs-event-details">
         <h3>
          Healthy Soils, Healthy Region Workshop
         </h3>
         <div class="cahnrs-event-venue">
