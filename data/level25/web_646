            <section class="block block-boxes block-boxes-os_boxes_html" delta="1376597837" id="block-boxes-1376597837" module="boxes">
             <div class="block-inner clearfix">
              <h2 class="block-title" ng-non-bindable="">
               Welcome
              </h2>
              <div class="block-content content" ng-non-bindable="">
               <div class="boxes-box" id="boxes-box-1376597837">
                <div class="boxes-box-content">
                 <p>
                  <img alt="CWS" class="media-element file-default file-os-files-small" height="169" src="//static.hwpi.harvard.edu/files/styles/os_files_small/public/division-of-science/files/stubbs_website.png?m=1544105665&amp;itok=BcYbULsC" title="" width="179"/>
                 </p>
                 <div class="paragraph">
                  <span>
                   <span>
                    <span>
                     <span style="color:#000000">
                      <span style="font-style:normal">
                       <span>
                        <span style="font-weight:normal">
                         <span>
                          <span>
                           <span style="text-transform:none">
                            <span>
                             <span>
                              <span>
                               <span>
                                <span style="text-decoration:none">
                                 <span style="vertical-align:baseline">
                                  <span>
                                   Welcome to
                                  </span>
                                  <span>
                                   the
                                  </span>
                                  <span>
                                   Faculty of Arts &amp; Sciences, Science Division.
                                  </span>
                                 </span>
                                </span>
                               </span>
                              </span>
                             </span>
                            </span>
                           </span>
                          </span>
                         </span>
                        </span>
                       </span>
                      </span>
                     </span>
                    </span>
                   </span>
                  </span>
                  <span>
                   <span>
                    <span>
                     <span style="color:#000000">
                      <span style="font-style:normal">
                       <span>
                        <span style="font-weight:normal">
                         <span>
                          <span>
                           <span style="text-transform:none">
                            <span>
                             <span>
                              <span>
                               <span>
                                <span style="text-decoration:none">
                                 <span style="vertical-align:baseline">
                                  <span>
                                   <span>
                                    <span>
                                     <span>
                                     </span>
                                    </span>
                                   </span>
                                  </span>
                                 </span>
                                </span>
                               </span>
                              </span>
                             </span>
                            </span>
                           </span>
                          </span>
                         </span>
                        </span>
                       </span>
                      </span>
                     </span>
                    </span>
                   </span>
                  </span>
                 </div>
                 <div class="paragraph">
                 </div>
                 <div class="paragraph">
                  <span>
                   <span>
                    <span>
                     <span style="color:#000000">
                      <span style="font-style:normal">
                       <span>
                        <span style="font-weight:normal">
                         <span>
                          <span>
                           <span style="text-transform:none">
                            <span>
                             <span>
                              <span>
                               <span>
                                <span style="text-decoration:none">
                                 <span style="vertical-align:baseline">
                                  <span>
                                   <span>
                                    <span>
                                     <span>
                                      <span>
                                       The Science Division strives to provide faculty, students, and staff with the necessary resources to achieve excellence in scholarship, learning,
                                      </span>
                                      <span>
                                       and research built on a
                                      </span>
                                      <span>
                                       <span>
                                        <a href="https://inclusionandbelongingtaskforce.harvard.edu/" style="text-decoration:none" target="_blank">
                                         <span>
                                          <span>
                                           <span style="color:#0563c1">
                                            <span style="text-decoration:none">
                                             foundation of inclusion
                                            </span>
                                           </span>
                                          </span>
                                         </span>
                                        </a>
                                       </span>
                                       .
                                      </span>
                                     </span>
                                    </span>
                                   </span>
                                  </span>
                                 </span>
                                </span>
                               </span>
                              </span>
                             </span>
                            </span>
