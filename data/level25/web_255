           <script type="text/javascript">
            jQuery("a:contains('FIND')").hover(function(){
        jQuery("ul.menu li a:contains('Books & Media')").click(function(e) {
            jQuery("#tabs ul li a:contains('Books & Media')").click();
        });
        jQuery("ul.menu li a:contains('Articles')").click(function(e) {
            jQuery("#tabs ul li a:contains('Articles')").click();
        });
    });
           </script>
           <script type="text/javascript">
            jQuery(document).ready(function() {
        jQuery("#tabs").tabs({
            create: function(event, ui) {
                //executed after tab is created.
                jQuery('#tabs').css('visibility','visible');
            },
            show: function (event, ui) {
                //on every tabs clicked
            }
        });
        jQuery('input[class!=searchAllBox][type="text"]').addClass("idleField");
        jQuery('input[type="text"]').focus(function() {
            jQuery(this).removeClass("idleField").addClass("focusField");
            if (this.value == this.defaultValue){
                this.value = '';
            }
            if(this.value != this.defaultValue){
                this.select();
            }
        });
        jQuery('input[type="text"]').blur(function() {
            jQuery(this).removeClass("focusField").addClass("idleField");
            if (jQuery.trim(this.value) == ''){
                this.value = (this.defaultValue ? this.defaultValue : '');
            }
        });
    });
           </script>
