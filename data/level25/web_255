  <script type="text/javascript">
   jQuery(document).on('click', '#superfish-1 a', function(){
		var htmlString = jQuery( this ).text();
		_gaq.push(['_trackEvent', 'Menu Link', 'Click', htmlString]);
	});

	jQuery(document).on('click', '#block-aggregator-feed-2 .item-list ul li a', function(){
		var htmlString = jQuery( this ).text();
		_gaq.push(['_trackEvent', 'News and Announcements', 'Click', htmlString]);
	});

	jQuery(document).on('click', '.nivoSlider a', function(){
		var htmlString = jQuery( this ).children("img").attr("alt");
		_gaq.push(['_trackEvent', 'Feature Box', 'Click', htmlString]);
	});

	function headerSearchSubmit(option) {
		if (option == 'articles') {
			jQuery("#headerSearchForm").attr("action", '//www.lib.purdue.edu/system/search/dlPrimoAlma.php?searchType=simple&searchQ=' + jQuery("#headerSearchInput").val());
		}else if (option == 'booksMedia') {
			jQuery("#headerSearchForm").attr("action", '//www.lib.purdue.edu/system/search/dlPrimoAlmaBooksMedia.php?Search_Arg=' + jQuery("#headerSearchInput").val());
		}else {
			jQuery("#headerSearchForm").attr("action", '//www.google.com/cse?cx=015675504521142124930:gt_fnei4sns&q=' + jQuery("#headerSearchInput").val());
		}

		jQuery('#headerSearchForm').submit();
	}
  </script>
