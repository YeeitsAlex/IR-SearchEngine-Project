        <section class="block block-bean block-odd clearfix" id="block-bean-reader-services">
         <div class="block-inner">
          <h2 class="block-title">
           Reader Services
          </h2>
          <div class="content">
           <div class="entity entity-bean bean-node-list clearfix">
            <div class="content">
             <div class="sidebar-list">
