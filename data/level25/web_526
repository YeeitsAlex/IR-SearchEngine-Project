jQuery(document).ready(function($){
			var hash = window.location.hash;
    		var anchor = $('a[href$="'+hash+'"]');
    		var anchorId = document.getElementById(hash);
    		if (anchor.length > 0){
        		anchor.click();
                $('html, body').animate({
    				scrollTop: $(anchorId).offset().top
					}, 200);                
                
    		}
	 });

