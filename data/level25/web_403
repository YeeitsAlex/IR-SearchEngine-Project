	$("img").each(function() {
		if ($(this).attr('class') == "image-to-link")
		{
			var src = $(this).attr('src');
			var a = $('<a/>').attr('href', src);
			$(this).wrap(a);
		}
		if ($(this).attr('class') == "image-to-link-no-border")
		{
			var src = $(this).attr('src');
			var a = $('<a/>').attr('href', src);
			$(this).wrap(a);
		}
		
	});
});
