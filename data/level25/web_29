      function utilitiesVisitURL(sel)
			{
				var url = sel.value;
				if (url!="")
				{
//					window.location.replace(url);
//					document.getElementById('quicklinksForm').action = url;
//					document.getElementById('quicklinksForm').submit();
					var frm = sel.form;
					frm.action = url;
					frm.submit();
				}
			}
     </script>
