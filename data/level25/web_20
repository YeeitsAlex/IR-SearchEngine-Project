  <script type="text/javascript">
   var max = getMaxHeight();
          document.getElementById("left_side").style.height = max + 20 + "px";
          document.getElementById("center").style.height = max + "px"
          document.getElementById("right_side").style.height = max + 20 + "px";

          function getMaxHeight() {
              var max = 0;
              //res = "resources Section", wnew = "Whats New Section"
              var res = document.getElementById("left_side").offsetHeight;
              var main = document.getElementById("center").offsetHeight;
              var wnew = document.getElementById("right_side").offsetHeight;

              if (res > max) max = res;
              if (main > max) max = main;
              if (wnew > max) max = wnew;
              return max;
          }
  </script>
