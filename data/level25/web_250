     <a href="/">
      <h1 class="ihouse-wordmark text-hide">
       International House at the University of Chicago
      </h1>
     </a>
    </div>
    <nav class="main-nav clearfix hidden-xs">
     <div class="util-wrap">
      <ul class="util-nav list-inline">
       <li>
