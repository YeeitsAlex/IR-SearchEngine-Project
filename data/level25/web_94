	bbpage.add_pageLoaded(function(){
		if ($('.DonationMessage').length > 0) {
			$('.DonationMessage').each(function() {
			    var cell = $.trim($(this).text());
			    if (cell.length == 0){
			        $(this).addClass('emptyTD');
