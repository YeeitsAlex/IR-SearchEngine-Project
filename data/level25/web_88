       <script type="text/javascript">
        function submitSearch()
          {
            var form = document.getElementById( "searchForm" );
            var text = form.elements["searchbar"].value;
            if ( "Search Site     " == text ) return false;

            form.action = "/press/search.html";
            form.submit();
            return true;
          }
       </script>
       <form id="searchForm" onsubmit="return submitSearch();">
        <div>
         <input class="s" id="searchbar" name="clause" onblur="if (this.value == '') {this.value = 'Search Site     ';}" onfocus="if (this.value == 'Search Site     ') {this.value = '';}" type="text" value="Search Site     "/>
         <input class="button" type="submit" value=""/>
        </div>
       </form>
      </div>
