								 "<ViewFields>" +
									"<FieldRef Name='Id' />" +
									"<FieldRef Name='Title' />" +
									"<FieldRef Name='Byline' />" +
									"<FieldRef Name='Image' />" +
								 "</ViewFields>" +
								 
								 "<RowLimit>1</RowLimit>" +
								 
						     "</View>";
	
	// @Document Load
	(
		function() {
			ExecuteOrDelayUntilScriptLoaded(get_featured_story, "sp.js");
		}
	)();
	
	// @Build Featured Story Box
	function build_featured_story_box(id, title, subtitle, byline, image) {		
		let html = '';
		{	
			let newsroom_story_url = newsroom_url + '?&amp;newsid=' + id;
			let read_more_text = "Read the full story";
			
			html += '<section class="featurettes">' +
						'<div class="infoBox">' +
							'<figure class="image">' +
								'<a href="' + newsroom_story_url + '" title="' + read_more_text + '">' +
									'<img src="' + image + '">' + 
								'</a>' +
							'</figure>' +
							
							'<div class="text">' +
								
								'<h3 class="iBHeadline">' +
									'<a href="' + newsroom_story_url + '" title="' + read_more_text + '">' +
										title +
									'</a>' +
								'</h3>' +
								
								'<h4 class="iBsubheadline">' +
									subtitle +
								'</h4>' +
								
								'<div class="introText">' +
									byline +
								'</div>' +
								
								'<p class="learnMore">' +
									'<a style="font-family: source_sans_probold, Helvetica, Arial, sans-serif;color: #555;" href="' + newsroom_story_url + '" title="' + read_more_text + '" class="moreLink" >' +
										read_more_text +
									'</a>' +
								'</p>' +

								'<p class="learnMore">' +
									'<a style="font-family: source_sans_probold, Helvetica, Arial, sans-serif;color: #555;" href="/academics/colleges/PublicHealth/About/news/Pages/DeanArchive.aspx" title="Deans Note Archive" class="moreLink" >' +
										"Dean&rsquo;s Note archives" +
									'</a>' +
								'</p>' +

							'</div>' +
						'</div>' +
					'</section>';
		}

		let featured_story_element = document.getElementById('featured_story');
		featured_story_element.innerHTML = html;
	}
	
	// @Query Success
	function get_featured_story_success() {
		let list_item_count = this.list_items.get_count();
		
		if(list_item_count > 0) {
			let list_item_enumerator = this.list_items.getEnumerator();
			if(list_item_enumerator.moveNext()) {
				let list_item = list_item_enumerator.get_current();
				
				let story_id           = list_item.get_item('ID');
				let story_title        = list_item.get_item('Title');
				let story_subtitle     = '';
				let story_byline       = list_item.get_item('Byline');
				let story_image        = list_item.get_item('Image').get_url();
				
				build_featured_story_box(story_id, story_title, story_subtitle, story_byline, story_image);
			}
		}
		else {
			console.log("ERROR: No featured story was retrieved (but query was successful)");
		}
	}
	
	// @Query Failure
	function get_featured_story_failure() {
		console.log("ERROR: Retrieval of featured story failure");
	}
	
	function get_featured_story() {
		let context = new SP.ClientContext(list_location);
		let list = context.get_web().get_lists().getByTitle(list_title);
		
		let caml_query = new SP.CamlQuery();
		caml_query.set_viewXml(caml_query_text);
		this.list_items = list.getItems(caml_query);
		
		context.load(this.list_items);
		context.executeQueryAsync(
			Function.createDelegate(this, this.get_featured_story_success),
			Function.createDelegate(this, this.get_featured_story_failure),
		);
	}
                              </script>
                             </div>
                            </td>
                           </tr>
                          </table>
