                        <script type="text/javascript">
                         var current=1;


    $(document).ready(function () {
			$("#linkImage").addClass("link1background").removeClass("link2background link3background");//initialize rotator to first window
				if ($("#linkImage").hasClass('link1background')) {
					$("#link1").addClass('hoverBack');
					$("#link2,#link3").removeClass('hoverBack');
					$("#linkImage1,#linkImage1Pane").show()
					$("#linkImage2,#linkImage3,#linkImage2Pane,#linkImage3Pane,#linkImage0Pane").hide();
				}
	

		window.setInterval(function(){//rotates through the rotator
			if(current===1){
				$("#linkImage").addClass("link1background").removeClass("link2background link3background");
				if ($("#linkImage").hasClass('link1background')) {
					$("#link1").addClass('hoverBack');
					$("#link2,#link3").removeClass('hoverBack');
					$("#linkImage1,#linkImage1Pane").show()
					$("#linkImage2,#linkImage3,#linkImage2Pane,#linkImage3Pane,#linkImage0Pane").hide();
				}
			}else if(current===2){
				$("#linkImage").addClass("link2background").removeClass("link1background link3background");
				if ($("#linkImage").hasClass('link2background')) {
					$("#link2").addClass('hoverBack');
					$("#link1,#link3").removeClass('hoverBack');
					$("#linkImage2,#linkImage2Pane").show()
					$("#linkImage1,#linkImage3,#linkImage1Pane,#linkImage3Pane,#linkImage0Pane").hide();
				}
			}else if(current===3){
				$("#linkImage").addClass("link3background").removeClass("link1background link2background");
				if ($("#linkImage").hasClass('link3background')) {
					$("#link3").addClass('hoverBack');
					$("#link1,#link2").removeClass('hoverBack');
					$("#linkImage3,#linkImage3Pane").show()
					$("#linkImage1,#linkImage2,#linkImage1Pane,#linkImage2Pane,#linkImage0Pane").hide();
				}
			}
			
			if(current>=3){
				current = 0;
			}else{
				current++;
				console.log(current);
			}
		}, 3000);
		
        $("#link1").mouseover(function () {//changes if mouseover
            $("#linkImage").addClass("link1background").removeClass("link2background link3background");
            if ($("#linkImage").hasClass('link1background')) {
                $("#link1").addClass('hoverBack');
                $("#link2,#link3").removeClass('hoverBack');
                $("#linkImage1,#linkImage1Pane").show()
                $("#linkImage2,#linkImage3,#linkImage2Pane,#linkImage3Pane,#linkImage0Pane").hide();
            }
        });

        $("#link2").mouseover(function () {//--
            $("#linkImage").addClass("link2background").removeClass("link1background link3background");
            if ($("#linkImage").hasClass('link2background')) {
                $("#link2").addClass('hoverBack');
                $("#link1,#link3").removeClass('hoverBack');
                $("#linkImage2,#linkImage2Pane").show()
                $("#linkImage1,#linkImage3,#linkImage1Pane,#linkImage3Pane,#linkImage0Pane").hide();
            }
        });

        $("#link3").mouseover(function () {//--
            $("#linkImage").addClass("link3background").removeClass("link1background link2background");
            if ($("#linkImage").hasClass('link3background')) {
                $("#link3").addClass('hoverBack');
                $("#link1,#link2").removeClass('hoverBack');
                $("#linkImage3,#linkImage3Pane").show()
                $("#linkImage1,#linkImage2,#linkImage1Pane,#linkImage2Pane,#linkImage0Pane").hide();
            }
        });		

    });
                        </script>
