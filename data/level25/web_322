   <script type="text/javascript">
    var oldOnBeforeUnload = window.onbeforeunload;
    var oldonload = window.onload;
    var url_substring = window.location.search.substring(1);
    dt = new Date();
    var loadtime = null;
    var responsetime = null;
    
        //load function
        window.onload = function() {
            if (oldonload) {
                captureLoadTime();
                oldonload();
            }
            else
                captureLoadTime();

        }

        //calculates the request / response time with the load time
        function captureLoadTime() {
            var time_load = new Date();
            var time_unload = window.name;
            if (time_unload) {
                responsetime = ((time_load.getTime() - time_unload) / 1000);
                if (url_substring == "RT" || url_substring == "rt") {
                displayresponsetime();
                }
            }
        }


       //page load time calculation
        function calc_loadtime() {
            loadtime = ((new Date() - dt) / 1000);
            //createCookie('Loadtime', loadtime,0);

        }
        
        //detect document load state for IE
        document.onreadystatechange = function() {
            if (document.readyState == "complete") {
                calc_loadtime();
            }
        }

        //detect document load state for mozilla
        if (document.addEventListener)  
        {
            document.addEventListener("DOMContentLoaded", calc_loadtime, false);
        }

        //unload function to invoke unload time
        window.onbeforeunload = function() {
            var ret;
            if (oldOnBeforeUnload) {
                ret = oldOnBeforeUnload();
            }
            captureTimeOnBeforeUnload();
            if (ret) return ret;
        }
        
        //captures the time when the request is sent out
        function captureTimeOnBeforeUnload() {
            var time_unload = new Date();
            window.name = time_unload.getTime();
         }
   </script>
