                               $(document).ready(function () {
	ExecuteOrDelayUntilScriptLoaded(getStudentHomeNews, "sp.js");
});

function getStudentHomeNewsSucceeded(sender, args) {
    var count = this.ListItemsCollStudent.get_count();
	var fullCode = '<table><tbody><tr><td>';
	
    if (count > 0) {
        var listItemEnumerator = ListItemsCollStudent.getEnumerator();

        while (listItemEnumerator.moveNext()) {
            var oListItem = listItemEnumerator.get_current();
			
            var title = oListItem.get_item('Title');
			var thisId = oListItem.get_id();
			
			fullCode += '<div id="linkitem" class="item link-item"><a href="http://www.ucdenver.edu/academics/colleges/PublicHealth/About/news/Pages/Newsroom.aspx?&newsid=' + thisId + '" target="_blank" title="' + title + '">' + title + '</a></div>';
		}
    }
    
    fullCode += '</td></tr></tbody></table>';
    
    $('#CSPHStudentHomeNews').html(fullCode);
    $('#CSPHStudentHomeNewsTitle').show();
    $('#CSPHStudentHomeNews').show();
}

function getStudentHomeNewsFailed(sender, args) {}

function getStudentHomeNews() {
    var ctx = new SP.ClientContext('/academics/colleges/PublicHealth/');
    var list = ctx.get_web().get_lists().getByTitle('Newsroom');

    var camlQuery = new SP.CamlQuery();
    camlQuery.set_viewXml("<View><Query><Where><And><Eq><FieldRef Name='Category' /><Value Type='Choice'>Student & Alumni News</Value></Eq><Eq><FieldRef Name='Archive' /><Value Type='Boolean'>1</Value></Eq></And></Where><OrderBy><FieldRef Name='Date_x0020_Published' Ascending='FALSE' /><FieldRef Name='Weight' Ascending='FALSE' /></OrderBy></Query><ViewFields><FieldRef Name='Title' /><FieldRef Name='Byline' /><FieldRef Name='Date_x0020_Published' /><FieldRef Name='Category' /><FieldRef Name='Content' /><FieldRef Name='Image' /></ViewFields><RowLimit>3</RowLimit></View>");
    this.ListItemsCollStudent = list.getItems(camlQuery);

    ctx.load(this.ListItemsCollStudent);
    ctx.executeQueryAsync(
        Function.createDelegate(this, this.getStudentHomeNewsSucceeded),
        Function.createDelegate(this, this.getStudentHomeNewsFailed)
    );
}
