#site-navigation, .main-navigation ul li ul li a {
    background-color: #64646a;
}

/*removing the featured image from showing at the top of a single post */
.single .featured-image {
  display: none;
}

/* To increase the leading between lines in an ordered list and an unordered list */

.entry-content li {
    margin-bottom: 1.6em;
    font-size: 1em;
    line-height: 1.6em;
    list-style: disc;
}

