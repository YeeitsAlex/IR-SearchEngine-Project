<script type="text/javascript">
 var rotateId = 0;
function placeControls()
{
    var newPos = jQuery('.feature:visible img').height() - jQuery('.carousel-buttons').height() - 20;
    jQuery('.carousel-buttons').css('top', newPos+'px').css('display', 'block');
}

function featureNext()
{
    var numArticles = jQuery(".features article").length;
    var index = jQuery(".features article:visible").index()+1;
    var newIndex = 0;

    ( index == numArticles ) ? newIndex = 1 : newIndex = index + 1;

    jQuery(".features article").fadeOut(350).attr('aria-hidden', 'true');
    jQuery(".features article:nth-child(" + (newIndex) + ")").fadeIn(350).attr('aria-hidden', 'false');

    jQuery("#dotIndicators button:nth-child(" + index + ") span").removeClass("active-dot").addClass("inactive-empty-dot");
    jQuery("#dotIndicators button:nth-child(" + (newIndex) + ") span").addClass("active-dot").removeClass("inactive-empty-dot");
}

function changeFeature(newIndex)
{
    var currentHeight = jQuery('.features').height();
    var index = newIndex;

    jQuery('.features').css('height', currentHeight);

    jQuery(".features article").css('position', 'absolute').fadeOut(350).attr('aria-hidden', 'true');
    jQuery(".features article:nth-child(" + (index) + ")").attr('aria-hidden', 'false').fadeIn(350, function()
    {
        jQuery('.features').css('height', 'auto');
        jQuery(".features article").css('position', 'relative');
    });

    jQuery("#dotIndicators button span").removeClass("active-dot").addClass("inactive-empty-dot");
    jQuery("#dotIndicators button:nth-child(" + (index) + ") span").addClass("active-dot").removeClass("inactive-empty-dot");

    window.clearInterval(rotateId);
    if ( jQuery(window).width() >= 992 )
    {
        jQuery("#pause").hide();
        jQuery("#play").show();
    }
}

jQuery(document).ready(function()
{
  jQuery(".video-carousel").slick({

        infinite: true,
        slidesToShow: 1,
        slidesToScroll: 1,
        mobileFirst: true,
        prevArrow: '<div class="slick-prev" role="button" aria-label="Previous" data-role="none" type="button" style="display:block"><span class="glyphicon glyphicon-triangle-left">&nbsp;</span></div>',
        nextArrow: '<div class="slick-next" role="button" aria-label="Next" data-role="none" type="button" style="display:block"><span class="glyphicon glyphicon-triangle-right">&nbsp;</span></div>',
        responsive: [{
            breakpoint: 600,
            settings: {
                slidesToShow: 2,
            }
        },{
            breakpoint: 768,
            settings: {
                slidesToShow: 1,
            }
        },{
            breakpoint: 992,
            settings: {
                slidesToShow: 4,
            }
        } ]
    });

    jQuery(window).on('load', function()
    {
        placeControls();
    });

    jQuery(window).resize(function()
    {
        waitForFinalEvent(function() {
           placeControls();
            //...
          }, 500, "some unique string");
    });

 jQuery("#next").click(function()
    {
      var numArticles = jQuery(".features article").length;
      var index = jQuery(".features article:visible").index()+1;
      var newIndex = 0;

      ( index == numArticles ) ? newIndex = 1 : newIndex = index + 1;

      changeFeature(newIndex);
    });

  jQuery("#prev").click(function()
    {
      var numArticles = jQuery(".features article").length;
      var index = jQuery(".features article:visible").index()+1;
      var newIndex = 0;

      ( index == 1 ) ? newIndex = numArticles : newIndex = index - 1;

      changeFeature(newIndex);
    });


  jQuery("#dotIndicators button").click(function()
    {
      var index = jQuery(this).index()+1;
      changeFeature(index);
    });

  jQuery("#pause").click(function()
    {
        jQuery("#pause").hide();
        jQuery("#play").show().focus();

        window.clearInterval(rotateId);
    });

  jQuery("#play").click(function()
    {
        jQuery("#play").hide();
        jQuery("#pause").show().focus();

        rotateId = window.setInterval(featureNext, 10000);
    });

  if ( jQuery(window).width() >= 992 && jQuery("div.features article").length > 1 )
  {
    rotateId = window.setInterval(featureNext, 10000);
  }

var waitForFinalEvent = (function() {
  var timers = {};
  return function(callback, ms, uniqueId) {
    if (!uniqueId) {
      uniqueId = "Don't call this twice without a uniqueId";
    }
    if (timers[uniqueId]) {
      clearTimeout(timers[uniqueId]);
    }
    timers[uniqueId] = setTimeout(callback, ms);
  };
})();

});
</script>
<!-- close .main-content-inner -->
