            $(document).ready(function(){
                $('.megamenu').megaMenuCompleteSet({
                    menu_speed_show : 200, // Time (in milliseconds) to show a drop down
                    menu_speed_hide : 100, // Time (in milliseconds) to hide a drop down
                    menu_speed_delay : 0, // Time (in milliseconds) before showing a drop down
                    menu_effect : 'open_close_slide', // Drop down effect, choose between 'hover_fade', 'hover_slide', etc.
                    menu_click_outside : 1, // Clicks outside the drop down close it (1 = true, 0 = false)
                    menu_show_onload : 0, // Drop down to show on page load (type the number of the drop down, 0 for none)
                    menu_responsive: 1, // 1 = Responsive, 0 = Not responsive
                    hide: function() {

                        if ( $("a.responsive-menu").hasClass("open") )
                            $("a.responsive-menu").toggleClass("open");

                        if ( $(".hedgehog-navbar").hasClass("open") )
                            $(".hedgehog-navbar").toggleClass("open");

                        if ( $(".responsive-nav-container").hasClass("open") )
                            $(".responsive-nav-container").toggleClass("open");

                        if ( !matchMedia( "only screen and (max-width : 768px)").matches )
                            return true;

                        if ( ['INPUT', 'TEXTAREA'].indexOf(document.activeElement.nodeName) !== -1 )
                            document.activeElement.blur();

                        return false;
                    }
                });


            });
  </script>
