    <script>
     $(document).ready(function() {
  $('.site-manual .chapter-title').click(function(e) {
    e.preventDefault();
    $(e.target).parents('.chapter').find('.chapter-contents').slideToggle();
  });
  if($('body').is('.site-toc')){
    var newParent = document.querySelector('div.screensteps-container-wrapper');
    var oldParent = document.querySelector('div.screensteps-group');

    while (oldParent.childNodes.length > 0) {
        newParent.insertBefore(oldParent.childNodes[0], newParent.childNodes[0]);
    }

    var title = document.querySelector("h1");
    title.innerHTML = title.innerHTML.replace("Canvas @ Yale Help Site", "What can we help you with?");
  }
  // Accessibility Footer
  	var x = document.createElement("FOOTER");
	x.setAttribute("id", "footer-area");
	document.body.appendChild(x);
	var y = document.createElement("P");
	y.innerHTML = "<a id=\"footer-text\" href=\"https://accessibility.yale.edu\" target=\"_blank\">Accessibility Information</a>";
	document.getElementById("footer-area").appendChild(y);
});
    </script>
