  <script src="../Content/js/jquery.js">
  </script>
  <script src="../Content/js/jquery.cookie.js">
  </script>
  <script type="text/javascript">
   $(function () {
        if ($.cookie('showdisclaimer') == null || $.cookie('showdisclaimer') != "false") {
            $('#PageModal').modal('show');
        }
    });
    function DisclaimerCheck() {
        if ($('#DiscCheck').is(':checked')) {
            $.cookie('showdisclaimer', 'false', { expires: 365 });
            $('#PageModal').modal('hide');
        }
    };
  </script>
  <script>
   function appClick(event, element, pagelink) {
      window.open(pagelink);
    }
  </script>
  <script>
   $('.filter-all').click(function(){
       $('.filter-all').addClass('active');
       $('.filter-app').removeClass('active');
       $('.align').show();
    });
    $('.filter-app').click(function(){
      $('.filter-app').removeClass('active');
      $('.filter-all').removeClass('active');
      var filterId = $(this).attr('id');
      $('#' + filterId).addClass('active');
      $('.align').hide();
      $('.' + filterId + 'show').show();
    });
    $('.filter-all').addClass('active');
  </script>
