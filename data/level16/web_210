         <section class="block block-boxes block-boxes-os_boxes_columns" delta="1462475532" id="block-boxes-1462475532" module="boxes">
          <div class="block-inner clearfix">
           <h2 class="block-title" ng-non-bindable="">
            Calendar
           </h2>
           <div class="block-content content">
            <div class="boxes-box" id="boxes-box-1462475532">
             <div class="boxes-box-content">
              <!-- Two column 33-66 -->
              <div class="at-panel gpanel panel-display two-33-66 clearfix">
               <div class="region region-two-33-66-first">
                <div class="region-inner clearfix">
                 <div class="block block-boxes block-boxes-os_sv_list_box no-title" delta="os_events_upcoming" id="block-boxes-os-events-upcoming" module="boxes">
                  <div class="block-inner clearfix">
                   <div class="block-content content" ng-non-bindable="">
                    <div class="boxes-box" id="boxes-box-os_events_upcoming">
                     <div class="boxes-box-content">
                      <div class="os-sv-list os_sv_list_box sv-list-event sv-list-view-sidebar_teaser sv-list-layout-list sv-grid-3" data-delta="os_events_upcoming" data-page="0" id="box-os_events_upcoming-page">
                       <article class="node node-event article event-start clearfix" id="node-1121068" role="article" target="_top">
                        <div class="event-start">
                         <span class="event-year">
                          2019
                         </span>
                         <span class="event-start-month">
                          Mar
                         </span>
                         <span class="event-start-day">
                          12
                         </span>
                        </div>
