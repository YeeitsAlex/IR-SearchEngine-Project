jQuery(document).ready(function () {

    jQuery("#edit-go").click(function(){ 
    var value = jQuery("#edit-jump").val();

    //ljw 2019-01-31 added (begin)--update our city cookie
    //still 365 days (same as in php side of popup_homeinfo module)
    //source: https://www.w3schools.com/js/js_cookies.asp
    var d = new Date();
    d.setTime(d.getTime() + (365*24*60*60*1000));
    var expires = "expires=" + d.toUTCString();
    document.cookie = "my_city" + "=" + value + ";" + expires + ";path=/";
    //ljw 2019-01-31 added (end)--update our city cookie

