     <main class="content" id="genesis-content">
      <article class="post-1004 page type-page status-publish entry" itemscope="" itemtype="https://schema.org/CreativeWork">
       <header class="entry-header">
        <h1 class="screen-reader-text">
         Home
        </h1>
        <h2 class="entry-title">
         <a href="https://aysps.gsu.edu/">
          Home
         </a>
        </h2>
       </header>
       <div class="entry-content" itemprop="text">
        <div class="pf-content">
         <div class="vc_row wpb_row vc_row-fluid vc_row-no-padding" data-vc-full-width="true" data-vc-full-width-init="false" data-vc-stretch-content="true">
          <div class="wpb_column vc_column_container vc_col-sm-12 vc_hidden-xs">
           <div class="vc_column-inner">
            <div class="wpb_wrapper">
             <link href="https://fonts.googleapis.com/css?family=Lato:700%2C400" media="all" property="stylesheet" rel="stylesheet" type="text/css"/>
             <script src="https://aysps.gsu.edu/wp-content/plugins/revslider/public/assets/js/jquery.themepunch.tools.min.js?rev=5.4.8" type="text/javascript">
             </script>
             <script src="https://aysps.gsu.edu/wp-content/plugins/revslider/public/assets/js/jquery.themepunch.revolution.min.js?rev=5.4.8" type="text/javascript">
             </script>
             <div class="rev_slider_wrapper fullscreen-container" data-source="gallery" id="rev_slider_66_1_wrapper" style="background:transparent;padding:0px;">
              <div class="rev_slider fullscreenbanner" data-version="5.4.8" id="rev_slider_66_1" style="display:none;">
