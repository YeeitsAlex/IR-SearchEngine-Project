  $("a.fancy").fancybox({
    openEffect    : 'none',
    closeEffect   : 'none',
    overlay : {
        opacity: 0.8,
        closeClick: true
    },
    nextEffect: 'fade',
    prevEffect: 'fade',
    fitToView: true,
    loop: true,
    arrows: true,
    padding: 0,
    beforeShow: function(){
        $("body").css({'overflow':'hidden'});
    },
    afterClose: function(){
        $("body").css({'overflow':'visible'});
    }
  });
  
  $("a.fancy-frame").fancybox({
    maxWidth  : 870,
    maxHeight : 530,
    fitToView : false,
    width   : '70%',
    height    : '70%',
    autoSize  : false,
    closeClick  : false,
    openEffect  : 'none',
    closeEffect : 'none',
    padding: 0,
    beforeShow: function(){
        $("body").css({'overflow':'hidden'});
    },
    afterClose: function(){
        $("body").css({'overflow':'visible'});
    }
  });
  
  $("a.fancy-media").fancybox({
    openEffect  : 'none',
    closeEffect : 'none',
    helpers : {
      media : {}
    },
    padding: 0,
    beforeShow: function(){
        $("body").css({'overflow':'hidden'});
    },
    afterClose: function(){
        $("body").css({'overflow':'visible'});
    }
  });
  
