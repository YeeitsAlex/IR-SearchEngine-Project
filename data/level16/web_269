         <div class="vc_row-full-width vc_clearfix">
         </div>
         <div class="upb_bg_img" data-animation-repeat="repeat" data-bg-animation="left-animation" data-bg-animation-type="h" data-bg-img-position="" data-bg-img-repeat="no-repeat" data-bg-img-size="cover" data-bg-override="0" data-bg_img_attach="scroll" data-custom-vc-row="" data-fadeout="" data-fadeout-percentage="30" data-image-id="1855" data-img-parallax-mobile-disable="true" data-is_old_vc="" data-overlay="false" data-overlay-color="" data-overlay-pattern="" data-overlay-pattern-opacity="" data-overlay-pattern-size="" data-parallax-content="" data-parallax-content-sense="30" data-parallx_sense="30" data-row-effect-mobile-disable="true" data-rtl="false" data-theme-support="" data-ultimate-bg="url(https://finance.gsu.edu/files/2017/11/20170816_MLB_Capitol_Skyline_002.jpg)" data-ultimate-bg-style="vcpb-vz-jquery" data-upb-bg-animation="" data-upb-overlay-color="" data-vc="5.6">
         </div>
         <div class="vc_row wpb_row vc_row-fluid" data-vc-full-width="true" data-vc-full-width-init="false">
          <div class="wpb_column vc_column_container vc_col-sm-12">
           <div class="vc_column-inner">
            <div class="wpb_wrapper">
             <div class="vc_separator wpb_content_element vc_separator_align_center vc_sep_width_100 vc_sep_border_width_2 vc_sep_pos_align_center separator-title vc_separator-has-text">
              <span class="vc_sep_holder vc_sep_holder_l">
               <span class="vc_sep_line" style="border-color:#cccccc;">
               </span>
              </span>
              <h4>
               Deadlines and Announcements
              </h4>
              <span class="vc_sep_holder vc_sep_holder_r">
               <span class="vc_sep_line" style="border-color:#cccccc;">
               </span>
              </span>
             </div>
             <div class="templatera_shortcode">
              <div class="vc_row wpb_row vc_row-fluid vc_column-gap-30">
               <div class="wpb_column vc_column_container vc_col-sm-6">
                <div class="vc_column-inner">
                 <div class="wpb_wrapper">
                  <div class="wpb_text_column wpb_content_element ">
                   <div class="wpb_wrapper">
                    <p>
                     <span style="color: #cc0000">
                      <strong>
                       January 2019 Month EndÂ Financial is closed on Tuesday, February 20th, 2019
                      </strong>
                     </span>
                     <span style="color: #cc0000">
                     </span>
                    </p>
                    <p>
                     <a href="https://finance.gsu.edu/download/fy-18-annual-financial-report/?wpdmdl=2665">
                      <strong>
                       FY 2018 Georgia State Annual Financial Report
                      </strong>
                     </a>
                    </p>
                   </div>
                  </div>
                 </div>
