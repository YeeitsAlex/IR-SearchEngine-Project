           <script id="checkbox-group-template" type="text/x-template">
            <div :style="menuStyles" class="kappa">

          <h2 class="checkbox-group__header" v-if="header">{{header}}</h2>

          <div class="vue-checkbox select-all-checkbox">
            <vue-checkbox
              :id="selectAllCheckbox.id"
              :count="selectAllCheckbox.count"
              :label="selectAllCheckbox.label"
              :labelColor="selectAllCheckbox.labelColor"
              :disabled="selectAllCheckbox.disabled"
              :checked="selectAllCheckbox.checked"
              @change="onChangeAll"
            ></vue-checkbox>
          </div>

        <div class="row tablet-row mobile-row">
          <div class="span3 tablet-span6 xmobile-span12">
            <div class="vue-checkbox" v-for="(checkbox, i) in checkboxes" v-if="i <= checkboxes.length/2">   
                <vue-checkbox
                  :id="checkbox.id"
                  :count="checkbox.count"
                  :label="checkbox.label"
                  :labelColor="checkbox.labelColor"
                  :activeCheckboxColor="checkbox.activeCheckboxColor"
                  :activeBorderColor="checkbox.activeBorderColor"
                  :disabled="checkbox.disabled"
                  :checked="checkbox.checked"
                  @change="onChange"
                ></vue-checkbox>
            </div>
          </div>
          <div class="span3 tablet-span6 xmobile-span12">
            <div class="vue-checkbox" v-for="(checkbox, y) in checkboxes" v-if="y > checkboxes.length/2">   


                <vue-checkbox
                  :id="checkbox.id"
                  :count="checkbox.count"
                  :label="checkbox.label"
                  :labelColor="checkbox.labelColor"
                  :activeCheckboxColor="checkbox.activeCheckboxColor"
                  :activeBorderColor="checkbox.activeBorderColor"
                  :disabled="checkbox.disabled"
                  :checked="checkbox.checked"
                  @change="onChange"
                ></vue-checkbox>

            </div>
          </div>
        </div>

          <div class="checkbox-group__button-container" v-if="saveButton">
