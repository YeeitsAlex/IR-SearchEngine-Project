           <script id="checkbox-template" type="text/x-template">
            <div class="checkbox-container" :class="[ classes ]">

          <div class="checkbox-group"
            :style="checkboxStyles"
            @click="toggle()">
            
            <input type="checkbox"
              class="checkbox"
              :id="id || uniqueId"
              :name="name"
              :value="value"
              :label="label"
              :disabled="disabled"
              :checked="checked"
              @change="toggle()"
            >
          </div>

          <label class="checkbox_label"
            :for="uniqueId"
            @click="toggle()"
