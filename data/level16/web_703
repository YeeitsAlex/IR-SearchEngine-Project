            <section class="block block-boxes block-boxes-os_sv_list_box" delta="1495035896" id="block-boxes-1495035896" module="boxes">
             <div class="block-inner clearfix">
              <h2 class="block-title" ng-non-bindable="">
               News
              </h2>
              <div class="block-content content" ng-non-bindable="">
               <div class="boxes-box" id="boxes-box-1495035896">
                <div class="boxes-box-content">
                 <div class="os-sv-list os_sv_list_box sv-list-news sv-list-view-sidebar_teaser sv-list-layout-list sv-grid-3" data-delta="1495035896" data-page="0" id="box-1495035896-page">
                  <article class="node node-news article modified-in-os_news_process_node clearfix" id="node-1158950" role="article" target="_top">
                   <div class="field field-name-field-photo field-type-image field-label-hidden view-mode-sidebar_teaser">
                    <div class="field-items">
                     <figure class="clearfix field-item even">
                      <img alt="Medicare Dental" class="image-style-os-square-100-100" height="100" ng-non-bindable="" src="//static.hwpi.harvard.edu/files/styles/os_square_100_100/public/oralhealthprimarycare/files/oral_health_older.jpg?m=1551718436&amp;itok=0jT8eMna" title="Medicare Dental Benefit Will Improve Health And Reduce Health Care Costs" width="100"/>
                     </figure>
                    </div>
                   </div>
                   <div class="node-content" ng-non-bindable="">
                    <h1 class="node-title" ng-non-bindable="" rel="nofollow">
                     <a href="https://cipcoh.hsdm.harvard.edu/news/medicare-dental-benefit-will-improve-health-and-reduce-health-care-costs" target="_top">
                      Medicare Dental Benefit Will Improve Health And Reduce Health Care Costs
                     </a>
                    </h1>
                    <div class="field field-name-field-news-date field-type-datestamp field-label-hidden view-mode-sidebar_teaser">
                     <div class="field-items">
                      <div class="field-item even">
                       <span class="date-display-single">
                        Monday, March 4, 2019
                       </span>
                      </div>
                     </div>
                    </div>
                   </div>
                  </article>
                  <article class="node node-news article modified-in-os_news_process_node clearfix" id="node-1156758" role="article" target="_top">
                   <div class="field field-name-field-photo field-type-image field-label-hidden view-mode-sidebar_teaser">
                    <div class="field-items">
                     <figure class="clearfix field-item even">
                      <img alt="Dental Care" class="image-style-os-square-100-100" height="100" ng-non-bindable="" src="//static.hwpi.harvard.edu/files/styles/os_square_100_100/public/oralhealthprimarycare/files/istock_elderlypatient_small.jpg?m=1550156160&amp;itok=d-9ybhbU" title="Successful Dental Treatment Program for SUD Patients in Utah" width="100"/>
                     </figure>
                    </div>
                   </div>
                   <div class="node-content" ng-non-bindable="">
