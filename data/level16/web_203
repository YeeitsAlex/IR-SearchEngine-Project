     <main class="content" id="genesis-content">
      <article class="post-324 page type-page status-publish entry" itemscope="" itemtype="https://schema.org/CreativeWork">
       <header class="entry-header">
        <h1 class="screen-reader-text">
         Robinson College of Business – Georgia State University – Atlanta, Georgia
        </h1>
        <h2 class="entry-title">
         <a href="https://robinson.gsu.edu/">
          Robinson College of Business – Georgia State University – Atlanta, Georgia
         </a>
        </h2>
       </header>
       <div class="entry-content" itemprop="text">
        <div class="pf-content">
         <div class="vc_row wpb_row vc_row-fluid">
          <div class="wpb_column vc_column_container vc_col-sm-12 vc_hidden-lg vc_hidden-md vc_hidden-sm vc_col-has-fill vc_general vc_parallax vc_parallax-content-moving" data-vc-parallax="1.5" data-vc-parallax-image="https://robinson.gsu.edu/files/2018/01/research_slider.jpg">
           <div class="vc_column-inner">
            <div class="wpb_wrapper">
             <div class="vc_empty_space" style="height: 32px">
              <span class="vc_empty_space_inner">
              </span>
             </div>
             <div class="wpb_text_column wpb_content_element vc_custom_1551112640708">
              <div class="wpb_wrapper">
               <h1 style="text-align: left">
                Some Were Born to Outpace
               </h1>
               <h4 style="text-align: left">
                Join us to solve real business problems.
               </h4>
              </div>
             </div>
             <div class="vc_empty_space" style="height: 32px">
              <span class="vc_empty_space_inner">
              </span>
             </div>
            </div>
           </div>
          </div>
         </div>
         <div class="vc_row wpb_row vc_row-fluid vc_custom_1496690398755">
          <div class="wpb_column vc_column_container vc_col-sm-12 vc_hidden-sm vc_hidden-xs">
           <div class="vc_column-inner vc_custom_1498669989901">
            <div class="wpb_wrapper">
             <link href="https://fonts.googleapis.com/css?family=Open+Sans:700%7CLato:700" media="all" property="stylesheet" rel="stylesheet" type="text/css"/>
             <div class="rev_slider_wrapper fullwidthbanner-container" data-source="gallery" id="rev_slider_47_1_wrapper" style="margin:0px auto;background:transparent;padding:0px;margin-top:0px;margin-bottom:0px;">
              <div class="rev_slider fullwidthabanner" data-version="5.4.8" id="rev_slider_47_1" style="display:none;">
               <ul>
                <li data-description="" data-easein="default" data-easeout="default" data-hideafterloop="0" data-hideslideonmobile="off" data-index="rs-101" data-masterspeed="300" data-param1="" data-param10="" data-param2="" data-param3="" data-param4="" data-param5="" data-param6="" data-param7="" data-param8="" data-param9="" data-rotate="0" data-saveperformance="off" data-slotamount="default" data-thumb="https://robinson.gsu.edu/files/2019/03/womenlead_slider-100x50.jpg" data-title="Slide" data-transition="fade">
