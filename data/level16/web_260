        jQuery(document).ready(function ($) { if
                ($(".newBooksSearchTip .searchTip").contents().length > 0) { var customTips =
                $("#customSearchTip"); customTips.empty();
                customTips.append($(".newBooksSearchTip").contents()); $("#customSearchTip
                .newBooksSearchHelp").show(); $(".newBooksSearchTip").remove(); } else { // We're on
                the new book page, so just remove all this non-sense // and make it work like one
                would expect. $(".newBooksSearchTip").remove(); } });
       </script>
