  <script type="text/javascript">
   var msg = "Your LibCat session will end in 2 minutes!";
                     var timeOut = 30;
                     var grace =  2;
                     var graceSeconds;
                     var seconds;
                     var alreadyRan = false;

                     function tcc_display_timeout(msg) {
                        seconds = seconds > 0 ? seconds-1 : 0;

                        msg = "Your session will end in " + seconds + " seconds.";

                        document.getElementById('tcc_timeout_message').innerHTML=msg;
                        document.getElementById('tcc_timeout_container').style.display='inline';

                       if (alreadyRan == false){
                           $('#tcc_timeout_container').reveal({
                              animation: 'none',                        //fade, fadeAndPop, none
                              closeonbackgroundclick: false,            //if you click background will modal close?
                              dismissmodalclass: 'close-reveal-modal'   //the class of a button or element that will close an open modal
                           });
                              alreadyRan = true;
                              $('body').css('overflow', 'hidden');
                        }

                        setTimeout("tcc_display_timeout()",1000);
                     }


                     function timedMsg(time, grace, txtMsg) {
                        msg = txtMsg;
                        grace = grace;
                        timeOut = (time - grace) * 60000;
                        seconds = grace * 60;

                        graceSeconds = (grace * 60);

                        setTimeout("tcc_display_timeout(msg)",timeOut);
                     }






               /* trying to find the text 'Available online' so we can highlight it */
               $(document).ready(function() {


//
//loadBookCovers();
// Moved this to the body on load per install documents.  Fixes IE book covers.  DCH

                  /*looking for the text on the search results page*/
                  $(function() {
                     /* tamu 20110302 JT I feel dirty doing this in js but just cant find where this stray comma is in the xsl */
                     var findComma = $('#resultList .resultListTextCell div:contains(", Available")').replaceWith('<div class="line5Link highlightAvailableOnline"> Available online</div>');

                     var foundin = $('#resultList .resultListTextCell div:contains("Available online")').addClass("highlightAvailableOnline");
                     foundin = $('#resultList .resultListTextCell div:contains("Available")').addClass("highlightAvailable");
                     foundin = $('#resultList .resultListTextCell div:contains("available")').addClass("highlightAvailable");
                     foundin = $('#resultList .resultListTextCell div:contains("Checked out")').addClass("highlightCheckedOut");

                     /* looking for this text on a result page */
                     foundin = $('.bibTags .bibTag .subfieldData:contains("Available online")').addClass("highlightAvailableOnline");
                     foundin = $('.bibTags .bibTag .subfieldData:contains("World Wide Web")').addClass("highlightAvailableOnline");
                     foundin = $('.bibTags .bibTag .fieldLabelSpan:contains("Internet Access:")').addClass("highlightAvailableOnline");
                     foundin = $('.bibTags .bibTag .subfieldData:contains("electronic book")').addClass("highlightAvailableOnline");

                     /* checked out, not checked out */
                     foundin = $('.bibTags .bibTag .subfieldData:contains("Available")').addClass("highlightAvailable");
                     foundin = $('.bibTags .bibTag .subfieldData:contains("Checked Out")').addClass("highlightCheckedOut");
                     foundin = $('.bibTags .bibTag .subfieldData:contains("Renewed")').addClass("highlightCheckedOut");
                  });



               	$.GBSV.init();


                  timedMsg(timeOut, grace, msg);
                  //timedMsg(.5, .4, 'bla');
               });


                setLogoff();
  </script>
