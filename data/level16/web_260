 <script>
  /* Fix for galaxy S3 rendering of select field controls 
         * http://jsbin.com/iMuRULA/3 
         */

        var nua = navigator.userAgent;
        var is_android = ((nua.indexOf('Mozilla/5.0') > -1 && nua.indexOf('Android ') > -1 && nua.indexOf('AppleWebKit') > -1) && !(nua.indexOf('Chrome') > -1));
        //is_android = true; // DEBUG
        if(is_android) {
          jQuery('select.form-control').removeClass('form-control').addClass('android-form-control');
        }
 </script>
