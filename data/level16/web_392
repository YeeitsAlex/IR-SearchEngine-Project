                 <div class="elementor-column-wrap elementor-element-populated">
                  <div class="elementor-widget-wrap">
                   <div class="elementor-element elementor-element-39c2f92 elementor-widget elementor-widget-counter" data-element_type="counter.default" data-id="39c2f92">
                    <div class="elementor-widget-container">
                     <div class="elementor-counter">
                      <div class="elementor-counter-number-wrapper">
                       <span class="elementor-counter-number-prefix">
                       </span>
                       <span class="elementor-counter-number" data-delimiter="," data-duration="2000" data-to-value="70">
                       </span>
                       <span class="elementor-counter-number-suffix">
                       </span>
                      </div>
