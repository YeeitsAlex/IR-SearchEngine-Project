        <section>
         <div class="content">
          <section class="block-hms-mods-content system_main_block" id="block-hms-mods-content">
           <article about="/alumni" class="mode--full landing_page" data-history-node-id="74" id="node-74" role="article">
            <div class="row landing-page-content">
             <div class="columns small-12">
              <h1 class="node-title">
               <span class="field-wrapper">
                Alumni
               </span>
              </h1>
             </div>
            </div>
            <div class="view-mode-full">
             <div class="field-wrapper field field--name-field-components field--type-entity-reference-revisions field--label-hidden field__items">
              <div class="field__item">
               <div class="bundle--events_component row paragraph paragraph--type--events-component paragraph--view-mode--default">
                <div class="columns small-12">
                 <h2>
                  <h2 class="component-title field__item">
                   Upcoming Events
                  </h2>
                 </h2>
                </div>
                <div class="event--items columns small-up-1 medium-up-2">
                 <div class="field-wrapper field field--name-field-events field--type-entity-reference field--label-hidden field__items">
                  <div class="field__item">
                   <div class="columns small-12 medium-6 large-6">
                    <article about="/event/soma-weiss-student-research-day" class="mode--event_card event" data-history-node-id="176" id="node-176" role="article">
                     <div class="event-cal">
                      <div class="short-date">
                       <div class="month">
                        Mar
                       </div>
                       <div class="day">
                        12
                       </div>
                      </div>
                     </div>
                     <div class="event-details">
                      <h3>
                       <a href="/event/soma-weiss-student-research-day" rel="bookmark">
                        Soma Weiss Student Research Day
                       </a>
                      </h3>
                      <div class="content">
                       Tuesday, March 12, 2019
