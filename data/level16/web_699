              jQuery("body").addClass("esgbox-hidearrows");
          }
			var i = 0,
				multiple = false;
			a = a.slides;
			for(var b in a) {
				i++;
				if(i > 1) {
					multiple = true;
					break;
				}
			}
			if(!multiple) jQuery("body").addClass("esgbox-single");
			if(c.type === "image") jQuery(".esgbox-button--zoom").show();
		},
