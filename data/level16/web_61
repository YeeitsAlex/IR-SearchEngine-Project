             <div class="panel-pane pane-views-panes pane-gw-news-panel-pane-9">
              <div class="pane-content">
               <div class="view view-gw-news view-id-gw_news view-display-id-panel_pane_9 view-dom-id-818dfacf47fffb5a27814f0e145fb39f">
                <div class="view-content">
                 <div class="views-row views-row-1 views-row-odd views-row-first views-row-last">
                  <div about="/news-test" class="node node-article" id="node-599" typeof="sioc:Item foaf:Document">
                   <div class="node-headline clearfix">
                    <div class="node-title clearfix">
                     <a href="https://app.joinhandshake.com/external_feeds/1668/public.rss?token=PFi2POa24C3BU2qClDPhFxRTOqayuRgZ8kmtpEX0WfhK_t4SImZA2Q" target="_blank">
                      News Test
                     </a>
                    </div>
