<script type="text/javascript">
 //Functions for disabling home page help popup message
    function SetHelpMsgStorage() {
        $("#lgScreen-help").fadeOut(500);
        document.getElementById("smScreen-help").style.display = "none";
        localStorage.setItem("helpMsg", "hide");
    }
   $("#helpMsgDisableOpt").on("change", function () {
        SetHelpMsgStorage();
    });
    function GetHelpMsgStorage() {
        if (localStorage.helpMsg == "hide") {
            document.getElementById("lgScreen-help").style.display = "none";
            document.getElementById("smScreen-help").style.display = "none";
        }
        else {
            document.getElementById("helpMsgDisableOpt").checked = false;
        }
    }

//Necessary to ensure image map coordinates adapt to different screen sizes
    $(document).ready(function () {
        //        $('#horzLifecycle').imageMapResize();
        $('img[usemap]').rwdImageMaps();

        $("#popup-help").delay(500).slideDown(500);

        $("#popup-help-close").click(function () {
            $("#popup-help").slideUp(500);
        });

       GetHelpMsgStorage();
    });
</script>
