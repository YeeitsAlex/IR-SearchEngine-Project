          <script>
           //hidden input values should be set with js to prevent wrong inputvalues when navigating back to this page
            pp_jquery("#hv_headlines_lastshown-3018887").val("5");
            pp_jquery("#hv_headlines_loaded-3018887").val("6");
            var stopservice3018887 = 0;
            var last_shown3018887 = 5;
            var loaded_items3018887 = 6;
            var totalreleases3018887 = 50;
            var corrected_totalreleases3018887 = totalreleases3018887 - 5;
            var pp_scrollHeight3018887 = pp_jquery(".pp_bigheadlines_height-3018887 .mm_listitem").outerHeight(true);
            var bigheadlinescontainerHeight3018887 = pp_scrollHeight3018887 * 5 - 10;

            var count = 0;

            function morenews3018887() {
                var last_shown3018887 = pp_jquery('#hv_headlines_lastshown-3018887').val();
                var loaded_items3018887 = pp_jquery('#hv_headlines_loaded-3018887').val();

                last_shown3018887 = parseInt(last_shown3018887) + 1;
                pp_jquery('#hv_headlines_lastshown-3018887').val(last_shown3018887);

                pp_jquery(".button_less-3018887").css("opacity",1);
                pp_jquery(".button_less-3018887").attr("onclick","backnews3018887();").attr('tabindex', '0');
                // Calculate height of scrolldiv
                var pp_scrollHeight3018887 = pp_jquery(".pp_bigheadlines_height-3018887 .mm_listitem").outerHeight(true);
                count++;

                pp_jquery(".button_more-3018887").removeAttr("onclick").attr('tabindex', '-1');
                pp_jquery(".pp_bigheadlines_items-3018887").animate({
                    marginTop: "-=" + pp_scrollHeight3018887                  }, 400, function() {
                    if (last_shown3018887 < totalreleases3018887){
                        pp_jquery(".button_more-3018887").attr("onclick","morenews3018887();").attr('tabindex', '0');

                    } else {
                        pp_jquery(".button_less-3018887").focus();
                    }

                    // update tabindex of headline-item links so only visible items are part of the tabindex.
                    var nonVisibleItems = pp_jquery('#pp-headline-3018887 .mm_listitem .pp_bigheadlines_heading a[tabindex="-1"]');
                    if (nonVisibleItems.length === 0) {
                        pp_jquery('#pp-headline-3018887 .mm_listitem:first-child a').attr('tabindex', -1);
                    } else {
                        var lastNonVisibleItem = nonVisibleItems[nonVisibleItems.length -1];
                        pp_jquery(lastNonVisibleItem).closest('.mm_listitem').next('.mm_listitem').find('a').attr('tabindex', -1);
                    }
                  });

                if (stopservice3018887 == 0) {

                    show_headlines3018887(loaded_items3018887,1);

                }

                if (last_shown3018887 >= totalreleases3018887) {

                    pp_jquery(".button_more-3018887").css("opacity",0.2);
                    stopservice3018887 = 1;

                } else {

                    if(loaded_items3018887 < totalreleases3018887){
                        limitstartnew = parseInt(loaded_items3018887) + 1;
                        pp_jquery('#hv_headlines_loaded-3018887').val(limitstartnew);
                    }
                }

            }

            function backnews3018887(){
                last_shown3018887 = parseInt(pp_jquery('#hv_headlines_lastshown-3018887').val()) - 1;
                pp_jquery('#hv_headlines_lastshown-3018887').val(last_shown3018887);
                pp_jquery(".button_more-3018887").css("opacity",1);
                pp_jquery(".button_more-3018887").attr("onclick","morenews3018887();").attr('tabindex', '0');
                // Calculate height of scrolldiv
                var pp_scrollHeight3018887 = pp_jquery(".pp_bigheadlines_height-3018887 .mm_listitem").outerHeight(true);
                count--;

                pp_jquery(".button_less-3018887").removeAttr("onclick").attr('tabindex', '-1');
                pp_jquery(".pp_bigheadlines_items-3018887").animate({
                    marginTop: "+=" + pp_scrollHeight3018887                  }, 400, function() {

                    if (last_shown3018887 > 5){
                        pp_jquery(".button_less-3018887").attr("onclick","backnews3018887();").attr('tabindex', '0');
                    }else {
                        pp_jquery(".button_more-3018887").focus();
                    }

                    // update tabindex of headline titles and other links so only visible items are part of the tabindex.
                    var nonVisibleItems = pp_jquery('#pp-headline-3018887 .mm_listitem .pp_bigheadlines_heading a[tabindex="-1"]');
                    var lastNonVisibleItem = nonVisibleItems[nonVisibleItems.length -1];
                    pp_jquery(lastNonVisibleItem).closest('.mm_listitem').find('a').not('.pp_bigheadlines_more').attr('tabindex', 0);
                  });

                if (last_shown3018887 == 5){
                    pp_jquery(".button_less-3018887").css("opacity",0.2);

                }
            }

            function getHeight3018887(){
                pp_jquery('.pp_bigheadlines_height-3018887').css("height", bigheadlinescontainerHeight3018887);
            }

            pp_jquery(document).ready(function() {

                getHeight3018887();

                
            });


	function show_headlines3018887(limitstart, limitend){

		var tags_addon = '';
		var multimedia_count = '';
		var sharerelease = '';
				
		pp_jquery.getJSON('//news.osu.edu/services/getheadlines.php?releases=["341430","341450","341277","341274","341085","340611","340534","340361","340138","340093","339779","339776","339762","339044","338784","338805","336883","338695","338516","338543","338356","338402","338228","338217","337958","337968","336720","336697","336683","336640","336213","335980","335968","335831","335735","335633","333626","333631","333625","333609","333542","332502","332386","332229","332200","332189","332180","332047","331873","330621"]' + tags_addon + multimedia_count + '&preview=&subtitle=&substring=200&video=1&limitstart=' + limitstart + '&limitend=' + limitend, function(data) {

		if (!data) {

			stopservice3018887 = 1;

		} else {

			var items = '';

			pp_jquery.each(data, function(key, val) {

				if (key != 'error'){

						var date = val['date'];
						var date_split = date.split(" ");
						var date_day = date_split[0];
						var date_month = date_split[1];
						var date_year = date_split[2];
						var tags_items = '';
						var multimedia_count = '';
						var sharerelease = '';

                        var addthisDescription = val['message'].replace(/"/g, "");

						
												
						if(val['subtitle']){
							var subtitle = "<h4 class=\"pp_bigheadlines_subheading\">" + val['subtitle'] + "</h4>";
						} else {
							var subtitle = "";
						}

						items += "<div class=\"mm_listitem\">";
												items += "<div class=\"pp_unit pp_one-of-four pp_bigheadlines_id-3018887\">";
						items += "<div class=\"pp_bigheadlines_image\" onclick=\"window.location='" + val['caseurl'] + "'\" style=\"" + val['featuredimage'] + "\" aria-hidden=\"true\" role=\"presentation\" tabindex=\"-1\">" + val['videothumbnail'] + "</div>";
						items += "</div>";
						items += "<div class=\"pp_unit pp_three-of-four pp_bigheadlines_data\">";
																		items += "<div class=\"pp_bigheadlines_date\"><div class=\"pp_date_month\">" + date_month + "</div><div class=\"pp_date_day\">" + date_day + "<span class=\"pp_date_day_comma\">,</span></div><div class=\"pp_date_year\">" + date_year + "</div></div>";
						
						items += "<div class=\"pp_bigheadlines_title\">";
						items += "<h3 class=\"pp_bigheadlines_heading\" title=\"" + val['title'] + "\" class=\"";
						items += '';
						items += "\">";
						items += "<a href=\"" + val['caseurl'] + "\">" + val['title'] + "</a>";
						items += "</h3>" + subtitle + "</div>";
						items += sharerelease;
						items += "<div class=\"pp_bigheadlines_text\">" + val['message'] + "</div>";
						items += "<a class=\"pp_bigheadlines_more\" title=\"" + val['title'] + "\" href=\"" + val['caseurl'] + "\" aria-hidden=\"true\" role=\"presentation\" tabindex=\"-1\">Read more</a>";
						items += tags_items;
						items += multimedia_count;
						items += "</div>";
						items += "</div>";
				}

			});

				//pp_jquery(".preloader").fadeOut();
				pp_jquery(".pp_bigheadlines_items-3018887").append(items);
				
				getHeight3018887();
			}
		});
	}
                           pp_jquery(window).resize(function() {

               getHeight3018887();
               pp_jquery(".pp_bigheadlines_items-3018887").css("marginTop", -pp_scrollHeight3018887 * count);

           });
          </script>
          <script type="text/javascript">
           function switchCoveragesets(direction) {

        var current_coverageset = Number(pp_jquery('#current_coverageset').val());
        var coveragesets = Number(pp_jquery('#total_coveragesets').val());

        var divid = "#coverageset" + current_coverageset;

          pp_jquery('' + divid).fadeOut('fast', function() {

            if (direction == "next") {
            current_coverageset = current_coverageset + 1;
            } else {
            current_coverageset = current_coverageset - 1;
            }
            // Set in hidden field
            pp_jquery('#current_coverageset').val(current_coverageset);

            var newdivid = "#coverageset" + current_coverageset;

            pp_jquery('' + newdivid).fadeIn('fast', function() {
            // Animation complete.
            });

              if (current_coverageset == coveragesets) {
                  var nextbutton = document.getElementById('coverage_nextbutton');
                  var nexthref = document.getElementById('coverage_nexthref');
                  pp_jquery(nextbutton).attr('class', 'pp_headlines_next_disabled');
                  nexthref.removeAttribute("href");
              }
              if (current_coverageset == 1) {
                  var previousbutton = document.getElementById('coverage_previousbutton');
                  var previoushref = document.getElementById('coverage_previoushref');
                  pp_jquery(previousbutton).attr('class', 'pp_headlines_prev_disabled');
                  previoushref.removeAttribute("href");
              }
              if (current_coverageset > 1) {
                  var previousbutton = document.getElementById('coverage_previousbutton');
                  var previoushref = document.getElementById('coverage_previoushref');
                  pp_jquery(previousbutton).attr('class', 'pp_headlines_prev');
                  previoushref.setAttribute('href', 'javascript:switchCoveragesets("previous")');
              }
              if (current_coverageset < coveragesets) {
                  var nextbutton = document.getElementById('coverage_nextbutton');
                  var nexthref = document.getElementById('coverage_nexthref');
                  pp_jquery(nextbutton).attr('class', 'pp_headlines_next');
                  nexthref.setAttribute('href', 'javascript:switchCoveragesets("next")');
              }

          });

        document.getElementById("current_coverageset").value = current_coverageset;
}
          </script>
