          <script type="text/javascript">
           pp_jquery(function() {
        pp_jquery.support.placeholder = false;
        newelement = document.createElement('input');
        if('placeholder' in newelement) pp_jquery.support.placeholder = true;
    });

    pp_jquery(function() {
        if(!pp_jquery.support.placeholder) {
            var active = document.activeElement;
            pp_jquery(':text').focus(function () {
                if (pp_jquery(this).attr('placeholder') != '' && pp_jquery(this).val() == pp_jquery(this).attr('placeholder')) {
                    pp_jquery(this).val('').removeClass('hasPlaceholder');
                }
            }).blur(function () {
                if (pp_jquery(this).attr('placeholder') != '' && (pp_jquery(this).val() == '' || pp_jquery(this).val() == pp_jquery(this).attr('placeholder'))) {
                    pp_jquery(this).val(pp_jquery(this).attr('placeholder')).addClass('hasPlaceholder');
                }
            });
            pp_jquery(':text').blur();
            pp_jquery(active).focus();
            pp_jquery('form').submit(function () {
                pp_jquery(this).find('.hasPlaceholder').each(function() { pp_jquery(this).val(); });
            });
        }
    });
          </script>
