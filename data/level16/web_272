   <div class="firstBand" id="firstBand">
    <div class="section">
     <div class="region region-firstband">
      <div class="block block-views first last odd" id="block-views-1ec2892fb0efca8f25a6315707452245">
       <div class="view view-homepage-full-width-slider view-id-homepage_full_width_slider view-display-id-block view-dom-id-453deea86d9508d18d25e04370165600">
        <div class="view-content">
         <div class="flexslider optionset-fullwidth" id="fullwidth-slider">
          <ul class="slides">
           <li data-thumb="https://art.unca.edu/sites/default/files/styles/wide_slideshow_1280x512/public/images/pages/node/add/page/drawingdiscourse.jpg?itok=93RiORXQ" data-thumb-alt="">
            <a href="https://art.unca.edu/drawing-discourse-2019">
             <img alt="" src="https://art.unca.edu/sites/default/files/styles/wide_slideshow_1280x512/public/images/pages/node/add/page/drawingdiscourse.jpg?itok=93RiORXQ" typeof="foaf:Image"/>
            </a>
            <div class="flex-caption-topRight">
             <ul class="fullslider">
              <li>
               <h2>
                Drawing Discourse 2019
               </h2>
              </li>
              <li>
               <p>
                Exhibition at Warren Wilson College from January 18, 2019 through February 22, 2019
               </p>
              </li>
              <li class="promo-button">
               <a href="https://art.unca.edu/drawing-discourse-2019">
                Drawing Discourse International Juried Exhibition 2019
               </a>
              </li>
             </ul>
            </div>
           </li>
           <li data-thumb="https://art.unca.edu/sites/default/files/styles/wide_slideshow_1280x512/public/images/pages/file/ajax/field_representative_image/und/0/form-uAunqcIUYv2_y024MVgdsblSpYImaVORzMoCw81kEQA/CC407C84-C64C-4DB9-B3E4-974A5D2EA014.jpeg?itok=dgMCJ8OG" data-thumb-alt="Two men with yellow paint from forehead to mid nose and white paint below. Both have their eyes closed. ">
            <a href="https://art.unca.edu/shawn-winebrenner-2018-bfa-candidate">
