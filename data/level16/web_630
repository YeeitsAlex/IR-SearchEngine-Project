function switchToLibraryCatalog() {
  document.getElementById("search_scope").value = "default_scope";
  document.getElementById("tab").value = "books";
  document.getElementById("everything").checked=false;
}
function switchToEverything() {
  document.getElementById("search_scope").value = "everything";
  document.getElementById("tab").value = "everything";
  document.getElementById("library_catalog").checked=false;
}
