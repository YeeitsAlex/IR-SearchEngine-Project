        <!-- BEGIN .information -->
        <div class="information wide">
         <h2 class="headline text-center">
          Extension Sustainability
         </h2>
         <p>
          Extension Sustainability provides credible information and trainings fostering increased awareness and behavior change to improve environmental, social, and economic conditions.
         </p>
         <a class="more-link" href="https://extensionsustainability.usu.edu/about/" rel="bookmark">
          Read More
         </a>
         <!-- END .information -->
        </div>
        <!-- END .featured-page -->
       </div>
       <!-- END .content -->
      </div>
      <!-- END .row -->
     </div>
     <!-- BEGIN .row -->
     <div class="row news-section">
