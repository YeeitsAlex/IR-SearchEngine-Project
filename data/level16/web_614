   <script language="javascript">
    function DetectPopupBlocker()
{
  var bPopupBlocker;
  var newWin = window.open('','','top=5000,left=5000,width=1,height=1,scrollbars=no');
  if (newWin)
  {
    bPopupBlocker = false;
    newWin.close();
  }
  else
  {
    bPopupBlocker = true;
  }
  return bPopupBlocker;
}
if(DetectPopupBlocker()==true)
{
  document.forms[0]._popupBlockerExists.value='true';
}
else
{
  document.forms[0]._popupBlockerExists.value='false';
}
   </script>
   <script language="javascript">
    function ValidateCaseNumber(Element)
{
   if (Element==null) return null;
    if(Element.value!='' && Element.className!='watermark')
    {
        var re =/^\d*$/;
        if (re.test(Element.value) == true)
        {
	          return true;
	       }
        else
        {
           (event)?event.returnValue=false:'';
           alert('Your case number must be all numbers.');
           Element.select();
           Element.focus();
           return false;
        }
    }else{
      return true;
    }
}
   </script>
   <script language="javascript">
    var ImgProgress = new Array(4);
for (var i=0;i<ImgProgress.length;i++)
{
ImgProgress[i]=new Image(0,0);
ImgProgress[i].src = '/images/progress' + (i+1) + '.gif';
}
function WaitALittleBit()
{
setTimeout('InProcessPleaseWait()',750);
}
function InProcessPleaseWait()
{
document.forms[0].style.visibility='hidden';
_UniqueWaitControlID.style.left = (document.body.offsetWidth / 2) - (_UniqueWaitControlID.offsetWidth / 2);
_UniqueWaitControlID.style.top = (document.body.offsetHeight / 2) - (_UniqueWaitControlID.offsetHeight / 2);
_UniqueWaitControlID.style.position = 'absolute';
_UniqueWaitControlID.style.visibility='visible';
AnimationImage();
}
var ImgIndex=0;
function AnimationImage()
{
window.document._UniqueWaitControlID_Image1.src=ImgProgress[ImgIndex%ImgProgress.length].src;
ImgIndex++;
if (document.readyState!='complete')
{
setTimeout('AnimationImage()',250);
}
else
{
document.forms[0].style.visibility='visible';
_UniqueWaitControlID.style.visibility='hidden';
}
}
   </script>
   <script language="javascript">
    function onsubmitEventsHandler(){
if(ValidateCaseNumber(window.document.forms[0].mainContent_LeftColumnContent_ctl00_txtNumber)==false){return false;}
if(ValidateCaseNumber(window.document.forms[0].mainContent_LeftColumnContent_ctl00_txtNumber2)==false){return false;}
if(WaitALittleBit()==false){return false;}
return true;}
   </script>
