         <script type="text/javascript">
          $(function() {
			$("#buckeye_alert").buckeyeAlert({
				url: "//www.osu.edu/feeds/emergency-alert.rss", 
				messageClass: "content-wrapper clearfix",
				callback: function () { 
				  // Do something here...
				}
			});

			$(document).on('click', '#buckeye_alert a', function() {
				$(this).attr({'target':'_blank'});
				return true;
			});

			$(document).on('click', 'a', function() {
				var href = $(this).attr('href');
				if(href === "#") return;
				if(href.slice(0, 4) !== 'http') return;
				if(href.indexOf('//hr.osu.edu') === -1) {
					ga('ohr.send', 'event', 'Link', 'External', this.href);
					//console.log("ga('ohr.send', 'event', 'Link', 'External', " + this.href + ");");
				}
				if(href.indexOf('.pdf') > -1) {
					ga('ohr.send', 'event', 'Link', 'PDF', this.href);
					//console.log("ga('ohr.send', 'event', 'Link', 'PDF', " + this.href + ");");
				}
				if(href.indexOf('.doc') > -1 || href.indexOf('.docx') > -1) {
					ga('ohr.send', 'event', 'Link', 'Word', this.href);
					//console.log("ga('ohr.send', 'event', 'Link', 'Word', " + this.href + ");");
				}
				if(href.indexOf('.xls') > -1 || href.indexOf('.xlsx') > -1) {
					ga('ohr.send', 'event', 'Link', 'Excel', this.href);
					//console.log("ga('ohr.send', 'event', 'Link', 'Excel', " + this.href + ");");
				}
				if(href.indexOf('.csv') > -1) {
					ga('ohr.send', 'event', 'Link', 'CSV', this.href);
					//console.log("ga('ohr.send', 'event', 'Link', 'CSV', " + this.href + ");");
				}
			});

			$(document).on('click', 'a.ga-nav', function() {
				var navInfo = $(this).attr('data-nav');
				var linkInfo = $(this).attr('data-link-info');
				if(linkInfo != '') {
					ga('ohr.send', 'event', 'Navigation', navInfo, linkInfo);
					//console.log("ga('ohr.send', 'event', 'Navigation', '" + navInfo + "', '" + linkInfo + "');");
				}
			});

			$(document).on('click', 'a.ga-widgets', function() {
				var navInfo = $(this).attr('data-nav');
				var linkInfo = $(this).attr('data-link-info');
				if(linkInfo != '') {
					ga('ohr.send', 'event', 'Widgets', navInfo, linkInfo);
					//console.log("ga('ohr.send', 'event', 'Navigation', '" + navInfo + "', '" + linkInfo + "');");
				}
			});

			$(document).on('click', 'a.ga-oe', function() {
				var navInfo = $(this).attr('data-nav');
				var linkInfo = $(this).attr('data-link-info');
				if(linkInfo != '') {
					ga('ohr.send', 'event', 'Open Enrollment', navInfo, linkInfo);
					console.log("ga('ohr.send', 'event', 'Open Enrollment', '" + navInfo + "', '" + linkInfo + "');");
				}
			});


		});
         </script>
