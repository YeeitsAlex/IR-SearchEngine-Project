  <script src="/scr/ebscohostsearch.js" type="text/javascript">
  </script>
  <script type="text/javascript">
   function search() {
      if ($('#searchboxScope').val() == 'site') {
        searchSite();
      } else {
        searchPrimo();
      }
    }
    
    function searchPrimo() {
      searchQuery = $('#searchboxText').val();
      searchScope = $('#searchboxScope').val();
//      window.alert(searchScope);
      if (searchScope == 'articles') {
        newUrl = 'https://wrlc-gwahlth.primo.exlibrisgroup.com/discovery/search?query=any,contains,' + searchQuery + '&tab=CentralIndex&search_scope=CentralIndex&vid=01WRLC_GWAHLTH:01WRLC_GWAHLTH&offset=0';
      } else if (searchScope == 'catalog') {
        newUrl = 'https://wrlc-gwahlth.primo.exlibrisgroup.com/discovery/search?query=any,contains,' + searchQuery + '&tab=LibraryCatalog&search_scope=MyInstitution&vid=01WRLC_GWAHLTH:01WRLC_GWAHLTH&offset=0';
      } else if (searchScope == 'consortium') {
        newUrl = 'https://wrlc-gwahlth.primo.exlibrisgroup.com/discovery/search?query=any,contains,' + searchQuery + '&tab=WRLC&search_scope=DN_and_CI&vid=01WRLC_GWAHLTH:01WRLC_GWAHLTH&offset=0';
      } else {
        newUrl = 'https://wrlc-gwahlth.primo.exlibrisgroup.com/discovery/search?query=any,contains,' + searchQuery + '&tab=Everything&search_scope=MyInst_and_CI&vid=01WRLC_GWAHLTH:01WRLC_GWAHLTH&offset=0';
      }
      window.location.href = newUrl;
    }
    
    function searchSite() {
      // window.alert('test');
      searchQuery = $('#searchboxText').val();
      newUrl = '/websearch.cfm?searchtype=site&q=' + searchQuery;
      window.location.href = newUrl;
    }
    
    function changeScope(scope) {
      // sync hidden scope dropdown with the one just changed
      $('#searchboxScope').val(scope).change(); 
      $('#searchboxScope-sm').val(scope).change();

      // update searchbox placeholder text
      if (scope == 'site') {
        $('#searchboxText').attr("placeholder", "Search the website and research guides");
      } else {
        $('#searchboxText').attr("placeholder", "Find books, articles & more...");
      }
      
      return true;
    }
    
    function catchEnter(e) {
      if (e.keyCode == 13) {
        return true;
      } else {
        return false;
      }
    }
  </script>
