      <script type="text/javascript">
       function loadGuide(guideSelector) {
    var thisGuide = guideSelector.options[guideSelector.selectedIndex];
    if (thisGuide.value != '#') {
      // window.alert(thisGuide.innerHTML);
      // _gaq.push(['_trackEvent', 'guides', thisGuide.innerHTML]);
      window.open(thisGuide.value,'_top');
    }
  }
  
  document.addEventListener("DOMContentLoaded", function(event) { 
    // $("#guideList option").not(".opt-always").not(".opt-recommended").hide();
    $('#guideLimits input').click(function() {
      $("#guideList option").show();
      if ($("#chkRecommended").prop('checked')) {
        $("#guideList option").not(".opt-always").not(".opt-recommended").hide();
      }
      if ($("#chkCitation").prop('checked')) {
        $("#guideList option").not(".opt-always").not(".opt-citation").hide();
      }
      switch ( $('input[name=optSchool]:checked').val() ) {
        case 'GWSPH':
          $("#guideList option").not(".opt-always").not(".opt-GWSPH").hide(); break;
        case 'SMHS':
          $("#guideList option").not(".opt-always").not(".opt-SMHS").hide(); break;
        case 'SON':
          $("#guideList option").not(".opt-always").not(".opt-SON").hide(); break;
      }
    });
    
  });
      </script>
