  <script type="text/javascript">
   $(function () {
            LoadMoreNews();

            // Homepage Rotator
            $('.home-rotator .slides').slick({
                dots: true,
                fade: true,
                adaptiveHeight: true,
                infinite: true
            });

            //Pause video when opening full video
            //$('.video-overlay-buttons a').click(function () {
            //    $('#divVideo video').get(0).pause();
            //})

        });

        function LoadMoreNews() {
            var pg = parseInt($('#hdnPG').val());
            pg++;

            $.ajax({
                type: 'POST',
                async: 'false',
                contentType: "application/json; charset=utf-8",
                url: '/admin/resources/TAMF.asmx/LoadHomepageNews',
                data: "{ pg: '" + pg + "' }",
                dataType: 'text',
                success: function (data) {
                    $('#divNews').html(data);
                    $('#hdnPG').val(pg);
                },
                error: function (xhr, status, error) {
                    var err = eval("(" + xhr.responseText + ")");
                    console.log(err.Message);
                }
            });
        }
  </script>
