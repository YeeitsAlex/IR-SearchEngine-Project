<script>
 function swapMenu() {
var x = document.getElementById( "mini" );
if ( x.className.indexOf( "w3-show" ) == -1 ) {
x.className += " w3-show";
} else {
x.className = x.className.replace( " w3-show", "" );
}
}
function toggleSearch() {
var x = document.getElementById( "search-key" );
if ( x.className.indexOf( "w3-show" ) == -1 ) {
x.className += " w3-show";
} else {
x.className = x.className.replace( " w3-show", "" );
}
}
</script>
