      <article class="post-10 page type-page status-publish hentry" id="post-10">
       <header class="entry-header">
        <h1 class="page-title">
         Home
        </h1>
       </header>
       <!-- .entry-header -->
       <div class="post-content">
        <div class="entry-content clearfix">
         <p>
         </p>
         <table class="table10" style="margin: 0%; height: 483px;" width="827">
          <tbody>
           <tr>
            <td style="padding: 5% 0% 0% 0%;">
             <img alt="" class="alignnone size-full wp-image-98" height="163" src="http://leic.tennessee.edu/wp-content/uploads/2017/05/LEIC_trans_long-01.png" width="1415"/>
            </td>
           </tr>
