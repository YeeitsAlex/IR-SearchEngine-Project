    <script type="text/javascript">
     jQuery.noConflict();
				jQuery(document).ready(function($) {

					if(!$('#menu-toggle').is(':visible')) {
						$('.match-height-desktop').matchHeight();
					}					

					var width = $(window).width(), height = $(window).height();
					addHelix();

					$(window).on('resize', function() {
						if($(window).width() != width || $(window).height() != height) {
							addHelix();
						}
					});

					function addHelix() {
						var wrapper = $('#homepage-helix-wrapper');
						var width = $(wrapper).width();
						var currentCount = $(wrapper).children().length;
						var helixCount = div(width, 60);

						if(helixCount > 0 && helixCount != currentCount) {
							$(wrapper).empty();
							for (var i = 0; i < helixCount; i++) {
								$(wrapper).append('<div class="helix-bg"></div>');
							}
						}
					}

					function div(val, by) {
    					return (val - val % by) / by;
					}

					var screenWidth = jQuery(window).width();
					if(screenWidth <= 570) {
						var greenBlockHeight = jQuery(".front-hero-section > .container:first-child").outerHeight() + 300;
						jQuery(".front-hero-section").css("height", greenBlockHeight);
					}
				});
    </script>
   </div>
