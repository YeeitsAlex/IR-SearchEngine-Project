        .b-top-nav__dropdomn.nav__dropdomn_size3 .b-top-nav__2level_wrap {
            width: 33%;
        }
        .b-top-nav__1level > a:hover {
            color: #ffffff;
        }
        .b-top-nav__1level > a:focus {
            color: #ffffff;
        }
    }
    
    @media screen and (max-width: 1024px) {
        /* New Change 05/06 */
        .b-header-ico-group .fa {
            display: inherit;
        }
        .b-header-r--icon .b-header-ico-group {
            right: 45px;
        }
        header.animated .b-ico-dropdown {
            border-color: transparent transparent #D8A801 transparent;
            top: inherit;
        }
        /* End New Change 05/06 */
        /* End New Change 05/06 */
    }
    
    @media screen and (min-width: 1100px) {
        .myLeoLink {
            top: 5px;
        }
    }
    
    
    /* This is to add ada compliant link and laebl discriptions.  Just add as span class aroud the words you don't want to appear. */
    
    .adahide {
        height: 1px;
        width: 1px;
        position: absolute;
        overflow: hidden;
        top: -10px;
    }
    label span.adahide {
        height: 1px;
        width: 1px;
        position: absolute;
        overflow: hidden;
        top: -10px;
    }
    
    
    /* Main Drop Down Link */
    
    .b-top-nav__link > a {
        display: block;
