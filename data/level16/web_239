  <script type="text/javascript">
   submitSecondSearch = function () {
				var currentSite = window.location.protocol + "//" + window.location.hostname;
				var searchUrl = "/pages/results.aspx?k=";
				var query = encodeURIComponent($("#second-search-box").val());
				window.location.href = currentSite + searchUrl + query;
			}
			$(document).ready(function () {
				$("#second-search-button").click(function () {
					submitSecondSearch();
				});
				$("#second-search-box").keypress(function (e) {
					if (e.which == 13) {
						submitSecondSearch();
					}
				});
			});
  </script>
