  <script>
   var section = "home";
var activeLink = "";
var serviceDownWarning = "";

section = (section != "") ? section : "home";
$(document).ready(function () {
        $('.thetip').tooltipster();
        if (activeLink != "") {
                // $("a:contains('" + activeLink + "')").css("color","#000000");
                $("li:contains('" + activeLink + "')").addClass("active");
        };
        $('#' + section).addClass("active");
        $("#goSearchButton").click(function () {
                $("#gsc-i-id1").val($("#searchTextBox").val());
                $(".gsc-search-button").click();
                $("#gSearch").css("display", "");
                $(".gsc-search-box").css("display", "none");
        });
        $("#searchTextBox").keyup(function (event) {
                if (event.keyCode == 13) {
                        $("#goSearchButton").click();
                }
        });
        $(".social").click(function (event) {
                event.preventDefault();
                event.stopPropagation();
                var dUrl = $(this).attr("data-url");
                window.open(dUrl, '_blank');
        })
        $(window).scroll(function () {
                if ($(window).scrollTop() > 100) {
                        $("#toTop").parent().css("display", "block");
                } else {
                        $("#toTop").parent().css("display", "none");
                }
        });
        $(function () {
                var $affixElement = $('ul[data-spy="affix"]');
                $affixElement.width($affixElement.parent().width());
        });
        var messagePages = new Array();
        // messagePages.push("Parking Home");
        messagePages.push("Permit Purchase");
        messagePages.push("Permit Registration");
        messagePages.push("Pay Citation");
        messagePages.push("Appeal Citation");
        // messagePages.push("Bicycle Registration");
        messagePages.push("U-Pass");
        var strDownMessage = "Website services will be down due to the Holiday break. Services will resume on Janurary 3rd, 2017."
        if ((serviceDownWarning == true) && (messagePages.indexOf(activeLink) >= 0)) {
                BootstrapDialog.alert({
                        type: BootstrapDialog.TYPE_WARNING,
                        title: "Website Services Announcement:",
                        message: strDownMessage
                });
        }
});
  </script>
