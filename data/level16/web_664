    <script type="text/javascript">
     function mobileNav() {
				var x = document.getElementById("mobile_links");
				if (x.style.display === "block") {
					x.style.display = "none";
					var mobile_submenu = document.getElementsByClassName("mobile-submenu");
					for (var i = 0; i < mobile_submenu.length; i++) {
						mobile_submenu[i].style.display = 'none';
					}
				} else {
					x.style.display = "block";
				}
			}

			var prev_mob_id = '';
			function openMobileSubNav(id){
				if (prev_mob_id != '' && prev_mob_id != id){
					document.getElementById(prev_mob_id).style.display = 'none';
				}
				else if (prev_mob_id == id.replace(/\s/g, "")){
					document.getElementById(id).style.display = 'none';
					prev_mob_id = '';
					return;
				}

				prev_mob_id = id;
				var x = document.getElementById(id);
				if (x.style.display === "none") {
					x.style.display = "block";
				} else {
					x.style.display = "none";
				}
			}
    </script>
