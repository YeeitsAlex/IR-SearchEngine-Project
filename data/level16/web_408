    <div id="mainPageContent">
     <h1 class="page__title title" id="page-title">
      Welcome
     </h1>
     <div id="sideNavigation">
      <section class="region region-sidebar">
       <div class="block block-menu-block first last odd" id="block-menu-block-3" role="navigation">
        <div class="menu-block-wrapper menu-block-3 menu-name-menu-secondary-navigation---prot parent-mlid-0 menu-level-1">
