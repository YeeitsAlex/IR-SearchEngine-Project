    function stickyHeader(n,t,i){headerHeight=t+i;var r=0;$(window).scrollTop()>=headerHeight+18?n==0?(r=i<40?0:-12,smallSticky(headerHeight,t,i,r)):n==1&&(r=-108,bigSticky(i,t,r)):$(window).scrollTop()<=0&&(r=-108,noSticky(i,r))}function smallSticky(n,t,i,r){$(".facetctrl-sticky").hasClass("active")&&$(".facetctrl-sticky").removeClass("facetctrl-sticky-big").addClass("facetctrl-sticky-small");$("#utility-menu").removeClass("utility-menu-offset");$("#left-social-sticky").hasClass("sticky-js")&&($("#left-social-sticky").removeClass("sticky-js-big sticky-js-small"),$("#left-social-sticky").removeClass("sticky-js-big-smooth").addClass("sticky-js-small-smooth"));$(".site-header").addClass("stickyFixed");$(".site-header").css("margin-top",-t);$(".site-header").css("top",0);$(".site-header .container .site-home .mobile-hidden a img").css({transform:"translateY(46px)",width:"30%"});i<40?($(".site-header").css("transform","translate3d(0,0,0) translateY(0px )"),$(".site-header").css("-webkit-transform","translate3d(0,0,0) translateY(0px )"),$(".page-header:first").css("height",n+18),$(".site-header .container .site-home .mobile-hidden a img").css({transform:"",width:""})):($(".site-header").css("transform","translate3d(0,0,0) translateY("+(t-85)+"px )"),$(".site-header").css("-webkit-transform","translate3d(0,0,0) translateY("+(t-85)+"px )"),$(".page-header:first").css("height",n+18),$(".site-header").css("transition","transform 200ms ease-out"),$(".site-header .eta a").css("display","none"));$(".universal-banner").addClass("stickyFixed");$("body").hasClass("noSticky")?$(".universal-banner").css("margin-top",-(i+20)):($(".universal-banner").css("transform","translate3d(0,0,0) translateY("+-(i+20)+"px )"),$(".universal-banner").css("-webkit-transform","translate3d(0,0,0) translateY("+-(i+20)+"px )"),$(".universal-banner").css("transition","transform 200ms ease-out"));$("#utility-menu").parent().css("marginTop",r);$("body").addClass("smallSticky").removeClass("noSticky bigSticky")}function bigSticky(n,t,i){$(".facetctrl-sticky").hasClass("active")&&$(".facetctrl-sticky").removeClass("facetctrl-sticky-small").addClass("facetctrl-sticky-big");$("#utility-menu").addClass("utility-menu-offset");$("#left-social-sticky").hasClass("sticky-js")&&($("#left-social-sticky").removeClass("sticky-js-big sticky-js-small"),$("#left-social-sticky").removeClass("sticky-js-small-smooth").addClass("sticky-js-big-smooth"));$(".site-header").addClass("stickyFixed");$(".site-header").css("margin-top",-t);$(".site-header").css("transform","translate3d(0,0,0) translateY("+(t+n-1)+"px )");$(".site-header").css("-webkit-transform","translate3d(0,0,0) translateY("+(t+n-1)+"px )");$(".site-header").css("transition","transform 300ms ease-out");$(".site-header .container .site-home .mobile-hidden a img").css({transform:"",width:""});n<40?$(".site-header .eta a").css("display","none"):($(".site-header .eta a").css("display",""),$(".page-header:first").css("height",headerHeight+18));$(".universal-banner").addClass("stickyFixed");$(".universal-banner").css("margin-top",-n);$(".universal-banner").css("transform","translate3d(0,0,0) translateY("+n+"px )");$(".universal-banner").css("-webkit-transform","translate3d(0,0,0) translateY("+n+"px )");$(".universal-banner").css("transition","transform 300ms ease-out");$("#utility-menu").parent().css("marginTop",i);$("body").addClass("bigSticky").removeClass("smallSticky noSticky")}function noSticky(n,t){$("#utility-menu").addClass("utility-menu-offset");$(".facetctrl-sticky").removeClass("facetctrl-sticky-big facetctrl-sticky-small");$(".universal-banner").removeClass("stickyFixed");$(".universal-banner").css("margin-top",0);$(".universal-banner").css("transform","");$(".universal-banner").css("transition","");$(".site-header").removeClass("stickyFixed");$(".site-header").css("margin-top",0);$(".site-header").css("transform","");$(".site-header").css("-webkit-transform","");$(".site-header").css("transition","");$(".site-header .container .site-home .mobile-hidden a img").css({transform:"",width:""});$(".site-header .eta a").css("display","");$("#utility-menu").parent().css("marginTop",t);$(".page-header:first").css({height:18});$("#left-social-sticky").removeClass("big-header-margin small-header-margin");$("body").addClass("noSticky").removeClass("smallSticky bigSticky")}jQuery(function(n){function t(){var t=n(".universal-banner-search-toggle"),i=n(".universal-banner-search-input .universal-site-search-query").val();if(t.hasClass("toggled")&&n.trim(i)){n(".universal-banner-search-input .universal-site-search-button").click();return}t.toggleClass("toggled");t.hasClass("toggled")&&window.setTimeout(function(){n(".universal-site-search-query").focus()},300);n(".universal-banner-search-input").toggleClass("open")}if(!window.headerinit){window.headerinit=1;n(document).on("click",".universal-banner-search-toggle",function(){return t(this),!1});n(".universal-banner-search-toggle").on("click",function(){return t(this),!1});n(".slider-container").on("close",function(){n(".universal-banner-hamburger").removeClass("open");n(".universal-banner-menu-toggle").removeClass("toggled");n("body").removeClass("slider-is-open")});n(document).on("click",".universal-banner-menu-toggle",function(){return n(this).hasClass("toggled")?(n(".slider-close").eq(0).click(),n(".universal-banner-hamburger").removeClass("open"),n(".universal-banner-menu-toggle").removeClass("toggled"),n("body").removeClass("slider-is-open")):(n(".slider-open").eq(0).click(),n("body").addClass("slider-is-open"),n(".universal-banner-hamburger").addClass("open"),n(".universal-banner-menu-toggle").addClass("toggled")),!1});n(".skipto").on("click",function(){console.log("skip to main content");n(n(this).attr("href")).get(0).focus()})}});$("#utility-menu").addClass("utility-menu-offset");$(".page-header")[0]||$(".site-header").after('<div class="page-header inherit-bg" style="height: 18px"><\/div>');$(".facetctrl-page-list").parent().css("clear","