.log(i.hasClass("open")),t.close()),event.preventDefault(),!1)});n("a",t).each(function(){var i=n(this);i.on("focus",function(){clearTimeout(t.data("timer2"))});i.on("blur",function(){clearTimeout(t.data("timer2"));t.data("timer2",setTimeout(function(){t.close()},100))})})})});jQuery(function(n){n(".hero-story .fade-in.in-viewport-trigger").one("framework.responsive-in-viewport",function(){n(this).addClass("animate")})});jQuery(function(){$(".ua-touch").length||$(".hero-header").hover(function(){$(this).addClass("hover");$("#repaint").remove();$("html").append('<span style="opacity:0" id="repaint">.<\/span>')},function(){$(this).removeClass("hover");$("#repaint").remove()})});jQuery(function(n){function t(){var t={};n(".equal-heights-item").each(function(){var i=n(this).offset().top,r;n(this).data("equal-heights-group",i);n(this).css("min-height","auto");r=n(this).height();t[i]=t[i]||0;r>t[i]&&(t[i]=r)});n(".equal-heights-item").each(function(){var i=n(this).data("equal-heights-group"),r=t[i];n(this).css("min-height",r)})}n(window).on("load",function(){t()});n(document).on("framework.resize",function(){t()})});jQuery(function(n){function t(){n(".one-line").each(function(){var u=n(this).height(),t,i,r;n(this).css("white-space","nowrap");t=n(this).height();n(this).css("white-space","normal");console.log(u+" "+t);i=n(this).data("original");r=n(this).data("abbreviated");u>t?n(this).html(n(this).html().replace(i,r)):n(this).html(n(this).html().replace(r,i))})}n(window).load(function(){t()})});jQuery(function(n){n(".in-viewport-trigger").on("framework.in-viewport",function(){n(this).addClass("in-viewport-triggered")})});$(".email-modal-form").each(function(){var t=$(this).data("trigger"),i=$(this).data("title"),n=$(this);$(t).click(function(t){t.preventDefault();var r=new framework.Dialog({body:n.html(),title:i,width:"495px",height:"568px",backgroundColor:"white",submit:function(){$(".btn-submit",n).click()}});r.open()})});$("input[type='radio'], input[type='checkbox']").