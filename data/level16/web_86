                     <div class="block-inner clearfix">
                      <h2 class="block-title" ng-non-bindable="">
                       Renew a Book
                      </h2>
                      <div class="block-content content" ng-non-bindable="">
                       <div class="boxes-box" id="boxes-box-1502468822">
                        <div class="boxes-box-content">
                         <p>
                          Renew a book, check due dates and more.
                         </p>
                         <p>
                          <a class="os-button" href="http://hollis.harvard.edu" title="">
                           Renew a Book
                          </a>
                         </p>
                        </div>
                       </div>
                      </div>
                     </div>
                    </section>
                   </div>
                  </div>
