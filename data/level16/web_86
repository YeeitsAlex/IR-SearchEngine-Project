                <div class="boxes-box-content">
                 <div class="file file-html file-html-embed" id="file-208296">
                  <h2 class="element-invisible">
                   <a href="">
                    code to display help text and set action for home page search
                   </a>
                  </h2>
                  <div class="content">
                   <div class="file-info">
                   </div>
                   <div class="field field-name-field-html-code field-type-text-long field-label-hidden view-mode-full">
                    <div class="field-items">
                     <div class="field-item even">
                      <script type="text/javascript">
                       function quickSearchPrimo() {
		document.getElementById("book-search").query.value = "any,contains," + document.getElementById("book-search").primoQueryTemp.value.replace(/[,]/g," ");
	}

	function downloadJSAtOnload() {
		jQuery("form.os-libsearch input[type=radio]").click(function() {
			if (window[jQuery(this).attr("name")] != null) {
				var values = window[jQuery(this).attr("name")][jQuery(this).val()];
				jQuery("form.os-libsearch:visible").attr("action", values.action);
				jQuery("form.os-libsearch:visible").attr("method", values.method);
				jQuery("div.os-lib-detail:visible").html(values.detail);
				jQuery("input.os-search-string:visible").attr("name", values.query);
				jQuery("form.os-libsearch:visible input.os-addl-param").remove();
				if (values.params != null) {
					for (var i = 0; i < values.params.length; i++) {
						jQuery("form.os-libsearch:visible").append('<input type="hidden" class="os-addl-param" name="' + values.params[i][0] + '" value="' + values.params[i][1] + '">');
					}
				}
				if (values.submit != null) {
					jQuery("form.os-libsearch:visible").attr("onsubmit", "return " + values.submit);
				}
				else {
					jQuery("form.os-libsearch:visible").attr("onsubmit", "");
				}
			}
		});
		var cx = '009416300272322837553:weusj9mtfju';
		var gcse = document.createElement('script'); gcse.type = 'text/javascript'; gcse.async = true;
		gcse.src = 'https://www.google.com/cse/cse.js?cx=' + cx;
		var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(gcse, s);
           
                // New home page search tabs
		jQuery("a#ui-id-1").click(function() {
			_gaq.push(['_trackEvent', 'tab', 'Hmpg-Hollis+']);
		});
		jQuery("a#ui-id-2").click(function() {
			_gaq.push(['_trackEvent', 'tab', 'Hmpg-OtherSources']);
		});
		jQuery("#boxes-box-1404403227 button").click(function() {
			_gaq.push(['_trackEvent', 'External Search', 'Hmpg-OtherSources-Searchbtn']);
		});
		jQuery("#boxes-box-1400857056 button").click(function() {
			_gaq.push(['_trackEvent', 'External Search', 'Hmpg-Hollis-Searchbtn']);
		});
		jQuery("#block-boxes-1502468726 a.os-button").click(function() {
			_gaq.push(['_trackEvent', 'Hmpg-Widget', 'Hmpg-SpecialCollections']);
		});
	}
	if (window.addEventListener) window.addEventListener("load", downloadJSAtOnload, false);
	else if (window.attachEvent) window.attachEvent("onload", downloadJSAtOnload);
	else window.onload = downloadJSAtOnload;
                      </script>
                     </div>
                    </div>
                   </div>
                  </div>
                 </div>
                </div>
               </div>
              </div>
             </div>
            </div>
           </div>
          </div>
