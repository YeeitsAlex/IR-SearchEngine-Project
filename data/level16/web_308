                 <div class="media">
                  <p class="col-img">
                   <img alt="GW; Together, We Raise High!" class="enlivenem" src="/sites/g/files/zaxdzs2226/f/image/GW_RaiseHigh_Animate2.svg" style="height:150px; width:150px"/>
                  </p>
                 </div>
                 <header class="col-h">
                  <h3 class="promo-title">
                  </h3>
                 </header>
                 <section class="col-copy">
                  <p>
                  </p>
                 </section>
