                 <div id="vid_5d83b82e-e80b-47a3-ba41-096b2e763f41" style="display:none;" unselectable="on">
                 </div>
                </div>
               </div>
               <div class="col span_6_of_12">
                <h2 class="ms-rteElement-H2withB2">
                 News
                 <a href="/News">
                  <span style="padding-left:2em;font-size:14px;font-weight:normal;">
                   (view all news)
                  </span>
                 </a>
                </h2>
                <div class="ms-rtestate-read ms-rte-wpbox">
                 <div class="ms-rtestate-notify ms-rtestate-read 96af0573-5f39-4dd6-997e-804a25c19e5a" id="div_96af0573-5f39-4dd6-997e-804a25c19e5a" unselectable="on">
                  <table cellpadding="0" cellspacing="0" style="width:100%">
                   <tr>
                    <td class="s4-wpcell-plain " id="MSOZoneCell_WebPartWPQ2" valign="top">
                     <table border="0" cellpadding="0" cellspacing="0" class="s4-wpTopTable " width="100%">
                      <tr>
                       <td valign="top">
                        <div allowdelete="false" allowremove="false" class="ms-WPBody noindex " haspers="false" id="WebPartWPQ2" style="" webpartid="e887d3c4-7b60-42d0-8da1-47900714eb45" webpartid2="96af0573-5f39-4dd6-997e-804a25c19e5a" width="100%">
                         <script src="../../../../../_scripts/_spscripts/jquery.SPServices-2013.01.min.js" type="text/javascript">
                         </script>
                         <script type="text/javascript">
                          var monthNames = ["","Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"];
var newsCats = ["News","UTCVM in the News","News You Can Use","Dog Bite Prevention","SCAVMA","UTCVM Green","Research","HABIT"];
var theDate = new Date();
var lastDate = new Date();
//get last 30 days of news
lastDate.setDate(theDate.getDate()-180);
var theMonth = "0"+(lastDate.getMonth()+1);
theMonth = theMonth.substring(theMonth.length -2);
var theDate = "0"+(lastDate.getDate());
theDate = theDate.substring(theDate.length -2);
lastDate = lastDate.getFullYear()+"-"+theMonth+"-"+theDate+"T00:00:01Z";
// need this date format:  2015-02-05T15:37:44Z

$(document).ready(function() {
//get featured item
  $().SPServices({
    operation: "GetListItems",
    async: false,
	CAMLRowLimit: 1,
    CAMLQuery: "<Query>" +
		"<Where>" +
			"<And>" +
	            "<Eq>" +
					"<FieldRef Name='active'/><Value Type='Integer'>1</Value>" + 
	            "</Eq>" +
				"<Eq>"+
					"<FieldRef Name='FeaturedArticle'/><Value Type='Integer'>1</Value>" + 
				"</Eq>" +
	         "</And>" +
	        "</Where>" +
		    "<OrderBy>" +
		        "<FieldRef Name='dateCreated' Ascending='False'/>" +
		    "</OrderBy>" +
		"</Query>",
	webURL: topMainSite+"News/",
    listName: "NewsItems",
    CAMLViewFields: "<ViewFields><FieldRef Name='ID' /><FieldRef Name='Title' /><FieldRef Name='shortNews' /><FieldRef Name='FeaturedArticle'/><FieldRef Name='longNews' /><FieldRef Name='Title' /><FieldRef Name='dateCreated' /></ViewFields>",
    completefunc: function (xData, Status) {
						/*Error testing code
							alert(xData.responseText);
							if (Status == "error") {
							alert ("Unable to communicate with Sharepoint Server!");
							}else{
							alert ("Status = "+Status);
							}            
							*/
		var liHtml = '';
		$(xData.responseXML).SPFilterNode("z:row").each(function() { 
      	//split date
      	var tmpDate = $(this).attr("ows_dateCreated").split("-");
      	var tmpMonth = parseInt(tmpDate[1]);
      	var tmpDay = parseInt(tmpDate[2]);
      	var tmpYear = parseInt(tmpDate[0]);
      	var newsDate = "<span style='font-size:140%'>"+monthNames[tmpMonth]+"</span>" + " "+ tmpDay +", "+tmpYear;
        liHtml += "<li><div class='newsTitle'><a href='"+topMainSite+"/News/Pages/NewsArticle.aspx?news"+ $(this).attr("ows_ID") +"'>"+ $(this).attr("ows_Title")+"</a></div>" +
					"<div class='newsBrief'>"+ $(this).attr("ows_shortNews") +"</div>" +
					//"<div class='readMore'><a href='"+topMainSite+"News/Pages/current_news.aspx#news"+ $(this).attr("ows_ID") +"'>...read more...</a></div></li>";
					"<div class='readMore'><a href='a href='"+topMainSite+"News/Pages/NewsArticle.aspx?news"+ $(this).attr("ows_ID") +"'>...read more...</a></div></li>";        
      });
    $("#newsList").append(liHtml);
    }
  });


//get top news
  $().SPServices({
    operation: "GetListItems",
    async: false,
	CAMLRowLimit: 3,
    CAMLQuery: "<Query>" +
		"<Where>" +
			"<And>" +
				"<Eq>"+
					"<FieldRef Name='FeaturedArticle'/><Value Type='Integer'>0</Value>" + 
				"</Eq>" +
			"<And>"+
	            "<Eq>" +
					"<FieldRef Name='active'/><Value Type='Integer'>1</Value>" + 
	            "</Eq>" +
	            //add endDate news checker!!
	            "<Geq>" +
				"<FieldRef Name='startDate' />" +
				  "<Value IncludeTimeValue='TRUE' Type='DateTime'>"+ lastDate +"</Value>" + 
	            "</Geq>" +
	         "</And>" +
	         "</And>" +
	        "</Where>" +
		    "<OrderBy>" +
		        //"<FieldRef Name='TopFeature' Ascending='False'/>" +
		        "<FieldRef Name='dateCreated' Ascending='False'/>" +
		    "</OrderBy>" +
		"</Query>",
	webURL: topMainSite+"News/",
    listName: "NewsItems",
    CAMLViewFields: "<ViewFields><FieldRef Name='ID' /><FieldRef Name='Title' /><FieldRef Name='shortNews' /><FieldRef Name='longNews' /><FieldRef Name='Title' /><FieldRef Name='dateCreated' /></ViewFields>",
    completefunc: function (xData, Status) {
						/*Error testing code
							alert(xData.responseText);
							if (Status == "error") {
							alert ("Unable to communicate with Sharepoint Server!");
							}else{
							alert ("Status = "+Status);
							}            
							*/
		$(xData.responseXML).SPFilterNode("z:row").each(function() { 
      	//split date
      	var tmpDate = $(this).attr("ows_dateCreated").split("-");
      	var tmpMonth = parseInt(tmpDate[1]);
      	var tmpDay = parseInt(tmpDate[2]);
      	var tmpYear = parseInt(tmpDate[0]);
      	var newsDate = "<span style='font-size:140%'>"+monthNames[tmpMonth]+"</span>" + " "+ tmpDay +", "+tmpYear;
        var liHtml = "<li><div class='newsTitle'><a href='"+topMainSite+"News/Pages/NewsArticle.aspx?news"+ $(this).attr("ows_ID") +"'>"+ $(this).attr("ows_Title")+"</a></div>" +
					"<div class='newsBrief'>"+ $(this).attr("ows_shortNews") +"</div>" +
//					"<div class='readMore'><a href=topMainSite+'News/Pages/current_news.aspx#news"+ $(this).attr("ows_ID") +"'>...read more...</a></div></li>";
					"<div class='readMore'><a href=topMainSite+'News/Pages/NewsArticle.aspx?news"+ $(this).attr("ows_ID") +"'>...read more...</a></div></li>";        
        $("#newsList").append(liHtml);
      });
    }
  });
});
                         </script>
                         <ul class="shortNewsList" id="newsList">
                         </ul>
                        </div>
                       </td>
                      </tr>
                     </table>
                    </td>
                   </tr>
