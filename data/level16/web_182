          <section class="row fw-row dynamic-header-change" data-header-style="">
           <div class="spb-row " data-col-spacing="0" data-col-v-pos="default" data-content-stretch="false" data-image-movement="fixed" data-row-height="content-height" data-row-type="color" data-wrap="full-width-contained" style="margin-top: 0px;margin-bottom: 0px;padding-top: 10px;padding-bottom: 10px;">
            <div class="spb_content_element clearfix" style="">
             <section class="row ">
