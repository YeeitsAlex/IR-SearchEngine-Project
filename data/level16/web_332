                    <section class="block block-boxes block-boxes-os_boxes_manual_list" delta="1430144828" id="block-boxes-1430144828" module="boxes">
                     <div class="block-inner clearfix">
                      <h2 class="block-title" ng-non-bindable="">
                       Featured Event
                      </h2>
                      <div class="block-content content" ng-non-bindable="">
                       <div class="boxes-box" id="boxes-box-1430144828">
                        <div class="boxes-box-content">
                         <div class="item-list">
                          <ul class="featured-links title-only">
                           <li>
                            <article class="node node-event article clearfix" id="node-1153348" role="article">
                             <div class="event-content">
