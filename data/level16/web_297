-controls-direction').attr('aria-label','carousel buttons').attr('aria-controls', 'soliloquy-container-842');soliloquy_container_842.find('.soliloquy-controls-direction a.soliloquy-prev').attr('aria-label','previous');soliloquy_container_842.find('.soliloquy-controls-direction a.soliloquy-next').attr('aria-label','next');$(window).trigger('resize');soliloquy_container_842.parent().attr('data-soliloquy-loaded', 1);},onSlideBefore: function(element, oldIndex, newIndex, oldelm ){soliloquy_container_842.find('.soliloquy-active-slide').removeClass('soliloquy-active-slide').attr('aria-hidden','true');$(element).addClass('soliloquy-active-slide').attr('aria-hidden','false');},onSlideAfter: function(element, oldIndex, newIndex){}});});
 </script>
 <script>
  var superSearch = new Bloodhound({
//  datumTokenizer: Bloodhound.tokenizers.obj.whitespace('post_title'),
  datumTokenizer: Bloodhound.tokenizers.whitespace,
  queryTokenizer: Bloodhound.tokenizers.whitespace,
  remote: {
    url: 'https://sds.utk.edu/wp-json/ja/v2/search?term=%QUERY',
    wildcard: '%QUERY'
  }
});




$('#remote .typeahead').typeahead({
  hint: false,
  highlight: true,
  minLength: 2,
  async: true,
  autoselect: true
},
{
  name: 'search-content',
//  limit: 5,
  display: 'post_title',
  source: superSearch,
  templates:{
    header: '<h3 class="typeaheadTitle">Suggestions</h3>',
    notFound: '<h3 class="typeaheadTitle">No results on this site</h3><input type="submit" name="btnF" class="btn-supersearch searchListItem tt-suggestion tt-selectable"  value="Search the site">',
    pending: '<h3 class="typeaheadTitle">Searching...</h3>',
    footer: '<input type="submit" name="btnF" class="btn-supersearch searchListItem tt-suggestion tt-selectable"  value="See all results">',
    suggestion: Handlebars.compile('<div class="searchListItem"><a href="https://sds.utk.edu/?p={{ID}}" class="searchLink" aria-label="{{post_title}}"><span class="suggestion-head">{{post_title}}</span></a></div>')
  }
}).on('typeahead:selected', function(event, selection) {
  url = "https://sds.utk.edu/" + "?p=" + (selection.ID);
  window.location.href = url;
});
 </script>
</link>

