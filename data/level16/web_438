                         <div class="field-items">
                          <div class="field-item even">
                           <img alt="" class="image-style-4-col-header" height="140" src="https://privacy.stanford.edu/sites/g/files/sbiybj10186/f/styles/4-col-header/public/services.jpg?itok=2gi8pIs7" typeof="foaf:Image" width="370"/>
