        if ($(window).scrollTop() > 0) {
            $('header').addClass('hide-sitetitle');
            $('#rave-alert').addClass('hideraveonscroll');
        }
        else{
            $('header').removeClass('hide-sitetitle');
            $('#rave-alert').removeClass('hideraveonscroll');
        }
    });
});
