  <script>
   if (window.NodeList && !NodeList.prototype.forEach) {
			NodeList.prototype.forEach = function (callback, thisArg) {
				thisArg = thisArg || window;
				for (var i = 0; i < this.length; i++) {
					callback.call(thisArg, this[i], i, this);
				}
			};
		}

		var hbsdiSearchFields = document.querySelectorAll( '.di-c-search__input' );

		hbsdiSearchFields.forEach( initAutoComplete );

		function initAutoComplete( searchField, index ) {
			var hbsdiSearchClear = searchField.nextElementSibling;

			searchField.setAttribute( 'id', 'searchform-' + index );
			hbsdiSearchClear.setAttribute( 'aria-controls', 'searchform-' + index );

			new autoComplete({
				selector: searchField,
				minChars: 1,
				delay: 0,
				source: function( term, suggest ){
					term = term.toLowerCase();

					var choices = hbsdi_topics;
					var matches = [];

					for ( i=0; i< hbsdi_topics.length; i++ )
						if ( ~choices[i].toLowerCase().indexOf( term ) ) matches.push( choices[i] );

					suggest( matches );
				},
				onSelect: function( event, term, field ) {
					searchField.parentElement.parentElement.parentElement.submit();
				}
			});

			if ( searchField.value === '' ) {
				hbsdiSearchClear.classList.add( 'js-is-hidden' );
			} else {
				hbsdiSearchClear.classList.remove( 'js-is-hidden' );
			}

			searchField.addEventListener( 'keyup', function() {
				if ( this.value === '' ) {
					hbsdiSearchClear.classList.add( 'js-is-hidden' );
				} else {
					hbsdiSearchClear.classList.remove( 'js-is-hidden' );
				}
			});

			hbsdiSearchClear.addEventListener( 'click', function() {
				searchField.value = '';
				hbsdiSearchClear.classList.add( 'js-is-hidden' );
			});
		}

		function hbsdiFilterResultsOnChange( selectField ) {
			selectField.parentElement.parentElement.submit();
		}
  </script>
 </body>
</html>

