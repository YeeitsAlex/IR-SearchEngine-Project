   <script>
    jQuery(function ($, undefined) {
  // Google Analytics: https://developers.google.com/analytics/devguides/collection/analyticsjs/creating-trackers#working_with_multiple_trackers
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
  
  ga('create', 'UA-43424144-2', 'auto');
  ga('create', 'UA-1617721-2', 'auto', 'tamu'); // TAMU's main tracking code
  ga('send', 'pageview');
  ga('tamu.send', 'pageview');
  
  /**
  * Function that tracks a click on an outbound link in Analytics
  * This function takes a valid URL string as an argument, and uses that URL string
  * as the event label. Setting the transport method to 'beacon' lets the hit be sent
  * using 'navigator.sendBeacon' in browser that support it.
  */
  var trackOutboundLink = function (url) {
    ga('send', 'event', 'outbound', 'click', url, {
      'transport': 'beacon',
      'hitCallback': function () {
        document.location = url
      }
    });
  };
  
  $('#MenuV .Vertical.Content > ul').BindMENU({
    MenuStyle: 'DefaultMenu',
    Orientation: 'Vertical',
    Event: 'hover',
    Speed: 'fast'
  });
  $('#MenuV .Vertical.Home > ul').BindMENU({
    MenuStyle: 'DefaultMenu',
    Orientation: 'Vertical',
    Event: 'hover',
    Speed: 'fast'
  });
  
  /* Remove unfilled table rows */
  $.each($('.table tr td:first-child'), function (i, v) {
    if ($(v).html() === '') $(v).parent().remove();
  });
  
  /* This function handles the Bootstrap bug of collapsed accordion panels not getting
   * the "collapsed" class added to them after being opened then closed again.
   */
  $('.accordion').on('hide', function (e) {
    $(e.target).parent().find('.accordion-toggle').addClass('collapsed');
  });
  
  //Carousel
  $('.carousel').carousel({
    interval: false //5000
  });
  
  //Tooltips
  $('*[data-toggle=tooltip]').tooltip();
  
  //Popovers
  $('*[data-toggle=popover]').popover({
    trigger: 'hover'
  });
  
  // Open modal with fixed Body
  $('.modal').on('shown', function () {
    $('body').css({ overflow: 'hidden' });
    $('body').addClass('modal-open');
  })
  .on('hidden', function () {
    $('body').css({ overflow: '' });
    $('body').removeClass('modal-open');
  });
});
   </script>
