    <div class="full-width" id="primary">
     <div id="content">
      <article class="post-892 page type-page status-publish hentry" id="post-892" role="article">
       <div class="entry-content">
        <div class="leftcolumn">
         <img alt="abroad" class="alignnone wp-image-900 size-full" height="181" src="https://cie.utk.edu/files/2013/01/abroad.png" width="281"/>
        </div>
       </div>
