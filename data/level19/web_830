  <script type="text/javascript">
   $(document).ready(function() {
					function setCurrent(current, images){
						current = (current >= images.length - 1) ? 0 : current + 1;
						console.log("Current = " + current);
						$("div#rotating-images img").css("display", "none");
						images.eq(current).fadeIn("slow").css("display", "block");
						return current;
					}
					function addNumbers(images){
						var numbers = document.getElementById("rotating-numbers");
						var ul = document.createElement("ul");
						$.each(images, function(idx){
							var src = $(this).attr('src');
							var li = document.createElement("li");
							var a = document.createElement("a");
							var litxt = document.createTextNode("" + parseInt(idx + 1));
							
							li.appendChild(litxt);
							li.addEventListener("click", function () {
								clearInterval(timeout);
									current = setCurrent(idx - 1, images);
									timeout = setInterval(function(){
									current = setCurrent(idx, images);	
								}, 5000);
							});
							ul.appendChild(li);
							
						});
						numbers.appendChild(ul);
					}
					var rotating_images = $("div#rotating-images img");
					if(rotating_images && rotating_images.length > 1){
						rotating_images.eq(0).css("display", "block");
						var current = 0;
						
						var add_numbers = false;
				
						if (add_numbers)
							addNumbers(rotating_images);

						var timeout = setInterval(function(){
							current = setCurrent(current, rotating_images);
						}, 5000);
					}
				});
  </script>
