   <div id="loginDialog" title="Login">
    <!--<iframe id="loginFrame" onLoad="checkLocation(this.contentWindow.location)" src="https://gail.uga.edu/general-site-pages/user-login-for-dialog-box">
	</iframe>-->
    <h2 id="waitMsg">
     Please wait...
    </h2>
   </div>
   <button id="opener">
    Login
   </button>
   <script type="text/javascript">
    /* jQuery dialog box */
	$(function() {
		// The dialog box
		$( "#loginDialog" ).dialog({
			autoOpen: false,
			show: {
				effect: "fade",
				duration: 300
			},
			hide: {
				effect: "fade",
				duration: 300
			},
			position: {
				my: "center",
				at: "center",
				of: window
			},
			height: 475,
			width: 510,
			modal: true,
			resizable: false,
			close: function(event, ui) {
				if ($("#loginFrame").length > 0) {
					$("#loginFrame").remove();
				}
				$("#waitMsg").show();
			}
		});
	});
	
	var bbpageLoginHelper = Sys.WebForms.PageRequestManager.getInstance();
	bbpageLoginHelper.add_pageLoaded(function(){
		// If event or payment table is found, add click listener to login link
		//if ($(".EventTable").length || $(".PaymentPart_FormContainer").length) {
		if ($(".EventTable").length) {
			$("a[title='Login to BBIS or Create a new account']").click(function(event) {
				//$("#loginDialog").empty();
				var loginURL = (window.location.protocol + "//" + window.location.hostname + "/general-site-pages/user-login-for-dialog-box");
				// Add code to create and append iframe and assign source only when login is clicked
				var loginFrame = document.createElement("iframe");
				var loginDialog = document.getElementById("loginDialog");
				loginFrame.setAttribute("id","loginFrame");
				loginFrame.setAttribute("onLoad","checkLocation(this.contentWindow.location)");
				loginFrame.setAttribute("src", loginURL);
				loginDialog.appendChild(loginFrame);
				
				setTimeout(function() {
					$("#waitMsg").hide();
				}, 2000);
				
				event.preventDefault();
				$( "#loginDialog" ).dialog( "open" );
				$( "#loginDialog" ).dialog( "option", "position", { my: "center", at: "center", of: window } );
			});
		}
	});
	
	function checkLocation(loc) {
		if (loc != $("#loginFrame").attr("src")) {
			var dialogIframe = document.getElementById("loginFrame");
			var innerIframeDoc = dialogIframe.contentDocument || dialogIframe.contentWindow.document;
			$("#loginFrame").hide();
			$("#loginDialog").append('<p style="width:500px;text-align:center;">Processing...</p>');
			location.reload();
		}
	}
   </script>
