			tempS = setTimeout(function(){
			thisObj.find("div.ops").each(function(i){
				var tA=$(this);
				setTimeout(function(){ tA.animate({left:"0"},300);},50*i);
			});
		},200);

	},function(){
		if(tempS){ clearTimeout(tempS); }
