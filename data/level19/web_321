    <script type="text/javascript">
     if (typeof TTI == "undefined") { var TTI = {}; }

TTI.baseURL = '';
TTI.stylesheetURL = 'https://allfaiths.ucenter.tamu.edu/wp-content/themes/rtc';

jQuery(document).ready(function(){
  //////var cs = { lat: 30.600, long: -96.3400 };
  var subdomainHost = location.hostname.split('.').shift();
  //////console.log('subdomain',subdomainHost);
  
  TTI.landmarks = TTI.getLandmarks();
  var selectedLandmark = TTI.landmarks.detect(function(lm){
    return lm.spec.slug == subdomainHost;
  });
  
  ////console.log('selected',selectedLandmark);
  
  ///var center = { lat: 30.613996947930257, long: -96.34167551994324 };
  var center = { lat: selectedLandmark.spec.lat, long: selectedLandmark.spec.long };

  var G = google.maps;
  
  var myOptions = {
      center: new G.LatLng(center.lat, center.long),
      zoom: 17,
      mapTypeId:google.maps.MapTypeId.ROADMAP
  };
  var map = new google.maps.Map(document.getElementById("map_canvas"),
          myOptions);
        
  /****                                                                               
  google.maps.event.addListener(map, "rightclick", function(event) {
      var lat = event.latLng.lat();
      var lng = event.latLng.lng();
      console.log(lat,lng);
  });
  ****/
/*
  TTI.landmarks.each(function(lm){
    lm.renderOn(map);
  });
  */
  selectedLandmark.renderOn(map,{
    showInfoWindow: false
  });
});
    </script>
