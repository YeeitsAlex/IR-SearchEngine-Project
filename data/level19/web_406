   <script type="text/javascript">
    $(function() {

	$('#demos').cycle({
		fx: 'scrollRight',
		timeout: 6000,
		speed: '300',
		prev: '#prev',
		next: '#next',
		pager: '#nav',
		height: '363px',
		pagerAnchorBuilder: pagerFactory
	});

	function pagerFactory(idx, slide) {
		var s = idx > 4 ? ' style="display:none"' : '';
		return '<li'+s+'><a href="#">'+(idx+1)+'</a></li>';
	};

// the following gives the slideshow navigation links a class so they can be hooked into via css and positioned correctly

	$("ul#nav li").addClass(function() {
	  return "item-" + $(this).index();
	});
});

$(function() {
	$('.seminarHighlight ul').cycle({
		fx: 'fade',
		timeout: 7000,
		speed: 'fast',
		containerResize: 0
	});
});

$(function() {
		$('#countdown_dashboard').countDown({
			targetDate: {
				'day': 15,
				'month': 8,
				'year': 2011,
				'hour': 0,
				'min': 0,
				'sec': 3
			},
		omitWeeks: true
		});
	});
   </script>
