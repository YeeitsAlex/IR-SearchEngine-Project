if ( 'https://t-square.gatech.edu/portal/tool/7348209c-b842-421e-aa31-8ba5ae71b96a/main'.indexOf('http://') == 0 || 'https://t-square.gatech.edu/portal/tool/7348209c-b842-421e-aa31-8ba5ae71b96a/main'.indexOf('https://') == 0 ) {

    function timeoutd7e70ccb_a001_493f_bc4b_f5193851cd19() { 
        var myframe = $("#idd7e70ccb_a001_493f_bc4b_f5193851cd19");
        var loaded = false;
        window.console && console.log("https://t-square.gatech.edu/portal/tool/7348209c-b842-421e-aa31-8ba5ae71b96a/main timeout check...");
        try {
            var iframeLoc = myframe.get(0).contentWindow.location;
            iframeLoc = String(iframeLoc);
            var windowLoc = String(window.location);
            window.console && console.log("iframe.location="+windowLoc);
            var ipos = -1;
            var slashcount = 0;
            for (var i=0; i<windowLoc.length; i++ ) {
                if ( windowLoc[i] == '/' ) slashcount++;
                if ( slashcount == 3 ) {
                    ipos = i;
                    break;
                }
            }
            if ( ipos == -1 ) {
                loaded = false;
            } else { 
                var prefix = windowLoc.substring(0, ipos);
                window.console && console.log("iframe.prefix="+prefix);
                loaded = iframeLoc.indexOf(prefix) == 0;
            }
        } catch(ex) {  // This is an expected/normal/hoped for occurance
            loaded = true;
        }
        if ( ! loaded ) {
            $('#alertd7e70ccb_a001_493f_bc4b_f5193851cd19').show();
        } else {
            window.console && console.log('https://t-square.gatech.edu/portal/tool/7348209c-b842-421e-aa31-8ba5ae71b96a/main load success');
        }

    }

    var timerd7e70ccb_a001_493f_bc4b_f5193851cd19 = false;
    $(document).ready(function() {
        timerd7e70ccb_a001_493f_bc4b_f5193851cd19 = window.setTimeout("timeoutd7e70ccb_a001_493f_bc4b_f5193851cd19()",8000);
    });
} else {
    window.console && console.log('https://t-square.gatech.edu/portal/tool/7348209c-b842-421e-aa31-8ba5ae71b96a/main does not start with http:// or https://');
}
