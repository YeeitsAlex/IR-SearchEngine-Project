           <article class="post-26 page type-page status-publish hentry" id="post-26">
            <header class="page-header">
             <h1 class="page-title">
              Front Page
             </h1>
            </header>
            <!-- .entry-header -->
            <div class="entry-content">
             <ul id="navBoxes">
              <li>
               <a href="https://cm.maxient.com/reportingform.php?UNCGreensboro&amp;layout_id=5">
               </a>
