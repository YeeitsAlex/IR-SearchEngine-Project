 <script type="text/javascript">
  function switchSocialMedia(t, i, extend) {
	extend = extend || false;
	[].slice.call(document.getElementsByClassName('social-media-box')[0].childNodes).filter(function(e) {return e.tagName == 'DIV';}).forEach(function(e) {e.style.display = 'none';});
	document.getElementById(t).style.display = 'inherit';
	jQuery(i).siblings().removeClass('selected');
	jQuery(i).addClass('selected');
	document.getElementsByClassName('social-media-box')[0].style.height = parseInt([].slice.call(document.getElementById(t).childNodes).filter(function(e) {return e.nodeType == 1;})[0].style.height) + (extend ? 50 : 0) + 'px';
}
 </script>
