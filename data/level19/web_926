    <script type="text/javascript">
     function fadeIn(e) {
	if (!e.style.opacity) e.style.opacity = 0;
	if (!e.fadeSpeed) e.fadeSpeed = 0.01;
	if (e.fadeSpeed > 0) {
		e.style.opacity = parseFloat(e.style.opacity) + e.fadeSpeed;
		if (parseFloat(e.style.opacity) > 1) e.style.opacity = 1;
		if (parseFloat(e.style.opacity) == 1) e.fadeSpeed = false;
		else setTimeout(function() {fadeIn(e);}, 5);
	}
	else if (e.fadeSpeed < 0) setTimeout(function() {fadeIn(e);}, 5);
}

function fadeOut(e) {
	if (!e.style.opacity) e.style.opacity = 1;
	if (!e.fadeSpeed) e.fadeSpeed = -0.01;
	if (e.fadeSpeed < 0) {
		e.style.opacity = parseFloat(e.style.opacity) + e.fadeSpeed;
		if (parseFloat(e.style.opacity) < Math.abs(e.fadeSpeed)) e.style.opacity = 0;
		if (parseFloat(e.style.opacity) == 0) e.fadeSpeed = false;
		else setTimeout(function() {fadeOut(e);}, 5);
	}
	else if (e.fadeSpeed > 0) setTimeout(function() {fadeOut(e);}, 5);
}
var featuredSlider;
var overlay, leftShade, rightShade;
jQuery(document).ready(function() {
	featuredSlider = jQuery('.featured-slider').bxSlider({
		slideWidth: 960,
		minSlides: 1,
		maxSlides: 1,
		moveSlides: 1,
		adaptiveHeight: false,
		pager: false,

		onSlideBefore: function(c, oldIndex, index) {
			[].slice.call(document.getElementsByClassName('i' + (oldIndex))).forEach(function(e) {fadeOut(e);});
		},
		
		onSlideAfter: function(c, oldIndex, index) {
			[].slice.call(document.getElementsByClassName('i' + (index))).forEach(function(e) {fadeIn(e);});
		}
	});

	document.getElementById('slide-left').addEventListener('click', function() {featuredSlider.goToPrevSlide();});
	document.getElementById('slide-right').addEventListener('click', function() {featuredSlider.goToNextSlide();});
});
    </script>
