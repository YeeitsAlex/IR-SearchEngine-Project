      <script>
       $(document).ready(function() {
		$("#libsearch .nav-tabs li").not(".research").click(function(e) {
			e.preventDefault();
			var id = $(this).children("a").attr("href").substring(1);
			$("#libsearch .tab-pane").hide();
			$("#libsearch #"+id).show();
			$("#libsearch .nav-tabs li").removeClass("active");
			$(this).addClass("active")
		});
	});
      </script>
