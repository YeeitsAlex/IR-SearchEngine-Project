  <script>
   window.jQuery || document.write('<script src="/assets/js/vendor/jquery.min.js"><\/script>')
  </script>
  <script src="/assets/js/app.js">
  </script>
  <script src="/assets/js/respond.js">
  </script>
  <script src="/assets/js/slitslider/jquery.ba-cond.min.js">
  </script>
  <script src="/assets/js/slitslider/jquery.slitslider.js">
  </script>
  <script>
   var $info = $("#hp-information-for"),
  	  displayPanel = false;
  $info.find(".tabs").on("click", "li", function(){
  	var $this = $(this),
		select = $(this).attr('data-item'),
		$panels = $info.find(".panels"),
		$prevPanel = $panels.find('.active'),
		$panel = $panels.find('[data-item="'+select+'"]');
	
	if(!(select === $prevPanel.attr('data-item'))){
	
		$this.siblings().removeClass('active');
		$this.addClass('active');
	
		$prevPanel.hide();
		$prevPanel.removeClass('active');
		$panel.addClass('active').show();
	
		if($prevPanel.length === 0)
			$panels.css({height:0, display:'block'}).animate({"height":"400px"});
			
	} else {
	
		$this.removeClass('active');
		$panel.removeClass('active');
		$panels.animate({'height':''}, function(){ 
			$(this).css({height:'', display:''});
			$prevPanel.hide();
		});
	
	}
  });
  if (jQuery(".sl-slider").length){
    var Page = (function() {
      var $slider = $("#hero-slider"),
	  $slides = $slider.find('.sl-slide'),
	  nav = function(){
	  	var $nav = $("#nav-dots");
	    for(var i = 0; i < $slides.length; i++)
		  if(i===0)
		    $nav.append('<span class="nav-dot-current"></span>');
		  else
		    $nav.append('<span></span>');
		return $nav;
	  },
	  slitslider = $slider.slitslider({
	    onBeforeChange : function( slide, pos ) {
          $nav.removeClass( 'nav-dot-current' );
	      $nav.eq( pos ).addClass( 'nav-dot-current' );
        }
      }),
      init = function() {initEvents();},
	  initEvents = function() {
	    $nav = nav().find('> span');
        $nav.each( function( i ) {
	      $( this ).on( 'click', function( event ) {
	        var $dot = $( this );
	        if( !slitslider.isActive() ) {
              $nav.removeClass( 'nav-dot-current' );
		      $dot.addClass( 'nav-dot-current' );
	        }
	        slitslider.jump( i + 1 );
	        return false;
	      });
	    });
      };
	  return { init : init };
    })();
    Page.init();
  }
  </script>
