.hero-slider a, .hero-slider a:hover, .hero-slider a:focus {
  border-bottom: none;
} .hero-slider a:hover, .hero-slider a:focus {
  text-decoration: underline;
}

.hero-slider .slick--skin--fullwidth .views-field-body {
  max-width: 1140px;
  max-height: 25vw;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 2.5em;
  margin: 0 auto;
  font-size: 1.1em;
  text-shadow: 1px 1px 1px rgba( 98, 100, 102, 0.9 ),
               2px 2px 1px rgba( 0, 0, 0, 0.9 );
  z-index: 2;
  overflow: hidden;
}
.hero-slider .slick--skin--fullwidth .views-field-body p {
  display: inline-block;
  padding: 0 1em;
  background: none;
  margin: 0;
}
.hero-slider .slick--skin--fullwidth .views-field-body p,
.hero-slider .slick--skin--fullwidth .views-field-body a,
.hero-slider .slick--skin--fullwidth .views-field-body a:hover,
.hero-slider .slick--skin--fullwidth .views-field-body a:focus,
.hero-slider .slick--skin--fullwidth .views-field-body a:visited,
.hero-slider .slick--skin--fullwidth .views-field-body a:active {
  color: white;
}

