             <script>
              <!--//--><![CDATA[// ><!--

jQuery(document).ready(function () {
    /* --- Wheel Tab Markup Data Collection --- */
    var wheelTabSettings = { tabTitles: [], markerColors: [], backgroundImageURLs: [], gaClickEventLabels: [] };
    var gaClickEventLabelPagePrefix = "Homepage";
    jQuery("#this_is_rice_wheel_tabs > div").children().each( function( index ) {
        wheelTabSettings.tabTitles.push( this.dataset.tabTitle );
        wheelTabSettings.markerColors.push( this.dataset.markerColor );
        wheelTabSettings.backgroundImageURLs.push( this.dataset.backgroundImage );
        wheelTabSettings.gaClickEventLabels.push( gaClickEventLabelPagePrefix + ": " + this.dataset.tabTitle );
    });

    /* --- Wheel Tabs Initialization --- */
    var wheelTabSubType = wheelizateTabSubTypes.Left;
    if( document.documentElement.clientWidth < 768 ) { wheelTabSubType = wheelizateTabSubTypes.Top; }
    var wheelTabs = new wheelizateTab("this_is_rice_wheel_tabs");
    wheelTabs.selectedTab = 0;
    wheelTabs.tabType = wheelizateTabTypes.Apart;
    wheelTabs.tabSubType = wheelTabSubType;
    wheelTabs.tabThemeType= wheelizateTabThemeTypes.Solid;
    wheelTabs.tabWheelType = wheelizateTabWheelTypes.Donut;
    wheelTabs.wheelRadius = 250;
    wheelTabs.tabTitles = wheelTabSettings.tabTitles;
    wheelTabs.tabTooltips = wheelTabSettings.tabTitles;
    wheelTabs.iconHeight = 40;
    wheelTabs.iconWidth = 40;
    wheelTabs.clockwise = true;
    wheelTabs.wheelAnimation = "easeOut";
    wheelTabs.paneAnimation = "fade";
    wheelTabs.animateTime = 750;
    wheelTabs.minHeight = 300;
    wheelTabs.sliderSeconds = 0;
    wheelTabs.keyEnabled = true;
    wheelTabs.keyOnFocus = true;
    wheelTabs.marker = true;
    wheelTabs.createTab();

    /* --- Wheel Tabs Layout Edits --- */
    document.getElementById("this_is_rice_wheel_tabs-wheel").classList.add("col-6");
    jQuery(".slide-content-theme-right .rice-slide-content-container").addClass("push-6");
  
    /* --- Wheel Tabs Event Handlers --- */
    var wheelTabTransitions = function ( event ) { 
        if( typeof event.data.url !== "undefined") { document.getElementById("this_is_rice_pane_block").style.backgroundImage = 'url(' + event.data.url + ')'; }
        if( typeof event.data.color !== "undefined") { document.getElementById("wheelnav-this_is_rice_wheel_tabs-wheel-marker").style.stroke = event.data.color; }
        if( window.ga && ga.create ) { ga( 'send', 'event', 'WheelTabs', 'Click', event.data.clickEventLabel ); }
    }
    for(var index = 0; index < wheelTabs.wheelNav.navItems.length; index++) {
        jQuery( "#"+wheelTabs.wheelNav.navItems[index].navTitle.id ).on("mousedown", {"url": wheelTabSettings.backgroundImageURLs[index], "color": wheelTabSettings.markerColors[index], "clickEventLabel": wheelTabSettings.gaClickEventLabels[index] }, wheelTabTransitions);
        jQuery( "#"+wheelTabs.wheelNav.navItems[index].navSlice.id ).on("mousedown", {"url": wheelTabSettings.backgroundImageURLs[index], "color": wheelTabSettings.markerColors[index], "clickEventLabel": wheelTabSettings.gaClickEventLabels[index] }, wheelTabTransitions);
    }

    /* --- Set Marker Color --- */
    document.getElementById("wheelnav-this_is_rice_wheel_tabs-wheel-marker").style.stroke = wheelTabSettings.markerColors[0];

    /* --- Marker Color CSS for Tab Content Pane --- */
    jQuery(" #this_is_rice_wheel_tabs-content > .wtab-pane " ).each(function( index ) {
        jQuery( this ).css({
            "border-color": wheelTabSettings.markerColors[index]
        });
    });

    /* --- Block Wheel Focus Highlight on Click --- */
    var $wheel = jQuery(" #this_is_rice_wheel_tabs-wheel ");
    var refocusable = function() { this.blur(); /*Most Browsers*/ this.hideFocus=false; /*IE*/ this.style.outline=null; /*Mozilla*/ };
    $wheel.mousedown(function() { 
        this.blur(); /*Most Browsers*/
        this.hideFocus = true; /*IE*/
        this.style.outline = 'none'; /*Mozilla*/
    });
    $wheel.mouseup(refocusable);
    $wheel.mouseout(refocusable);

  /* --- Highlight Nav Links on Scroll --- */

  // Returns a function, that, as long as it continues to be invoked, will not
  // be triggered. The function will be called after it stops being called for
  // N milliseconds. If `immediate` is passed, trigger the function on the
  // leading edge, instead of the trailing.
  function debounce(func, wait, immediate) {
	var timeout;
	return function() {
		var context = this, args = arguments;
		var later = function() {
			timeout = null;
			if (!immediate) func.apply(context, args);
		};
		var callNow = immediate && !timeout;
		clearTimeout(timeout);
		timeout = setTimeout(later, wait);
		if (callNow) func.apply(context, args);
	};
  };
  var highlightLinksOnScroll = debounce(function() {
      console.log("highlightLinksOnScroll");
      if( jQuery( window ).scrollTop > 50 ) {
          console.log("scrollTop > 50");
          jQuery( ".rice-megamenu-highlight-link-on-scroll" ).addClass( ".highlighted" );
      }
      else {
          console.log("else");
          jQuery( ".rice-megamenu-highlight-link-on-scroll").removeClass( ".highlighted" );
      }
  }, 250);
  jQuery( window ).scroll(highlightLinksOnScroll);

});

//--><!]]>
             </script>
            </div>
           </div>
