               <div class="vc_grid-container vc_clearfix wpb_content_element vc_basic_grid" data-initial-loading-animation="fadeIn" data-vc-grid-settings='{"page_id":4,"style":"all","action":"vc_get_vc_grid_data","shortcode_id":"1551122339585-3ac7a696-5dbd-1","tag":"vc_basic_grid"}' data-vc-post-id="4" data-vc-public-nonce="4f045fd45e" data-vc-request="https://nextgen.gsu.edu/wp-admin/admin-ajax.php">
