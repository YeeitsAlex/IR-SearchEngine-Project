<section aria-label="" class="ribbon style-0" role="complementary">
 <div class="container">
  <div class="row">
   <div class="col-lg-3 order-lg-1">
    <div class="ribbon-callout">
     <h2>
      <span class="ecu-h3">
       Family Weekend Sponsors
      </span>
      <strong>
       <br/>
      </strong>
     </h2>
    </div>
   </div>
   <div class="col-lg-9 order-lg-12">
    <div class="row">
     <div class="col-sm-3">
