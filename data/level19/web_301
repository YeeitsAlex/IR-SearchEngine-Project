     <div class="video-wrapper">
      <div class="video-container" style="background: url('/_assets/img/north-campus.jpg') center center / cover no-repeat;">
       <video autoplay="" class="video-js" data-setup='{ "fluid": true, "textTrackSettings": false	}' id="homepage-video" loop="" muted="" playsinline="">
        <source media="all and (min-width:640px)" src="/_assets/video/visit-georgia-360.mp4" type="video/mp4"/>
