    (function( $ ) {
    
     var images = ['Austin.jpg', 'Berlin.jpg', 'Capitol.jpg', 'Madrid.jpg', 'London.png', 'DC.jpg'];//Name of image files used for the randomization.
    
     $('.BackTAMU').css({'background-image': 'url(/App_Themes/PPIP/img/bg/' + 
              images[Math.floor(Math.random() * images.length)] + ')'}); //Inject into the class BackTAMU to select as background image a random image based off our var images.
    })(jQuery);
   </script>
