  <script>
   $(function(){
            $('#navigation').slicknav();

            $("#navigation>li>ul>li").on('mouseenter mouseleave', function (e) {
              if ($('ul', this).length) {
                  var elm = $('ul:first', this);
                  var off = elm.offset();
                  var l = off.left;
                  var w = elm.width();
                  var docH = $(".nav-wrap").height();
                  var docW = $(".nav-wrap").width();

                  var isEntirelyVisible = (l + w <= docW);

                  if (!isEntirelyVisible) {
                      $(this).addClass('edge');
                  } else {
                      $(this).removeClass('edge');
                  }
              }
            });


        });
  </script>
