   <script>
    $( document ).ready(function() {
        $( ".sidebar-nav > li.dropdown-submenu > a").each(function( index ) {
        if ($(this).attr('onclick')){
            var columnHTML = "<div class='col-sm-3 hidden-xs'><div class='footer-list'><div class='footer-list-title'><a onclick=\""+$(this).attr('onclick')+"\" href='"+$(this).attr('href')+"'>"+$(this).text().toUpperCase()+"</a></div>";
        } else {
            var columnHTML = "<div class='col-sm-3 hidden-xs'><div class='footer-list'><div class='footer-list-title'><a href='"+$(this).attr('href')+"'>"+$(this).text().toUpperCase()+"</a></div>";
        }
        $( this ).parent().children("ul").children("li").children("a").each(function(index){
            if ($(this).attr('onclick')){
                columnHTML = columnHTML + "<div class='footer-list-item'><a onclick=\""+$(this).attr('onclick')+"\" href='"+$(this).attr('href')+"'>"+$(this).text()+"</a></div>";
            } else {
                columnHTML = columnHTML + "<div class='footer-list-item'><a href='"+$(this).attr('href')+"'>"+$(this).text()+"</a></div>";
            }
        });
        columnHTML = columnHTML + "</div></div>";
        $("#tempFooterUGA").remove();
        $("#pagefooter > .container > #UGAfooterNav").append(columnHTML);
    });
    });
   </script>
   <div class="alert alert-warning" id="cookiePolicyId" style="padding: 10px; width: 100%; border: 1px solid gray; position: fixed; right: 0px; bottom: 0px;">
    <i class="fa fa-info-circle" title="Info">
     <span class="sr-only">
      Info
     </span>
    </i>
    This site uses cookies to ensure you get the best experience.
    <a class="btn btn-sm btn-warning fancybox ajax" href="index.cfm?FuseAction=Public.cookiePolicy" title="Read cookie policy">
     Read cookie policy
    </a>
    Accept?
    <button class="btn btn-sm btn-warning" data-dismiss="alert" onclick="setCookie( cname = 'no-cookies', cvalue = 'false' );">
     Yes
    </button>
    <button class="btn btn-sm btn-warning" data-dismiss="alert" onclick="setCookieEndOfSession( cname = 'no-cookies', cvalue = 'true' );">
     No
    </button>
   </div>
   <script language="JavaScript" type="text/javascript">
    function setCookie( cname, cvalue ) {
		var d = new Date();
	    d.setTime(d.getTime() + (10000*24*60*60*1000));
	    var expires = "expires="+ d.toUTCString();
	    document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
	}
	function setCookieEndOfSession( cname, cvalue ) {
	    document.cookie = cname + "=" + cvalue + ";path=/";
	}
   </script>
