              <div class="views-row views-row-2 views-row-even">
               <div class="views-field views-field-field-date-and-time">
                <div class="field-content">
                 <span class="date-display-single" content="2019-03-15T00:00:00-04:00" datatype="xsd:dateTime" property="dc:date">
                  March 15, 2019 (All day)
                 </span>
                </div>
               </div>
