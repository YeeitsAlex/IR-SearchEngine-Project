         <!--front panel regions beg-->
         <div class="at-panel gpanel panel-display content clearfix" id="content-panels">
          <div class="region region-content-top">
           <div class="region-inner clearfix">
            <div class="block block-boxes block-boxes-os_slideshow_box no-title" delta="1482421289" id="block-boxes-1482421289" module="boxes">
             <div class="block-inner clearfix">
              <div class="block-content content" ng-non-bindable="">
               <div class="boxes-box" id="boxes-box-1482421289">
                <div class="boxes-box-content">
                 <div id="layout_3_1_overlay">
                  <div class="slide">
                   <ul class="rslides">
                    <li>
                     <a href="https://worldwide.harvard.edu/">
                      <img alt="Photo by Ashiley Lee, class of 2019" class="image-style-os-slideshow-31-980" height="327" src="//static.projects.iq.harvard.edu/files/styles/os_slideshow_3%3A1_980/public/international_affairs/files/lee_ashiley_2019_landscape_china2.jpg?m=1517435091&amp;itok=Vm_wRgdl" title="https://worldwide.harvard.edu/" width="980"/>
                     </a>
                     <div class="caption slide-copy">
                      <h2>
                       <a href="https://worldwide.harvard.edu/">
                        Harvard Worldwide
                       </a>
                      </h2>
                      <p>
                       <a href="https://worldwide.harvard.edu/">
                        Learn more about Harvard's global engagement on the new Harvard Worldwide website
                       </a>
                      </p>
                     </div>
                    </li>
                   </ul>
