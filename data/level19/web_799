  <script type="text/javascript">
   $.ajaxSetup({
    cache: false
});
	
//$(document).ready(function(){
  replaceEmptyFeedText('div.super-rss-reader-widget');
  window.setTimeout(function () { reload('div.super-rss-reader-widget'); }, 30000);

//});

function reload(selector) {
  try
  {
	 
    $.get(document.URL, function(data) {

      var newData = $(data).find(selector);

      $(selector).replaceWith(newData);

      replaceEmptyFeedText(selector);

    });

    window.setTimeout(function () {reload(selector);}, 30000);    
  }
  catch(e)
  {
    window.setTimeout(reload(selector), 50000);
  }
}

function replaceEmptyFeedText(selector){
   if($(selector).text().trim() == "No items.")
   { 
      $(selector).html('<div class="srr-wrap  srr-style-none"><div><div>There are no active emergencies and the university is operating under normal conditions.</div></div></div>');
   }
}
  </script>
