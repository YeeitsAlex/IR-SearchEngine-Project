  </script>
  <script type="text/javascript">
   if ( typeof soliloquy_slider === 'undefined' || false === soliloquy_slider ) {soliloquy_slider = {};}jQuery('#soliloquy-container-1378').css('height', Math.round(jQuery('#soliloquy-container-1378').width()/(1240/500)));jQuery(window).load(function(){var $ = jQuery;var soliloquy_container_1378 = $('#soliloquy-container-1378'),soliloquy_1378 = $('#soliloquy-1378');soliloquy_slider['1378'] = soliloquy_1378.soliloquy({slideSelector: '.soliloquy-item',speed: 800,pause: 3000,auto: 1,useCSS: 0,keyboard: true,adaptiveHeight: 1,adaptiveHeightSpeed: 400,infiniteLoop: 1,mode: 'fade',pager: 1,controls: 1,nextText: '',prevText: '',startText: '',stopText: '',onSliderLoad: function(currentIndex){soliloquy_container_1378.find('.soliloquy-active-slide').removeClass('soliloquy-active-slide').attr('aria-hidden','true');soliloquy_container_1378.css({'height':'auto','background-image':'none'});if ( soliloquy_container_1378.find('.soliloquy-slider li').length > 1 ) {soliloquy_container_1378.find('.soliloquy-controls').fadeTo(300, 1);}soliloquy_1378.find('.soliloquy-item:not(.soliloquy-clone):eq(' + currentIndex + ')').addClass('soliloquy-active-slide').attr('aria-hidden','false');soliloquy_container_1378.find('.soliloquy-clone').find('*').removeAttr('id');soliloquy_container_1378.find('.soliloquy-controls-direction').attr('aria-label','carousel buttons').attr('aria-controls', 'soliloquy-container-1378');soliloquy_container_1378.find('.soliloquy-controls-direction a.soliloquy-prev').attr('aria-label','previous');soliloquy_container_1378.find('.soliloquy-controls-direction a.soliloquy-next').attr('aria-label','next');},onSlideBefore: function(element, oldIndex, newIndex){soliloquy_container_1378.find('.soliloquy-active-slide').removeClass('soliloquy-active-slide').attr('aria-hidden','true');$(element).addClass('soliloquy-active-slide').attr('aria-hidden','false');},onSlideAfter: function(element, oldIndex, newIndex){},});});
  </script>
