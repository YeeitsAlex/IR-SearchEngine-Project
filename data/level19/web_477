    <div class="container">
     <div class="row">
      <div class="col-md-12">
       <div class="nav-tabs-responsive">
        <ul class="nav nav-tabs">
         <li class="active" role="presentation">
          <a data-toggle="tab" href="#new">
           New
          </a>
         </li>
         <li role="presentation">
          <a data-toggle="tab" href="#civil-rights">
           Civil Rights
          </a>
         </li>
         <li role="presentation">
          <a data-toggle="tab" href="#newspapers">
           Newspapers
          </a>
