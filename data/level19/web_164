  <script type="text/javascript">
   //<![CDATA[

function reCaptchaCallback() {
	jQuery('#form_feedback').trigger('submit');
}

jQuery(function() {
	jQuery('#form_feedback').submit(function(e) {
		e.preventDefault();
		var $f = jQuery(this);
		var jsonPost = jQuery.ajax({
			method: 'POST',
			url: $f.attr('action'),
			data: $f.serialize(),
			dataType: 'html',
			success: function(data) {
				jQuery('#dialog_feedback_result').html(data);
				grecaptcha.reset();
				$f.trigger('reset');
			}
		});
	});
});

//]]>
  </script>
