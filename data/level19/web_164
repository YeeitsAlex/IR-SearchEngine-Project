      <script type="text/javascript">
       //<![CDATA[
var j = window.jQuery;
window.addEvent('domready', function() {
	j('#action_login, #action_login2, #action_login_menu').on('click', function(e) {
		e.preventDefault();
		e.stopPropagation();
		j('#box_home_newuser').hide();
		j('#box_home_login').fadeIn(250);
	});
	
	j('#action_newuser').on('click', function(e) {
		e.preventDefault();
		e.stopPropagation();
		j('#box_home_newuser').fadeIn(250);
	});
	
	j('.action-login-close').on('click', function(e) {
		e.preventDefault();
		j('#box_home_login').hide();
		j('#box_home_newuser').hide();
		j('#box_home_forgot').hide();
	});
	
	j('.action-login-forgot').on('click', function(e) {
		e.preventDefault();
		j('#box_home_login').hide();
		j('#box_home_forgot').fadeIn(250);
	});
	
	if(window.location.hash == '#login')
	{
		j('#action_login').trigger('click');
	}
	else if(window.location.hash == '#newuser')
	{
		j('#action_newuser').trigger('click');
	}
});
//]]>
      </script>
