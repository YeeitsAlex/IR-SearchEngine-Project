      (function(__){
                __(function(){
                    (function(imageUrlArray){
                        if(imageUrlArray.length == 0){
                            return;
                        }
                        __('#homepage-background-image').css('background-image', "url('" + imageUrlArray[Math.floor(Math.random() * imageUrlArray.length)] + "')");
                    })([
                                                    "_includes/images/sections/emory-campus.jpg"
                             ,                                                     "_includes/images/sections/emory-atlanta-2.jpg"
                                                                        ]);
                    var __captionSet = __('#caption-set .caption');
                    __captionSet.addClass('hidden');
                    __(__captionSet[Math.floor(__captionSet.length * Math.random())]).removeClass('hidden');
                });
            })(jQuery);
     </script>
