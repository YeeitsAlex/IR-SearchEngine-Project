<!--Animate the Read More items http://jsfiddle.net/4aPpy/ -->
<script>
 jQuery(document).ready(function(){
  jQuery(".flip").click(function($){
    jQuery(jQuery(this).nextAll(".panel")[0]).slideToggle("slow");
