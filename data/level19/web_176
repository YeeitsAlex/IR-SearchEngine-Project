                      <div class="views-slideshow-cycle-main-frame-row-item views-row views-row-3 views-row-even">
                       <div class="views-field views-field-vsx-slide-image">
                        <div class="field-content">
                         <img alt="" height="2880" src="https://library.uncc.edu/sites/default/files/multimedia_lab_1_0.jpg" width="5760"/>
                        </div>
                       </div>
                      </div>
                     </div>
                     <div class="views-slideshow-cycle-main-frame-row views_slideshow_cycle_slide views_slideshow_slide views-row-5 views_slideshow_cycle_hidden views-row-odd" id="views_slideshow_cycle_div_make_uncc_edu_slideshow-block_8_4">
                      <div class="views-slideshow-cycle-main-frame-row-item views-row views-row-4 views-row-odd views-row-last">
                       <div class="views-field views-field-vsx-slide-image">
                        <div class="field-content">
                         <img alt="" height="2671" src="https://library.uncc.edu/sites/default/files/t3_1_0.jpg" width="5342"/>
                        </div>
                       </div>
                      </div>
                     </div>
                    </div>
