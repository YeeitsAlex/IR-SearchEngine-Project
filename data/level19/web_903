  <section class="hero container hero-bg1 hero-large" data-stellar-background-ratio="0.5" data-stellar-vertical-offset="340">
   <div class="row">
    <div class="col-5" style="z-index:2;">
     <div class="hero-title no-blur" data-stellar-background-ratio="0.5" data-stellar-vertical-offset="380">
      <h2>
       Medieval and Early Modern Studies Program
      </h2>
     </div>
    </div>
   </div>
  </section>
  <div class="underhero container">
   <div class="row">
    <!-- block: Social Links -->
