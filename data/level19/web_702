<!-- SCRIPT FOR JQUERY DIALOG BOXES -->
<script src="js-plugin/respond/respond.min.js" type="text/javascript">
</script>
<script>
 $(document).ready(function () {
        $("#dialog-link").click(function (event) {
            $("#dialog").dialog("open");
            event.preventDefault();
        });

        if ($("#dialog").length) {
            $("#dialog").dialog({
                autoOpen: false,
                modal: true,
                width: 400,
                buttons: [{
                    text: "Close",
                    click: function () {
                        $(this).dialog("close")
                    }
                }]
            });
        }
    });
</script>
