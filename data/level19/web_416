<script>
 jQuery(document).ready(function() {
	jQuery(".ratetable").hide();
  jQuery("select").change(function() {
    var price = jQuery(this).val();
    if (price == "low") {
      jQuery(".ratetable").not(".low").hide();
      jQuery(".low").show();
    } else if (price == "high") {
      jQuery(".ratetable").not(".high").hide();
      jQuery(".high").show();
    } else {
      jQuery(".ratetable").hide();
    }
  });
});
</script>
