   <!-- functions for mobile -->
   <script>
    $(document).ready(function(){
		
		location.hash && $(location.hash + '.collapse').collapse('show');

		//Enable swiping...
		$('.carousel-inner').swipe( {
			//Generic swipe handler for all directions
			swipeLeft:function(event, direction, distance, duration, fingerCount) {$(this).carousel('next');},
			swipeRight: function() {$(this).carousel('prev');},
		}); 

	// If window < 768, hides toplinks just before page load
			if ($(window).width() < 751 ) {
				$('#toplinks').hide();
				$('footer .panel-collapse').removeClass("in");
				$('footer .panel-collapse').css({"height":"0","overflow":"hidden"});
			}  else {/*$('#toplinks').show();*/}
		
		$(window).resize(function() {
			if ($(window).width() < 751 ) {
				$('#toplinks').hide();
				$('footer .panel-collapse').removeClass("in");
				$('footer .panel-collapse').css({"height":"0","overflow":"hidden"});
			} else {
				$('#toplinks').show();
				$('footer .panel-collapse').addClass('in');
				$('footer .panel-collapse').css({"height":"","overflow":""});
			}
		});
	
		// toggles the search box & links on clicking the search link 
		$( "#toggle-search" ).click(function() {
			$( "#toplinks" ).slideToggle();
		});
		
		// toggle off-canvas nav
		$('#locl').click(function() {$('.row-offcanvas').toggleClass('active');});
		
		// hides web-font icons from screen readers
		$('.fa, .fcs-icon').attr( "aria-hidden", "true" );
			
		//Function to import BS breakpoints & window width into JS are in footer
		
		// conditionally change megamenu css to fix iPad z-index bug (carousel covers menu)
		if (navigator.userAgent.match(/iPad/i)) {
			$('.megamenu .dropdown-menu').css({"position":"relative"});
		}
		
		// lightbox-bs functions
		var text = $('<div id="lightbox-modal" class="modal fade" tabindex="-1" role="dialog"><div class="modal-dialog"><div class="modal-content"><button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button><div class="modal-body"><img src="" alt=""><h4></h4><p></p></div></div></div></div>');
		$('body').append(text);

		$('.lightbox-bs').click(function(event){
				event.preventDefault();
				var title = $(this).children().attr("alt"),
					  src = $(this).attr("href"),
					  css = {
									'maxWidth': $(window).width() - 100,
									'maxHeight': $(window).height() - 140,
						};
				$('#lightbox-modal .modal-body h4').html(title);
        $('#lightbox-modal').find('img').attr('src', src);
        $('#lightbox-modal').find('img').attr('alt', title);
				$('#lightbox-modal').find('img').css(css);
				$('#lightbox-modal').modal({show:true});
		});	
		
			$('.lightbox-bs-dl a').click(function(event){
				event.preventDefault();
				var title = $(this).text(),
					  htm   = $(this).parent().siblings().html();
				$('#lightbox-modal').css('maxWidth','100%');
				$('#lightbox-modal .modal-body h4').html(title);
				$('#lightbox-modal .modal-body p').html(htm);
				$('#lightbox-modal').find('.modal-dialog').css('maxWidth','60em');
				$('#lightbox-modal').modal({show:true});
		});	
		$('#lightbox-modal').on('hidden.bs.modal', function (e) {$('body').css('padding-right','0');})
		
		// lazy loading
		$("img.lazy").lazy();
		
		// timeline functions
		var my_posts = $(".timeline-badge [rel=tooltip]");
		var size = $(window).width();
		for(i=0;i<my_posts.length;i++){
			the_post = $(my_posts[i]);
			if( (i%2)!=0 && size >=767 ){
				the_post.tooltip({ placement: 'left'});
				the_post.css("cursor","pointer");
			}else{
				the_post.tooltip({ placement: 'right'});
				the_post.css("cursor","pointer");
			}
		}
		
	// Floating Labels
    $('[data-toggle="floatLabel"]').attr('data-value', $(this).val()).on('keyup change', function() {
		$(this).attr('data-value', $(this).val());
	});
	
	//Add keypress event on every link where exist class="btn"
    $('a.btn').keypress(function(e) {
        if (e.which == 32) {
					e.preventDefault();
					/*$(e.currentTarget).click();*/
					/*$(e.currentTarget).trigger("click");*/
					$(location).attr('href', $(e.currentTarget).attr("href"));
			}
				/*alert($(e.currentTarget).attr("href"));*/
    });
		
		//Remove focus outlines except for keyboard access
		function handleFirstTab(e) {
			if (e.keyCode === 9) {
				document.body.classList.add('tab-focus-outline');

				window.removeEventListener('keydown', handleFirstTab);
				window.addEventListener('mousedown', handleMouseDownOnce);
			}
		}

		function handleMouseDownOnce() {
			document.body.classList.remove('tab-focus-outline');

			window.removeEventListener('mousedown', handleMouseDownOnce);
			window.addEventListener('keydown', handleFirstTab);
		}

	window.addEventListener('keydown', handleFirstTab);
	 
	}); // End Document.ready
   </script>
