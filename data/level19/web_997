function pageNav() {
  if ($(window).width() >= 733) {
    $('#drop1, #drop2, #drop3, #drop4, #drop5').removeClass('f-dropdown');
  } 
   if ($(window).width() <= 733) {
    $('#drop1, #drop2, #drop3, #drop4, #drop5').addClass('f-dropdown');
  } 
}
$(window).resize(function() {
  $(document).foundation('dropdown', 'reflow');
  pageNav();
});
  </script>
