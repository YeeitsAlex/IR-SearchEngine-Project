  <script type="text/javascript">
   <!-- Hide from other browsers 
function form_init() {
 document.body.style.backgroundColor="white";
 document.forms[0].p_id.focus() }
function submitme() {
var f = document.forms[0];
if (f.p_id.value.length < 9) {
    f.p_id.focus();
    alert ("University ID must be 9 characters in length.\n Please provide a value.\n");
} else if (f.p_username.value == "") {
    f.p_username.focus();
    alert ("Computer Username is blank.\n Please provide a value.\n");
} else if (f.p_last_name.value == "") {
    f.p_last_name.focus();
    alert ("Last Name is blank.\n Please provide a value.\n");
} else if (f.p_first_name.value == "") {
    f.p_first_name.focus();
    alert ("First Name is blank.\n Please provide a value.\n");
} else if (f.p_birth_year.value.length < 4) {
    f.p_birth_year.focus();
    alert ("Invalid birth year (YYYY).\n Please provide a value.\n");
} else if (!f.p_acctpol.checked) {
    f.p_acctpol.focus();
    alert ("You must accept University policies.\n");
} else {
    document.forms[0].submit()} }
function clearme() {
document.forms[0].reset();
document.forms[0].p_id.focus() }
// End script hiding -->
  </script>
