    if (document.body) {
      var jsHiddenWarning = document.getElementById("JSErrorOccurred");
      if (!jsHiddenWarning) {
        document.body.innerHTML += "<div id='JSErrorOccurred' class='hidden'></div>";
      }
    } else {
      window.onload = function() {
        var jsHiddenWarning = document.getElementById("JSErrorOccurred");
        if (!jsHiddenWarning) {
          document.body.innerHTML += "<div id='JSErrorOccurred' class='hidden'></div>";
        }
      }
    }
  }
