     <section class="block block-block clearfix" id="block-block-7">
      <p>
      </p>
      <h2 style="text-align:center">
       WELCOME TO GRADUATE EDUCATION AT UNC CHARLOTTE
      </h2>
     </section>
     <section class="block block-views clearfix" id="block-views-factoids-block-2">
      <div class="view view-factoids view-id-factoids view-display-id-block_2 factoid view-dom-id-11275d536598bf835eb2cd01b39e03c0">
