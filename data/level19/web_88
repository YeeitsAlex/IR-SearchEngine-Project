   if ( typeof soliloquy_slider === 'undefined' || false === soliloquy_slider ) {soliloquy_slider = {};}var soliloquy_width_1305 = jQuery('#soliloquy-container-1305').width() < 965 ? 965 : jQuery('#soliloquy-container-1305').width();jQuery('#soliloquy-container-1305').css('height', Math.round(soliloquy_width_1305/(965/475))).fadeTo(300, 1);jQuery(document).ready(function($){var soliloquy_container_1305 = $('#soliloquy-container-1305'),soliloquy_1305 = $('#soliloquy-1305'),soliloquy_holder_1305 = $('#soliloquy-1305').find('.soliloquy-preload');if ( 0 !== soliloquy_holder_1305.length ) {var soliloquy_src_attr = 'data-soliloquy-src';soliloquy_holder_1305.each(function() {var soliloquy_src = $(this).attr(soliloquy_src_attr);if ( typeof soliloquy_src === 'undefined' || false === soliloquy_src ) {return;}var soliloquy_image = new Image();soliloquy_image.src = soliloquy_src;$(this).attr('src', soliloquy_src).removeAttr(soliloquy_src_attr);});}soliloquy_slider['1305'] = soliloquy_1305.soliloquy({slideSelector: '.soliloquy-item',speed: 400,pause: 5000,auto: 1,keyboard: 1,useCSS: 0,startSlide: 0,autoHover: 0,autoDelay: 0,infiniteLoop: 1,mode: 'horizontal',pager: 0,controls: 1,autoControls: 0,autoControlsCombine: 0,nextText: '',prevText: '',startText: '',stopText: '',onSliderLoad: function(currentIndex){soliloquy_container_1305.find('.soliloquy-active-slide').removeClass('soliloquy-active-slide').attr('aria-hidden','true');soliloquy_container_1305.css({'height':'auto','background-image':'none'});if ( soliloquy_container_1305.find('.soliloquy-slider li').length > 1 ) {soliloquy_container_1305.find('.soliloquy-controls').fadeTo(300, 1);} else {soliloquy_container_1305.find('.soliloquy-controls').addClass('soliloquy-hide');}soliloquy_1305.find('.soliloquy-item:not(.soliloquy-clone):eq(' + currentIndex + ')').addClass('soliloquy-active-slide').attr('aria-hidden','false');$('.soliloquy-clone .soliloquy-video-holder').remove();soliloquy_container_1305.find('.soliloquy-controls-direction').attr('aria-label','carousel buttons').attr('aria-controls', 'soliloquy-container-1305');soliloquy_container_1305.find('.soliloquy-controls-direction a.soliloquy-prev').attr('aria-label','previous');soliloquy_container_1305.find('.soliloquy-controls-direction a.soliloquy-next').attr('aria-label','next');$(window).trigger('resize');soliloquy_container_1305.parent().attr('data-soliloquy-loaded', 1);},onSlideBefore: function(element, oldIndex, newIndex, oldelm ){soliloquy_container_1305.find('.soliloquy-active-slide').removeClass('soliloquy-active-slide').attr('aria-hidden','true');$(element).addClass('soliloquy-active-slide').attr('aria-hidden','false');},onSlideAfter: function(element, oldIndex, newIndex){}});});
  </script>
