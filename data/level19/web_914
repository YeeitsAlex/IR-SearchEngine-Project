   $(document).ready(function(){
  if (!isMobile.any() || isMobile.iPad()) {
    $('#research-feed-burn').remove();
  };
  $.ajax({
    type: "GET",
    url: "http://news.rice.edu/tag/research/feed/?cat=-4",
    dataType: "xml",
    success: function(xml) {

      $(xml).find('item:lt(7)').each(function(){
        var title = $(this).find('title').text();
        var link = $(this).find('link').text();
        var image = $(this).find('image').text();
        var alt = $(this).find('alt').text();

        $('<li></li>').html("<div class=thumb><img src="+image+" alt='"+alt+"'></div><a href="+link+"/#>"+title+"</a>").appendTo("#research-feed");
      });
},
    error: function() {
      $('#research-feed').css('display','none');
      $('#research-feed-burn').css('display','block');
}
  });

});
