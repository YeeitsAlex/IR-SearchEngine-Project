  <script>
   var video_src = 'https://www.youtube.com/embed/F3gUKyoG_7w?rel=0&amp;controls=1&amp;showinfo=0';
var youtube_id = 'F3gUKyoG_7w';
var vid_width = '560';
var vid_height = '315';

jQuery(document).ready(function(){
  jQuery("body").prepend(
    '<a id="video_blanket" href="javascript:close_video();"></a><div id="video_player_wrapper"><a id="video_player_close" href="javascript:close_video();">&#215;</a>' +
    '<iframe width="' + vid_width + '" id="video_player" height="' + vid_height + '" src="' + video_src + '" frameborder="0" allow="encrypted-media" allowfullscreen></iframe>'
  );
});

function play_video(){
  jQuery("#video_blanket").css("display", "block");
  jQuery("#video_player_close").css("display", "block");
  jQuery("#video_player_wrapper").css("display", "block");
  var player_frame = document.getElementById("video_player").contentWindow;

  jQuery("#video_player_wrapper > iframe").attr('src', jQuery("#video_player_wrapper > iframe").attr('src') + '&autoplay=1');

}

function close_video(){
  jQuery("#video_player_wrapper").remove();
  jQuery("body").prepend(
    '<div id="video_player_wrapper"><a id="video_player_close" href="javascript:close_video();">&#215;</a><iframe width="' + vid_width + '" id="video_player" height="' + vid_height + '" src="' + video_src +
    '" frameborder="0" allow="encrypted-media" allowfullscreen></iframe>'
  );
  jQuery("#video_blanket").css("display", "none");
  jQuery("#video_player_close").css("display", "none");
}
  </script>
  <script>
   var isMobile = {
    Android: function() {
        return navigator.userAgent.match(/Android/i);
    },
    BlackBerry: function() {
        return navigator.userAgent.match(/BlackBerry/i);
    },
    iOS: function() {
        return navigator.userAgent.match(/iPhone|iPad|iPod/i);
    },
    iPhone: function() {
        return navigator.userAgent.match(/iPhone/i);
    },
    iPad: function() {
        return navigator.userAgent.match(/iPad/i);
    },
    Opera: function() {
        return navigator.userAgent.match(/Opera Mini/i);
    },
    Windows: function() {
        return navigator.userAgent.match(/IEMobile/i);
    },
    any: function() {
        return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());
    }
};






// affix nav bar to top of window as user scrolls
  
  $('document').ready(function(){
    $(window).scroll(function(){
      if($(this).scrollTop() > 1 ) {
        $('#resource-bar').addClass('f-nav2');
      }else {
        $("#resource-bar").removeClass('f-nav2');
      }
      if($(this).scrollTop() > 70) {
        $('#topnav').addClass('f-nav');
      } else {
        $('#topnav').removeClass('f-nav');
      }
    });
    if ($(window).width() < 901) {
      $(window).scroll(function(){
        if ($(this).scrollTop() > 100) {
          $('#topnav').addClass('f-nav');
        } else{
          $('#topnav').removeClass('f-nav');
        };
      });
    } else{};
  });




