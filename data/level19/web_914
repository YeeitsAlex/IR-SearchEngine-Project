   <script type="text/javascript">
    jQuery('#cres_content .bigMedTitle').each(function () {
       if (jQuery(this).html().trim().toLowerCase() == 'Contact us'.toLowerCase()) {
          this.innerHTML = "<a href='https://rice.giftlegacy.com/?VID=14573&DID=8739&pageID=21'>Contact us</a>";
          var ct = jQuery(this).next().find('a');
          if (ct.text().toLowerCase() == 'Contact us'.toLowerCase()){
            ct.replaceWith("contact us");
          }
       }
   });
   </script>
