  <script type="text/javascript">
   function addFrame(){ 
  if(document.getElementById("PresentsFrame")!=null) 
    return; 
  var bgObj; 
  try{ 
    bgObj = document.createElement(
      "<iframe id=\"PresentsFrame\" " +
      "name=\"PresentsFrame\" " +
      "style=\"z-index: 9999; position:fixed; overflow: hidden; border:1px solid #555; top: 50%; left: 50%; width: 800px; height: 600px; margin: 300px 0px 0px 400px;\" " +
     "scrolling=\"no\"></iframe>"
     ); 
  }
  catch(e){ 
    bgObj = document.createElement('iframe'); 
    bgObj.setAttribute('id','PresentsFrame'); 
    bgObj.setAttribute('name','PresentsFrame'); 
    bgObj.setAttribute('scrolling','no'); 
    bgObj.setAttribute('style','z-index: 9999; position:fixed; overflow: hidden; border:1px solid #555555; top: 50%; left: 50%; width: 800px; height: 600px; margin-top:-300px; margin-left:-400px;'); } 
    document.body.appendChild(bgObj); }
  </script>
