                <div class="view-content">
                 <div class="views-row views-row-1 views-row-odd views-row-first views-row-last">
                  <div about="/remedy-email" class="node node-article" id="node-127" typeof="sioc:Item foaf:Document">
                   <div class="node-headline clearfix">
                    <div class="node-title clearfix">
                     <a href="/remedy-email">
                      Remedy Email
                     </a>
                    </div>
                    <span class="rdf-meta element-hidden" content="Remedy Email" property="dc:title">
                    </span>
