<link href="https://cdn.jsdelivr.net/npm/zebra_datepicker/dist/css/bootstrap/zebra_datepicker.min.css" rel="stylesheet"/>
<script>
 $(document).ready(function(){
        $('.edit').click(function(e){
          e.preventDefault();
          var btn = e.currentTarget;
          var loading = '<i class="fa fa-refresh fa-spin" aria-hidden="true"></i> Loading, please wait...';
          if(btn.href != '')
          {
            $('#modal .modal-body').html(loading);
            $('#modal').foundation('open');
            var jqxhr = $.get(btn.href, function() {
              console.log('get form successful');
            }).done(function(data){
              $('#modal .modal-title').html(btn.title);
              $('#modal .modal-body').html(data);
              $('#modal .modal-footer').show();
              $('#modal input[type=file]').change(function(){
                  var label = $(this).prev();
                  label.text(this.files[0].name);
                  label.addClass('warning');
              });
              activateDatepicker();
            }).fail(function(){
              // on error, load target href
                window.location = btn.href;
            });
          } else {
            alert('button has no href!');
          }
        });

        $('.confirm').click(function(e){
          e.preventDefault();
          var btn = e.currentTarget;
          if(btn.href != '')
          {
              $('#modal .modal-title').html("Please confirm");

              if(btn.href.indexOf('destroy') == -1)
                  $('#modal .modal-body').html("<p>Are you sure you want to " + btn.title + "?</p><form action="+btn.href+" method='POST'><input type='hidden' name='_method' value='PATCH'></form>");
              else
                  $('#modal .modal-body').html("<p>Are you sure you want to " + btn.title + "?</p><form action="+btn.href+" method='POST'><input type='hidden' name='_method' value='DELETE'></form>");

              $('#modal #modal-submit').html("Continue");
              $('#modal .modal-footer').show();
              $('#modal').foundation('open');
          } else {
            alert('button has no href!');
          }
        });
                activateDatepicker();
      });
      var activateDatepicker = function() {
        $('[type="date"]').each(function(){
          if($(this).prop('type') != 'date') {
            $(this).Zebra_DatePicker({
              format: 'Y-m-d',
              open_on_focus: true,
              zero_pad: false,
            });
          }
        });
        $('[type="time"]').each(function(){
          if($(this).prop('type') != 'time') {
            $(this).Zebra_DatePicker({
              format: 'H:i',
              open_on_focus: true,
              show_clear_date: false
            });
          }
        });
      };
      var deactivateDatepicker = function() {
        $('[type="date"],[type="time"]').each(function(){
          var datepicker = $(this).data('Zebra_DatePicker');
          if(datepicker)
            datepicker.destroy();
        });
      };
</script>

