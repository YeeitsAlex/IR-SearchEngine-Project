      <div class="panel-pane pane-views-panes pane-calendar-pane-panel-pane-1 homepage-calendar" id="homepage-calendar">
       <h1 class="pane-title">
        Calendar
       </h1>
       <div class="view view-calendar-pane view-id-calendar_pane view-display-id-panel_pane_1 view-dom-id-302b11c8903f810bc5417366b8157543">
        <div class="view-content">
         <table class="views-table cols-0 table table-striped table-0">
          <tbody>
           <tr class="odd views-row-first">
            <td class="views-field views-field-field-event-date">
             <span class="date-display-single">
