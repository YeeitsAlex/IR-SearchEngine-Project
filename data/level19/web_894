<script language="javascript">
 $(document).ready(function() {
        $('.toggleSection').click( function (){
            $('.ToggleItem').slideToggle('700');
	    });
        
        $('#toggleAll').click( function (){
            $('.itemsList').slideToggle('700');
            $.each($('.item'), function() { 
                var currentitem = $(this);
                if ($(currentitem).hasClass('open')) {
                $(currentitem).removeClass( 'open' ).addClass( "close" ); 
                }
                else {
                    $(currentitem).removeClass( 'close' ).addClass( "open" );
                };
            });
        });
        
        function openClose(checkClass) {
            if ($(checkClass).hasClass('open')) {
                $(checkClass).removeClass( 'open' ).addClass( "close" ); 
                }
                else {
                    $(checkClass).removeClass( 'close' ).addClass( "open" );
                };
        }
    });
</script>
