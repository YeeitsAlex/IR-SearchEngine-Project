  <script>
   jQuery(document).ready(function($) {
    $('.tooltip[id^="tooltip_"]').each(function(){
       $(this).qtip({
          content: $('#data_' + $(this).attr('id')),
		  style: {
        	tip: false
			},
          position: {
          my: 'bottom center',
          at: 'top center',
          target: 'mouse'
      }
    });
  });
});
  </script>
