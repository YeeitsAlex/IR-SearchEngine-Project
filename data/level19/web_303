                <div class="photo-links-wrapper container">
                 <div class="row">
                  <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
                   <div class="photo-link-wrapper" data-delta="0" id="photo-link-wrapper-1-0">
                    <a class="round photo-link photo-link-icon" href="/language-programs/self-study">
                     <i class="babel-icon-student-classroom">
                     </i>
                    </a>
                    <a class="photo-link-caption" href="/language-programs/self-study">
                     Self Study Language Program
                    </a>
                   </div>
                  </div>
                  <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
                   <div class="photo-link-wrapper" data-delta="1" id="photo-link-wrapper-1-1">
                    <a class="round photo-link photo-link-icon" href="/vll">
                     <i class="babel-icon-vll">
                     </i>
                    </a>
                    <a class="photo-link-caption" href="/vll">
                     Virtual Language Lab
                    </a>
                   </div>
                  </div>
                  <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
                   <div class="photo-link-wrapper" data-delta="2" id="photo-link-wrapper-1-2">
                    <a class="round photo-link photo-link-icon" href="/services/resources">
                     <i class="babel-icon-resource">
                     </i>
                    </a>
                    <a class="photo-link-caption" href="/services/resources">
                     Resources
                    </a>
                   </div>
                  </div>
                 </div>
