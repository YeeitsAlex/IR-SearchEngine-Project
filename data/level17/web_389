// Stellar Parallax code

if( !isMobile.any() ) {

  jQuery(function(){
      jQuery.stellar({
      horizontalScrolling: false,
      responsive: false,
      parallaxBackgrounds: true
      });
  });

}

  jQuery(document).ready(function($){
  // browser window scroll (in pixels) after which the "back to top" link is shown
  var offset = 300,
    //browser window scroll (in pixels) after which the "back to top" link opacity is reduced
    offset_opacity = 1200,
    //duration of the top scrolling animation (in ms)
    scroll_top_duration = 700,
    //grab the "back to top" link
    $back_to_top = $('.cd-top');

  //hide or show the "back to top" link
  $(window).scroll(function(){
    ( $(this).scrollTop() > offset ) ? $back_to_top.addClass('cd-is-visible') : $back_to_top.removeClass('cd-is-visible cd-fade-out');
    if( $(this).scrollTop() > offset_opacity ) {
      $back_to_top.addClass('cd-fade-out');
    }
  });

  //smooth scroll to top
  $back_to_top.on('click', function(event){
    event.preventDefault();
    $('body,html').animate({
      scrollTop: 0,
      }, scroll_top_duration
    );
  });

});
$('img').parent('a').css('outline','none');
</script>
<script>
 // Google Analytics
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-2249859-1', 'auto');
  ga('send', 'pageview');
  ga('require', 'displayfeatures');
</script>
<script>
 $(document).ready(function(){
  var overCallBack = function(){$(this).find('.dropdown').fadeIn(70);$(this).find('a[data-dropdown]').addClass('nav-active');};
  var outCallBack = function(){$(this).find('.dropdown').fadeOut(350);$(this).find('a').removeClass('nav-active');};

  var intentSettings  = {
    sensitivity: 2,
    interval: 100,
    timeout: 350,
    over: overCallBack,
    out: outCallBack
  };

  $('.mobnav li.nav-item a').removeAttr('data-dropdown');
  $('li.nav-item a').click(function(){
   $(this).parent().removeClass('nav-item');
});

   $('li.nav-item').hoverIntent(intentSettings);
   $('.mobnav li.nav-item a').removeAttr('data-dropdown');
  $('li.nav-item a').click(function(){
  if($(this).hasClass('nav-active')) {
  location.assign($(this).attr('href'));
  }
});

});
if (!isMobile.any()) {
    $('nav>.navrow').accessibleMegaMenu();
};
$('a[data-dropdown]').click(function(event){
  if (isMobile.any() && $(window).width() > 900) {
    if ($(this).hasClass('nav-active')) {
      console.log("Yes we're open");
    location.assign($(this).attr('href'));
    } else{
      console.log('we are closed');
      event.preventDefault();
    };
  } else{
    console.log('you are still on desktop');
  };
});
if(isMobile.any()) {
  $('*:not(#topnav)').click(function(){
    $(".dropdown").css('display','none');
  });
}
</script>
<script>
 $(document).ready(function(){
  if (isMobile == true) {
    $('#research-feed-burn').remove();
  }
  $.ajax({
    type: "GET",
    url: "https://news.rice.edu/tag/research/feed/?cat=-4",
    dataType: "xml",
    success: function(xml) {

      $(xml).find('item:lt(7)').each(function(){
        var title = $(this).find('title').text();
        var link = $(this).find('link').text();
        var image = $(this).find('image').text();
        var alt = $(this).find('alt').text();

        $('<li></li>').html("<div class=thumb><img src="+image+" alt='"+alt+"'></div><a href="+link+"/#>"+title+"</a>").appendTo("#research-feed");
      });
},
    error: function() {
      $('#research-feed').css('display','none');
      $('#research-feed-burn').css('display','block');
}
  });

});
</script>
<script src="/_scripts/carousels2.js" type="text/javascript">
</script>
<script>
 $('#old-browser').on('click',function(){
    $(this).css('display','none');
  });
</script>
