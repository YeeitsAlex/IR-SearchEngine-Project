   <div class="timing_details">#{details}</div>
  </script>
  <script id="glide:page_timing_detail_line" type="application/xml">
   <div class="timing_detail_line #{has_children}" data-children="#{child_count}">#{HTML:name}: #{ms}
         #{children}</div>
  </script>
