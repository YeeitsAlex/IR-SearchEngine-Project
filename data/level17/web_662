 <div class="k2t-content no-sidebar" style="">
  <div class="container k2t-wrap">
   <!-- Main -->
   <main class="k2t-main page" role="main">
    <article class="post-24 page type-page status-publish hentry" id="post-24">
     <div class="page-entry">
      <div id="ld_courses_listing">
       <div class="vc_row wpb_row vc_row-fluid">
        <div class="wpb_column vc_column_container vc_col-sm-12">
         <div class="vc_column-inner ">
          <div class="wpb_wrapper">
           <div class="wpb_text_column wpb_content_element ">
            <div class="wpb_wrapper">
             <link href="http://fonts.googleapis.com/css?family=Open+Sans:800%2C400" media="all" property="stylesheet" rel="stylesheet" type="text/css"/>
             <div class="rev_slider_wrapper fullwidthbanner-container" data-source="gallery" id="rev_slider_1_1_wrapper" style="margin:0px auto;background:transparent;padding:0px;margin-top:0px;margin-bottom:0px;">
              <!-- START REVOLUTION SLIDER 5.4.7.1 fullwidth mode -->
              <div class="rev_slider fullwidthabanner" data-version="5.4.7.1" id="rev_slider_1_1" style="display:none;">
               <ul>
                <!-- SLIDE  -->
                <li data-description="" data-easein="default" data-easeout="default" data-hideafterloop="0" data-hideslideonmobile="off" data-index="rs-50" data-link="https://youtu.be/Jbr1HoanJic" data-masterspeed="300" data-param1="" data-param10="" data-param2="" data-param3="" data-param4="" data-param5="" data-param6="" data-param7="" data-param8="" data-param9="" data-rotate="0" data-saveperformance="off" data-slotamount="default" data-target="_blank" data-thumb="http://engineering.uic.edu/wp-content/uploads/2019/01/FESP-100x50.jpg" data-title="Slide" data-transition="fade">
                 <!-- MAIN IMAGE -->
                 <img alt="Student working in lab" class="rev-slidebg" data-bgfit="cover" data-bgposition="center center" data-bgrepeat="no-repeat" data-lazyload="http://engineering.uic.edu/wp-content/uploads/2019/01/FESP.jpg" data-no-retina="" height="632" src="http://engineering.uic.edu/wp-content/plugins/revslider/admin/assets/images/dummy.png" title="FESP" width="1200"/>
