       <script type="text/javascript">
        $(document).ready(function () {
        $("#caps").hide();
        var options = {
            caps_lock_on: function () {
                $("#caps_lock_status").text("You have your Caps Lock on.");
                $("#caps").show();
            },
            caps_lock_off: function () {
                $("#caps_lock_status").text(" ");
                $("#caps").hide();
            }
        };

        $("input[type='password']").capslock(options);

        $('#ctl00_ContentPlaceHolder1_Logon1_btnAuthenticate').click(function (event) {
            var str = $('#ctl00_ContentPlaceHolder1_Logon1_txtPassword').val();
            if (inputHasInvalidChars(str)) {
                displayClientError();
                event.preventDefault();
            }
        });



        function displayClientError(visible) {
            if (visible == true) {
                $('#pnlBadChar').show();
            }
            else {
                $('#pnlBadChar').hide();
            }
        };

        function inputHasInvalidChars(str) {
            //console.log("string passed = " + str);
            return /[<>]/.test(str);
        };
    });
       </script>
