        <script>
         $(document).ready(function(){
      $(".gateway-1 > div").hide()
      var numFacts = $(".gateway-1 .factoid").length // number of factoids
      var factsShowing = [] // selected indexes of factoids
      var html = ""
      // loop through 1-3, increment manually 
      for(i=0;i<3;){
      // generate random number, if that number hasn't been picked yet show the factoid at that number
      var next = (Math.floor(Math.random() * numFacts)) // generate number between  0 - max index (1 less than numFacts)
      if(factsShowing.indexOf(next) == -1)
      {	
      factsShowing.push(next) // add to selected list
      i++; // increment if valid random index
      }
      }
      
      // re-assign numerical index array to objects 
      factsShowing[0] = $(".gateway-1 .factoid:eq(" + factsShowing[0] +")")
      factsShowing[1] = $(".gateway-1 .factoid:eq(" + factsShowing[1] +")")
      factsShowing[2] = $(".gateway-1 .factoid:eq(" + factsShowing[2] +")")
      
      // in backwards order, push to front to display in order selected
      for(i=2;i>=0;i--){
      $(".gateway-1 ").prepend(factsShowing[i].show())
      }
      });
        </script>
