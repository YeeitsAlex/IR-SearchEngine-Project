   <!-- shows/hides form elements -->
   <script type="text/javascript">
    $(document).ready(function(){
        
        //Hide div w/id extra_rooms
       $("#extra_rooms").css("display","none");
        // Add onclick handler to checkbox w/id checkme_rooms
       $("#checkme_rooms").click(function(){
        // If checked
        if ($("#checkme_rooms").is(":checked"))
        {
            //show the hidden div
            $("#extra_rooms").show("fast");
        }
        else
        {      
            //otherwise, hide it 
            $("#extra_rooms").hide("fast");
        }
      });
	  
	  
	  //Repeat for the extra_venues
	   //Hide div w/id extra_venues
       $("#extra_venues").css("display","none");
        // Add onclick handler to checkbox w/id checkme_venues
       $("#checkme_venues").click(function(){
        // If checked
        if ($("#checkme_venues").is(":checked"))
        {
            //show the hidden div
            $("#extra_venues").show("fast");
        }
        else
        {      
            //otherwise, hide it 
            $("#extra_venues").hide("fast");
        }
      });
    
    });
   </script>
