     <script type="text/javascript">
      jQuery.noConflict();
			jQuery(document).ready(function($) {
				$('.accordion-toggle').bind("click touchstart", function(e) {
					$(this).parent().siblings('.home-accordion-content').slideToggle();
					$(this).toggleClass('open');
					if($(this).hasClass('open')) {
						$(this).attr('aria-expanded', true);
					} else {
						$(this).attr('aria-expanded', false);
					}
					e.preventDefault();
				});

				var firstVideoId = "";
				var currentIndex = 0;
				var videoPlayers = videojs.getAllPlayers();
				
				videoPlayers.forEach(function(player, i, object) {
					if(player.id() == "hero-video-1") {
						object.splice(i, 1);
					} else {
						if(firstVideoId === "") {
							firstVideoId = player.id();
							startNextVideo(player);
						} else {
							player.pause();
						}
					}
				});

				function startNextVideo(player) {
					player = videojs(player.id());

					if(player.paused()) {
						player.play();
					}
					player.on('ended', function() {					
						if(currentIndex < videoPlayers.length - 1) {
							currentIndex++;
						} else {
							currentIndex = 0;
						}

						$('#'+this.id()).parent().toggle();
						$('#'+videoPlayers[currentIndex].id()).parent().toggle();

						// reset current video and remove 'ended' listener
						this.pause();
						this.currentTime(0);
						this.off('ended');

						startNextVideo(videoPlayers[currentIndex]);
					});
				}
			});
     </script>
    </div>
