  <script type="text/javascript">
   jQuery.noConflict();
	function runMatchHeightContent() {
		jQuery('.match-height').matchHeight();

		if (570 < jQuery(window).width()) {
			jQuery('.match-height-tablet').matchHeight();
		}
	}

    function doOnResize(f,min=0,max=10000){
        $docWidth=jQuery( document ).width();
        $docWidth=(((min==0)&&(max==0))||((min<=$docWidth)&&(max>=$docWidth)));
            if($docWidth){ f(); }
            jQuery( window ).resize(function() {
                if($docWidth){
                    f();
                }
            });
    };

	function scrollPageTo($item, $time, $offset) {
		if($item.length > 0) {
        	jQuery('html, body').animate( { "scrollTop" : $item.offset().top-$offset.outerHeight() }, {
            	duration:$time,
            	step: function(now, fx) {
                	fx.end=$item.offset().top-$offset.outerHeight();
            	}
        	});
        }
    }

    jQuery(window).on('load', function () {
        if(jQuery(".post-template-default").length) {
            $offset=jQuery('.section-related-news .figure-with-img').first().height()/2;
            jQuery('.section-related-news-container').css('margin-top',$offset + 100);
            jQuery('.section-related-news .figure-with-img').css('margin-top',(0-$offset));
        }

        if(jQuery(".page-template-default").length) {
            $offset=jQuery('.section-related-news .figure-with-img').first().height()/2;
            jQuery('.section-related-news-container').css('margin-top',$offset + 100);
            jQuery('.section-related-news .figure-with-img').css('margin-top',(0-$offset));
        }

        if(jQuery(".page-template-page-centers").length) {
            $offset=jQuery('.section-related-news .figure-with-img').first().height()/2;
            jQuery('.section-related-news-container').css('margin-top',$offset + 100);
            jQuery('.section-related-news .figure-with-img').css('margin-top',(0-$offset));
        }    

        if(jQuery(".page-template-page-research-main-php").length) {
            $offset=jQuery('.section-related-news .figure-with-img').first().height()/2;
            jQuery('.section-related-news').css('margin-top',(0-$offset));
        }
        
        if(jQuery(".page-template-page-research-disorder-details").length) {
            $offset=jQuery('.section-related-news .figure-with-img').first().height()/2;
            jQuery('.section-related-news .related-thumb').css('margin-top',(0-$offset));
            jQuery('.section-related-news').prev().css('padding-bottom',$offset);
        }
    });

	jQuery(document).ready(function($) {
		runMatchHeightContent();


        if ( $( ".section-publications-pubmed" ).length ){
            $( ".section-publications-pubmed a" ).each(function(  ) {
                if ($( this ).text().startsWith("PMID") || $( this ).text().startsWith("PMC")) {
                    $( this ).attr('target', '_blank');
                }
            })
        }

        $(".mio-button").click(function() {
            console.log('vasia');
            $(".mobile-instructions-overlay").addClass("mobile-instructions-hidden");
        });

		$(window).on('resize', function() {
			runMatchHeightContent();
		});

		$(document).ajaxSuccess(function() {
			setTimeout(function() {
	            if (570 < $(window).width()) {
			  		$('.match-height-tablet').matchHeight('remove').matchHeight();
				}
            }, 750);
		});

		if (570 < $(window).width() && $(window).width() < 1025) {
			$('.profile-hero-media').height(window.innerHeight + 'px');
		}

		$('.title-read-more a').bind("click touchstart", function(e) {
			$parent = $(this).parent('.title-read-more');
			$parent.next().slideToggle();
			$(this).toggleClass('open');
			if($(this).hasClass('open')) {
				$(this).attr('aria-expanded', true);
				$(this).find('.label').html('Read Less');
			} else {
				$(this).attr('aria-expanded', false);
                $(this).find('.label').html('Read More');
			}
			e.preventDefault();
		});

		$('.entry-content table.responsive-table').basictable({
		    breakpoint: 570
		});

		if (570 < $(window).width()) {
	  		$('.match-height-tablet').matchHeight();
		}

        $("#search-form-toggle").click(function() {
            $(this).attr("aria-expanded","true");
            $(this).toggle();

            $(".utility-menu-wrapper .search-form").toggleClass('active');
            $(".utility-menu-wrapper .search-field").animate({
                width: 90,
                'padding-top' : 2,
                'padding-right' : 6,
                'padding-bottom' : 2,
                'padding-left' : 6,
            },500);

            setTimeout(function(){
                $(".utility-menu-wrapper .search-field").focus();
            }, 400);
        });

        $("#filter-form-toggle").click(function() {
            $(this).attr("aria-expanded","true");
            $(".close-filter-form").attr("aria-expanded","true");

            $("#filter-form-toggle").hide();

            $(".filter-form").toggleClass('active');
        });

        $(".close-filter-form").click(function() {
            $("#filter-form-toggle").attr("aria-expanded","false");
            $(this).attr("aria-expanded","false");

            $("#filter-form-toggle").show();

            $(".filter-form").removeClass('active');
        });



		$('.frm_checkbox input').change(function(){
            if($(this).is(":checked")) {
                $(this).parent().addClass('checked');
            }else{
                $(this).parent().removeClass('checked');
            }
        });

        $( ".frm_radio input,.frm_checkbox input" ).focusin(function() {
            $(this).parent().addClass('focused');
        });
        $( ".frm_radio input,.frm_checkbox input" ).focusout(function() {
            $(this).parent().removeClass('focused');
        });



        function setRadioStyle(el){
            el.parent().find('.frm_radio').each(function(){
                var input=$(this).find('input[type="radio"]');
                if(input.is(":checked")) {
                    input.parent().addClass('checked');
                }else{
                    input.parent().removeClass('checked');
                }
            });
        }
        $('.frm_radio').click(function(){
            setRadioStyle($(this));
        });
        $('.frm_radio').each(function(){
            setRadioStyle($(this));
        });

        
        $('.anchor-link').click(function(){
            if($(this).attr("href")){
                var goto=$(this).attr('href');
                $(this).attr('goto',goto);
                $(this).removeAttr('href');
            }else{
                var goto=$(this).attr('goto');}
                if(goto){
                    scrollPageTo($(goto),1000,$('header.site-header'));
            }return false;});


        doOnResize(function(){
            if($(".page-template-page-research-main-php").length) {
                $offset=$('.section-related-news .figure-with-img').first().height()/2;
                $('.section-related-news').css('margin-top',(0-$offset));
            }

            if ($(".post-template-default").length) {
                $offset = $('.section-related-news .figure-with-img').first().height() / 2;
                $('.section-related-news-container').css('margin-top', $offset + 100);
                $('.section-related-news .figure-with-img').css('margin-top', (0 - $offset));

                if (($('.single-post-media').length) && ($('.slider').length)) {
                    let c=0.7;
                    var newSize = $('.single-post-media').outerHeight() * c;
                    var oldSize = 568;
                    if($(window).width()>570){
                        if (($('.single-post-media').outerHeight() * c) < oldSize) {
                            $('.slider').css('height', newSize);
                            $('.slider img').css('max-height', newSize);
                        } else {
                            $('.slider').css('height', oldSize);
                            $('.slider img').css('max-height', oldSize);
                        }
                    }else{
                        $('.slider').css('height', '');
                        $('.slider img').css('max-height', '');
                    }
                }
            }

            if($(".page-template-default").length) {
                $offset=$('.section-related-news .figure-with-img').first().height()/2;
                $('.section-related-news-container').css('margin-top',$offset + 100);
                $('.section-related-news .figure-with-img').css('margin-top',(0-$offset));
            }

            if($(".page-template-page-centers").length) {
                $offset=$('.section-related-news .figure-with-img').first().height()/2;
                $('.section-related-news-container').css('margin-top',$offset + 100);
                $('.section-related-news .figure-with-img').css('margin-top',(0-$offset));
            }

            if($(".page-template-page-research-disorder-details").length) {
                $offset=$('.section-related-news .figure-with-img').first().height()/2;
                $('.section-related-news .related-thumb').css('margin-top',(0-$offset));
                $('.section-related-news').prev().css('padding-bottom',$offset);
            }

            if($(".single-post-media .col-single-post-media video-js").length) {
                var o=$('.col-single-post-media .slider');
                $(".col-single-post-media video-js").height(o.height()).width(o.width());
            }


        });
    });
  </script>
