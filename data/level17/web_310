<script src="https://www.google.com/jsapi?key=ABQIAAAAPrxZwyLH1quGPzcQTSnt2xRO9V8f_wvZ8Pu_Kp1GhO5Ipa7J1hQqfY_snZQNW_9WZZCcW6D_cJ_fSw" type="text/javascript">
</script>
<script type="text/javascript">
 google.load("feeds", "1");
    
     function OnLoad() {
      var feed = new google.feeds.Feed("http://www.utk.edu/events/rss/feed.php?l=18");
      feed.setNumEntries(5);
      feed.load(function(result) {
      if (!result.error) {
      var container = document.getElementById("feed");
      for (var i = 0; i < result.feed.entries.length; i++) {
        var entry = result.feed.entries[i];
        var li = document.createElement("li");
        var a = document.createElement("a");
        var url = entry.link;
        a.appendChild(document.createTextNode(entry.title));
        a.setAttribute('href', url);
        container.appendChild(li);
        li.appendChild(a);
      }
      }
      });
    }
    google.setOnLoadCallback(OnLoad);
</script>
