            <div id="ctl00_PlaceHolderMain_UIVersionedContent6_RichHtmlField1_label" style="display:none">
             Page Content
            </div>
            <div aria-labelledby="ctl00_PlaceHolderMain_UIVersionedContent6_RichHtmlField1_label" class="ms-rtestate-field" id="ctl00_PlaceHolderMain_UIVersionedContent6_RichHtmlField1__ControlWrapper_RichHtmlField" style="display:inline">
             <div class="ms-rtestate-read ms-rte-wpbox">
              <div class="ms-rtestate-notify ms-rtestate-read dfc7e650-e704-408c-8f5c-370a14c23413" id="div_dfc7e650-e704-408c-8f5c-370a14c23413" unselectable="on">
               <table cellpadding="0" cellspacing="0" style="width:100%">
                <tr>
                 <td class="s4-wpcell-plain " id="MSOZoneCell_WebPartWPQ4" valign="top">
                  <table border="0" cellpadding="0" cellspacing="0" class="s4-wpTopTable " width="100%">
                   <tr>
                    <td valign="top">
                     <div allowdelete="false" allowremove="false" class="ms-WPBody " haspers="false" id="WebPartWPQ4" style="" webpartid="b39d2bdb-471f-4b95-88e1-cec3654bc11e" webpartid2="dfc7e650-e704-408c-8f5c-370a14c23413" width="100%">
                      <style type="text/css">
                       @import url("/scripts/responsive/css/home.css");
                      </style>
                      <script src="/scripts/responsive/js/jquery.flexslider.js" type="text/javascript">
                      </script>
                      <script type="text/javascript">
                       $(document).ready(function() {
      var soapEnv = "<soapenv:Envelope xmlns:soapenv='http://schemas.xmlsoap.org/soap/envelope/'> \
          <soapenv:Body> \
            <GetListItems xmlns='http://schemas.microsoft.com/sharepoint/soap/'> \
              <listName>ContentSlider</listName> \
              <viewFields> \
                <ViewFields> \
                  <FieldRef Name='Picture'/>\
		<FieldRef Name='Caption'/> \
                </ViewFields> \
              </viewFields> \
            </GetListItems> \
          </soapenv:Body> \
        </soapenv:Envelope>";

      $.ajax({
        url: "../_vti_bin/lists.asmx",
        type: "POST",
        dataType: "xml",
        data: soapEnv,
        complete: renderSlides,
        contentType: "text/xml; charset=\"utf-8\""
      });
    });

   <!-- Append slides capsuled in LI elements -->
    function renderSlides(xData, status) 
    {	
      $(xData.responseXML).find("z\\:row, row").each(function() {
	var url = $(this).attr("ows_Picture");
	if (url.indexOf(',') > 0)
	{
	url = url.substring(0, url.indexOf(','));
	
	}

	var caption = $(this).attr("ows_Caption");

if ( !caption ) 
     { var liHtml = "<li><img src=" + url + " width=100%></li>";}

else  { var liHtml = "<li><img width=100% src=" + url + "><p class=flex-caption>"  + $(this).attr("ows_Caption")  + "</p></li>";}
        $(".slides").append(liHtml);
    });

$('.flexslider').flexslider();
			  
  }
                      </script>
                      <div class="flexslider">
                       <ul class="slides">
                       </ul>
                      </div>
                     </div>
                    </td>
                   </tr>
                  </table>
                 </td>
                </tr>
               </table>
              </div>
              <div id="vid_dfc7e650-e704-408c-8f5c-370a14c23413" style="display:none;" unselectable="on">
              </div>
             </div>
             <div>
             </div>
             <div>
              <p class="line-height: 24px; ms-rteForeColor-3" style="line-height:24px;font-size:11pt;">
               <strong>
                We hope that you will
               </strong>
               <a href="https://advanceutia.com/gala2019">
                <strong>
                 join hosts Steve and Ann Bailey
                </strong>
               </a>
               <strong>
                at our spectacular annual gala on Friday, April 26, in Knoxville. Our 2019 Gala theme is Conifers and Cocktails, and we will recognize Alan Solomon as an avid collector of conifers. The memorable evening will have music, drinks, hors d'oeuvres, dinner and live and silent auctions to benefit our beautiful State Botanical Garden.
               </strong>
              </p>
              <p class="line-height: 24px;" style="line-height:24px;font-size:11pt;">
               <img alt="" src="/utg/PictureLibrary/gardenlogo-sm.png" style="margin:5px;width:364px;"/>
               <br/>
               Recognized in 2013 as the official botanical gardens of the state of Tennessee, the University of Tennessee Gardens located in Knoxville,
               <a href="http://west.tennessee.edu/ornamentals/default.asp" target="_blank" title="University of Tennessee Gardens in Jackson, Tennessee">
                Jackson
               </a>
               and
               <a href="/plateaugardens/Pages/default.aspx">
                Crossvil
                <span>
                 le
                </span>
               </a>
               are part of the UT Institute of Agriculture. Some 4,000 annuals, perennials, herbs, tropical plants, trees, shrubs, vegetables and ornamental grasses are evaluated each year. Both the Knoxville and Jackson gardens are TN Certified Arboreta. The gardens are open during all seasons and are free to the public.
              </p>
              <div class="ms-rtestate-read ms-rte-wpbox">
               <div class="ms-rtestate-notify ms-rtestate-read cd543bbb-9016-421e-9c0d-9ab57c382e0d" id="div_cd543bbb-9016-421e-9c0d-9ab57c382e0d" unselectable="on">
               </div>
               <div id="vid_cd543bbb-9016-421e-9c0d-9ab57c382e0d" style="display:none;" unselectable="on">
               </div>
              </div>
              <div class="ms-rtestate-read ms-rte-wpbox">
