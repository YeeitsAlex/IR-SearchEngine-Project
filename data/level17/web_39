              <div class="ut-col ut-span4-12" id="PageContent3">
               <div id="ctl00_PlaceHolderMain_UIVersionedContent6_ctl01_label" style="display:none">
                Page Content 3
               </div>
               <div aria-labelledby="ctl00_PlaceHolderMain_UIVersionedContent6_ctl01_label" class="ms-rtestate-field" id="ctl00_PlaceHolderMain_UIVersionedContent6_ctl01__ControlWrapper_RichHtmlField" style="display:inline">
                <img alt="Heading for the Garden Calendar Heading" src="/utg/PictureLibrary/gardencalendar_1.png" style="margin:0px;width:100%;"/>
                <div class="ms-rtestate-notify ms-rtestate-read cb1484be-a76e-496b-9d00-e64ca236680b" id="div_cb1484be-a76e-496b-9d00-e64ca236680b" unselectable="on">
                 <table cellpadding="0" cellspacing="0" style="width:100%">
                  <tr>
                   <td class="s4-wpcell-plain " id="MSOZoneCell_WebPartWPQ3" valign="top">
                    <table border="0" cellpadding="0" cellspacing="0" class="s4-wpTopTable " width="100%">
                     <tr>
                      <td valign="top">
                       <div allowdelete="false" allowremove="false" class="ms-WPBody " haspers="false" id="WebPartWPQ3" style="" webpartid="00519123-a83d-4ece-b84b-0df0912cdec9" webpartid2="cb1484be-a76e-496b-9d00-e64ca236680b" width="100%">
                        <script src="/scripts/jquery.SPServices-2013.02a.min.js" type="text/javascript">
                        </script>
                        <script type="text/javascript">
                         // change listName to fit calendar name on site
$(document).ready(function() {
function setMaxHeight(){
	 $('.calUpcomingEvents .item').css("height", "auto");
	var height= 0;
	 $('.calUpcomingEvents').find('.right:visible').each(function () {
	console.log($(this).outerHeight());
        if (height < $(this).outerHeight()) height = $(this).outerHeight();
        
    
    });   
	
    // Set the height for the element wrap.
    $('.calUpcomingEvents .item').css("height", (height) + "px");
	
    // Unset height
    height = 0;


}
$(window).load(function(){setMaxHeight();})
$().SPServices({
    operation: "GetListItems",
    async: false,
    listName: "Calendar of Events",
    CAMLRowLimit: 7,
    CAMLViewFields: "<ViewFields>" +
            "<FieldRef Name='Title' />" +
            "<FieldRef Name='EventDate' />" +
            "<FieldRef Name='EndDate' />" +
            "<FieldRef Name='Location' />" +
            "<FieldRef Name='Description' />" +
            "<FieldRef Name='fRecurrence' />" +
            "<FieldRef Name='RecurrenceData' />" +
            "<FieldRef Name='fAllDayEvent' />" +
            "<FieldRef Name='EncodedAbsUrl' />" +
            "<FieldRef Name='ID' />" +
        "</ViewFields>",
    CAMLQuery: '<Query>' +
            '<Where>' +
                '<DateRangesOverlap>' +
                    '<FieldRef Name="EventDate" />' +
                    '<FieldRef Name="EndDate" />' +
                    '<FieldRef Name="RecurrenceID" />' +
                    '<Value Type="DateTime">' +
                        '<Now />' +
                    '</Value>' +
                '</DateRangesOverlap>' +
            '</Where>' +
            '<OrderBy>' +
                '<FieldRef Name="EventDate" />' +
            '</OrderBy>' +
        '</Query>',
    CAMLQueryOptions: '<QueryOptions>' +
            '<RecurrencePatternXMLVersion>v3</RecurrencePatternXMLVersion>' +
            '<ExpandRecurrence>TRUE</ExpandRecurrence>' +
        '</QueryOptions>',
    completefunc: function (xData, Status) {
    
		// define our variables before the looping begins
		var tempMonth;
		var tempDay; //if you need it - Sunday, Monday,Tuesday,etc
		//define monthNames  (the var statement should all be on one line)
		var monthName=["January","February","March","April","May","June","July","August","September","October","November","December"]; 
		// **  remember - January will be monthName[0] NOT monthName[1] ** 
		// if you want January to be month1 then, add "", before "January"
		
		// format time from 24hr to 12hr
		var getFormattedTime = function (fourDigitTime){
    		var hours24 = parseInt(fourDigitTime.substring(0,2), 10);
    		var hours = ((hours24 + 11) % 12) + 1;
    		var amPm = hours24 > 11 ? ' pm' : ' am';
    		var minutes = fourDigitTime.substring(2);

    		return hours + minutes + amPm;
			};

       	// function for returning each row
       	$(xData.responseXML).SPFilterNode("z:row").each(function() {
      	
      		// split date/time up to usuable parts
       		var ddd = ($(this).attr("ows_EventDate").split(" ")[0]);
       		var ttt = ($(this).attr("ows_EventDate").split(" ")[1]);
			tArr = ttt.split(":");		
			dArr = ddd.split("-");  // ex input "2013-12-30" ;
       		var isodd= dArr[1] + "/" + dArr[2] + "/" +dArr[0];
			var isott = tArr[0] + ":" + tArr[1];		
        	var idd= new Date(isodd);   
        
        	var dd    = idd.getDate();
        	var mm = idd.getMonth(); //January is 0!
        	var allday = $(this).attr("ows_fAllDayEvent");
        	if (allday == 1) {
         	var newtime = "All Day Event"
         	}else{
            	var newtime = getFormattedTime(isott);
				}
        	
			// get Month name and Day
			tempMonth = monthName[mm]; //this will store the MonthName
			tempDay = dd;
			
			// get URL for the link to item
			var url = $(this).attr("ows_EncodedAbsUrl") + "";
			if(($(this).attr("ows_EncodedAbsUrl") + "").match(/000$/))
				{
					url = url.substring(0, url.lastIndexOf("/", url.length));
					url = url + "/DispForm.aspx?ID=" + $(this).attr("ows_ID");
				}
			var link = "<a href='" + url + "'><span class='title'>" + $(this).attr("ows_Title") + "</span></a>";       
		
			// if field is undefined then leave it blank
			if (typeof($(this).attr("ows_Location")) == 'undefined') {
			var Location = "";
			}else{ 
			var Location = "Location: "  + $(this).attr("ows_Location");
				}
        	
        	// write out html 
	  		var Title = $(this).attr("ows_Title");
	    	var liHtml = "<div class='dfwp-item'><div class='xslCalendar' xmlns:ddwrt='http://schemas.microsoft.com/WebParts/v2/DataView/runtime'>" +
	   		"<div class=item link-item'  style='min-height:50px'><div class='description xslCal'><div class='left'><span class='month'>" + tempMonth + "</span><span class='day'>" + 
	   		dd + "</span></div><div class='right'>" + link +"<span class='location'>" + Location + "</span><span class='time'>Time: " + newtime + "</span></div></div></div></div></div>";	
	   
	   
	  		$(".calUpcomingEvents, dfwp-list").append(liHtml);
	   
         });
    }
});
});
                        </script>
                        <div class="calUpcomingEvents dfwp-list" style="width:100%">
                        </div>
                       </div>
                      </td>
                     </tr>
                    </table>
                   </td>
                  </tr>
                 </table>
                </div>
                <div id="vid_cb1484be-a76e-496b-9d00-e64ca236680b" style="display:none;" unselectable="on">
                </div>
               </div>
              </div>
              <div class="ut-col ut-span4-12" id="PageContent4">
               <div id="ctl00_PlaceHolderMain_UIVersionedContent6_ctl02_label" style="display:none">
                Page Content 4
               </div>
               <div aria-labelledby="ctl00_PlaceHolderMain_UIVersionedContent6_ctl02_label" class="ms-rtestate-field" id="ctl00_PlaceHolderMain_UIVersionedContent6_ctl02__ControlWrapper_RichHtmlField" style="display:inline">
                <img alt="Heading for the Garden Tips Column" src="/utg/PictureLibrary/gardentips_1.png" style="margin:0px;width:100%;"/>
                <div class="ms-rtestate-read ms-rte-wpbox">
                 <div class="ms-rtestate-notify ms-rtestate-read 93f40ef7-c625-4377-910e-0dca12de3aa4" id="div_93f40ef7-c625-4377-910e-0dca12de3aa4" unselectable="on">
                  <table cellpadding="0" cellspacing="0" style="width:100%">
                   <tr>
                    <td class="s4-wpcell-plain " id="MSOZoneCell_WebPartWPQ6" valign="top">
                     <table border="0" cellpadding="0" cellspacing="0" class="s4-wpTopTable " width="100%">
                      <tr>
                       <td valign="top">
                        <div allowdelete="false" allowremove="false" class="ms-WPBody " haspers="false" id="WebPartWPQ6" style="" webpartid="2b5bfeb9-d957-417c-a342-97880314440a" webpartid2="93f40ef7-c625-4377-910e-0dca12de3aa4" width="100%">
                         <div class="cbq-layout-main" id="cbqwpctl00_ctl30_g_93f40ef7_c625_4377_910e_0dca12de3aa4">
                          <ul class="dfwp-column dfwp-list" style="width:100%">
                           <li class="dfwp-item">
                            <div class="item" xmlns:ddwrt="http://schemas.microsoft.com/WebParts/v2/DataView/runtime">
                             <div class="link-item">
                              <a href="/_layouts/CopyUtil.aspx?Use=id&amp;Action=dispform&amp;ItemId=1&amp;ListId=fb8106d9-8a01-4743-ba51-d468944ff11c&amp;WebId=f5a9cd01-3f67-4d67-a6c5-9af16e12a112&amp;SiteId=e1537950-3692-4b51-a8be-e94485c3ae88&amp;Zone=&amp;Source=https%3A%2F%2Fag%2Etennessee%2Eedu%2Futg%2FPages%2Fdefault%2Easpx" title="">
                               March #1
                              </a>
                              <div class="description">
                               Evaluate your vegetable garden plans. Often a smaller garden with fewer weeds and insects will give you more produce.
                              </div>
                             </div>
                            </div>
                           </li>
                           <li class="dfwp-item">
                            <div class="item" xmlns:ddwrt="http://schemas.microsoft.com/WebParts/v2/DataView/runtime">
                             <div class="link-item">
                              <a href="/_layouts/CopyUtil.aspx?Use=id&amp;Action=dispform&amp;ItemId=2&amp;ListId=fb8106d9-8a01-4743-ba51-d468944ff11c&amp;WebId=f5a9cd01-3f67-4d67-a6c5-9af16e12a112&amp;SiteId=e1537950-3692-4b51-a8be-e94485c3ae88&amp;Zone=&amp;Source=https%3A%2F%2Fag%2Etennessee%2Eedu%2Futg%2FPages%2Fdefault%2Easpx" title="">
                               March #2
                              </a>
                              <div class="description">
                               Broccoli, cabbage, Brussel sprouts, lettuce, chard, onions and potatoes should be planted this month.
                              </div>
                             </div>
                            </div>
                           </li>
                           <li class="dfwp-item">
                            <div class="item" xmlns:ddwrt="http://schemas.microsoft.com/WebParts/v2/DataView/runtime">
                             <div class="link-item">
                              <a href="/_layouts/CopyUtil.aspx?Use=id&amp;Action=dispform&amp;ItemId=3&amp;ListId=fb8106d9-8a01-4743-ba51-d468944ff11c&amp;WebId=f5a9cd01-3f67-4d67-a6c5-9af16e12a112&amp;SiteId=e1537950-3692-4b51-a8be-e94485c3ae88&amp;Zone=&amp;Source=https%3A%2F%2Fag%2Etennessee%2Eedu%2Futg%2FPages%2Fdefault%2Easpx" title="">
                               March #3
                              </a>
                              <div class="description">
                               When night temps get above 40 degrees, feed your pansies with a water-soluble fertilizer such as 20-20-20.
                              </div>
                             </div>
                            </div>
                           </li>
                           <li class="dfwp-item">
                            <div class="item" xmlns:ddwrt="http://schemas.microsoft.com/WebParts/v2/DataView/runtime">
                             <div class="link-item">
                              <a href="/_layouts/CopyUtil.aspx?Use=id&amp;Action=dispform&amp;ItemId=4&amp;ListId=fb8106d9-8a01-4743-ba51-d468944ff11c&amp;WebId=f5a9cd01-3f67-4d67-a6c5-9af16e12a112&amp;SiteId=e1537950-3692-4b51-a8be-e94485c3ae88&amp;Zone=&amp;Source=https%3A%2F%2Fag%2Etennessee%2Eedu%2Futg%2FPages%2Fdefault%2Easpx" title="">
                               March #4
                              </a>
                              <div class="description">
                               Now is a great time to remove weeds from your garden beds. Doing so now will make the plant beds look better and prevent them from going to seed; therefore, making fewer weeds next year.
                              </div>
                             </div>
                            </div>
                           </li>
                           <li class="dfwp-item">
                            <div class="item" xmlns:ddwrt="http://schemas.microsoft.com/WebParts/v2/DataView/runtime">
                             <div class="link-item">
                              <a href="/_layouts/CopyUtil.aspx?Use=id&amp;Action=dispform&amp;ItemId=5&amp;ListId=fb8106d9-8a01-4743-ba51-d468944ff11c&amp;WebId=f5a9cd01-3f67-4d67-a6c5-9af16e12a112&amp;SiteId=e1537950-3692-4b51-a8be-e94485c3ae88&amp;Zone=&amp;Source=https%3A%2F%2Fag%2Etennessee%2Eedu%2Futg%2FPages%2Fdefault%2Easpx" title="">
                               March #5
                              </a>
                              <div class="description">
                               Apply a pre-emergent herbicide to your lawn, if you had crabgrass and other summer weeds in the past. The timing of application is important, and a good indicator is to do it just as forsythia begins to show some color.
                              </div>
                             </div>
                            </div>
                           </li>
                           <li class="dfwp-item">
                            <div class="item" xmlns:ddwrt="http://schemas.microsoft.com/WebParts/v2/DataView/runtime">
                             <div class="link-item">
                              <a href="/_layouts/CopyUtil.aspx?Use=id&amp;Action=dispform&amp;ItemId=6&amp;ListId=fb8106d9-8a01-4743-ba51-d468944ff11c&amp;WebId=f5a9cd01-3f67-4d67-a6c5-9af16e12a112&amp;SiteId=e1537950-3692-4b51-a8be-e94485c3ae88&amp;Zone=&amp;Source=https%3A%2F%2Fag%2Etennessee%2Eedu%2Futg%2FPages%2Fdefault%2Easpx" title="">
                               March #6
                              </a>
                              <div class="description">
                               Sow nasturtiums this month. Soak seed overnight in water. Cover with three-quarter inches of soil.
                              </div>
                             </div>
                            </div>
                           </li>
                           <li class="dfwp-item">
                            <div class="item" xmlns:ddwrt="http://schemas.microsoft.com/WebParts/v2/DataView/runtime">
                             <div class="link-item">
                              <a href="/_layouts/CopyUtil.aspx?Use=id&amp;Action=dispform&amp;ItemId=7&amp;ListId=fb8106d9-8a01-4743-ba51-d468944ff11c&amp;WebId=f5a9cd01-3f67-4d67-a6c5-9af16e12a112&amp;SiteId=e1537950-3692-4b51-a8be-e94485c3ae88&amp;Zone=&amp;Source=https%3A%2F%2Fag%2Etennessee%2Eedu%2Futg%2FPages%2Fdefault%2Easpx" title="">
                               March #7
                              </a>
                              <div class="description">
                               March is a good time to shop for and add lungwort (Pulmonaria) and Lenten rose (Helleborus) to your garden. Lungwort is an early flowering shade perennial that often struggles in the heat and humidity of our Tennessee climate, so be aware that Pulmonaria longifolia cultivars and hybrids are much more durable. Good selections are 'Roy Davidson,' 'E.B. Anderson,' 'Trevi Fountain' and for its vigor and heat tolerance, 'Diana Claire.'
                              </div>
                             </div>
                            </div>
                           </li>
                          </ul>
                         </div>
                        </div>
                       </td>
                      </tr>
                     </table>
                    </td>
                   </tr>
                  </table>
                 </div>
                 <div id="vid_93f40ef7-c625-4377-910e-0dca12de3aa4" style="display:none;" unselectable="on">
                 </div>
                </div>
                <p>
                 <a href="/utg/Pages/monthlytasks.aspx" title="Link to monthly gardening tips">
                  <strong>
                   View gardening tips for the entire year.
                  </strong>
                 </a>
                </p>
