  <!-- Le javascript
    ================================================== -->
  <!-- Placed at the end of the document so the pages load faster -->
  <!-- The JavaScript -->
  <script src="scripts/jquery.min.js" type="text/javascript">
  </script>
  <script src="scripts/showhide.js" type="text/javascript">
  </script>
  <script type="text/javascript">
   //used to initialize the menu navigation
		$(function () {
		    /**
		     * the menu
		     */
		    var $menu = $('#ldd_menu');

		    /**
		     * for each list element,
		     * we show the submenu when hovering and
		     * expand the span element (title) 
		     */
		    $menu.children('li').each(function () {
		        var $this = $(this);
		        var $span = $this.children('span');
		        $span.data('width', $span.width());
		        $this.bind('mouseenter', function () {
		            $menu.find('.ldd_submenu').stop(true, true).hide();
		            $span.stop().animate({
		                '0px': '510px'
		            }, 300, function () { //change 0x to 'width' to restore bounce
		                $this.find('.ldd_submenu').slideDown(300);

		            });
		        }).bind('mouseleave', function () {
		            $this.find('.ldd_submenu').stop(true, true).hide();
		            $span.stop().animate({
		                '0px': $span.data('width') + 'px'
		            }, 300);
		        });
		    });
		});

		function positionFooter() {
		    var mFoo = $("#footer");
		    if ((($(document.body).height() + mFoo.height()) < $(window).height() && mFoo.css("position") == "fixed") || ($(document.body).height() < $(window).height() && mFoo.css("position") != "fixed")) {
		        mFoo.css({
		            position: "fixed",
		            bottom: "0px"
		        });
		    } else {
		        mFoo.css({
		            position: "static"
		        });
		    }
		}
		$(document).ready(function () {
		    positionFooter();
		    $(window).scroll(positionFooter);
		    $(window).resize(positionFooter);
		    $(window).load(positionFooter);
		});
  </script>
  <script src="scripts/jquery.heyday.stickyfooter">
  </script>
