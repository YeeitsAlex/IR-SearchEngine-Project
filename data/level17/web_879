                myApp = myApp || (function () {
                        var pleaseWaitDiv = jQuery('<div class="modal" id="pleaseWait" data-backdrop="static" data-keyboard="false"><div class="modal-content"><div class="modal-header"><h1>Please wait...</h1></div></div></div>');
                        return {
                                showPleaseWait: function() {
                                        pleaseWaitDiv.modal();
                                },
                                hidePleaseWait: function () {
                                        pleaseWaitDiv.modal('hide');
                                },
                
                        };
                })();
         function showProgress(){
         jQuery('#submitTicketButton').button('loading');
         }
    function hideProgress(){
         jQuery('#submitTicketButton').button('reset');
         }
