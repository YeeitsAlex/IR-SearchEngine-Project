  <script language="javascript" type="text/javascript">
   jQuery(document).ready(function(){
        jQuery('.typeahead').attr('placeholder','Search by phrase or keyword');
jQuery("#schools-dropdown").bind({
        mouseover:function(){
                jQuery(".flyout-schools").show();
},
        mouseout:function(){
                jQuery(".flyout-schools").hide();
}
});
        });
    jQuery(document).ready(function() {
        jQuery('.typeahead').typeahead({                                
                  name: 'taxonomy',                                                          
                  remote: {
                                url: '/sims/helpcenter/kb/autocomplete/taxonomy.json?inst_name=UTHSC',
                                replace: function(url, uriEncodedQuery){
                                val = jQuery('.typeahead').val();
                                if(!val) return url;
                                return url + '&q=' + encodeURIComponent(encodeURIComponent(val))
                                },
                                filter: function (data) {
                                return data;
                            },                                                              
                                },                                        
                  limit: 5                                                                   
                });
        });
    
    jQuery(document).ready(function(){
if(jQuery("#enablePop").val()){
        chatIntro(); //JMR UPDATE   
}    
        });      

