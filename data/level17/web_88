  <script type="text/javascript">
   <!--//--><![CDATA[//><!--
startList = function() {
if (document.all&&document.getElementById) {
cssdropdownRoot = document.getElementById("cssdropdown");
for (x=0; x<cssdropdownRoot.childNodes.length; x++) {
node = cssdropdownRoot.childNodes[x];
if (node.nodeName=="LI") {
node.onmouseover=function() {
this.className+=" over";
}
node.onmouseout=function() {
this.className=this.className.replace(" over", "");
}
}
}
}
}

if (window.attachEvent)
window.attachEvent("onload", startList)
else
window.onload=startList;

//--><!]]>
  </script>
  <script language="JavaScript" type="text/JavaScript">
   var im= new Array()
im[0] = 'i/ACERF1.png'; 
im[1] = 'i/CCCUMission1.png'; 
im[2] = 'i/research2.png'; 

var iml= new Array()
iml[0] = 'research.php'; 
iml[1] = 'about.php'; 
iml[2] = 'research-facil.php';

var linkUrl = "";
var time = 5000; 
var i=0;
var j=1;
var clearColor;
var timer;
var mouseOver = false;

function slideI(){ 

if(mouseOver==true){
if(i==0){i=3}
else{
i--}
}

document.getElementById('ima').setAttribute('src',im[i]); 
document.getElementById('linkUrl').setAttribute('href', iml[i]); 

	for(clearColor = 0; clearColor < im.length; clearColor++ ){
document.getElementById("btn"+clearColor).setAttribute("style", "background-color: #AACBD1;");
	}
document.getElementById("btn"+i).setAttribute("style", "background-color: #7CBE5C;");

i++;
if(i==im.length){i=0} 
timer = setTimeout('slideI()',time);
}

onload=function(){
slideI();
} 

function clicked(j){
clearTimeout(timer);
i=j;
slideI();
}

function mouseIsOver(){
mouseOver = true;
}

function mouseIsOut(){
mouseOver = false;
clearTimeout(timer);
timer = setTimeout('slideI()',time);
}
  </script>
