         <a class="link-container height-100 remove-external-link-icon" href="https://stem.arizona.edu/events/210-tucson-water-festival">
          <div class="card-block">
           <div class="h5 card-subtitle text-muted">
            <div class="field-uaqs-date">
             <div class="field-items">
              <div class="field-item even">
               <span class="date-display-single" content="2019-03-28T07:30:00-07:00" datatype="xsd:dateTime" property="dc:date">
                March 28, 2019 7:30am
               </span>
              </div>
             </div>
            </div>
           </div>
