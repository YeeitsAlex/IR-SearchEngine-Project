  <script id="listings-results-tpl" type="text/html">
   <div class="results">
<% if (results.length) { %>
    <% _.each(results, function(result, idx) { %>
    <div class="result <%= (idx !== 0 && (idx + 1) % 2 == 0) ?  'odd' : '' %>">
        <h3>
            <a href="<%= result.url %>">
                <%= result.title %>
            </a>
        </h3>
        <div class="info">
            <div class="type"><span><%= result.type %></span><% if (result.has_minor) { %> <span>minor</span><% } %><% if (result.has_graduate) { %> <span>graduate</span><% } %></div>
            <div class="departments">
                <% _.each(result.departments, function(department, idx) { %>
                    <a href="<%= department.url %>"><%= department.title %></a>
                <% }); %>
            </div>
        </div>
    </div>
    <% }); %>
<% } else { %>
    <div class="result no-results">
        <p>No Results.</p>
    </div>
<% } %>
</div>
  </script>
