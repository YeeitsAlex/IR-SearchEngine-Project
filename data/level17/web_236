        <% if (counts.majors > 0 ) { %>
        <li>
            <a href="#" data-filter="major"><%= counts.majors %> Majors</a>
        </li>
        <% } %>
        <% if (counts.minors > 0 ) { %>
        <li>
            <a href="#" data-filter="minor"><%= counts.minors %> Minors</a>
        </li>
        <% } %>
        <% if (counts.graduates > 0 ) { %>
        <li>
            <a href="#" data-filter="graduate"><%= counts.graduates %> Graduate</a>
        </li>
        <% } %>
        <li>
            <a href="#" data-filter="all">All</a>
        </li>
    </ul>

    <div class="results">
        <%= list_results({ results: results, get_interest_names: get_interest_names }) %>
    </div>

    <div class="buttons">
        <a href="#" id="load-more-results" class="btn btn-purple">Load More</a>
    </div>
