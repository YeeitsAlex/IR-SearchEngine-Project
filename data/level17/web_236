  <script id="interest-match-tpl" type="text/html">
   <div class="row">
<% _.each(results, function(result, idx) { %>
<% if (idx % 3 == 0 && idx != 0) { %>
</div>
<div class="row">
<% } %>
    <div class="result one-third <%= result.type %>">
        <div class="bg">
        <% if (result.type == 'major') { %>
        <a href="<%= result.url %>" class="<%= result.type %> <% if (result.image) { %>has-image<% } %>">
        <% } %>
            <div class="text">
                <div class="type">
                    <%= result.type %>
                    <div class="matches"><%= result.matches %> interests</div>
                </div><!-- .type -->
                <h3><%= result.title %></h3>
                <p><%= result.content %></p>
                <% if (result.links) { %>
                <ul>
                    <% _.each(result.links, function(link, idx) { %>
                    <li>
                        <a href="<%= link.link %>" alt="<%= link.link_alt %>" class="document-link"><%= link.text %></a>
                    </li>
                    <% }); %>
                </ul>
                <% } %>
                <p class="interests">
                    <strong>Matched Interests:</strong> <br/>
                    <%= get_interest_names(result.matched_interests) %>
                </p>
            </div>
            <% if (result.image) { %>
            <img src="<%= result.image.url %>" alt="<%= result.image.alt %>" title="<%= result.image.title %>" />
            <% } %>
        <% if (result.type == 'major') { %>
        </a><!-- .result -->
        <% } %>
        </div><!-- .bg -->
    </div><!-- .result -->
<% }); %>
</div><!-- .row -->
  </script>
