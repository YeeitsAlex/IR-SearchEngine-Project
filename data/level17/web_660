         document.addEventListener("DOMContentLoaded", function() {
        $(function(){
            var ua = window.navigator.userAgent;
            var msie = ua.indexOf("MSIE ");
            if(!msie > 0)
                return;

            $("<link/>", {
                rel: "stylesheet",
                type: "text/css",
                href: "/css/ie.css"
            }).appendTo("head");
        });
    });
