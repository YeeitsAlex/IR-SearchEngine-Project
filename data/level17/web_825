  <script eval="true" language="javascript">
   if (typeof(amb) != undefined){
			top.SNC = top.SNC || {};
   			if (top.SNC.channelListeners)
   				for (var listener in top.SNC.channelListeners) {
   					var lstnr = top.SNC.channelListeners[listener];
   					if (lstnr != null)
	   					lstnr.unsubscribe();
  					}
   			top.SNC.channelListeners = top.SNC.channelListeners || {};
		}
