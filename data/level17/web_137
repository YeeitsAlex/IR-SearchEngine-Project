        <!-- Page title -->
        <h1>
         Welcome!
        </h1>
        <div class="main-content">
         <!--        <div class="welcome welcome-links">
            <div class="welcome-content"> -->
         <script language="JavaScript">
          <!-- Hide from old browsers
