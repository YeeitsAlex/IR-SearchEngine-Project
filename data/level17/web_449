</script>
<script>
 (function($) {
  var container = document.getElementById('mm_live_pane__container');
  var template = document.getElementById('mm_live_pane__template').innerHTML;

  $.getJSON('https://mediamagnet.osu.edu/api/v1/items.json?channel_id[]=409&channel_id[]=410&channel_id[]=445&channel_id[]=581&channel_id[]=577&channel_id[]=579&channel_id[]=413&channel_id[]=578&channel_id[]=580&channel_id[]=533&channel_id[]=531&channel_id[]=582&channel_id[]=532&per_page=4', function(data) {
    $.each(data.items, function(idx, item) {
      item['formatted_published_at'] = moment(item['published_at'], 'YYYY-MM-DD HH:mm:ss Z').fromNow();
      var html = Mustache.to_html(template, item);
      $(html).hide().appendTo(container).fadeIn();
    });
  }).fail(function() {
    $(container).append('<p>Cannot load content.</p>');
  });
})(jQuery);
</script>
<script src="https://odee.osu.edu/sites/default/files/js/js_ayF7RcRwfGIQuGRBApf27qJOEbP661XmfpT-xEpWDZ0.js">
</script>
