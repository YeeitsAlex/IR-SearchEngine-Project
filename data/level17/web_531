       <div id="rt-sidebar-b">
        <div class="rt-block">
         <div class="module-surround">
          <div class="module-title">
           <h2 class="title">
            ON TWITTER
           </h2>
          </div>
          <div class="module-content">
           <div class="custom">
            <p>
             <a class="twitter-timeline" data-aria-polite="polite" data-chrome="noheader, nofooter, noborders, transparent, noscrollbar" data-dnt="true" data-link-color="#9C0000" data-show-replies="false" data-width="170" href="https://twitter.com/OSUGlobal">
             </a>
            </p>
            <script type="text/javascript">
             !function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+"://platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");

twttr.ready(function (twttr) {
    twttr.events.bind("rendered", function (event) {
        var widgetFrame = event.target;
        var alltweets = widgetFrame.contentDocument.querySelectorAll('ol.timeline-TweetList li.timeline-TweetList-tweet.customisable-border');
        var loadmorebtn = widgetFrame.contentDocument.querySelectorAll('div.timeline-LoadMore');
        var tweetactions = widgetFrame.contentDocument.querySelectorAll('ul.timeline-Tweet-actions');
        var tweetmedia = widgetFrame.contentDocument.querySelectorAll('div.timeline-Tweet-media');
        var tweetmetadata = widgetFrame.contentDocument.querySelectorAll('div.timeline-Tweet-metadata');
        var tweeticon = widgetFrame.contentDocument.querySelectorAll('div.timeline-Tweet-brand');
        var tweetreplyto = widgetFrame.contentDocument.querySelectorAll('div.timeline-Tweet-inReplyTo');

        var i = 0;
        var counter = 1;

        for (i = 0; i < alltweets.length; i++) {
            if (counter < 4) {
                var rtelement = alltweets[i].querySelector('div.timeline-Tweet-retweetCredit');
				
                if (rtelement != null) {
                    alltweets[i].style.display = "none";
                }
                else {
                    counter++;
                }
            }
            else {
                alltweets[i].style.display = "none";
            }
        }

        if (loadmorebtn !== null) {
           for (i = 0; i < loadmorebtn.length; ++i) {
               loadmorebtn[i].style.display = "none"; // hide load more button
           };
        };
		
        if (tweetreplyto !== null) {
           for (i = 0; i < tweetreplyto.length; i++) {
               tweetreplyto[i].style.display = "none"; // hide reply to
           };
        };
		
        if (tweetactions !== null) {
           for (i = 0; i < tweetactions.length; i++) {
               tweetactions[i].style.display = "none"; // hide tweet action
           };
        };
		
        if (tweetmedia != null) {
           for (i = 0; i < tweetmedia.length; i++) {
               tweetmedia[i].style.display = "none"; // hide tweet media (e.g. images, etc.)
           };
        };
		
        if (tweetmetadata !== null) {
           for (i = 0; i < tweetmetadata.length; i++) {
               tweetmetadata[i].style.display = "none"; // hide tweet metadata (e.g. time tweet was posted)
           };
        };
		
        if (tweeticon !== null) {
           for (i = 0; i < tweeticon.length; i++) {
               tweeticon[i].style.display = "none"; // hide tweet icon
           };
        };
    });
});
            </script>
