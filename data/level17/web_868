              <div class="media media-element-container media-default">
               <div class="field field-name-field-html-code field-type-text-long field-label-hidden view-mode-default">
                <div class="field-items">
                 <div class="field-item even">
                  <script src="https://code.jquery.com/jquery-1.11.1.min.js">
                  </script>
                  <script type="text/javascript">
                   <!--//--><![CDATA[// ><!--


//this should take the href from the "download-link" class ([download] link in page) and put it in the "entity-link" class (title link in page)

$(document).ready(function(){
//alert("hello");
//$(".entity-link").css("background-color","#f00");
//.file.file-document .file-info .entity-link

$(document).on('mouseover mouseout','.file.file-document .file-info', function(e) {
    $curDownloadLink = $(this).children(".download-link").attr('href');  
    window.status = $(this).next(".download-link").attr('href');
    $(this).children('.entity-link').attr('href',$curDownloadLink);
    return false;
});

// the link should already be changed from the rollover, but just incase, this makes sure the download-link href is called.

$(document).on('click','.file.file-document .file-info', function(e) {
    e.preventDefault();
    $curDownloadLink = $(this).children(".download-link").attr('href');
    //alert("hello again");
    //alert($curDownloadLink);
    $(this).children('.entity-link').attr('href',$curDownloadLink);
    //this will load the doc in the current window
    //location.href=$curDownloadLink;
    // this will make the doc pop up in a new window
    window.open($curDownloadLink, "formWindow", "width=600,height=600,scrollbars=yes, resizable=yes,toolbar=no,menubar=no");
    return false;
});

});

//--><!]]>
                  </script>
