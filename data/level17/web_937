                 <!-- Two column 2x50 -->
                 <div class="at-panel gpanel panel-display two-50 clearfix">
                  <div class="region region-two-50-first">
                   <div class="region-inner clearfix">
                    <section class="block block-boxes block-boxes-os_sv_list_box" delta="1537986058" id="block-boxes-1537986058" module="boxes">
                     <div class="block-inner clearfix">
                      <h2 class="block-title" ng-non-bindable="">
                       Upcoming Events
                      </h2>
                      <div class="block-content content" ng-non-bindable="">
                       <div class="boxes-box" id="boxes-box-1537986058">
                        <div class="boxes-box-content">
                         <div class="os-sv-list os_sv_list_box sv-list-event sv-list-view-teaser sv-list-layout-list sv-grid-3" data-delta="1537986058" data-page="0" id="box-1537986058-page">
                          <article class="node node-event node-teaser article event-start clearfix" id="node-540801" role="article" target="_top">
                           <div class="event-start">
                            <span class="event-year">
                             2019
                            </span>
                            <span class="event-start-month">
                             Mar
                            </span>
                            <span class="event-start-day">
                             13
                            </span>
                           </div>
                           <div class="event-content">
