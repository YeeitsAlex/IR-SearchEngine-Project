   $(document).foundation(pageNav());

function pageNav() {
  if ($(window).width() >= 725) {
    $('#drop1').removeClass('f-dropdown');
  } 
   if ($(window).width() <= 725) {
    $('#drop1').addClass('f-dropdown');
  } 
}
$(window).resize(function() {
  $(document).foundation('dropdown', 'reflow');
  pageNav();
});
  </script>
