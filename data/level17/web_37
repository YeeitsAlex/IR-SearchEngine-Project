     <div class="gumm-layout-element ios-slider-layout-element span12" id="ios-slider-element-5c84b5645e46e">
      <div class="row-fluid ">
       <!-- BEGIN slider area -->
       <div class="iosSliderContainer bluebox-slider-wrap" style="padding-bottom:25%;">
        <div class="iosSlider loading">
         <div class="slider">
          <img alt="" class="swipe-item" data-attachment-id="271" data-comments-opened="0" data-image-description="" data-image-meta='{"aperture":"0","credit":"","camera":"","caption":"","created_timestamp":"0","copyright":"","focal_length":"0","iso":"0","shutter_speed":"0","title":"","orientation":"0"}' data-image-title="ARCH case" data-large-file="https://aerosols.wustl.edu/files/2016/03/ARCH-case-2loyiil-e1460409639346-1024x281.png" data-medium-file="https://aerosols.wustl.edu/files/2016/03/ARCH-case-2loyiil-e1460409639346-350x96.png" data-orig-file="https://aerosols.wustl.edu/files/2016/03/ARCH-case-2loyiil-e1460409639346.png" data-orig-size="1648,453" data-permalink="https://aerosols.wustl.edu/home/arch-case/" height="360" src="https://aerosols.wustl.edu/files/2016/03/ARCH-case-2loyiil-e1460409639346-1440x360.png" width="1440">
