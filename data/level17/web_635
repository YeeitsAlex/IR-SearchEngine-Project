   <!-- Pivot Funding Opps Search Widget v1.1 20100115
This Search Widget was developed by ProQuest for use with the Pivot service to search for funding opportunities
Access to Pivot is limited to authenticated users only.
To create a Pivot Funding Opps Search Widget visit: http://pivot.cos.com/widgets
 -->
   <script type="text/javascript">
    function pivot_search() {
var url="http://pivot.cos.com/funding/results?full=";
var full=document.getElementById("pivot_search_txt");
document.location=url + encodeURIComponent(full.value);
}
function handleKeyPress(e,form){ 
var key=e.keyCode || e.which; 
if (key==13) pivot_search();
}
   </script>
   <div id="pivot_search_box" style="border: 1px solid rgb(102, 102, 102); background-color: rgb(255, 255, 255); margin: 0px 1em 1em 0px; padding: 1em 0.75em; line-height: 18px;">
    <div id="pivot_search_hdr" style="display: block; margin-right: 5px; float: none;">
     <a href="http://pivot.cos.com/funding_main" style="text-decoration: none" tabindex="0">
