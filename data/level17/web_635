  <script type="text/javascript">
   //This makes the "sticky" menu bar work
	window.addEvent('domready', function() {
		if($('access') != null){new NaMooSticky('menu','access',0,0);}
		if($('secondary') != null){new NaMooSticky('widgets','secondary',23,90);}
		//if($('tertiary') != null){new NaMooSticky('tertiary',23,100);}
	}); 

	var NaMooSticky = new Class({
		n: 0,
		r: 0,
		initialize: function(what,accessid,n,r){
			this.n = n;
			this.r = r;
			this.access = $(accessid);
			this.position_saver = this.access.getPosition().y;
			this.x_position = this.access.getPosition().x;
			window.addEvent('scroll',eval('this.' + what).bind(this));
		},
		menu: function() {
			if (window.innerWidth > 850) {
				var num1 = this.n;
				var num2 = this.r;
			} else if (window.innerWidth <= 850) {
				var num1 = this.n - 30;
				var num2 = this.r + 10; 
			} else if (window.innerWidth <= 525) {
				var num1 = this.n;
				var num2 = this.r;
			}
			this.stick(num1,num2);
		},
		widgets: function() {
			if (window.innerWidth > 850) {
				var num1 = this.n;
				var num2 = this.r;
			} else if (window.innerWidth <= 850) {
				var num1 = this.n + 40;
				var num2 = this.r + 50; 
			}
			this.stick(num1,num2);
		},
		stick:function(n,r) {
			if (this.position_saver< window.getScroll().y+r) {
				this.access.setStyles({'position':'fixed','top':n});
				this.access.setStyles({'opacity':'0.9'});	
				this.access.setPosition(this.x_position);
			}
			else {
				this.access.setStyles({'position':'relative','top':0});	
				this.access.setStyles({'opacity':'1'});           
			}
		}
		//525px
	});

	window.addEvent('domready',function() {
		new foot();
	});

	var foot = new Class({
		initialize: function() {
			window.addEvent('onresize',this.br.bind(this));
		},
		br: function() {
			if (window.innerWidth <= 850) {
				$('disappear').style.display = "block";
			} else {
				$('disappear').style.display = "none";
			}
		}
	});
  </script>
