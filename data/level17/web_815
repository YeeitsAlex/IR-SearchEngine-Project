       <script>
        $(document).ready(function() {
			
			
			
			$('.carousel').carousel({
				interval: 7000	
			});
			
			$('#wrap').on('click', function() {
				$('.nav-selected').removeClass('nav-selected');
				$('#camp-life').css('color', '#bb0000');
				$('.sub-nav').hide();
			});
			
			$('a.load').on('click', function() {
				$('#header-fluid').css("background-image", "url(/global/images/subtle-patterns/" + $(this).data('pattern') +".jpg)");
				return false;
			});
			
			
			$('#logo').on({
				mouseenter:
					function() {
						$('.nav-selected').removeClass('nav-selected');
						$('#camp-life').css('color', '#bb0000');
						$('.sub-nav').hide();
						//$('.sub-nav').css('height', '0px');
					},
				mouseleave:
					function() {
						
					}
			});
			
			$('.parent').on({
				mouseenter:
					function() {
						
						if ( $(this).data('target') == 'campus-life' ) {
							$('#camp-life').css('color', '#fff');
						} else {
							$('#camp-life').css('color', '#bb0000');
						}
						
						$('.nav-selected').removeClass('nav-selected');
						
						$('.sub-nav').hide();
						
						$(this).addClass('nav-selected');
						
						$('#'+$(this).data('target')).css( 'height', 'auto' );
						
						/*$('#'+$(this).data('target')).slideToggle(); */
						
						$('#'+$(this).data('target')).show();
						
						/*$('#'+$(this).data('target')).stop().animate({
							height: "180px"
						})*/
					},
				mouseleave:
					function(e) {
						if (e.offsetY < 29) {
							$('.nav-selected').removeClass('nav-selected');
							$('#camp-life').css('color', '#bb0000');
							$('.sub-nav').hide();
						}
					}
			});
			
			
			$('.nav-close').on({
				mouseenter:
					function() {
						
					},
				mouseleave:
					function() {
						
					}
			});
			
			
			$('.sub-nav').on({
				mouseenter:
					function() {
						
					},
				mouseleave:
					function() {
						$(this).hide();
						$('#camp-life').css('color', '#bb0000');
						//$(this).slideToggle();
						/*
						$(this).stop().animate({
							height: '0px'
						});*/
						
						
						$('.nav-selected').removeClass('nav-selected');
						$(this).addClass('nav-selected');
					}
			});
			
			
			
			
			
		});
       </script>
