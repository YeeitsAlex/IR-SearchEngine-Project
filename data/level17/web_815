     <script>
      $(document).ready(function() {
		$('#search-moritz').focus(function() {
			if ( $(this).val() == 'Search Moritz' ) {
				$(this).val('');	
			}
		});
		$('#search-moritz').blur(function() {
			if ( $(this).val() == '' ) {
				$(this).val('Search Moritz');	
			}
		});
	});
     </script>
