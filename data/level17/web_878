$(document).ready(function () {$("a[rel='external']").on("click", function (event) {window.open(this.href);
return false;});
$("div.academic-unit a").on("click", function (event) {window.open(this.href);
return false;});
$("a[rel='dummy']").on("click", function (event) {$.getScript("/webroot/dummy.svc.php",function(){return false;});
return false;});

      var pkBaseURL = "https://grad.arizona.edu/anon/";
      try {
          var piwikTracker = Piwik.getTracker(pkBaseURL + "piwik.php", 1);
          piwikTracker.trackPageView();
          piwikTracker.enableLinkTracking();
      } catch( err ) {}
    

    $(".help-icon").each(
        function() {
            var my_help_id = "help-span-" + (Math.floor(Math.random() * (1000000 - 1)) + 1);

            var my_help_title = this.alt;
            if( $(this).prop("tagName") != "IMG") { my_help_title = $(this).attr("data-alt"); }

            $(this).before(
                $("<span>", {
                    id: my_help_id,
                    text: this.title,
                    title: my_help_title
                })
            );

            $("#"+my_help_id).dialog({
                autoOpen: false,
                modal: true,
                position: {
                    my: "center top",
                    at: "top+10%"
                }
            });

            $(this).click(
                function() {
                    $("#"+my_help_id).dialog("open");
                }
            );
        }
    );


$(function() {
    try {
        $(".sortable").sortable({
            axis: "y",
            update: handle_resort,
            handle: ".orderable"
        });
        $(".sortable").disableSelection();
    } catch(e) {}
});

function handle_resort( event, ui) {
    $(".sortable tr.sortable_row input.order_input").each(function(i){
        var orderInput = $(this).get()[0]; // the <input> elem
        orderInput.value = i+1;
        i = i+1;
    });
    $("#inputOrderSubmit").effect({effect:"highlight", color:"#66ff66"});
    $("#inputOrderSubmit").show();
}


var submitted = false
function disableSubmit(){
    if (submitted) {
        return false
    }

    submitted = true
    return true
}

$("form:not(#PdgPreviewViewForm)").on("submit", function (event) {return disableSubmit()
return false;});
$(".dismissive").on("click", function (event) {$(".dismissable").toggle(
        "slow", function() {}
    );
return false;});

  $(".error-message").each( function () {
    $(this).addClass("alert alert-warning text-uppercase")
      .prepend($("<span>", {
        class: "fas fa-exclamation-triangle",
        html: "&nbsp;",
        "aria-hidden": true
      }))
  })
  $(".message.error").each( function () {
    $(this).addClass("alert alert-warning text-uppercase")
      .prepend($("<span>", {
        class: "fas fa-exclamation-triangle",
        html: "&nbsp;",
        "aria-hidden": "true"
      }))
  })
  });
