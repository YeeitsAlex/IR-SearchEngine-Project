                         <script type="text/javascript">
                          $(document).ready(function() {
  
     $().SPServices({
    operation: "GetListItems",
    async: false,
    listName: "Alumni",
    CAMLViewFields: "<ViewFields>" +
            "<FieldRef Name='Title' />" +
            "<FieldRef Name='Bio' />" +
            "<FieldRef Name='Graduation' />" +
            "<FieldRef Name='Picture' />" +
            "<FieldRef Name='Position' />" +
            "</ViewFields>",
    CAMLQuery: '<Query>' +
                '<OrderBy>' +
                	'<FieldRef Name="Title" />' +
            	'</OrderBy>' +
        '</Query>',
    completefunc: function (xData, Status) {
    
    var counter = 0; 	    
  	var itemCount = parseFloat($(xData.responseXML).SPFilterNode("rs:data").attr("ItemCount"));  
  	var SelectedItem = Math.floor(Math.random()* itemCount);
	 	
	 	$(xData.responseXML).SPFilterNode("z:row").each(function() {
		counter = counter + 1;
		
		if (counter == SelectedItem) {
		
		 
			var Name = $(this).attr("ows_Title");
			var Bio = $(this).attr("ows_Bio");
			var Picture = $(this).attr("ows_Picture");
			var Graduation = $(this).attr("ows_Graduation");
			var PictureURL = Picture.substr(0, Picture.indexOf(','));
			if ($(this).attr("ows_Position") == undefined )
	  			{ var Position = "";}
	  		else 
	  			{ var Position = $(this).attr("ows_Position");}

		
			var showChar = 999;
    		var ellipsestext = "...";
			var liHtml = "<img class='AlumniPhoto' style='Float:left' src=" + PictureURL + "><h2>" + Name + "</h2><strong>"+Graduation+"<br/>"+Position+"</strong>"+Bio;
			$(".Alumni").append(liHtml);	
    		$('.Alumni').each(function() {
        		var content = $(this).html();   
 	            var c = content.substr(0, showChar);           
 	            var html = c + '<span class="moreellipses">' + ellipsestext + '&nbsp;</span>&nbsp;&nbsp;<a href="alumni.aspx#'+Name+'" class="morelink">More</a></span>';
 			    $(this).html(html);
      			 });
 			}
 			
		});
	
   

 	
 
   			
       
      
      }// Completes the function

  
			   
          
}); // Completes SP Service

  


});
