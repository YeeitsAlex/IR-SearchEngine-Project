                         <script language="javascript" type="text/javascript">
                          //get current date  used to check again dates in list
dt = new Date();

//main site url - makes sure the script would work anywhere in the site directory
var baseURL = window.location.protocol + "//" + window.location.host + "/"

$(document).ready(function() {

	var data = [];
	// get all the current news articles
  $().SPServices({
    operation: "GetListItems",
    async: false,
    
    //** edit below with the library site url
	
	// **** edit below with your library name
    listName: "news",
    CAMLQuery: "<Query>" +
    
    			
    			//can use this to just grab active records, etc.
    			"<Where>" +
    				"<And>" + //Check and make sure employee is active
    					"<Leq>"+
    						"<FieldRef Name='StartDate' /><Value Type='DateTime'><Today /></Value>" +
    					"</Leq>"+
    					"<Geq>"+ //Check and make sure Department is correct (Pull from page?)
    						"<FieldRef Name='EndDate' /><Value Type='DateTime'><Today /></Value>"+
    					"</Geq>"+
    				"</And>" +
    			"</Where>" +
    			
    			
    			/* this is just the sorting */
    			"<OrderBy>"+
    				"<FieldRef Name='Priority' Ascending='True' />"+
    				"<FieldRef Name='StartDate' Ascending='False' />"+
    			"</OrderBy>"+
    			"</Query>",
    	   CAMLViewFields: "<ViewFields Properties='True' />",    						
		completefunc: function (xData, Status) {
		//the alert below will let you know if you are connecting to the data and returning anything
		
		//convert SPServices data to usable JSON data
		data =  $().SPServices.SPXmlToJson({
  				ns: $(xData.responseXML).SPFilterNode("z:row")
			});

        var newGroup = 0;
        //this is where you build your item blocks (loop through)
		for ( var i = 0; i < data.length; i++ )
		{
					
			
			//get title	
			var title = data[i].Title;
			var info = "";
			var link = "";
			var thumb = "";
			
			if (data[i].Description != "")
			{
				info = data[i].Description;
				
				if (info.length > 200)
				{
					info = info.trim(info).substring(0, 200).split(" ").slice(0, -1).join(" ") + "...";
				}
			}		
			var author = $(this).attr('ows_author0');
			if (author===undefined){
				author = '';
			}
		

			
			
			if (data[i].Link != undefined)
			{
				var link = data[i].Link
				if (link.indexOf(",") > 0)
				{
					link = link.substr(0, link.indexOf(","));
				}
				link = '<a target="_blank" href="' + link +'"> for more info </a>';
			}		
				
			
			var thumbHTML = "";	
			var thumbAlt = "";
			if (data[i].Thumbnail != undefined)
			{				
				var thumb = data[i].Thumbnail;
				if (thumb.indexOf(',') > 0)
				{
					thumb = thumb.substr(0, thumb.indexOf(","));
					thumbAlt = data[i].Thumbnail.substr(data[i].Thumbnail.indexOf(",") + 1);
				}
				thumbHTML = '<image alt="' + thumbAlt + '" src="'+thumb+'">';
			}	
				
			
				// build string 
				var liHtml='';
				
			
	
						liHtml = '<div><p class="listHead">'+title+'</p>' + thumbHTML + '<p>'+info+'</p><p>';
					if (link!==''){
						liHtml += link;
					}
					liHtml += '</div>';
					//this spits it out below and builds the ul listed below
			        $("#imageList").append(liHtml);	        
			    
	        }
	     
    	}	
	});   
});
                         </script>
