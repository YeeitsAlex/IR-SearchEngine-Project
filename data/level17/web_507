  <div class="vc_column-inner vc_custom_1485784948126">
   <div class="wpb_wrapper">
    <div class="vc_row wpb_row vc_inner vc_row-fluid vc_custom_1485736599504 vc_row-has-fill">
     <div class="wpb_column vc_column_container vc_col-sm-3">
      <div class="vc_column-inner">
       <div class="wpb_wrapper">
        <div class="wpb_single_image wpb_content_element vc_align_center">
         <figure class="wpb_wrapper vc_figure">
          <div class="vc_single_image-wrapper vc_box_border_grey">
           <img alt="thestateway-diversity" class="vc_single_image-img " height="150" src="https://thestateway.gsu.edu/files/2017/01/thestateway-diversity.jpg" title="thestateway-diversity" width="150"/>
          </div>
         </figure>
        </div>
       </div>
