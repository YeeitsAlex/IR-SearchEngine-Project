.eg-ajax-closer-wrapper.eg-acp-type1 {
    margin: 0 0 30px;
}
i.ultsl-arrow-left6 {
    display: none;
}
.wpb_revslider_element.wpb_content_element {
    margin-bottom: 0;
}
.uvc-main-heading.ult-responsive {
    max-width: 700px;
    margin-right: auto;
    margin-left: auto;
}
.ajax-left .vc_column-inner {
    margin-left: 15px;
    background: #eeeeee;
}
.eg-acp-light .eg-ajax-navbt	{	background:#eeeeee !important; color:#000;}
#admissions-cta {
    background-position: 0% 30% !important;
}

@media (max-width: 768px) {
.rankings {
    font-size: 24px !important;
}
}
@media (max-width: 1080px) {
.uvc-main-heading.ult-responsive h2 {
    font-size: 32px;
    line-height: 38px;
}
.uvc-sub-heading {
    font-size:21px;
    line-height: 39px;
}
}
@media (max-width: 1920px) {
    #admissions-cta {
   background-size: initial !important;
    }
}
.subscribe {
  width: 280px;
}
.uvc-sub-heading {
    font-size:26px;
    line-height: 34px;
}
.ajax-btn .ubtn {
    font-size: 17px;
line-height: 23px;
}

.subscribe .subscribe-gf-form.blue input[type="submit"] {
    background-color: transparent;
    border: 1px solid transparent;
}
}
