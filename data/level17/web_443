               <div about="/events-sidebar" class="node node-general-content clearfix" id="node-441" typeof="sioc:Item foaf:Document">
                <span class="rdf-meta element-hidden" content="" property="dc:title">
                </span>
                <div class="node-content clearfix ">
                 <div class="field field-name-body field-type-text-with-summary field-label-hidden prose">
                  <div class="field-items">
                   <div class="field-item even" property="content:encoded">
                    <iframe frameborder="0" height="300" scrolling="no" src="https://www.google.com/calendar/embed?showTitle=0&amp;showNav=0&amp;showPrint=0&amp;showTabs=0&amp;showCalendars=0&amp;mode=AGENDA&amp;height=300&amp;wkst=1&amp;bgcolor=%23FFFFFF&amp;src=2gku2fdleablu9m4iri4qlutmo%40group.calendar.google.com&amp;color=%2329527A&amp;ctz=America%2FNew_York" style=" border-width:0 " width="200">
