              <h2 class="pane-title">
               Follow Us
              </h2>
              <div class="pane-content">
               <div class="fb-social-likebox-plugin">
                <fb:like-box border_color="" colorscheme="light" force_wall="false" header="true" height="" href="https://www.facebook.com/GWPoliticalScience/" show_faces="true" stream="false" width="220">
