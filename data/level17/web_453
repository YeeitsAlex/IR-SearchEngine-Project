    $(".business a").each(function(){
      var url = location.href;
      urlRegExp = new RegExp(url.replace(/\/$/,'') + "$"); 
      if(urlRegExp.test(this.href.replace(/\/$/,''))){
                $thisAccLi = $(this);
                $thisAccLi.closest("a.accordion-btn").addClass('active');
                $thisAccLi.closest(".accordion-content").addClass('active');
                $thisAccLi.addClass('active');
        }
});
