    $(document).ready(function(){
			$('#toggleMenu').click(function(){
	  			$('#menu').slideToggle();
	  		});
	  		/*var images = ['coastal.jpg', 'dataScience.jpg', 'military.jpg', 'manufacturing-new.jpg', 'energy.jpg', 'pharmaco.jpg'];
			$('.panel_1').css({'background-image': 'url(img/bg/' + images[Math.floor(Math.random() * images.length)] + ')'});
	  		$('#currentImage li').click(function(){
	  			$('.panel_1').css({'background-image': 'url(img/bg/' + images[$(this).index()] + ')'});
	  		});*/


			$(".panel_1").backgroundCycle({
				imageUrls: [
					'img/bg/pharmaco4.jpg',
					'img/bg/dataScience1.jpg',
					'img/bg/pharmaco1.jpg',
					/*'img/bg/pharmaco2.jpg',*/
					'img/bg/pharmaco3.jpg',
					'img/bg/manufacturing-new.jpg',
					'img/bg/coastal.jpg',
					'img/bg/dataScience.jpg',
					
					/*'img/bg/military.jpg',*/
					/*'img/bg/energy.jpg',*/
					/*'img/bg/pharmaco.jpg',*/
				],
				fadeSpeed: 1000,
				duration: 5000,
				backgroundSize: SCALING_MODE_COVER
			});




	  		$( window ).resize(function() {
				if($(window).width()>769){
					$('#menu').show();
					$('#menu').css('display', 'inline-block');
					$('#toggleMenu').hide();
				}else{
					$('#menu').hide();
					$('#toggleMenu').show();
				}
			});

	  		$('#scroll2next').click(function(event){
			    $('html, body').animate({
			        scrollTop: $( $.attr(this, 'href') ).offset().top -100
			    }, 500);
			    event.preventDefault();
			});

		});
   </script>
