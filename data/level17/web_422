          </style>
          <script>
           var slideIndex = 0;
var timers = [];

var Timer = {
    Start : function() {
        showSlides();
    },
    Stop : function() {
        timers.forEach(function(t){
            clearTimeout(t);
        });
        timers.length = 0;
    },
    Reset : function(){
        this.Stop();
        this.Start();
    }
}

function showSlides() {
    var i;
    var slides = document.getElementsByClassName("fdn-slides");
    for (i = 0; i < slides.length; i++) {
        slides[i].style.display = "none"; 
    }
    slideIndex++;
    if (slideIndex > slides.length) {slideIndex = 1} 
    slides[slideIndex-1].style.display = "block"; 
    var t = setTimeout(showSlides, 5000); // Change image every 2 seconds
    timers.push(t);
}

function plusSlides(direction) {
    var slides = document.getElementsByClassName("fdn-slides");
    slides[slideIndex - 1].style.display = "none";
    slideIndex = slideIndex + direction; 
    slideIndex = slideIndex == 0 ? 
      slides.length : 
      slideIndex > slides.length ? 1 : slideIndex;
  
    if (direction < 0) slideIndex--;
    slideIndex = slideIndex == 0 ? slides.length : slideIndex;
  
    if (!slides[slideIndex - 1]) debugger;
    slides[slideIndex - 1].style.display = "block";
    Timer.Reset();
}

Timer.Start();
          </script>
