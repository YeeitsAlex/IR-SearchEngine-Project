             <div class="panel-pane pane-views-panes pane-gw-repeating-sidebar-panel-pane-2">
              <h2 class="pane-title">
              </h2>
              <div class="pane-content">
               <div class="view view-gw-repeating-sidebar view-id-gw_repeating_sidebar view-display-id-panel_pane_2 view-dom-id-454bd4e435758cd92d7ea5a7858229f8">
                <div class="view-content">
                 <div class="views-row views-row-1 views-row-odd views-row-first views-row-last">
                  <!-- Start List sidebar -->
                  <div about="/repeating-set" class="all-purpose-sidebar node node-list" id="node-50" typeof="sioc:Item foaf:Document">
                   <div class="image ">
                    <a href="https://secure2.convio.net/gwu/site/Donation2?df_id=1382&amp;1382.donation=form1&amp;set.SingleDesignee=1016&amp;utm_campaign=giving-tspppa&amp;utm_source=tspppa-general&amp;utm_medium=website" target="_blank">
                     <div class="image-wrapper" style="width:220px;">
                      <img alt="Make a Difference" height="80" src="https://tspppa.gwu.edu/sites/g/files/zaxdzs2001/f/styles/gw_all_purpose_sidebar_large/public/image/make-a-difference-give-today.jpg?itok=anfOtb4z" title="Make a Difference" typeof="foaf:Image" width="220">
                       <span class="play">
                        <span>
                        </span>
                       </span>
                       <div class="clearfix">
                       </div>
                      </img>
                     </div>
                    </a>
                   </div>
                   <h3>
                    <a href="https://secure2.convio.net/gwu/site/Donation2?df_id=1382&amp;1382.donation=form1&amp;set.SingleDesignee=1016&amp;utm_campaign=giving-tspppa&amp;utm_source=tspppa-general&amp;utm_medium=website" target="_blank">
                     Make a Difference
                    </a>
                   </h3>
                   <p>
                   </p>
                  </div>
                  <!-- End Allpurpose sidebar -->
                 </div>
                </div>
               </div>
