            <div class="panel-pane pane-node">
             <div class="pane-content">
              <div about="/home-page-jq-rotator" class="node node-general-content node-teaser clearfix" id="node-271" typeof="sioc:Item foaf:Document">
               <span class="rdf-meta element-hidden" content="" property="dc:title">
               </span>
               <div class="node-content clearfix ">
                <div class="field field-name-body field-type-text-with-summary field-label-hidden prose">
                 <div class="field-items">
                  <div class="field-item even" property="content:encoded">
                   <style type="text/css">
                    /* slideshow display style rules */
#slideshow_mab
{
	margin:		0 auto		;
	width:		940px		;
	height:		300px		;
	overflow:	hidden		;
	position:	relative	;
}

#slidesContainer_mab
{
	margin:		0 auto		;
	width:		940px		;
	height:		300px		;
	overflow:	hidden		;
	position:	relative	;
}

#slideshow_mab #slidesContainer_mab .slide_mab
{
	margin:		0 auto		;
	width:		940px		;
	height:		300px		;
}

.overlay_mab
{
	width: 			420px	;
	height: 		280px	;
	margin:	10px 10px 10px 510px	;
	position:	absolute	;
	//left:		0px;
	//top:		0px;
	font-family:Arial,Helvetica,sans-serif;
	text-align: left;
	
	background: rgba(255,255,255,0.8);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#CCffffff,endColorstr=#CCffffff);
}

.overlay_mab h1,
.overlay_mab p
{
	
	padding: 1px 5px	;
	text-align: justify	;
}

.overlay_mab h1
{
	margin:			0px		;
	font-weight:	bolder	;
	letter-spacing: -2px	;
}

/*	cross-browser disability of image highlight (allows you to hilight text) */
.imgslide_mab
{
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: -moz-none;
	-ms-user-select: none;
	user-select: none;
}

/* slideshow control style rules */
.control_mab
{
	display:	block		;
	width:		40px		;
	height:		300px		;
	position:	absolute	;
	cursor:		pointer		;
}
                   </style>
                   <script src="https://code.jquery.com/jquery-1.10.1.min.js">
                   </script>
                   <script src="https://code.jquery.com/jquery-migrate-1.2.1.min.js">
                   </script>
                   <script type="text/javascript">
                    $(document).ready( function(){
	var currentPosition	=	0				;
	var slideWidth		=	940				;
	var	slides		=	$('.slide_mab')		;
	var numberOfSlides	=  slides.length;
	var timerDelay		=	5000		;
	
	$('#slidesContainer_mab').css('overflow','hidden');
	
	slides
	.wrapAll('<div id="slideInner_mab"></div>')
	.css({
		'float'	:	'left',
		'width'	:	slideWidth
	});
	
	$('#slideInner_mab').css('width',slideWidth * numberOfSlides);
	
	var myTimer = setInterval( bannerTimer,timerDelay );
	
	function bannerTimer()
	{
		incrementPosition();
		slideImage(currentPosition);
	}
	
	//	move slideInner using margin-left
	function slideImage(position)
	{
		$('#slideInner_mab').animate(
				{'marginLeft'	:	slideWidth * (-position)},
				1000,
				//	if on last slide become first
				function()
				{
					if( position==numberOfSlides-1)
					{
						currentPosition = 0;
						$('#slideInner_mab').stop().css({'marginLeft':'0'});
					}
				}
			);
	}
	
	function incrementPosition()
	{
		currentPosition += 1;
		
		//	do not allow an overflow to occur
		if( currentPosition >= numberOfSlides )
			currentPosition = 0;
	}
});
                   </script>
                   <div id="slideshow_mab">
