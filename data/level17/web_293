   //Removing entries from dropdown Pickup location menu; temp existing in botlogo.html

 var select=document.getElementById('locx00');

for (i=0;i<select.length;  i++) {
   var v = select.options[i];
   if (select.options[i].value=='cga  ') {
     select.remove(i); //Billy Ireland Cartoons Library
   }
}

for (i=0;i<select.length;  i++) {
   var v = select.options[i];
   if (select.options[i].value=='spe  ') {
     select.remove(i); //Special Collections
   }
}
  </script>
