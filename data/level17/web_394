   <script type="text/javascript">
    $(document).ready(function(){
              //================================================================
              $(".secondary-navigation-container").accordion({
                header: '.link-group-header',
                alwaysOpen: false,
                active: false,
                collapsible: true,
                heightStyle: 'content'
              });
              $(".secondary-navigation-container").accordion( "option", "clearStyle", true );
              $(".secondary-navigation-container").accordion( "option", "animated", false );
              //================================================================
              $('.secondary-navigation-container .link-group-header').children('a').each(function(index){
                //alert('start');
                if($(this).attr('href') == '' || $(this).attr('href') == '#'){
                  //alert('continue');
                  $(this).parent().toggleClass('has-hidden-sub-navigation',1);
                  /*
                  $(this).bind("click",function(event){
                    alert('found');
                  });
                  */
                }
                else{
                  $(this).parent().unbind("click");
                }
              });
              //================================================================
              $(".secondary-navigation-container").bind( "accordionchangestart", function(event, ui) {
                //ui.oldHeader.toggleClass('has-visible-sub-navigation',0);
                //ui.oldHeader.toggleClass('has-hidden-sub-navigation',1);
                
                //ui.newHeader.toggleClass('has-hidden-sub-navigation',0);
                //ui.newHeader.toggleClass('has-visible-sub-navigation',1);
              });
              //================================================================
              var regExPattern = new RegExp('^((http://|https://)?www.nccu.edu)?/index.cfm');
              var linkIsActive = false;
              $('.secondary-navigation-container .metaData.priorityDirectory').each(function(index){
                var currentElement = $('.secondary-navigation-container .metaData.priorityDirectory').eq(index);
                //alert(currentElement.html().toLowerCase());
                if(currentElement.html().toLowerCase() === 'nccu.edu'){
                  //alert(currentElement.html().toLowerCase());
                  //alert(currentElement.parent().html().toLowerCase());
                  currentElement.parent().next().children('div').children('a').each(function(index){
                    var currentElement2 = currentElement.parent().next().children('div').children('a').eq(index);
                    //if(currentElement2.attr('href').toLowerCase() === '/index.cfm'){
                    //alert(regExPattern.test(currentElement2.attr('href').toLowerCase()));
                    //if(currentElement2.attr('href').toLowerCase().search(regExPattern)){
                    if(regExPattern.test(currentElement2.attr('href').toLowerCase())){
                      //makeLinkActive(currentElement2);
                      linkIsActive = true;
                      return false;
                    }
                  });
                  if(linkIsActive){
                    return false;
                  }
                }
              });
              if(linkIsActive == false){
                $('.secondary-navigation-container a').each(function(index){
                  var currentElement = $('.secondary-navigation-container a').eq(index);
                  //alert(currentElement);
                  //alert(currentElement.attr('href').toLowerCase())
                  //if(currentElement.attr('href').toLowerCase() === '/index.cfm'){
                  //alert(currentElement.attr('href').toLowerCase().search(regExPattern));
                  if(regExPattern.test(currentElement.attr('href').toLowerCase())){
                  //if(1){
                    makeLinkActive(currentElement);
                    linkIsActive = true;
                    return false;
                  }
                });
              }
              //================================================================
              function makeLinkActive(_currentElement){
                if(_currentElement.parent().hasClass('link-group-header')){
                  $(".secondary-navigation-container").accordion("option","active",_currentElement.parent());
                }
                else{
                  $(".secondary-navigation-container").accordion("option","active",_currentElement.parent().parent().prev());
                  _currentElement.css('text-decoration','underline');
                }
              }
              //================================================================
              $(".secondary-navigation-container").accordion( "option", "animated", 'slide' );
              //================================================================
            });
   </script>
   <script type="text/javascript">
    //-------------------------------------------------------
            $(document).ready(function(){
              $('.login-link').attr('onclick','');
              /*
              $('.search-button,#headerSearchSubmitBtn,.search-form')
                .attr('onclick','')
                .attr('onsubmit','');
              */
              $('.search-destination-dropdown').val('');
              $('.search-destination-dropdown').css('display','none');
              //-----------------------------------------------------
              str_search_term_default = 'Enter search term';
              $('.search-term').val(str_search_term_default);
              $('.search-term').focus(function(){
                if($(this).val() == str_search_term_default){
                  $(this).val('');
                }
              });
              $('.search-term').focusout(function(){
                if($(this).val() == ''){
                  $(this).val(str_search_term_default);
                }
              });
              $('.search-form').submit(function(){
                if($('.search-term').val() == str_search_term_default){
                  $('.search-term').val('');
                  return true;
                }
              });
              /*
              */
            });
   </script>
   <script type="text/javascript">
    $(document).ready(function(){
              $('#mobile-menu').css('display','none');
              $('#menu-icon-link').click(function(e){
                //alert('test');
                e.preventDefault();
                if($('#mobile-menu').css('display') == 'none'){
                  $('#mobile-menu').css('display','block');
                }
                else{
                  $('#mobile-menu').css('display','none');
                }
              });
              //-------------------------------------------
              $('#mobile-search-field-container').css('display','none');
              $('#search-icon-link').click(function(e){
                e.preventDefault();
                $(this).css('display','none');
                $('#mobile-search-field-container').css('display','block');
                $('#mobile-logo-link').css('display','none');
                $('#mobile-search-field').focus();
              });
              $('#mobile-search-field-container').focusout(function(){
                setTimeout(
                  function(){
                    var bit_container_has_focus = 0;
                    $('#mobile-search-field-container').children().addBack().each(function(){
                      //alert($(this).attr('id'));
                      //alert($(this).is(':focus'));
                      if(
                        $(this).is(':focus')
                        /*
                        || (
                          $(this).parent().is(':focus')
                          && $(this).parent().attr('id') == 'mobile-search-field-container'
                        )
                        */
                      ){
                        bit_container_has_focus = 1;
                      }
                      else{
                        //alert($(this).parent().attr('id'));
                      }
                    });
                    if(!bit_container_has_focus){
                      $('#mobile-search-field-container').css('display','none');
                      $('#mobile-logo-link').css('display','');
                      $('#search-icon-link').css('display','');
                    }
                  },
                  100
                );
              });
            });
   </script>
   <script type="text/javascript">
    $(document).ready(function(){
              if(window.location.hash == undefined || window.location.hash == ''){
                $(document).ready(function(){
                  window.scrollTo(0,0);
                });
              }
            });
   </script>
