    <script src="https://casls.uoregon.edu/wp-content/themes/caslstheme/multiselect/js/bootstrap-multiselect.js" type="text/javascript">
    </script>
    <link href="https://casls.uoregon.edu/wp-content/themes/caslstheme/multiselect/css/bootstrap-multiselect.css" rel="stylesheet" type="text/css"/>
    <script type="text/javascript">
     (function(jQuery) {
    	var $window = jQuery(window),
        $submenu = jQuery('.dropdown-submenu');

//     	$window.resize(function resize(){
//         	if ($window.width() < 1250) {
//             	return $submenu.addClass('pull-left');
//         	}
// 			$submenu.removeClass('pull-left');
//     	}).trigger('resize');
	})(jQuery);
        
    jQuery('#world-image').affix({
      offset:{
          top:jQuery("#world-top").height(),
          bottom: function () {
            return (this.bottom = jQuery('.site-footer').height())
          }
      }
    });
    
    !function(d,s,id) {
							var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';
							if(!d.getElementById(id)){js=d.createElement(s);
								js.id=id;js.src=p+'://platform.twitter.com/widgets.js';
								fjs.parentNode.insertBefore(js,fjs);}}(document, 'script', 'twitter-wjs');
    
	jQuery( document ).ready(function(){
		    
		jQuery('.rounded').wrap("<div class='row twitter-header'></div>");
		jQuery('.rounded').addClass('col-sm-6');
		jQuery('.twitter-header').append('<a href="https://twitter.com/CASLS_NFLRC" class="col-sm-6 twitter-follow-button pull-right" data-show-count="false" data-size="small">Follow @CASLS_NFLRC</a>');
    	
    	jQuery('html').removeClass('margin-top');
    	
		getAllProjects();
	jQuery.ajax({
		  type: "GET",
		  url: "https://casls.uoregon.edu/wp-content/themes/caslstheme/getFilters.php"
		})
		  .done(function( msg ) {
// 		    alert( "Data Saved: " + msg );
		    var json = JSON.parse(msg);
			
			var option = '<option value="8">Any</option>';
			for (i=0;i<json.levels.length;i++){				
			   option += '<option value="'+ json.levels[i]["id"] + '">' + json.levels[i]["name"] + '</option>';
			}
			jQuery('#levels').append(option);

			var option = '';
			for (i=0;i<json.languages.length;i++){
			   option += '<option value="'+ json.languages[i]["id"] + '">' + json.languages[i]["name"] + '</option>';
			   if (i == 0) {
			   		option += '<option value="15">No Language</option>';
			   }
			}
			jQuery('#languages').append(option);

			var option = '<option value="8">Any</option>';
			for (i=0;i<json.project_types.length;i++){
			   option += '<option value="'+ json.project_types[i]["id"] + '">' + json.project_types[i]["name"] + '</option>';
			}
			jQuery('#project_types').append(option);
		  });

	  
	jQuery( "#levels" ).change(function() {
		searchByFilters();
		});

	jQuery( "#languages" ).change(function() {
		searchByFilters();
		});

	jQuery( "#project_types" ).change(function() {
		searchByFilters();
		});
		
		jQuery('#languages').multiselect({
			buttonClass: 'col-sm-12 form-button',
			buttonContainer: '<div class="col-sm-4 form-control form-button-container" />',
			nonSelectedText: 'Languages'
		});
		
		jQuery('#levels').multiselect({
			buttonClass: 'col-sm-12 form-button',
			buttonContainer: '<div class="col-sm-4 form-control form-button-container" />',
			nonSelectedText: 'Education Level'
		});
		
		jQuery('#project_types').multiselect({
			buttonClass: 'col-sm-12 form-button',
			buttonContainer: '<div class="col-sm-4 form-control form-button-container" />',
			nonSelectedText: 'Project Type'
		});
		
		jQuery('.multiselect-selected-text').next().remove();
	});
	
	jQuery(window).load(function() {
            jQuery('#languages').multiselect('rebuild');
            jQuery('#levels').multiselect('rebuild');
            jQuery('#project_types').multiselect('rebuild');
        });
	

	function searchByFilters() {
		jQuery.ajax({
			  type: "POST",
			  url: "https://casls.uoregon.edu/wp-content/themes/caslstheme/submitSearch.php",
			  data: { level: jQuery( "#levels" ).val(),  language: jQuery( "#languages" ).val(),  project_type: jQuery( "#project_types" ).val()}
			})
			  .done(function( msg ) {
				jQuery('#searchResults').empty();
			    var json = JSON.parse(msg);

				var project = '';

				for (i=0;i<json.length;i++){
					if (json[i]['image_path'] != null) {
						project = 
							"<div class='col-xs-4 display-project'><a href='" + json[i]['link'] + "'>" +
							"<img class='img-responsive display-project-image' src='https://casls.uoregon.edu/wp-content/themes/caslstheme" + json[i]['image_path'] + "' style='float: right; margin-top: 10px; margin-left:10px; margin-right:10px; max-width:180px; max-height: 120px;'>" +
							"<div class='display-project-description'><p>" + json[i]['alias'] + "</p></ br><p>" + json[i]['description'] + 		 
							"</p></div>" + "<div class='display-project-title'><h4>" + json[i]['title'] + "</h4></div>" +
						  	"</a></div>";
						jQuery('#searchResults').append(project);
				} 
				else{
					project = 
						"<div class='col-xs-4 display-project'><a href='" + json[i]['link'] + "'>" +
							"<img class='img-responsive display-project-image' src='https://casls.uoregon.edu/wp-content/themes/caslstheme/images-programs/logos/Green-Placeholder.png" + "' style='float: right; margin-top: 10px; margin-left:10px; margin-right:10px; max-width:180px; max-height: 120px;'>" +
							"<div class='display-project-description'><p>" + json[i]['alias'] + "</p></ br><p>" + json[i]['description'] + 		 
							"</p></div>" + "<div class='display-project-title'><h4>" + json[i]['title'] + "</h4></div>" +
						  	"</a></div>";
					jQuery('#searchResults').append(project);
				}
				mod = '';
			  }
	});
	}
	function getAllProjects() {
		jQuery.ajax({
			  type: "GET",
			  url: "https://casls.uoregon.edu/wp-content/themes/caslstheme/getAllProjects.php"
			})
			  .done(function( msg ) {
				jQuery('#searchResults').empty();
//			    alert( "Data Saved: " + msg );
// 			    console.log(msg);
			    var json = JSON.parse(msg);
// 			    console.log(json);

				var project = '';
				for (i=0;i<json.length;i++){
					if (json[i]['image_path'] != null) {
						project = 
							"<div class='col-xs-4 display-project'><a href='" + json[i]['link'] + "'>" + 
							"<img class='img-responsive display-project-image' src='https://casls.uoregon.edu/wp-content/themes/caslstheme" + json[i]['image_path'] + "' style='float: right; margin-top: 10px; margin-left:10px; max-width:180px; max-height: 120px;' alt='"+ json[i]['alt_tag'] +"'>" +
							"<div class='display-project-description'><p>" + json[i]['alias'] + "</p></ br><p>" + json[i]['description'] + 		 
							"</p></div>" + "<div class='display-project-title'><h4>" + json[i]['title'] + "</h4></div>" +
						  	"</a></div>";
						jQuery('#searchResults').append(project);
				} 
				else{
					project = 
						"<div class='col-xs-4 display-project'><a href='" + json[i]['link'] + "'>" +
							"<img class='img-responsive display-project-image' src='https://casls.uoregon.edu/wp-content/themes/caslstheme/images-programs/logos/Green-Placeholder.png" + "' style='float: right; margin-top: 10px; margin-left:10px; margin-right:10px; max-width:180px; max-height: 120px;' alt='"+ json[i]['alt_tag'] +"'>" +
							"<div class='display-project-description'><p>" + json[i]['alias'] + "</p></ br><p>" + json[i]['description'] + 		 
							"</p></div>" + "<div class='display-project-title'><h4>" + json[i]['title'] + "</h4></div>" +
						  	"</a></div>";
					jQuery('#searchResults').append(project);
				}
				mod = '';
			  }
	});
	}
    </script>
