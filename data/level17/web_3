  </script>
  <script src="//cdn.uadigital.arizona.edu/lib/ua-bootstrap/v1.0.0-beta.15/ua-bootstrap.min.js">
  </script>
  <script src="/assets/js/jquery.waypoints.min.js">
  </script>
  <script type="text/javascript">
   function gaEventTag(ecategory, eaction, elable){
				gtag('event', eaction, {
					'event_category': ecategory,
					'event_label': elable
				});
			}
			var trackedbtn = document.querySelectorAll('.tracking_btn');
			for(var i = 0; i < trackedbtn.length; i++){
				trackedbtn[i].addEventListener('click', function(e){
					if(this.hasAttribute("data-trackclick")){
						var label = this.dataset.trackclick;
						gaEventTag('Button', 'click', label);
					}
				})
			}
  </script>
  <script type="text/javascript">
   $(function(){
					$('.dist_degree_click').click(function(){
						if(!$(this).hasClass('active')){
							$(this).addClass('active');
							$('.dist_degree_click').not($(this)).removeClass('active');
							$(this).attr("aria-expanded", "true");
							$('.dist_degree_click').not($(this)).attr("aria-expanded", "false");
							var thismatch = $(this).attr("data-distdegclk");
							$('.dist_degree_cont').attr("aria-hidden","true");
							$('.dist_degree_cont[data-distdeglst="'+thismatch+'"]').attr("aria-hidden","false");
						}
					})
				})
				//jQuery Waypoint viewed Element tracking
				function wayPointTrack(el){
					let trackSecTitle = el.dataset.tracktitle;
					var waypoint = new Waypoint({
						element: el,
						handler: function(){
							gaEventTag('Scroll', 'Viewed', trackSecTitle);
							this.destroy()
						}
					});
				}
				//All jQuery Waypoint tracking elements
				const allTrackingSections = document.querySelectorAll('.trackscrollelm');
				allTrackingSections.forEach(thisTrackingSection => wayPointTrack(thisTrackingSection));
  </script>
