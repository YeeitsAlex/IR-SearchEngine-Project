     <div data-role="collapsible" id="radio-fault">
      <h3>
       Fault Rupture
      </h3>
      <input class="radio-layers" id="FLT_00" name="radio-layers" type="radio" value="other"/>
      <label for="FLT_00">
       Extent
      </label>
      <a class="ui-btn ui-corner-all ui-shadow ui-btn-inline" data-rel="popup" href="#infocontent_Ext" id="info_Ext">
      </a>
      <div class="ui-content" data-position-to="window" data-role="popup" id="infocontent_Ext" style="max-width:280px">
       <a class="ui-btn-right" data-icon="delete" data-iconpos="notext" data-rel="back" data-role="button" data-theme="a" href="#">
        Close
       </a>
       <p>
        This is the extent of fault rupture corresponding to selected Cascadia earthquake scenario.
       </p>
      </div>
     </div>
     <div data-role="collapsible" id="radio-subsidence">
      <h3>
       Subsidence
      </h3>
      <input class="radio-layers" id="11" name="radio-layers" type="radio" value="other"/>
      <label for="11">
       Coastal Coseismic Subsidence
      </label>
