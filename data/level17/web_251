  <script type="text/javascript">
   var view = getCookie( "view" );
if( view )  {
    goLogin();
}    
else {
    if( device.desktop() ) {
        goLogin();
    }
    else {  
        // uncomment for production:
        setCookie( "view", "mobile" );
        goLogin( "mobile" );
    }
}
  </script>
