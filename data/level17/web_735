   <script>
    $(function() {

				particlesJS('section-home-1-particles', particles_config);
				//particlesJS('section-home-3-particles', particles_config);
				particlesJS('section-home-5-particles', particles_config);
				particlesJS('section-home-7-particles', particles_config);
				//particlesJS('section-home-9-particles', particles_config_thin);


				(function() { // Home UI circles/lines functionality

					$(window).resize(function() {
						$('.content-circle, .content-profile-circle, .content-sub-circle').each(function() {
							$(this).data('radius', Math.round($(this).outerWidth() / 2));
						});
					}).trigger('resize');

					var drawLine = function(section, start, end, options, callback) {
						$(section).append(function() {
							var line = $('<div class="content-circle-line"></div>').addClass(options && options.classes);
							var speed = (options && options.speed) || 500;

							var fullWidth = 0;
							$(window).resize(function() {
								if(start.hasClass('show')) {
									var startCenterLeft = start.offset().left - section.offset().left + start.data('radius');
									var startCenterTop = start.offset().top - section.offset().top + start.data('radius');
								} else {
									var startCenterLeft = start.offset().left - section.offset().left;
									var startCenterTop = start.offset().top - section.offset().top;
								}
								if(end.hasClass('show')) {
									var endCenterLeft = end.offset().left - section.offset().left + end.data('radius');
									var endCenterTop = end.offset().top  - section.offset().top + end.data('radius');
								} else {
									var endCenterLeft = end.offset().left - section.offset().left;
									var endCenterTop = end.offset().top  - section.offset().top;
								}

								var angle = Math.atan((endCenterTop - startCenterTop) / (endCenterLeft - startCenterLeft));
								if((endCenterTop - startCenterTop) > 0) { // Adjust the angle depending on if the line should be going up or down the page
									if(angle < 0) angle += Math.PI;
								} else {
									if(angle > 0) angle -= Math.PI;
								}

								fullWidth = Math.hypot(endCenterTop - startCenterTop, endCenterLeft - startCenterLeft);
								if(false && end.hasClass('show')) {
									var width = fullWidth;
								} else {
                                    if( line.width() > 0 ){
                                        var width = fullWidth;
                                    }else{
                                        var width = 0;
                                    }
								}

								line.css({
									width: width, height: 0,
									left: startCenterLeft,
									top: startCenterTop,

									'-moz-transform': 	 'rotate(' + angle + 'rad)',
									'-webkit-transform': 'rotate(' + angle + 'rad)',
									'-ms-transform': 	 'rotate(' + angle + 'rad)',
									'-o-transform': 	 'rotate(' + angle + 'rad)',
									'transform': 		 'rotate(' + angle + 'rad)'

								});
							}).trigger('resize');

							line.animate({ width: fullWidth }, speed, callback);

							return line;
						});
					};

					var step1 = function(stop) {
						var stop = stop || false;
						var section = $('#section-home-1-particles');
						var circle1 = $('h2.content-circle', section);
						circle1.addClass('show');

						window.setTimeout(function() {
							var circle2 = $('h4.content-circle', section);
							drawLine(section, circle1, circle2, { speed: 1000, classes: 'show-mobile' }, function() {
								circle2.addClass('show');
								window.setTimeout(function() {
									var circle3 = $('div.content-circle', section);
									drawLine(section, circle2, circle3, { speed: 1000, classes: 'show-mobile' }, function() {
										circle3.addClass('show');
										if(!stop) {
											window.setTimeout(function() {
												var timer = window.setInterval(function() {
													if(wp2 === true) {
														step2();
														window.clearInterval(timer);
													}
												}, 25);
											}, 1050);
										} 
									});
								}, 1050);
							});

						}, 1500);
					};

					var step2 = function(stop) {
						var stop = stop || false;
						var section = $('#section-home-1-particles');
						var start = $('div.content-circle', section);
						var end = $('.content-circle', '#section-home-7-particles');
						drawLine(section, start, end, {speed: 3500}, function() {
							end.addClass('show');
							if(!stop) {
								window.setTimeout(function() {
									var timer = window.setInterval(function() {
										if(wp3 === true) {
											step3();
											window.clearInterval(timer);
										}
									}, 25);
								}, 1050);
							} 
						});
					};

					var step3 = function(stop) {
						var stop = stop || false;
						var section = $('#section-home-7-particles');
						var start = $('.content-circle', section);
						var end = $('.content-circle', '#section-home-5-particles');
						drawLine(section, start, end, {speed: 2000}, function() {
							end.addClass('show');
							if(!stop) window.setTimeout(step4, 1050);
						});
					};

					var step4 = function(stop) {
						var stop = stop || false;
						var section = $('#section-home-5-particles');
						var start = $('.content-circle', section);
						var end1 = $('.content-profile-circle', section).first();
						var end2 = $('.content-profile-circle', section).last();
						drawLine(section, start, end1, {speed: 1000, classes: 'show-mobile'}, function() {
							end1.addClass('show');
						});
						drawLine(section, start, end2, {speed: 1000, classes: 'show-mobile'}, function() {
							end2.addClass('show');
						});
						if(!stop) {
							window.setTimeout(function() {
								var timer = window.setInterval(function() {
									if(wp5 === true) {
										step5();
										window.clearInterval(timer);
									}
								}, 25);
							}, 2050);
						} 
					};

					/*
					var step5 = function(stop) {
						var stop = stop || false;
						var section = $('#section-home-5-particles');
						var start = $('.content-circle', section);
						var end = $('.content-circle', '#section-home-7-particles');
						drawLine(section, start, end, {speed: 2000}, function() {
							end.addClass('show');
							if(!stop) {
								window.setTimeout(function() {
									var timer = window.setInterval(function() {
										if(wp6 === true) {
											step6();
											window.clearInterval(timer);
										} 
									}, 25);
								}, 1050);
							}
						});
					};

					var step6 = function(stop) {
						var stop = stop || false;
						var section = $('#section-home-7-particles');
						var start = $('.content-circle', section);
						var end = $('.content-circle', '#section-home-9-particles');
						drawLine(section, start, end, {speed: 2000}, function() {
							end.addClass('show');
							if(!stop) window.setTimeout(step7, 1050); 
						});
					};


					var step5 = function(stop) {
						var stop = stop || false;
						var section = $('#section-home-5-particles');
						var start = $('.content-circle', section);
						var end = $('.content-circle', '#section-home-9-particles');
						drawLine(section, start, end, {speed: 2000}, function() {
							end.addClass('show');
							if(!stop) window.setTimeout(step7, 1050); 
						});
					};

					var step7 = function() {
						var section = $('#section-home-9-particles');
						var start = $('.content-circle', section);
						var end1 = $('.content-sub-circle', section).eq(0);
						var end2 = $('.content-sub-circle', section).eq(1);
						var end3 = $('.content-sub-circle', section).eq(2);
						var end4 = $('.content-sub-circle', section).eq(3);
						var end5 = $('.content-sub-circle', section).eq(4);

						window.setTimeout(function() { $(window).trigger('resize'); }, 2050);

						drawLine(section, start, end1, {speed: 1000, classes: 'line-light show-mobile'}, function() {
							end1.addClass('show');
						});
						drawLine(section, start, end2, {speed: 1000, classes: 'line-light show-mobile'}, function() {
							end2.addClass('show');
						});
						drawLine(section, start, end3, {speed: 1000, classes: 'line-light show-mobile'}, function() {
							end3.addClass('show');
						});
						drawLine(section, start, end4, {speed: 1000, classes: 'line-light show-mobile'}, function() {
							end4.addClass('show');
						});
						drawLine(section, start, end5, {speed: 1000, classes: 'line-light show-mobile'}, function() {
							end5.addClass('show');
						});
					
					};
					*/

					var wp2 = false;
					var waypoint2 = new Waypoint({
						element: $('#section-home-2'),
						offset: '99%',
						handler: function(direction) {
							wp2 = true;
						}
					});

					var wp3 = false;
					var waypoint3 = new Waypoint({
						element: $('#section-home-4'),
						offset: '99%',
						handler: function(direction) {
							wp3 = true;
							wp2 = true;
						}
					});

					var wp5 = false;
					var waypoint5 = new Waypoint({
						element: $('#section-home-6'),
						offset: '99%',
						handler: function(direction) {
							wp5 = true;
							wp3 = true;
							wp2 = true;
						}
					});

					var wp6 = false;
					var waypoint6 = new Waypoint({
						element: $('#section-home-8'),
						offset: '99%',
						handler: function(direction) {
							wp6 = true;
							wp5 = true;
							wp3 = true;
							wp2 = true;
						}
					});

					var launched = false;
					$(window).resize(function() {
						if($(window).width() > 800) {
							if(!launched) window.setTimeout(step1, 500);
                            $('section > div').not('#section-home-1').find('.content-circle, .content-profile-circle, .content-sub-circle').addClass('show');
						} else {
							if(!launched) window.setTimeout(function() { step1(true); step4(true); step7(); }, 500);
							$('section > div').not('#section-home-1').find('.content-circle, .content-profile-circle, .content-sub-circle').addClass('show always-show');
						}
						launched = true;
					}).trigger('resize');

				})();

				$('#section-home-5 .content-profile-circle').click(function() {
					if($(this).hasClass('active')) return false;

					$('#section-home-5 .content-profile.active').removeClass('active');
					$('#section-home-5 .content-profile-circle.active').removeClass('active');

					var index = $(this).data('index');
					$('#section-home-5 .content-profile').filter('[data-index="' + index + '"]').addClass('active');
					$('#section-home-5 .content-profile-circle').filter('[data-index="' + index + '"]').addClass('active');

					return false;
				});

                $(window).resize(function() {

                	var height = Math.round($('.content-image').first().outerWidth() * 0.611111);
                	$('.content-image').css('height', height);

					$('iframe', '.callout-article-youtube, .callout-article-vimeo').each(function() {
						var ratio = $(this).attr('height') / $(this).attr('width');
						var width = $(this).width();
						$(this).css('height', Math.round(width * ratio));
					});

                }).trigger('resize');

            });
   </script>
