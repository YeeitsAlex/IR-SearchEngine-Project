            style = document.createElement('link');
            style.type = "text/css";
            style.id = "ff_style";
            style.rel = "stylesheet";
            style.href = "https://gardens.tamu.edu/wp-content/plugins/flow-flow-social-streams/css/public.css";
            style.media = "screen";
