  <script>
   /*place flame on util bar after scroll */
	var pagetop = $("#page-top"); 
	var fixedbar = $(".fixed-util-bar");	
	$(window).scroll( function() {
      if ($(window).scrollTop() > (pagetop.height() - fixedbar.height())) {
        $(".flame").show();
      } else {
		$(".flame").hide();
      }
	});
	/*fixed-util-bar on child sites may got to 2 lines in tablet view, causes logo area to be obscured. 
	if so, detect fixed-util-bar height and change #page-top .container .row top margin*/
	if ($('.fixed-util-bar').outerHeight(true) > 44){
	  $('#page-top .container .row').css('margin-top','89px');
	}
	else {
	  $('#page-top .container .row').css('margin-top','44px');
	}
  </script>
  <script type="text/javascript">
   var uvOptions = {};
(function() {
