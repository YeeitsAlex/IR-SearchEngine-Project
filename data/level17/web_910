           <script type="text/javascript">
            var closed = true;

$(window).load(function() {
  $('.btn-navbar').click(function(){
    if (closed == true){
      $('.wrapNav ul.mainNav').animate({height:'toggle'},1000);
      closed = false;
    }else{
      $('.wrapNav ul.mainNav').animate({height:'toggle'},1000);
      closed = true;
   };
  });

  $(window).resize(function() {
    var width = $(window).width();
    if (width < 754) {
            $('.wrapNav ul.mainNav').hide();
    }else {
            $('.wrapNav ul.mainNav').show();
    }
  });

});
           </script>
