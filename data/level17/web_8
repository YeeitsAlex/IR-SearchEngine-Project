<script>
 $(function() {
    var $form = $('#nl_subscribe form');
    $form.find('#newsletter_url').val(window.location.href);

    $form.submit(function(e) {
      if (arguments.callee.started) {
        e.preventDefault();
        return false;
      }
      arguments.callee.started = true;
      $.ajax({
        type: 'POST',
        url: 'https://subscribe.osu.edu/requests',
        data: $form.serialize(),
        success: function(data) {
          if (typeof fbq !== 'undefined') {
            fbq('track', 'Lead', {value: 1.00, currency: 'USD'});
          }
          if (typeof ga !== 'undefined') {
            ga('send', 'event', 'Subscribe Form', 'submit', 'Marketing External Email Subscribe');
          } else if (typeof gaTracker !== 'undefined') {
            gaTracker('send', 'event', 'Subscribe Form', 'submit', 'Marketing External Email Subscribe');
          }

          var $thanks = $('<div>').addClass('col-sm-12 text-center span-24'),
              $moreLink = $('<a>').text('Start exploring!').attr('href', 'https://insights.osu.edu/');

          $('<h4>').text('Thank you for subscribing')
                   .appendTo($thanks);
          $('<p>').text('Look for Ohio State Insights in your inbox this week.')
                  .appendTo($thanks);
          $('<p>').addClass('no-bot')
                  .text('Ready to immerse yourself in inspiring, engaging content right now? ')
                  .append($moreLink)
                  .appendTo($thanks);

          $('#nl_form').fadeOut('fast', function() {
            $(this).empty().append($thanks).fadeIn('fast');
          });
        },
        error: function(xhr) {
          var messages = ["An error occurred during your subscription request."],
              errors = xhr.responseJSON;

          if (errors instanceof Array) {
            messages = [];
            for (var i in errors) {
              messages.push(errors[i].title);
            }
          }

          $form.find('.input-group').addClass('has-error');
          $form.find('#newsletter_help').text(messages.join(', '));
          arguments.callee.started = false;
        }
      });

      e.preventDefault();
    });
  });
</script>
