   <script>
    document.addEventListener('DOMContentLoaded', function(){//set form question min-height with JS, no jquery needed
var $Qs = document.getElementsByClassName('form__inner__lead-form__question'); //get quesitons
if(typeof $Qs !== "undefined"){
var formQminheight = 0;
for (var i=0;i<$Qs.length;i++){ $Qs[i].style.display='block'; } //set questions to be visible            
var $formQWraps = document.getElementsByClassName('form__inner__lead-form__question__wrap');//get each question wrap
for (var i=0;i<$formQWraps.length;i++){         
var tempHeight = $formQWraps[i].clientHeight;
if (tempHeight > formQminheight) formQminheight=tempHeight;//if a question is the tallest question, set variable
}
if (formQminheight > 0 ) for (var i=0;i<$formQWraps.length;i++){$formQWraps[i].style.minHeight=formQminheight+"px";}//set min heights on each question
for (var i=0;i<$Qs.length;i++){$Qs[i].style.display='';}//remove display from questions
}
});
   </script>
