          <section class="av_textblock_section " itemscope="itemscope" itemtype="https://schema.org/CreativeWork">
           <div class="avia_textblock " itemprop="text">
            <h3 class="myh3">
             EVENTS AND DEADLINES
            </h3>
           </div>
          </section>
          <div class="avia-builder-widget-area clearfix avia-builder-el-3 el_after_av_textblock el_before_av_textblock ">
           <div class="widget clearfix widget_em_widget" id="em_widget-3">
