	function jl_htmlencode(value){
		if (value) {
			return $('<div/>').text(value).html();
		}else {
			return "";
		}
	}

	function jl_htmldecode(value){
		if (value) {
			return $('<div/>').html(value).text();
		}else {
			return "";
		}
	}
	
	function jl_updateTips( t ) {
		var tips = $( ".validateTips" );
		tips.text( t ).addClass( "ui-state-highlight" );
		
		/*
		setTimeout(function() {
			tips.removeClass( "ui-state-highlight", 1500 );
		}, 4000 );
		*/
	}

	function jl_checkLength( o, n, min, max ) {
		if ( o.val().length > max || o.val().length < min ) {
			o.addClass( "ui-state-error" );
			jl_updateTips( "Length of " + n + " must be between " +
				min + " and " + max + "." );
			return false;
		} else {
			return true;
		}
	}

	function jl_checkRegexp( o, regexp, n ) {
		patt = new RegExp(regexp);
		
		if ( !( patt.test( o.val() ) ) ) {
			o.addClass( "ui-state-error" );
			jl_updateTips( n );
			return false;
		} else {
			return true;
		}
	}
		
	function jl_checkList( o, n, min ) {
		if ( o.size() < min ) {
			o.addClass( "ui-state-error" );
			jl_updateTips( "You must choose at least " + min + " " + n + "." );
			return false;
		} else {
			return true;
		}
	}

	function jl_showalert(selector, message) {
		$("<p></p>").html(message).appendTo(selector).hide().slideDown("slow").delay(6000).slideUp("slow");
	}

			
