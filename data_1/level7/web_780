		$("#close-img").click(function(e){
			e.preventDefault();
			$("#current-img a").remove();
			$("#current-img").fadeOut();
		});

	 
	


function setPreviewFrame() {
	var browserWidth = $(window).width();
	//console.log("Browser WIDTH: " + browserWidth);
	var previewWidth = 540 - 12; // minus number of border on image	
	if (browserWidth <= 540) { // only affect the frame if we are viewing mobile content
		var previewWidth = browserWidth - 12; // minus number of border on image	
	} 
	var previewHeight = Math.round(previewWidth * 0.705);
	var leftMar = (browserWidth - (previewWidth + 12))/2;	
	$("#current-img").css({width: previewWidth + "px", height: previewHeight + "px", left: leftMar + "px"});
	
	
	//console.log("Preview WIDTH: " + previewWidth);
	//console.log("Preview HEIGHT: " + previewHeight);
	//console.log("LEFT: " + leftMar);
			
};

//set preview size if window resized, such as with phone rotation.
$(window).resize(function() {
	setPreviewFrame();
});
//Set initial preview frame size
setPreviewFrame();		


	
}); //end jquery ready function 

