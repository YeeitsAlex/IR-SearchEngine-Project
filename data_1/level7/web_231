   $(function() {
		var e = $("#ANRLoginEmail");
		var email = e.attr("value").trim();
		if (email.length < 1) {
			e.focus();
		} else {
			$("#ANRLoginPassword").focus();
		}

		$("#screen").attr("value", screen.width+'x'+screen.height);

		if (parent != window) parent.window.location = "https://ucanr.edu/portal/login/universallogin.cfm?appname=sites";
	});
  </script>
