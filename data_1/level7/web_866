function CheckLogonFields()
{
	var flag = true
	var strErrorMessage = "";
	
	if (document.form.strUserName.value == null || document.form.strUserName.value == "") {
		strErrorMessage = strErrorMessage + "Enter UCSBnetID." + CRLF;
		document.form.strUserName.focus();
		
		flag = false
	} 	
	
	if (document.form.strPassword.value == null || document.form.strPassword.value == "") {
		strErrorMessage = strErrorMessage + "Enter password." + CRLF;
		document.form.strPassword.focus();
		
		if (document.form.strUserName.value == null || document.form.strUserName.value == "") {
			document.form.strUserName.focus();
		}
		
		flag = false
	} 	
	
	return flag
}

