 (function($){
    'use strict';
	jQuery(document).ready(function($){ 
	
		  
	  var retina = window.devicePixelRatio > 1 ? true : false;
                 if(retina) {
        	jQuery('#logo .default-logo').attr('src', 'http://www.mimg.ucla.edu/wp-content/uploads/2017/03/UCLA_MIMG_Logo.png');
        	jQuery('#logo img').css('max-width', '400px');
						jQuery('#logo .white-logo').attr('src', 'http://www.mimg.ucla.edu/wp-content/uploads/2017/03/UCLA_MIMG_Logo.png');
						        }
                
		/* ------------------------------------------------------------------------ */
		/* Add PrettyPhoto */
		/* ------------------------------------------------------------------------ */
		
		var lightboxArgs = {			
						animation_speed: 'fast',
						overlay_gallery: true,
			autoplay_slideshow: false,
						slideshow: 5000, /* light_rounded / dark_rounded / light_square / dark_square / facebook */
									theme: 'pp_default', 
									opacity: 0.8,
						show_title: true,
						deeplinking: false,
			allow_resize: true, 			/* Resize the photos bigger than viewport. true/false */
			counter_separator_label: '/', 	/* The separator for the gallery counter 1 "of" 2 */
			default_width: 1200,
			default_height:640
		};

		jQuery("a[rel^='prettyPhoto']").prettyPhoto(lightboxArgs);
	
	
			});
}(jQuery))
