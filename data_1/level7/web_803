    var htmlDiv = document.getElementById("rs-plugin-settings-inline-css"); var htmlDivCss=".tp-caption.medium_thin_grey,.medium_thin_grey{font-size:34px;line-height:30px;font-weight:300;font-family:\"Open Sans\";color:rgb(102,102,102);text-decoration:none;background-color:transparent;padding:1px 4px 0px;border-width:0px;border-color:rgb(255,214,88);border-style:none;text-shadow:none;margin:0px}.tp-caption.Welcome-White,.Welcome-White{color:rgba(224,214,199,1.00);font-size:60px;line-height:70px;font-weight:500;font-style:normal;font-family:Arial,Helvetica,sans-serif;padding:10px 10px 10px 10px;text-decoration:none;text-align:left;background-color:rgba(0,0,2,0.65);border-color:transparent;border-style:none;border-width:0px;border-radius:5px 5px 5px 5px;text-shadow:none;margin:0px}";
				if(htmlDiv) {
					htmlDiv.innerHTML = htmlDiv.innerHTML + htmlDivCss;
				}else{
					var htmlDiv = document.createElement("div");
