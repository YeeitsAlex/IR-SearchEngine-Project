     <section id="centennial-news">
      <div class="container">
       <h2>
        From the Newsroom
       </h2>
       <script type="text/javascript">
        /* get news articles from RSS feed */
				$(function() {
					$.get('https://newsroom.ucla.edu/cats/ucla_centennial_campaign.xml', function(data) {
						var $xml = $(data);
						var newsCount=0;
						$xml.find("item").each(function() {
							var $this = $(this),
							item = {
								title: $this.find("title").text(),
								link: $this.find("link").text(),
								description: $this.find("description").text()
							}
							descriptionText = item.description;
							// truncate the description to required length
							descriptionText = descriptionText.replace(/^(.{130}[^\s]*).*/, "$1") + "...";
							/* show 4 articles */
							if(newsCount < 4) {
								newsArticle = "<article><h2><a target='_blank' href='"+item.link+"'>"+item.title+"</a></h2><p>"+descriptionText+"</p><a //target='_blank' href='"+item.link+"' class='read-story-link'>Read Story</a></article>";
								$("#centennial-news .container").append(newsArticle);
								newsCount++;
							}
						});
					});
				});
       </script>
      </div>
      <a class="more-updates-button" href="http://newsroom.ucla.edu/dept/ucla-centennial-campaign">
       View More Updates
      </a>
     </section>
