      <div class="container">
       <h1>
        Stories in the Spotlight
       </h1>
      </div>
      <div class="container">
       <div id="stories-listing-wrapper">
        <script src="js/stories.json" type="text/javascript">
        </script>
        <script type="text/javascript">
         $(function ($) {
                    const skipStories = 0; //NO. OF STORIES TO SKIP FROM STORIES.JSON
    				const numStoriesToShow = 4; // number of story items to show

                    $.each(jsonStories.slice(skipStories, numStoriesToShow), function (i, value) {
    					var storyItem = "<article><a href='" + value.storyURL + "'><img src='" + value.imagePath + "' alt='" + value.imageAltTag + "'></a><div class='story-details'><h2><a href='" + value.storyURL + "'>" + value.heading + "</a></h2><p>" + value.details + "</p><a href='" + value.storyURL + "' class='read-story-link'>Read Story</a></div></article>";
    					
                        $('#stories-listing-wrapper').append(storyItem);
    				});
    			});
        </script>
       </div>
      </div>
     </section>
     <section id="moreMomentum">
      <div class="container">
       <h1>
        More Great Acts
       </h1>
       <div id="more-momentum-listing-wrapper">
        <div id="load-more-momentum-stories-container">
        </div>
        <script src="js/moreGreatActs.json" type="text/javascript">
        </script>
        <script type="text/javascript">
         $(function ($) {
    				const skipStories = 0; //NO. OF STORIES TO SKIP FROM moreGreatActs.json
                    const numActsToShow = 6; // number of story items to show
                    
                    $.each(jsonGreatActs.slice(skipStories, numActsToShow), function (i, value) {
    					var storyItem = "<article><a href='" + value.storyURL + "'><img src='" + value.imagePath + "' alt='" + value.imageAltTag + "'></a><div class='story-details'><h2><a href='" + value.storyURL + "'>" + value.heading + "</a></h2><p>" + value.details + "</p><a href='" + value.storyURL + "' class='read-story-link'>Read Story</a></div></article>";
    					$('#load-more-momentum-stories-container').append(storyItem);
    					
    				});
    			});
        </script>
       </div>
