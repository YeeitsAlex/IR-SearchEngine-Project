  <script>
   //Analytics Tracking
		(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
		(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
		m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
		})(window,document,'script','//www.google-analytics.com/analytics.js','ga');
		ga('create', 'UA-9067969-4', 'auto', {'allowLinker': true});
		ga('create', 'UA-35157935-1', 'auto', 'clientTracker', {'allowLinker': true}); //UCLA Marketing & Special Events CRM
		ga('require', 'displayfeatures');
		ga('clientTracker.require', 'displayfeatures');
		ga('require', 'ecommerce', 'ecommerce.js');
		ga('clientTracker.require', 'ecommerce', 'ecommerce.js');
		ga('require', 'linker');
		ga('linker:autoLink', ['ipay.com', 'giveto.ucla.edu', 'giving.ucla.edu', 'ucla.edu'] );
		ga('send', 'pageview');
		ga('clientTracker.send', 'pageview');
		$(document).ready(function() {
			setTimeout(function() {
			$("#centennial-ribbon a, #nav-logo a, #nav-main-left li a, #nav-main-right li a, a.give_now_btn, a.icon-search, a.icon-contact, #hero-main .cta a, a.more-progress-button, #stories-listing-wrapper .story-details a, #stories-listing-wrapper article a, #more-momentum-listing-wrapper article a, #videos-side-by-side .video-cell a, #videos-side-by-side a, #centennial-news .container article h2 a, #centennial-news .container article a, #centennial-news a, footer ul li a, .footer-info-container ul li p a, .footer-info-container ul li a, #priorities #explore .container a, #centennial #explore .container a, #what-will-you .container a, #hero-main .link-container a, .fancybox-skin a").each(function() {
					$(this).click(function(event) {
					var link_text = $(this).text();
					var link_class = $(this).attr("class");
					var parent_div = $(this).closest("div").attr('id');
					var parent_section = $(this).closest("section").attr('id');
					var parent_class = $(this).closest("div").attr("class");
					var event_category = '';
					var event_label = '';
					var link_href = $(this).attr("href");
					var hero_home_main_area = $(this).closest("#hero-main").attr("id");

					link_href = link_href.replace("http://","").replace("https://","").replace("/","|");

					//TAKE CARE OF SEARCH AND CONTACT LINKS
					//link_class RETURNS CLASSES FOR TWO SPECIAL BUTTONS WITH ADDITIONAL SPACE AT THE END
					if(link_class == "iconFont icon-search "){
						link_text = "Search";
					} else if(link_class == "iconFont icon-contact ") {
						link_text = "Contact Us";
					}

					//DETERMINE EVENT CATEGORY AND ASSIGN EVENT LABEL

					if(parent_div == "nav-logo" || parent_div == "header" || parent_div == "centennial-ribbon" ) {
						event_category = "top_nav";
						event_label = event_category + "|" + link_href;
						if(link_href=="|") {
							event_label = event_category + "|" + parent_div;
						}
						if(link_href=="giveto.ucla.edu|") {
							event_label = event_category + "|" + link_text + " " + link_href;
						}
					}
					if(hero_home_main_area == "hero-main") {
						event_category = "hero_area";
						event_label = event_category + "|" + link_href;
					}
					if(link_class == "more-progress-button" || parent_div == "stories-listing-wrapper" || parent_section == "stories") {
						event_category = "home_top";
						event_label = event_category + "|" + link_href;
					}
					if(parent_div == "load-more-momentum-stories-container" || parent_section == "moreMomentum") {
						event_category = "home_mid";
						if($("body").hasClass("centennialStories")) {
							event_category = "stories";
						}
						event_label = event_category + "|" + link_href;
					}
					if(parent_class == "video-cell" || parent_section == "videos-side-by-side" || parent_section == "centennial-news" || parent_section == "what-will-you") {
						event_category = "home_bottom";
						event_label = event_category + "|" + link_href;
						if(parent_section == "centennial-news") {
							event_label = event_category + "|" + link_text;
							if(link_text == "Read Story") {
								event_label = event_category + "|" + link_text + "|" + link_href.slice(26);
							}
						}
						if(parent_div == "email-signup-input-wrapper") {
							event_label = event_category + "|" + "submit";
						}
						if(parent_section == "what-will-you") {
							event_label = event_category + "|" + link_href;
						}
					}
					if(/fancybox-close/i.test(link_class)) {
						event_label = event_category + "|" + "you_tube (video complete)";
					}
					if($("body").hasClass("centennialStories") && parent_section == "stories") {
						event_category = "stories";
						event_label = event_category + "|" + link_href;
						if(/play fancybox/i.test(link_class)) {
							event_label = event_category + "|" + link_href;
						}
					}
					if(/footer-link/i.test(link_class) || /footer-info-container/i.test(parent_class) || parent_class == "footer-links-container") {
						event_category = "footer";
						event_label = event_category + "|" + link_text;
					}
					if($("#priorities").length > 0 && parent_section == "explore") {
						event_category = "priorities";
						event_label = event_category + "|" + link_text;
						if(/give_now_btn/i.test(link_class)) {
							event_label = event_category + "|giving|give now centennial";
						}

					}
					if($("#progress").length > 0 && parent_section == "explore") {
						event_category = "progress";
						event_label = event_category + "|" + link_text;
						if(/give_now_btn/i.test(link_class)) {
							event_label = event_category + "|giving|give now centennial";
						}

					}

					//Create Event Label

					//remove extra tag text
					//event_label = event_label.replace("|undefined", "");
					event_label = event_label.replace("/", "|");
					event_label = event_label.replace("||", "|");
					console.log("---------------------");
					console.log("event_label: " + event_label);

					//FOR DEBUG
					//console.log('clicked on ' + link_text + ' with event_category ' + event_category + ', and parent_class ' + parent_class + ', parent div ' + parent_div + ', link class ' + link_class + ', parent section ' + parent_section + ', event_label ' + event_label + ', link_href ' + link_href);
					//event.preventDefault();
					ga('send', 'event', event_category, 'click', event_label);
					ga('clientTracker.send', 'event', event_category, 'click', event_label);
					}); /* end click function event */
			}); /* end each anchor function */

			//Parses the Url Query strings and returns an array
			var getUrlParam = function(url) {

				var tmp;
				var tmp_u   = url.split('?');

				if(typeof tmp_u[1] === 'undefined') return null;

				var tmp_p   = tmp_u[1].split('&');
				var param   = {};

				for(var i=0; i<tmp_p.length; i++) {

					tmp = tmp_p[i].split('=');
					param[tmp[0]] = tmp[1];
				}

				return param;
			}

			//send ga user id
			var sendID = function(id) {

				ga('set', 'dimension1', String(id) );
				ga('send', 'event', 'User ID', id);
			}

			//grab the parameters from the url query string
			var params = getUrlParam(document.URL);

			//send the id to google
			if( params !== null && params.uclaid !== null )
				sendID(params.uclaid);
		}, 1000);
		});
  </script>
