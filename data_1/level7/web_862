                    <meta content="A link between air pollution and Alzheimer’s disease" itemid="http://coeh.uci.edu/2018/07/31/pollution_ad/" itemprop="mainEntityOfPage" itemscope="" itemtype="https://schema.org/WebPage"/>
                    <meta content="2018-07-31" itemprop="datePublished"/>
                    <meta content="2018-08-27" itemprop="dateModified"/>
                    <div itemprop="publisher" itemscope="" itemtype="https://schema.org/Organization">
                     <meta content="UC Irvine Center for Occupational and Environmental Health" itemprop="name"/>
                    </div>
                    <div itemprop="author" itemscope="" itemtype="https://schema.org/Person">
