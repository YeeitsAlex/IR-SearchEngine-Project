  <script type="text/javascript">
   jQuery(document).ready(function() {

var rndNum = Math.ceil(Math.random() * 5);
  jQuery("body").css({ background: "url(/templates/pulvinar_wac/images/bg0"+rndNum + ".jpg) center center fixed no-repeat " });
      
/* this is where the social networking icons get swapped */   
  
   jQuery(function() {
  jQuery(".hoverswap").hover(
    function () {
      jQuery(this).attr("src", jQuery(this).attr("src").replace(/.png/, "-over.png"));
    },
    function () {
      jQuery(this).attr("src", jQuery(this).attr("src").replace(/-over.png/, ".png"));
    }
  );
});

/* random background  */   

});
jQuery(window).scroll(function() {    
    var scroll = jQuery(window).scrollTop();

    if (scroll >= 150) {
        jQuery(".right_side_mod").addClass("fix_right_side");
    } else {
        jQuery(".right_side_mod").removeClass("fix_right_side");
    }
});

jQuery(window).scroll(function() {    
    var scroll = jQuery(window).scrollTop();

    if (scroll >= 150) {
        jQuery(".main_menu_class").addClass("fix_left_side");
    } else {
        jQuery(".main_menu_class").removeClass("fix_left_side");
    }
});
  </script>
