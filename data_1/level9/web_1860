 <script>
  $(document).ready(function () {
    var target = document.location.hash.replace("#", "");
    if (target.length) {
        if(target=="option1"){
          showModal("title1","content1");
        }
        else if(target=="option2"){
            showModal("title2","content2");
        }
    }else{
        showModal("<div class='alert alert-danger'><strong>You are not logged in</strong></div>", "To take full advantage of all the content on this website tailored to your role at UCSF, please <a href='https://srs.ucsf.edu/hr.php?'>login via MyAccess</a>");
    }

    function showModal(title,content){
        $('#myModal .modal-title').html(title);
        $('#myModal .modal-body').html(content);
        $('#myModal').modal('show');
    }

});
 </script>
