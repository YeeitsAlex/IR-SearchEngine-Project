      <section class="block block-sils-custom even block-count-5 block-region-content-aside block-sils-search" id="block-sils-custom-sils-search">
       <div class="block-inner clearfix">
        <h2 class="block-title">
         Need help finding something?
        </h2>
        <div class="block-content content">
         <form accept-charset="UTF-8" action="/" id="sils-search-form" method="post">
          <div>
           <div class="search-text">
            I'm looking for information about
           </div>
           <div class="form-item form-type-select form-item-topic">
            <select class="form-select" id="edit-topic" name="topic">
             <option selected="selected" value="">
              topic
             </option>
             <option value="16">
              Admissions
             </option>
             <option value="20">
              Faculty and Staff
             </option>
             <option value="13">
              Graduate Programs
             </option>
             <option value="12">
              Undergraduate Programs
             </option>
             <option value="17">
              Student Funding
             </option>
             <option value="18">
              Careers
             </option>
             <option value="22">
              SILS News
             </option>
             <option value="23">
              SILS Events
             </option>
             <option value="19">
              Contact Information
             </option>
             <option value="26">
              Giving to SILS
             </option>
            </select>
           </div>
           <div class="search-text">
            with keywords
           </div>
           <div class="form-item form-type-textfield form-item-keyword">
            <input class="form-text" id="edit-keyword" maxlength="128" name="keyword" size="60" type="text" value=""/>
           </div>
           <input class="form-submit" id="edit-submit" name="op" type="submit" value="Search"/>
           <input name="form_build_id" type="hidden" value="form-IlD92HGkG4ZaaQrDMBrOMm_4p1bhlJY7xHV2TMSjc54"/>
