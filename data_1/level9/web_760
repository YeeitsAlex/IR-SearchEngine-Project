        <article class="post-10080 page type-page status-publish hentry" id="post-10080">
         <header class="entry-header">
          <h2 class="page-title">
           Home Too
          </h2>
         </header>
         <!-- .entry-header -->
         <div class="entry-content">
          <div class="panel-layout" id="pl-10080">
           <div class="panel-grid panel-has-style" id="pg-10080-0">
            <div class="siteorigin-panels-stretch panel-row-style panel-row-style-for-10080-0" data-stretch-type="full-stretched">
             <div class="panel-grid-cell" id="pgc-10080-0-0">
              <div class="so-panel widget widget_sow-hero panel-first-child panel-last-child" data-index="0" id="panel-10080-0-0-0">
               <div class="so-widget-sow-hero so-widget-sow-hero-default-a24d8f7ca0d2 so-widget-fittext-wrapper" data-fit-text-compressor="0.85">
                <div class="sow-slider-base " style="display: none">
                 <ul class="sow-slider-images" data-settings='{"pagination":true,"speed":800,"timeout":4000,"swipe":true}'>
                  <li class="sow-slider-image sow-slider-image-parallax" style="background-color: #333333">
                   <div class="sow-slider-image-container">
                    <div class="sow-slider-image-wrapper">
                     <h1 style="text-align: center">
                      Academic Affairs
                     </h1>
                     <h3 style="text-align: center">
                      THE UNIVERSITY OF UTAH
                     </h3>
                    </div>
                   </div>
                   <div class="sow-slider-image-overlay sow-slider-image-parallax" data-siteorigin-parallax='{"backgroundUrl":"https:\/\/academic-affairs.utah.edu\/wp-content\/uploads\/sites\/3\/2019\/02\/park-bkg.jpg","backgroundSize":[2000,1333],"backgroundSizing":"scaled"}' style="background-image: url(https://academic-affairs.utah.edu/wp-content/uploads/sites/3/2019/02/park-bkg.jpg);opacity: 0.62">
