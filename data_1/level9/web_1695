  function getStudentImageHtmlCode() {
    var dataIndex = Math.floor(Math.random() * imageUrls.length);
    var img = '<a title="Student Success" href=\"' + imageLinks[dataIndex] + '"><img src="';        
    img += imageUrls[dataIndex];
    img += '\" alt=\"' + imageAlts[dataIndex] + '\"/></a>';
    return img;
  }
