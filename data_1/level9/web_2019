    function VerifyQLForm() {
        var QLQueryHelp = "Enter a word or phrase";
        if (document.forms.QL.q.value == '' || document.QL.QuickLink.value == QLQueryHelp) {
            document.forms.QL.q.value = QLQueryHelp;
            document.forms.QL.q.select();
            alert('It appears that you clicked on the "Search" button but forgot to enter a UCI Campus Recreation search word. Please enter a word or phrase and try again.')
            return false;
        } else {
            return true;
        }
    }
