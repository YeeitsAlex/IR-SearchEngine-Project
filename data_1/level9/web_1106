  <script>
   $.getJSON( "js/bios.json?v=2", function(data) {			
			
			//add dropdown events
			$("#people-categories-list").change( function (data)  {		
				return function () {
					var category = this.options[this.selectedIndex].value;
					var newArray = [];
					
					//list all bios in this category					
					if( category == "all") { 
						newArray = data.slice(0);
					} else {
						$.each( data, function() {
							if($.inArray(category, this.category.split(" ")) > -1)  {
								newArray.push(this);
							
							}
						});	
					}
					
					$("#people-blurbs").empty();
					createBlurbGrid( $("#people-blurbs"), newArray, createFullProfileBlurb, 2, 5, false );
				}	
			}(data));
			
			$('#people-categories-list').trigger( 'change' );
			
		});


		
		//on document ready
		$(function() {
			addScrollLinks("body");

			// jssor slider
			jssor_1_slider_init();
				
			$(window).bind("scroll", function() {
				var atTop = $(this).scrollTop() < 200;
				$("#nav-wrap").toggleClass("nav-at-top", atTop );
				$("#nav-wrap").toggleClass("nav-fixed", !atTop );		
			});
			
			$(window).bind("resize", function() {
				var wwidth = $(window).width();
				var isMedium = wwidth < 1100;
				$("#nav-wrap").toggleClass("nav-med", isMedium );			
			});	
		
			//initial calculations
			$(window).trigger("scroll");
			$(window).trigger("resize");
		
			$('.hero-carousel').heroCarousel({
					easing: 'easeOutExpo',
					css3pieFix: true,
					prevText: '',
					nextText: ''
			});
			
			//jump to hashtag
			var hash = window.location.hash;
			_scrollTo(hash);

		});
				
		function createPeopleBlurb (dataArray) {
			var elementArray = [];
			
			var i = 0;
			while( i < dataArray.length ) {
				var data = dataArray[i];
				
				var d = document.createElement('div');
				var textwrap = document.createElement('div');
				var imgwrap = document.createElement('div'); 
				var img = document.createElement('img');
				var name = document.createElement('h3');
				var title = document.createElement('h4');
				var desc = document.createElement('p');
				var ellipsis = document.createElement('div');
				
				d.className = "people-blurb col col5";
				textwrap.className = "people-blurb-text";
				imgwrap.className = "people-img-wrap";
				img.className = "people-img";
				img.setAttribute('src', data.img);				
				name.innerHTML = data.name;
				title.innerHTML = data.title;
				desc.innerHTML = data.bio;
				
				textwrap.appendChild(name);
				textwrap.appendChild(title);
				
				imgwrap.appendChild(img);
				
				d.appendChild(imgwrap);
				d.appendChild(textwrap);
				
				//attaching lightbox				
				var lb_d = document.createElement('div');
				var lb_textwrap = textwrap.cloneNode(true);
				lb_textwrap.appendChild(desc);
				var lb_imgwrap = imgwrap.cloneNode(true);
				
				$(lb_textwrap).find(".ellipsis").remove();
				
				lb_d.appendChild(lb_imgwrap);
				lb_d.appendChild(lb_textwrap);
				lb_d.className = "people-lightbox clearfix";
				
				
				$(d).mousedown( function (content) {
					return function () {
						loadUrlLightBox(document.getElementById("people"), content, false);
					};
				}(lb_d));
				
				
				elementArray.push(d);
				i++;
			}
			
			return elementArray;
		}


		function createFullProfileBlurb (dataArray) {
			var elementArray = [];
			
			var i = 0;
			while( i < dataArray.length ) {
				var data = dataArray[i];
				
				var d = document.createElement('div');
				var textwrap = document.createElement('div');
				var imgwrap = document.createElement('div'); 
				var img = document.createElement('img');
				var name = document.createElement('h3');
				var title = document.createElement('h4');
				var desc = document.createElement('p');
				var ellipsis = document.createElement('div');
				
				d.className = "people-blurb col col5";
				d.setAttribute("url", "/people/" + data.name.replace(/\./g,"").replace(/ & /g," ").replace(/ /g, "-") + "?nav=0"); // )
				textwrap.className = "people-blurb-text";
				imgwrap.className = "people-img-wrap";
				img.className = "people-img";
				img.setAttribute('src', data.img);				
				name.innerHTML = data.name;
				title.innerHTML = data.title;
				desc.innerHTML = data.bio;
				
				textwrap.appendChild(name);
				textwrap.appendChild(title);
				
				imgwrap.appendChild(img);
				
				d.appendChild(imgwrap);
				d.appendChild(textwrap);
				
				//attaching lightbox				
				var lb_d = document.createElement('div');
				var lb_textwrap = textwrap.cloneNode(true);
				lb_textwrap.appendChild(desc);
				var lb_imgwrap = imgwrap.cloneNode(true);
				
				$(lb_textwrap).find(".ellipsis").remove();
				
				lb_d.appendChild(lb_imgwrap);
				lb_d.appendChild(lb_textwrap);
				lb_d.className = "people-lightbox clearfix";
				
				
				$(d).mousedown( function (content) {
					return function () {
						loadUrlLightBox(document.getElementById("content-container"), $(this).attr("url"), true);
					};
				}(lb_d));
				
				
				elementArray.push(d);
				i++;
			}
			
			return elementArray;
		}
  </script>
