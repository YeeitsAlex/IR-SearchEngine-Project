  function JointGiftOption(selectedValue, revealId)
    {
        $('#divJointGift').hide();
        if (selectedValue == "Y")
            $('#' + revealId).removeClass('hidden');
        else
            $('#' + revealId).addClass('hidden');
    }
    function GetWord(aString, delimit, wordNum) {

        var dl = delimit.length
        var pos = 0
        var tempstr = ""
        var nextpos = 0

        if (wordNum <= 0) {
            return aString
        }
        else {
            tempstr = aString

            for (var i = 1; i < wordNum; i++) {
                pos = tempstr.indexOf(delimit) + dl
                if (pos < dl) {
                    return ""
                } else {
                    tempstr = tempstr.substring(pos)
                }
            }

            nextPos = tempstr.indexOf(delimit)

            if (nextPos == -1) {
                return tempstr
            } else {
                return tempstr.substring(0, nextPos)
            }
        }
    }

    function SetOrgs(sType, sDDLID, sOrgListID, sFundListID) {
        var ddl = document.getElementById(sDDLID);
        var ddlHidden = document.getElementById("__" + sDDLID);
        var inp = document.getElementById(sOrgListID);
        var hSelectedOrgType = document.getElementById('hSelectedOrgType');
        hSelectedOrgType.value = sType;

        var sp = document.getElementById('lblWhich');
        if (sp != null) {
            if (sType == "S") {
                sp.innerHTML = "Which School Are You Giving To?";
            }
            else if (sType == "M") {
                sp.innerHTML = "Which Campus Area Are You Giving To?";
            }
            else if (sType == "C") {
                sp.innerHTML = "Which Cause Are You Giving To?";
            }
        }

        try {
            if (isPostBack) {
                sFundListID = 'xxyyzz';
                ddl = null;
            }
        }
        catch (err) { }
        var i = 0;
        var option;
        try {
            if (ddl != null && inp != null) {
                for (var option in ddl) {
                    ddl.remove(option);
                }
                option = document.createElement('option');
                option.value = "0";
                option.text = "--- Select ---";
                ddl.add(option);
                ddl.selectedIndex = 0;

                if (ddlHidden != null) {
                    ddlHidden.value = "--- Select ---|0|";
                }

                var ddlFund = document.getElementById(sFundListID);
                if (ddlFund != null) {
                    for (var fund in ddlFund) {
                        ddlFund.remove(fund);
                    }
                    option = document.createElement('option');
                    option.value = "";
                    option.text = "--- Select ---";
                    ddlFund.add(option);
                    ddlFund.selectedIndex = 0;
                }
                var sOrgList = inp.value;
                var sThisOrg = " ";
                var sThisCat;
                var iNumOrgs = 0;
                while (sThisOrg != "") {
                    i = i + 1;
                    sThisOrg = GetWord(sOrgList, ";", i);
                    sThisCat = GetWord(sThisOrg, "|", 1);
                    if (sThisCat == sType) {
                        iNumOrgs++;
                        option = document.createElement('option');
                        option.value = GetWord(sThisOrg, "|", 2);
                        option.text = GetWord(sThisOrg, "|", 3);
                        ddl.add(option);
                        if (ddlHidden != null) {
                            ddlHidden.value += option.text + "|" + option.value + "|";
                        }
                    }
                }
                var divWhichOrg = document.getElementById('divWhichOrg');
                if (iNumOrgs == 1) {
                    if (ddl.fireEvent) {
                        ddl.selectedIndex = 1;
                        divWhichOrg.style.display = "none";
                        ddl.fireEvent('onchange');
                    }
                    else if (ddl.dispatchEvent) {
                        ddl.selectedIndex = 1;
                        divWhichOrg.style.display = "none";
                        var oEvent = document.createEvent('HTMLEvents'); oEvent.initEvent('change', true, true); ddl.dispatchEvent(oEvent);
                    }
                    else
                        divWhichOrg.style.display = "block";
                }
                else {
                    divWhichOrg.style.display = "block";
                }
                if (!isPostBack) {
                    ddl.previousSibling.innerHTML = ddl.options[ddl.selectedIndex].text;
                    ddlFund.previousSibling.innerHTML = ddlFund.options[ddlFund.selectedIndex].text;
                }
            }
        }
        catch (err) { }
    }

    function SetPaymentType(sType) {
        var hSelectedPaymentType = document.getElementById('hSelectedPaymentType');
        if (hSelectedPaymentType != null) {
            hSelectedPaymentType.value = sType;
        }
        return false;
    }

    function Validate(bPayPal, bBlackbaud) {

        $('.field-error').removeClass('field-error');

        var keyVal = new Array();
        var sTmp;

        var kv = $('#hKeyAttributes').val().split("^");
        for (var i = 0; i < kv.length; i = i + 2) {
            keyVal[kv[i]] = kv[i + 1];
        }
        //Fund
        // keyVal['Fund_List'] = GetWord($('#hSelectedFund').val(), "-", 1) + "|" + $('#hAmount').val() + "|" + $('#hSelectedFundName').val() + "|||";
        keyVal['Fund_List'] = $('#hFundList').val();

        var sAppealCode = $('#hAppealCode').val();
        if ($('#hUseFundAppealCode').val() == "Y" && GetWord($('#hSelectedFund').val(), "-", 2) != "") {
            sAppealCode = (GetWord($('#hSelectedFund').val(), "-", 2))
        }
        keyVal['appeal_code'] = sAppealCode;
        //Amount
        keyVal['txt__Amount_Charged'] = $('#hTotalAmount').val();
        
        keyVal['txt__Member_ID'] = $('#hAdvanceID').val();
        keyVal['txt__First_Name'] = Clean($('#txtFirstName').val());
        keyVal['txt__Last_Name'] = Clean($('#txtLastName').val());
        if (Clean($('#txtBirthDate').val()) != "") {
            keyVal['txt__Birth_Date'] = Clean($('#txtBirthDate').val());
        }
        if (Clean($('#txtGradYear').val()) != "") {
            keyVal['txt__UCLA_Graduation_Year'] = Clean($('#txtGradYear').val());
        }
        if ($('#ddlJointGift').val() != "")
            keyVal['txt__Joint_Gift'] = $('#ddlJointGift').val();
        //else if ($('#hPledgePayment').val() != "Y")
        //    keyVal['txt__Joint_Gift'] = "N";
        //else
        //    keyVal['txt__Joint_Gift'] = $('#ddlJointGift').val();
        sTmp = $.trim(Clean($('#txtSpouse').val()));
        if (sTmp != null && sTmp.length > 0) {
            keyVal['txt__Spouse'] = sTmp;
            keyVal['txt__Spouse_UCLA_Graduation_Year'] = Clean($('#txtSpouseGradYear').val());
        }
        if ($('#cbMatchingGift').prop('checked')) {
            keyVal['txt__Matching_Gifts'] = "Will Mail";
            keyVal['txt__Matching_Company'] = Clean($('#txtEmployer').val());
            if ($('#rbSpousesEmployer').prop('checked')) {
                keyVal['txt__Matching_Company_Is'] = "Spouse's employer";
            }
            else {
                keyVal['txt__Matching_Company_Is'] = "My employer";
            }
            keyVal['txt__Send_Matching_Gift_To'] = "UCLA External Affairs, Attn: Matching Gift Services, 10920 Wilshire Boulevard, Suite 1100, Los Angeles, CA. 90095-1603";
        }
        //Address
        var raVal = $('#cbAddressBusiness').prop('checked');
        if (raVal)
            keyVal['txt__Address_Type'] = "B";
        else
            keyVal['txt__Address_Type'] = "H";

        var intl = $('#hInternational').val();
        if (intl == "True") {
            keyVal['txt__Street'] = Clean($('#txtStreetIntl').val());
            if (Clean($('#txtStreet2Intl').val()).length > 0)
                keyVal['txt__Street2'] = Clean($('#txtStreet2Intl').val());
            keyVal['txt__City'] = Clean($('#txtCityPostalCodeIntl').val());
            keyVal['txt__Country'] = $('#ddlCountry').val();

            if ($('#ddlCountry').val() == "CANADA" && $('#cbCanadian').prop('checked')) {
                var Canadian = document.getElementById("spCanadian");
                if (Canadian != null) {
                    var sCanadianText = Canadian.innerText;
                    keyVal['txtarea__Comments'] = "--- " + sCanadianText + " ---";
                }
            }
        }
        else {
            keyVal['txt__Street'] = Clean($('#txtStreet').val());
            if (Clean($('#txtStreet2').val()).length > 0)
                keyVal['txt__Street2'] = Clean($('#txtStreet2').val());
            keyVal['txt__City'] = Clean($('#txtCity').val());
            keyVal['txt__State'] = $('#ddlState').val();
            keyVal['txt__Zip'] = Clean($('#txtZip').val());
        }
        var rpVal = $('#cbPhoneBusiness').prop('checked');
        if (rpVal)
            keyVal['txt__Business_Phone'] = Clean($('#txtPhone').val());
        else
            keyVal['txt__Home_Phone'] = Clean($('#txtPhone').val());
        var reVal = $('#cbEmailBusiness').prop('checked');
        if (reVal)
            keyVal['txt__Email_Type'] = "B";
        else
            keyVal['txt__Email_Type'] = "H";
        keyVal['txt__Email'] = Clean($('#txtEmail').val());

        keyVal['Bio_Update_Code'] = "0";

        if ($('#cbIsParent').prop('checked')) {
            keyVal['txt__Parent'] = "Y";
        }

        if ($('#cbEmailUpdates').prop('checked')) {
            keyVal['EmailUpdatesText'] = $('#hValEmailUpdatesText').val();
            keyVal['txt__Email_Updates'] = "Y";
        }

        if ($('#cbAnonymous').prop('checked')) {
            keyVal['txt__Anonymous'] = "Y";
        }

        if ($('#cbSaveCCInfo').prop('checked')) {
            keyVal['SaveAccountInfo'] = "Y";
        }

        var paymentType = $('#hSelectedPaymentType').val();
        if (paymentType == "R") {
            keyVal['txt__Recurring_Payment_Method'] = "O";
            keyVal['txt__Pledge_Payment_Interval'] = $('#hPaymentSpec').val();
        }
        else if (paymentType == "P") {
            keyVal['txt__Total_Pledge_Amount_Including_This_Payment'] = keyVal['txt__Amount_Charged'];
            keyVal['txt__Amount_Charged'] = GetWord($('#hPaymentSpec').val(), ";", 3);
            keyVal['txt__Recurring_Payment_Method'] = "Automatic - your account will be charged on the 15th of the month due.";
            keyVal['txt__Pledge_Payment_Interval'] = GetWord($('#hPaymentSpec').val(), ";", 1);
            keyVal['txtarea__Payment_Schedule'] = $('#hPaymentSchedule').val();
            keyVal['PledgeSchedule'] = $('#hPledgeSchedule').val();
        }
        else if (paymentType == "D") {
            keyVal['Num_Deductions'] = $('#hPaymentSpec').val();
            if (keyVal['Num_Deductions'] > 0) {
                keyVal['txtarea__Payment_Schedule'] = $('#hPaymentSchedule').val();
                if (keyVal['Num_Deductions'] > 1) {
                    keyVal['txt__Total_Pledge_Amount_Including_This_Payment'] = keyVal['txt__Amount_Charged'] * keyVal['Num_Deductions'];
                }
            }
            keyVal['txt__Pledge_Payment_Interval'] = "1";
            keyVal['Source'] = "OLPRD";
            keyVal['Start_Month'] = $('#hDeductionStartMonthYYYYMM').val();
            keyVal['PledgeSchedule'] = $('#hDeductionDescription').val();
            keyVal['Bcc'] = $('#hDeductionEmails').val();
        }

        // Potentially reset premium after setting txt__Amount_Charged:
        if ($('#hPremiumAmount').val() == "A") {
            keyVal['Premium_Amount'] = keyVal['txt__Amount_Charged'];
        }
        else {
            keyVal['Premium_Amount'] = $('#hPremiumAmount').val();
        }

        if (bPayPal)
            keyVal['PPPayPage'] = "Y";

        if ($('#hTribute').val() != "") {
            keyVal['txt__Tribute_Type'] = Clean($('#hTribute').val());
            keyVal['txtarea__Tribute_Person_Occasion'] = Clean($('#hTributePersonOrOccasion').val());
            if ($('#hNotify').val() == "Y") {
                keyVal['txt__Honor_Title'] = Clean($('#hNotifyTitle').val());
                keyVal['txt__Honor_First_Name'] = Clean($('#hNotifyFirstName').val());
                keyVal['txt__Honor_Last_Name'] = Clean($('#hNotifyLastName').val());
                keyVal['txt__Honor_Home_Street1'] = Clean($('#hNotifyStreet').val());
                keyVal['txt__Honor_Home_Street2'] = Clean($('#hNotifyStreet2').val());
                keyVal['txt__Honor_Home_City'] = Clean($('#hNotifyCity').val());
                keyVal['txt__Honor_Home_State_Code'] = Clean($('#hNotifyState').val());
                keyVal['txt__Honor_Home_Country'] = Clean($('#hNotifyCountry').val());
                if (Clean($('#hNotifyCountry').val()) == '' || Clean($('#hNotifyCountry').val()) == 'UNITED STATES') {
                    keyVal['txt__Honor_Home_Zip_Code'] = Clean($('#hNotifyZip').val());
                }
                else {
                    keyVal['txt__Honor_Home_Intl_City_Postal_Code'] = Clean($('#hNotifyZip').val());
                }
                keyVal['txt__Honor_Home_Email'] = Clean($('#hTributeEmail').val());
                keyVal['txtarea__Honor_Signature'] = Clean($('#hNotifyNote').val());
            }
        }

        if ($('#hPledgePayment').val() == "Y") {
            keyVal['PledgePayment'] = "C";
        }
        
        //for (var index in keyVal) {
            //alert(index + " : " + keyVal[index]);
        //}
        setTimeout(function () {
            if ($('.field-error').length == 0)
                submitValues($('#hdDataCaptureUrl').val(), keyVal);
        }, 500)

        //SetDataCapturePage($('#hdDataCaptureUrl').val());
        return false;
    }

    function Clean(s) {
        //  var newString = escape(s);
        var newString = "";
        if (s != null) {
            var sUpper = s.toUpperCase();
            if (sUpper.indexOf("JAVASCRIPT") != -1 || sUpper.indexOf("JQUERY") != -1 || sUpper.indexOf("JSCRIPT") != -1) {
                newString = "";
            }
            else {
                newString = s.replace("<", "").replace(">", "");
            }
        }
        return newString;
    }
    
    function submitValues(url, params) {
        var form = ['<form method="POST" action="', url, '">'];

        for (var key in params)
            form.push('<input type="hidden" id="', key, '" name="', key, '" value="', params[key], '"/>');

        form.push('</form>');

        jQuery(form.join('')).appendTo('body')[0].submit();
    }
 </script>
