       <!-- Hero Section -->
       <section class="jumbotron no-padding no-margin hero-video" id="HeroShotTop">
        <div id="herovid-button">
         <button aria-label="Pause or Play Video" type="button">
          <i aria-hidden="true" class="fa fa-play fa-3">
          </i>
          /
          <i aria-hidden="true" class="fa fa-pause fa-3">
          </i>
         </button>
        </div>
        <div class="container-fluid no-padding">
         <div class="row no-margin no-padding">
          <video autoplay="" class="fadeIn" id="herovid" muted="" playsinline="" poster="/PublishingImages/som-home-video-still.jpg">
          </video>
         </div>
        </div>
       </section>
       <script>
        if (window.innerWidth < 768) {
		$("#HeroShotTop").css({
			"background-image" : "url(/PublishingImages/som-home-hero-768.jpg)",
			"background-repeat" : "no-repeat",
			"background-size" : "cover",
			"background-position" : "center center"
		});
	} else {
	    document.getElementById('herovid').innerHTML = '<source src="/Documents/som-video.mp4" type="video/mp4">';
	}
	$(window).resize(function() {
	  if (window.innerWidth < 768) {
		$("#HeroShotTop").css({
			"background-image" : "url(/PublishingImages/som-home-hero-768.jpg)",
			"background-repeat" : "no-repeat",
			"background-size" : "cover",
			"background-position" : "center center"
		});
	  } else {
	    document.getElementById('herovid').innerHTML = '<source src="/Documents/som-video.mp4" type="video/mp4">';
	  }
	});
	function vidRun() {
	    var inDesignMode = document.forms[MSOWebPartPageFormName].MSOLayout_InDesignMode.value;
	    var vid = document.getElementById("herovid");
	    var pauseButton = document.querySelector("#herovid-button button");
	
	    if (inDesignMode == "1") {
	        vid.pause();
	        pauseButton.style.display = "none";
	    }
	    if (window.innerWidth < 768) {
	        $('#herovid-button').hide();
	        $('#herovid').hide();
	    } else {
	        $('#herovid-button').show();
	        $('#herovid').show();
	    }
	
	    $(window).resize(function() {
	        if (window.innerWidth < 768) {
	            $('#herovid-button').hide();
	            $('#herovid').hide();
	        } else {
	            $('#herovid-button').show();
	            $('#herovid').show();
	        }
	    });
	
	    if (window.matchMedia('(prefers-reduced-motion)').matches) {
	        vid.removeAttribute("autoplay");
	        vid.pause();
	        pauseButton.innerHTML = "Paused"; /* Paused */
	    }
	
	    function vidFade() {
	        vid.classList.add("stopfade");
	    }
	
	    vid.addEventListener('ended', function() {
	        // only functional if "loop" is removed 
	        vid.pause();
	        // to capture IE10
	        vidFade();
	    });
	
	    pauseButton.addEventListener("click", function() {
	        vid.classList.toggle("stopfade");
	        if (vid.paused) {
	            vid.play();
	            $(pauseButton).html('<i class="fa fa-pause fa-5" aria-hidden="true"></i>');
	        } else {
	            vid.pause();
	            $(pauseButton).html('<i class="fa fa-play fa-5" aria-hidden="true"></i>');
	
	        }
	    })
	
	}
	$(document).ready(vidRun());
       </script>
