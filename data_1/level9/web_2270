        $('.news-image-wrapper.matching-heights IMG').addClass('img-responsive matching-heights');
	
	$(function () {
	  if (inDesignMode !== "1") {
	    var urlAttr = $(".news-image-wrapper img").attr("src");
	    $("#news-section .left-half").css({
		  "background-image" : "url(" + urlAttr + ")",
		  "background-size" : "cover",
		  "background-position" : "center center"
	    });
	    $(".news-image-wrapper img").remove();
	  }
	});
	
	if ($(window).width() < 990) {
	   $('.news-image-wrapper, .left-half').css('height', '350px');
	}
       </script>
