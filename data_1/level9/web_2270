        <script>
         // Script to hide alert message if content is empty
	  var inDesignMode = document.forms[MSOWebPartPageFormName].MSOLayout_InDesignMode.value;
	  
	  if ((inDesignMode !== "1") && ( !$.trim( $('#TopAlertBar .ms-rtestate-field').html() ).length )) {
	  	$('#TopAlertBar').css('display', 'none');
	  }
        </script>
