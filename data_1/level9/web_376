function getParameterByName(name, url) {
    if (!url) url = window.location.href;
    name = name.replace(/[\[\]]/g, "\\$&");
    var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
        results = regex.exec(url);
    if (!results) return null;
    if (!results[2]) return '';
    return decodeURIComponent(results[2].replace(/\+/g, " "));
}
	if(window.location.href.indexOf('?q=') > -1) {
		var query = getParameterByName('q');

		//console.log(query);
		var address = $('.linkoff').attr('href');
		$('.linkoff').attr('href', address + '?q=' + query);
		//$('#q').attr('value', query);
	}
   </script>
   <!-- -- ASSETS ---->
   <script type="text/javascript">
    RegExp.escape = function(string) {
    return string.replace(/[-\/\\^$*+?.()|[\]{}]/g, '\\$&')
};
function shuntAsset(id){                
    var eVideo = document.getElementById(id);  
    if(eVideo){                          
        var sVideo = eVideo.innerHTML;  
        var parser = new DOMParser();
        var doc = parser.parseFromString(sVideo, "text/html");
        sVideo = doc.body.textContent;  
        var reg = new RegExp("([^><]+)(?=(<|$))");
        var temp = sVideo;
        var rep;
        var count = 0;
        while(result = reg.exec(temp.trim())){
            if(count == 0){
				rep = RegExp.escape(result[0].trim());
            }
            else{
                rep = rep + "\\s*" + RegExp.escape(result[0].trim());
            }
            temp = temp.replace(new RegExp(RegExp.escape(result[0]) + "\\s*(?=(<|$))"), "").trim();
			if(reg.test(temp)){
				rep = rep + "(\\s*<[^>]*?>)*";
			}
            count++;
        }
        if(rep){
			//alert(rep);
			rep = rep.split(new RegExp("\\s+")).join("\\s+");
            var sDescription = document.getElementById('direct_jobDescriptionText').innerHTML; 
            document.getElementById('direct_jobDescriptionText').innerHTML = sDescription.replace(new RegExp(rep),sVideo);  
        }
        
    }   
}
function loadAssets(root){
    var i=1;    
    while(document.getElementById(root+i)){
        shuntAsset(root+i);
        i++;
    }
}
loadAssets('asset');
   </script>
