/* Display outline on focus */
:focus {
	color: #333;
	outline: #ccc solid 0px;
}

/* UCI */

header.site-header div.title-area p.site-title {
	font-weight: 800;
	font-size: 2.4rem;
	padding-top: 0.3rem;
}

header.site-header .search-form {
	margin-top: .4rem;
}

main.content article {
	border-radius: 5px;
}

main.content a { 
  color: #005594;
	text-decoration: underline;
	padding: 1px;
	border-radius: 2px;
}

main.content a:hover {
	background: #fbedb5;
	border-bottom: 0;
}

h3 {
	font-weight: 800;
	color: #333;
}

li#menu-item-96 {
	/* theme the Qualtrics menu link */
	float: right;
	font-weight: 800;
}

nav {
	box-shadow: 0 0 6px #aaa;
}

ul.menu.menu-primary {
	padding: 8px;
}

ul.menu.menu-primary li a {
	font-weight: 600;
	color: #005594;
}

ul.menu.menu-primary li:hover a {
	border-radius: 5px;
	background: #005594;
	color: #fff;
}

body.home h1 {display: none;}
  </style>
