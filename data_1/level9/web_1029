                jQuery("body.page-user").removeClass("not-front");
		jQuery(".not-front #recommend_panel_top").collapse();
		jQuery(".not-front #block-block-6 ul li:nth-child(2) a").addClass("collapsed");
		jQuery(".webform-client-form .captcha").addClass("col-sm-12");
		jQuery(".webform-client-form .form-actions").addClass("col-sm-12");
		jQuery('select#searchscope').change(function(){
			if (jQuery(this).val() == 'journals'){
				jQuery('#search_menu').val('title');
			}else if (jQuery(this).val() == 'online'){
				jQuery('#search_menu').val('title');
			}else{
				jQuery('#search_menu').val('keyword');
			}
		});
		var cbHref = jQuery("#messages-wrapper .messages a:contains('login')").attr("href");
		jQuery("#messages-wrapper .messages a:contains('login')").addClass("colorbox-node").attr("href", cbHref + "&width=500&height=500&displayview=full");
		// Allow Page URL to activate a tab's ID
		var taburl = document.location.toString();
		if( taburl.match('#') ) {
			jQuery('.nav-pills a[href=#'+taburl.split('#')[1]+']').tab('show');
		}
        jQuery(function(jQuery) {
            var num_cols = 3,
            container = jQuery('.split-list'),
            listItem = 'li',
            listClass = 'sub-list';
            container.each(function() {
                var items_per_col = new Array(),
                items = jQuery(this).find(listItem),
                min_items_per_col = Math.floor(items.length / num_cols),
                difference = items.length - (min_items_per_col * num_cols);
                for (var i = 0; i < num_cols; i++) {
                    if (i < difference) {
                        items_per_col[i] = min_items_per_col + 1;
                    } else {
                        items_per_col[i] = min_items_per_col;
                    }
                }
                for (var i = 0; i < num_cols; i++) {
                    jQuery(this).append(jQuery('<ul ></ul>').addClass(listClass));
                    for (var j = 0; j < items_per_col[i]; j++) {
                        var pointer = 0;
                        for (var k = 0; k < i; k++) {
                            pointer += items_per_col[k];
                        }
                        jQuery(this).find('.' + listClass).last().append(items[j + pointer]);
                    }
                }
            });
        });

		// Allow internal links to activate a tab.
		jQuery('a[data-toggle="tab"]').click(function (e) {
		e.preventDefault();
		jQuery('a[href="' + jQuery(this).attr('href') + '"]').tab('show');
		})
		var viewdbdescs = jQuery.cookie('viewdbdescs');
		if (("#db-layout").length || ("#recommended-dbs").length){
			if (viewdbdescs == "expand"){
				jQuery(".item-desc").show();
				jQuery(".item-infolink").hide();
				jQuery("span.db-desc-toggle").html("Hide Descriptions");
				jQuery("span.db-desc-toggle").removeClass('headerHidden');
				jQuery("span.db-desc-toggle").addClass('headerShown');
			} else {
				jQuery("span.db-desc-toggle").html("Show database descriptions");
				jQuery(".item-desc").hide();
				jQuery("span.db-desc-toggle").removeClass('headerShown');
				jQuery("span.db-desc-toggle").addClass('headerHidden');
			}
			jQuery(".item-infolink").click(function () {
				jQuery(this).next(".item-desc").toggle();
			});
			jQuery(".item-content .item-title").click(function(){
				location.href = jQuery(this).attr("href");
			});
			jQuery("span.db-desc-toggle").click(function(){
				if(jQuery('.headerHidden').length > 0) {
				jQuery(".item-infolink").fadeOut(500);
				jQuery(".item-desc").fadeIn(1500);
				jQuery("span.db-desc-toggle").html("Hide Descriptions");
				jQuery("span.db-desc-toggle").removeClass('headerHidden');
				jQuery("span.db-desc-toggle").addClass('headerShown');
				jQuery.cookie('viewdbdescs', 'expand', { path: '/databases/', expires: 10 });
				} else {
				jQuery(".item-infolink").fadeIn(2000);
				jQuery(".item-desc").fadeOut(1000);
				jQuery("span.db-desc-toggle").html("Show database descriptions");
				jQuery("span.db-desc-toggle").removeClass('headerShown');
				jQuery("span.db-desc-toggle").addClass('headerHidden');
				jQuery.cookie('viewdbdescs', null, { path: '/databases/'});
				}
				return false;
			});
		}
		(function(jQuery) {
			fakewaffle.responsiveTabs(['xs']);
		})(jQuery);
		jQuery('select#edit-jump').change(function(){
			var selected = jQuery("#edit-jump option:selected");
			if(selected.val() != 0){
				location.href = selected.val();
			}
		});
		jQuery('#views-exposed-form-Course-Guides-page-1').attr('action', '/research_portal/courses/help/');
		jQuery('.views-exposed-form input:submit').attr('value','GO');
		var table = jQuery('#coursesTable').DataTable({
			"order": [[ 0, "asc" ]],
			"lengthMenu": [[-1, 25, 50], ["All", 25, 50]],
			dom: "A" +
				"<'row header-row'<'col-sm-5'f><'col-sm-4'i><'col-sm-1'><'col-sm-2'<'pull-right'>>>" +
				"<'row'<'col-sm-12'tr>>" +
				"<'row'<'col-sm-9'i><'col-sm-3'p>>",     
			alphabetSearch: {
				column: 0
			},
			language: {
			 alphabetSearch: {
				infoDisplay: 'Course Title Index: ',
			 }
			},
			"oLanguage": {
				"sSearch": "Enter Course or Instructor:"
			},
			"columnDefs": [
			{
				"targets": [ 1 ],
				"visible": false,
				"searchable": false
			},            {
				"targets": [ 2 ],
				"visible": false,
				"searchable": false
			},
			{
				"targets": [ 5 ],
				"visible": false
			}],
			drawCallback: function(){
				jQuery('.alphabet ul').addClass('pagination');
				jQuery('.alphabet .alphabet-info-display').addClass('form-control-static');
				jQuery('.alphabet a.active').closest('li').addClass('active');
			}
		});
        jQuery(".find-your-librarian table").dataTable({
            "order": [[ 0, "asc" ], [ 1, "asc" ]],
            "lengthMenu": [-1],
            "dom": "<'row header-row'<'col-sm-5'f><'col-sm-4'i><'col-sm-1'><'col-sm-2'<'pull-right'>>>" +
            "<'row'<'col-sm-12'tr>>" +
            "<'row'<'col-sm-9'i><'col-sm-3'>>",
            "oLanguage": {
                "sSearch": "Find by Subject Area or Librarian:"
            },
            responsive: true
        }); 
		jQuery(".view-id-library_directory .views-table").dataTable({
			"order": [[ 0, "asc" ]],
			"lengthChange": false,
			"paging": false,
			"infoCallback": function( settings, start, end, max, total, pre ) {
				return total +" entries";
			},
			responsive: true
		}); 
	});
	function searchPrimo1() {
		var primoQueryValue1 = jQuery("#primoQueryTemp1").val().replace(/,/g," ");
		document.getElementById("primoQuery1").value = "any,contains," + primoQueryValue1;
		document.forms["primoSearchForm1"].submit();
	 }
	function searchPrimo2() {
		var primoQueryValue2 = jQuery("#primoQueryTemp2").val().replace(/,/g," ");
		document.getElementById("primoQuery2").value = "any,contains," + primoQueryValue2;
		document.forms["primoSearchForm2"].submit();
	 }
	function searchPrimo3() {
		var primoQueryValue3 = jQuery("#primoQueryTemp3").val().replace(/,/g," ");
		document.getElementById("primoQuery3").value = document.getElementById("exlidInput_scope_1").value + ",contains," + primoQueryValue3 + ",AND";
		document.forms["primoSearchForm3"].submit();
	 }
	function searchPrimo4() {
		var primoQueryValue4 = jQuery("#primoQueryTemp4").val().replace(/,/g," ");
		document.getElementById("primoQuery4").value = "any,contains," + primoQueryValue4;
		document.forms["primoSearchForm4"].submit();
	 }
</script>
