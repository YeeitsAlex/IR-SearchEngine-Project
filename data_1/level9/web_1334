   window.onload = function() {
  var redirectUrl = 'https:\x2F\x2Fwww.google.com\x2Faccounts\x2FAccountChooser?hd=uci.edu\x26continue=https%3A%2F%2Fdrive.google.com%2Fa%2Fuci.edu%2F\x26followup=https%3A%2F%2Fdrive.google.com%2Fa%2Fuci.edu%2F\x26osid=1\x26service=wise';
  var domain = 'uci.edu';
  var hash = window.location.hash;
  if (hash) {
  var match = hash.match(/[#&]Email=([^&]+)/);
  if (match) {
  redirectUrl += "&Email=" + match[1] + "@" + domain;
  }
  }
  window.location.replace(redirectUrl);
};
