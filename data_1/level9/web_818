    <script>
     window.onload = function()
{

var myElem = document.getElementById("campus-alert-content");
if (myElem != null)
{

	var xmlhttp = new XMLHttpRequest();
	xmlhttp.onreadystatechange = function() {
		if (xmlhttp.readyState == 4) {
			var response = JSON.parse(xmlhttp.responseText)
			document.getElementById("campus-alert-content").innerHTML = response.header;
		}
	}
	
	xmlhttp.open("GET", "https://www.cpp.edu/common/headers/safety-alert-third-party/default.php", true);
	xmlhttp.send();
}     
}
    </script>
    <script>
     function openMenu() {
  var check = document.getElementById("full-expanded-menu").style.display
  if (check == "block") { 
	document.getElementById("full-expanded-menu").style.display = "none";
  } else {
	document.getElementById("full-expanded-menu").style.display = "block";
  }
}
    </script>
