       <script type="text/javascript">
        function getfocus() {
	if (document.getElementById("file-viewers").style.display == 'block')
		{document.getElementById("file-viewers").style.display = "none"; 
		document.getElementById("file-viewers").style.height= "0px";}
	else
		{document.getElementById("adobe").focus();
		document.getElementById("file-viewers").style.display = "block"; 	
		document.getElementById("file-viewers").style.height= "150px"; 	}
	
  
}
       </script>
