     <script>
      ( function($) {
	/* Sets Home Page Layout or Archive to Two Columns, Content Left */
	if ($('body').hasClass('home') && $('body').hasClass('layout-3c-l') || $('body').hasClass('archive') && $('body').hasClass('layout-3c-l') || $('body').hasClass('singular-page') && $('body').hasClass('layout-3c-l') || $('body').hasClass('singular-post') && $('body').hasClass('layout-3c-l') && $('body').hasClass('category_id-2') || $('body').hasClass('singular-post') && $('body').hasClass('layout-3c-l') && $('body').hasClass('category_id-235') || $('body').hasClass('singular-post') && $('body').hasClass('layout-3c-l') && $('body').hasClass('category_id-234') || $('body').hasClass('error-404') && $('body').hasClass('layout-3c-l') ) {
        $('body').removeClass('layout-3c-l');
        $('body').addClass('layout-2c-l');
	}
	if ($('body').hasClass('date')) {
		$('body').removeClass('date');
	}
	if ($('body').hasClass('search')) {
		$('body').removeClass('search');
	}
})(jQuery);
     </script>
