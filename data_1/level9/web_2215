  <script type="text/javascript">
   function highlight(chosenID) {
			//put a border around the chosenID
			var theID = document.getElementById(chosenID);
			document.getElementById(chosenID).style.backgroundColor="#78a5dd";
		}

		function revert(chosenID) {
			//remove the border around the chosenID
			var theID = document.getElementById(chosenID);
			document.getElementById(chosenID).style.backgroundColor="#444";
		}

		function highlightText(chosenID) {
			//put a border around the chosenID
			var theID = document.getElementById(chosenID);
			document.getElementById(chosenID).style.color="#78a5dd";
		}

		function revertText(chosenID) {
			//remove the border around the chosenID
			var theID = document.getElementById(chosenID);
			document.getElementById(chosenID).style.color="#fff";
		}
  </script>
