  <script src="/js/all.min.js?v=2">
  </script>
  <script>
   $(document).foundation();
  </script>
  <script>
   $(document).ready(function () {

        $("#limit_graduate_division").click(function() {
          if ($(this).is(':checked')) {
            $("#department_filter").children("optgroup[label!='Graduate Division']").hide();
            $('#department_filter option:first').prop('selected', true);
          } else {
            $("#department_filter").children("optgroup").show();
          }
        });

        $('#searchButton').click(function (e) {
            e.preventDefault();
            var department_filter = '';
            $("#subjects").hide();
            $("#searchresults").html('');
            $.blockUI({ message: '<h3><img src="/images/ajax-loader.gif" /> searching</h3>',
                        css: { border: 'none',  padding: '10px', backgroundColor: '#fff', 'border-radius' :'10px', '-webkit-border-radius': '10px', '-moz-border-radius': '10px',  color: '#fff' } });

            if ($("#advanced_contents").is(":visible")) {
                department_filter = $("#department_filter").val();
                subject_filter = $("#subject_filter").val();
                include_course_description = $("#include_course_description").is(':checked') ? 1 : 0;
                include_prerequisite = $("#include_prerequisite").is(':checked') ? 1 : 0;
                include_restrictions = $("#include_restrictions").is(':checked') ? 1 : 0;
                limit_ipe = $("#limit_ipe").is(':checked') ? 1 : 0;
                limit_online = $("#limit_online").is(':checked') ? 1 : 0;
                limit_graduate_division = $("#limit_graduate_division").is(':checked') ? 1 : 0;
                limit_grading_letter_grade = $("#limit_grading_letter_grade").is(':checked') ? 1 : 0;
                limit_grading_pnp_su = $("#limit_grading_pnp_su").is(':checked') ? 1 : 0;
                limit_grading_in_process = $("#limit_grading_in_process").is(':checked') ? 1 : 0;
                limit_repeat_for_credit = $("#limit_repeat_for_credit").is(':checked') ? 1 : 0;
                limit_winter = $("#limit_winter").is(':checked') ? 1 : 0;
                limit_spring = $("#limit_spring").is(':checked') ? 1 : 0;
                limit_summer = $("#limit_summer").is(':checked') ? 1 : 0;
                limit_fall = $("#limit_fall").is(':checked') ? 1 : 0;

                $.ajax({
                    url: "/search",
                    cache: false,
                    dataType: 'html',
                    type: 'post',
                    data: { search: $("#search").val(), advanced: 1, department_filter: department_filter, subject_filter: subject_filter,
                     limit_ipe: limit_ipe, limit_winter : limit_winter, limit_spring : limit_spring, limit_summer : limit_summer,
                     limit_fall : limit_fall, limit_online: limit_online, _token: $("#searchform input[name=_token]").val(),
                     limit_grading_letter_grade : limit_grading_letter_grade, limit_grading_pnp_su : limit_grading_pnp_su, limit_grading_in_process: limit_grading_in_process,
                     limit_repeat_for_credit: limit_repeat_for_credit, limit_graduate_division: limit_graduate_division,
                     include_course_description: include_course_description, include_prerequisite: include_prerequisite, include_restrictions: include_restrictions }
                })
                .done(function (data) {
                    $("#searchresults").html(data);
                    $("#searchresults").show();
                    $.unblockUI();
                })
                .fail(function () {
                    $("#searchresults").html('<div class="row"><p style="text-align:center;">Failed to search course catalog.</p></div>');
                    $("#searchresults").show();
                    $.unblockUI();
                });
            } else if ($("#search").val() != '' ){
                $.ajax({
                    url: "/search",
                    cache: false,
                    dataType: 'html',
                    type: 'post',
                    data: { search: $("#search").val(), _token: $("#searchform input[name=_token]").val() }
                })
                .done(function (data) {
                    $("#searchresults").html(data);
                    $("#searchresults").show();
                    $.unblockUI();
                })
                .fail(function () {
                    $("#searchresults").html('<div class="row"><p style="text-align:center;">Failed to search course catalog.</p></div>');
                    $("#searchresults").show();
                    $.unblockUI();
                });
            } else {
                $("#searchresults").html('<div class="row"><p style="text-align:center;">No search entered. Please try again.</p></div>');
                $("#searchresults").show();
                $.unblockUI();
            }
        });

        $("#advanced_expand").click(function () {
            if ($("#advanced_contents").is(":visible")) {
                $(this).removeClass('expanded');
                $(this).addClass('collapsed');
                $("#advanced_contents").hide();
            } else {
                $(this).removeClass('collapsed');
                $(this).addClass('expanded');
                $("#advanced_contents").show();
            }
        });

        $('#clearSearchButton').click(function (e) {
            e.preventDefault();
            $("#search").val('');
            $("#searchresults").hide();
            $("#searchresults").html('');
            $("#subjects").show();
                        $("#department_filter").children("optgroup").show();
            $('#department_filter option:first').prop('selected', true);
            $('#subject_filter option:first').prop('selected', true);
            $("#include_course_description").attr('checked', false);
            $("#include_prerequisite").attr('checked', false);
            $("#include_restrictions").attr('checked', false);
            $("#limit_ipe").attr('checked', false);
            $("#limit_online").attr('checked', false);
            $("#limit_graduate_division").attr('checked', false);
            $("#limit_grading_letter_grade").attr('checked', false);
            $("#limit_grading_pnp_su").attr('checked', false);
            $("#limit_grading_in_process").attr('checked', false);
            $("#limit_repeat_for_credit").attr('checked', false);
            $("#limit_winter").attr('checked', false);
            $("#limit_spring").attr('checked', false);
            $("#limit_summer").attr('checked', false);
            $("#limit_fall").attr('checked', false);
            $("#advanced_contents").hide();

            $.ajax({
                url: "/clearsearch",
                cache: false,
                dataType: 'html',
                type: 'post',
                data: { _token: $("#searchform input[name=_token]").val() }
            })
            .done(function (data) {});
        });

        $("#search").focus();
        if ($("#limit_graduate_division").is(':checked')) {
            $("#department_filter").children("optgroup[label!='Graduate Division']").hide();
        }
        if ($("#search").val() != '') {
            $('#searchButton').trigger('click');
        }
    });
  </script>
 </body>
</html>
