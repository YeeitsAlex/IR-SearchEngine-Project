     <div id="dnn_ctr66247_ContentPane">
      <!-- Start_Module_66247 -->
      <div class="DNNModuleContent ModDNNHTMLC" id="dnn_ctr66247_ModuleContent">
       <div class="Normal" id="dnn_ctr66247_HtmlModule_lblContent">
        <h1>
         Welcome to the Center for Accessible Education
        </h1>
       </div>
      </div>
      <!-- End_Module_66247 -->
     </div>
     <div class="clear-fix">
