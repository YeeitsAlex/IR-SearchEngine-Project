})();
(function() {
if(location.href.match(/studentaffairs.ucla.edu\/quarterly-update/)) {
    var catH3 = document.querySelector('.DnnModule-LiveArticlesWidget .widgetmodule .categories h3');
    if(document.querySelector('.DnnModule-LiveArticlesWidget .widgetmodule .categories h3').innerText.trim().toUpperCase() === 'CATEGORIES') {
        catH3.innerText = 'Topics';
    }
}
})();
(function() {
    var activators = document.querySelectorAll('.js-modal-activate');
    if (activators.length === 0) {
        return;
    }
    var styles = document.createElement('style');
    styles.innerHTML = `
.js-hidden {
    display: none !important;
}
.js-close {
    font-size: 40px;
    color: #444;
    float: right;
    text-decoration: none;
    clear: both;
    display: inline-block;
    border: 2px solid #444;
    height: 2rem;
    width: 2rem;
    font-size: 2rem;
    border-radius: 50%;
    text-align: center;
    line-height: 2rem;
}
.js-modal {
    transition: opacity 0.5s;
    opacity: 1;
    padding-top: 1rem;
}
.js-invisible {
    opacity: 0;
}
@media (max-width: 550px) {
.js-page-mobile {
    min-height: 100vh;
    background-color: #EEE;
}
.js-page-mobile:nth-child(even) {
    background-coolor: #FFF;
}
}
`;
    document.head.appendChild(styles);
    for (var i = 0; i < activators.length; i++) {
        activators[i].addEventListener('click', function(evt) {
            evt.preventDefault();
            var activate = document.querySelector(evt.currentTarget.getAttribute('href'));
            var onactivateAppend = activate.getAttribute('data-onactivate-append');
            var onactivateHide = activate.getAttribute('data-onactivate-hide');
            var pos;
            var modal = document.createElement('div');
            modal.classList.add('js-modal');
            modal.classList.add('js-invisible');
            modal.innerHTML = `${activate.innerHTML}`;
            if (onactivateAppend) {
                pos = document.querySelector(onactivateAppend);
            }
            if (pos) {
                var close = document.createElement('a');
                var unhide = [];
                close.innerText = '×';
                close.title = 'Close';
                close.classList.add('js-close');
                close.addEventListener('click', function(evt) {
                    evt.preventDefault();
                    for (var c = 0; c < unhide.length; c++) {
                        unhide[c].classList.remove('js-hidden');
                    }
                    modal.classList.add('js-hidden');
                });
                if (onactivateHide) {
                    var hide = document.querySelectorAll(onactivateHide);
                    for (var c = 0; c < hide.length; c++) {
                        hide[c].classList.add('js-hidden');
                        unhide.push(hide[c]);
                    }
                }
                modal.insertBefore(close, modal.children[0]);
                pos.appendChild(modal);
                modal.scrollIntoView(true);
                window.requestAnimationFrame(function(e) {
                    window.requestAnimationFrame(function(e) {
                        modal.classList.remove('js-invisible');
                    });
                });
            }
        });
    }
}
)();
</script>
