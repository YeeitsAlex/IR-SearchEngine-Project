 (function() {
    if(document.querySelectorAll('.js-eventlist-activate').length !== 0) {
        var s = document.createElement('script');
        s.src = 'https://www.truebruinwelcome.ucla.edu/js/eventlist.js';
        s.async = true;
        var p = document.getElementsByTagName('script')[0];
        p.parentNode.insertBefore(s, p);
    }
})();
(function() {
    if(document.querySelector('[data-ucla="faq"]')) {
        var s = document.createElement('script');
        s.src = 'https://www.truebruinwelcome.ucla.edu/js/faq.js';
		s.async = true;
        var p = document.getElementsByTagName('script')[0];
        p.parentNode.insertBefore(s, p);
    }
})();
(function() {
