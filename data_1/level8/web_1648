          <script type="text/javascript">
           <!--//--><![CDATA[// ><!--

$(document).ready(function() {
var stickyNavTop = $('#main-menu').offset().top;
 
var stickyNav = function(){
var scrollTop = $(window).scrollTop();
      
if (scrollTop > stickyNavTop) { 
    $('#main-menu').addClass('sticky');
    $('#main').addClass('pushdown');
} else {
    $('#main-menu').removeClass('sticky'); 
    $('#main').removeClass('pushdown');
 }
};
 
stickyNav();
 
$(window).scroll(function() {
    stickyNav();
});

// Update search boxes in all tabs when one is changed.
var query_value = ''
$('#searchBox .tab-pane .form-control').change(function() {
	query_value = $('#searchBox .tab-pane.active .form-control').val();
	$('#searchBox .tab-pane .form-control').each(function() {
		$(this).val(query_value);
	});
});


});

// When the page is resized
	$(window).on('load resize', function(event){
		var windowSize = $(window).width();
		if(windowSize < 768){
			$('#block-views-library-hours-block-2').appendTo('#header-content > .region-header');
		} else {
			$('#block-views-library-hours-block-2').appendTo('#main-nav > .region-navigation');
		}
	});



//--><!]]>
          </script>
