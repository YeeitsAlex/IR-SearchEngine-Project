      // 
// show first content by default
$('#tabsy-nav li:first-child').addClass('active');
$('.contenty').hide();
$('.contenty:first').show();

// click function
$('#tabsy-nav li').click(function () {
$('#tabsy-nav li').removeClass('active');
$(this).addClass('active');
$('.contenty').hide();

var activeTab = $(this).find('a').attr('href');
$(activeTab).fadeIn();
return false;

});

// show first content by default
$('#tabsy2-nav li:first-child').addClass('active');
$('.contenty2').hide();
$('.contenty2:first').show();

// click function
$('#tabsy2-nav li').click(function () {
$('#tabsy2-nav li').removeClass('active');
$(this).addClass('active');
$('.contenty2').hide();

var activeTab = $(this).find('a').attr('href');
$(activeTab).fadeIn();
return false;

});
//
     </script>
     <!-- /ouc:div -->
    </div>
