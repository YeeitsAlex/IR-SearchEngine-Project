  <script type="text/javascript">
   $(document).ready(function () {            

            $('#main-nav').dropit({ action: 'mouseenter' });
            $('#main-nav').dropit({ action: 'click' });

            $('.search-input')
                .on("mouseover focus", function () {
                    $(this).addClass('search-input-hover');
                })
                .on("mouseleave blur", function () {
                    if (!$(this).is(":focus")) $(this).removeClass('search-input-hover');
                }
            );
      
            slidr.create('main-slidr', {
                //after: function(e) { console.log('in: ' + e.in.slidr); },
                //before: function(e) { console.log('out: ' + e.out.slidr); },
                breadcrumbs: true,
                //controls: 'bottom',
                //direction: 'vertical',
                fade: false,
                keyboard: true,
                overflow: true,
                pause: true,
                touch: true,
                //theme: '#222',
                //timing: { 'cube': '0.5s ease-in' },
                transition: 'cube'                                
            }).auto(5000);


            $('#mobile-hamburger').sidr();

            $('.popup-youtube, .popup-vimeo, .popup-gmaps').magnificPopup({
                disableOn: 700,
                type: 'iframe',
                mainClass: 'mfp-fade',
                removalDelay: 160,
                preloader: false,

                fixedContentPos: false
            });
           
        });
  </script>
