          <script type="text/javascript">
           $(document).ready(function () {
        //This is so that we can dynamically set the width of the distance input based on the selected value
        $('#Distance').change(function () {
            var $opt = $(this).find("option:selected");
            var $span = $('<span>').addClass('tester').text($opt.text());

            $span.css({
                'font-family': $opt.css('font-family'),
                'font-style': $opt.css('font-style'),
                'font-weight': $opt.css('font-weight'),
                'font-size': $opt.css('font-size')
            });

            $('body').append($span);
            // The 10px is for select open icon - it may vary a little per-browser
            $(this).width($span.width() + 12);
            $span.remove();
        });

        $('#Distance').change();
    });
          </script>
         </div>
