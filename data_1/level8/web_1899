                      <div class="n2-ss-layer n2-ow" data-adaptivefont="0" data-desktoplandscape="1" data-desktopportrait="1" data-desktopportraitalign="center" data-desktopportraitfontsize="100" data-desktopportraitheight="auto" data-desktopportraitleft="242" data-desktopportraitparentalign="center" data-desktopportraitparentvalign="middle" data-desktopportraittop="223" data-desktopportraitvalign="middle" data-desktopportraitwidth="131" data-mobilelandscape="1" data-mobileportrait="1" data-mobileportraitfontsize="300" data-mobileportraitleft="-174" data-mobileportraittop="249" data-parentid="" data-plugin="rendered" data-pm="absolute" data-responsiveposition="1" data-responsivesize="1" data-rotation="0" data-sstype="layer" data-tabletlandscape="1" data-tabletportrait="1" data-tabletportraitleft="392" data-tabletportraittop="254" style="left:242px;top:223px;width:131px;height:auto;overflow:visible;">
