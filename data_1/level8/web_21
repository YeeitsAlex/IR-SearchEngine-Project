<div class="linearize-level-2" role="complementary">
	<div class="ym-col3-home">
	    <div class="printonly"><h2>Calendar and Info</h2></div>
    <div class="featurebox">
    <h3>Academic calendar</h3><p>It's winter term. 
Spring term starts <span style="white-space: nowrap; text-decoration: none">April 1.</span><br />
Summer term starts <span style="white-space: nowrap; text-decoration: none">June 24.</span></p>
    <div id="triangle-bottomright-fixed"></div>
    </div>
    	</div>
	<div class="ym-col1-home">
    <div class="ym-clearfix hideprint">
            <div class="printonly"><h2>Feature Stories</h2></div>
      <div id="featureSlideShow">
	<div class="slidebutton"><a href="/in-the-news/">All news</a></div>
	<div id="featureTimerWrap">
		<div id="featureSlideTimer"></div>
	</div>
	<div id="featureSlideOverlay">
		<div class="backdrop"></div>
		<div id="featureSlideCaptionWrap"></div>
		<div id="featureSlideBtnWrap"></div>
	</div>
	<div id="featureSlideWrap"></div>
</div>
<script type="text/javascript">
	var slideSpeed = 5000;
	var slideTrans = false;
	var searchFor = ['"', "'"];
	var switchTo = ['&#34;', '&#39;'];
	var slideDelay = null;
	$(document).ready(function(){
		var source = 'https://ecampus.oregonstate.edu/includes/data/slideshow.xml';
		$.ajax({
			url: source,
			type: 'GET',
			dataType: 'xml',
			success: function(slideData) {
				$(slideData).find('slide').each(function(i){
					var del = $.trim($(this).attr('delay'));
					var img = $.trim($(this).find('image').get(0).firstChild.nodeValue);
					var cpt = $.trim($(this).find('caption').get(0).firstChild.nodeValue);
					var ctl = $.trim($(this).find('caption').get(0).getAttribute('title'));
					var lnk = $.trim($(this).find('link').get(0).firstChild.nodeValue);
					var ltl = $.trim($(this).find('link').get(0).getAttribute('title'));
					for (j=0; j<searchFor.length; j++)
					{
						while (cpt.indexOf(searchFor[j]) > -1)
						{
							cpt = cpt.replace(searchFor[j], switchTo[j]);
						}
						while (ctl.indexOf(searchFor[j]) > -1)
						{
							ctl = ctl.replace(searchFor[j], switchTo[j]);
						}
					}
					var output = '<div class="featureSlide" data-order="'+(i+1)+'" data-delay="'+del+'">';
					output += '<img class="featureSlideImg" src="'+img+'" alt="'+ctl+'" /></div>';
					$('#featureSlideWrap').append(output);
					output = '<div class="featureSlideCaption" data-order="'+(i+1)+'"><h3 class="featureCaptionTitle">'+ctl+'</h3><p>'+cpt+'</p><div><a class="featureSlideLink slideMoreBtn slideMoreBtnBlack" href="'+lnk+'">'+ltl+' &raquo;</a></div></div>';
					$('#featureSlideCaptionWrap').append(output);
					output = '<a class="featureSlideSelectBtn" data-order="'+(i+1)+'"></a>';
					$('#featureSlideBtnWrap').append(output);
				});
				$('.featureSlideSelectBtn').click(function(){
					if (!$(this).hasClass('current')) {
						clearTimeout(slideDelay);
						var from = $('.featureSlideSelectBtn.current').attr('data-order');
						var to = $(this).attr('data-order');
						featureSlideSwitch(to, from, true);
					}
				});
				$('#featureSlideShow').mousemove(function(){
					clearTimeout(slideDelay);
					$('#featureSlideTimer').stop();
				});
				$('#featureSlideShow').mouseleave(function(){
					setTimeout(function(){
					var prog = $('#featureSlideTimer').width() / $('#featureSlideShow').width();
						var del = $('.featureSlide.current').attr('data-delay');
						if (del != 'auto') {
							slideDelay = setTimeout(autoCycle, parseInt(del-(del*prog)));
							$('#featureSlideTimer').animate({'width': '100%'}, {duration: parseInt(del-(del*prog)), easing: 'linear'});
						}
						else {
							slideDelay = setTimeout(autoCycle, parseInt(slideSpeed-(slideSpeed*prog)));
							$('#featureSlideTimer').animate({'width': '100%'}, {duration: parseInt(slideSpeed-(slideSpeed*prog)), easing: 'linear'});
						}
					}, 700);
				});
				featureSlideSwitch(1);
				$('#featureSlideShow').fadeIn(500);
			}
		})
	});
	function autoCycle() {
		var from = $('.featureSlide.current').attr('data-order');
		if ($('.featureSlide.current').next('.featureSlide').length > 0)
			var to = $('.featureSlide.current').next('.featureSlide').attr('data-order');
		else
			var to = $('.featureSlide:first').attr('data-order');
		featureSlideSwitch(to,from);
	}
	function featureSlideSwitch(to, from, hold) {
		if (slideTrans == false) {
			slideTrans = true;
		if (from) {
		$('.current[data-order="'+from+'"]').removeClass('current');
		$('.featureSlideCaption[data-order="'+from+'"]').fadeOut({duration: 500, queue: false});
		}
		$('*[data-order="'+to+'"]').addClass('current');
		$('.featureSlideCaption[data-order="'+to+'"], .featureSlide[data-order="'+to+'"]').fadeIn({duration: 500, queue: false});
		var del = $('.featureSlide[data-order="'+to+'"]').attr('data-delay');
		$('#featureSlideTimer').stop().css('width', 0);
		setTimeout(function(){
				$('.featureSlide[data-order="'+from+'"]').css('display','none');
				slideTrans = false;
				clearTimeout(slideDelay)
				if (!hold)
				{
					if (del != 'auto') {
						slideDelay = setTimeout(autoCycle, parseInt(del));
						$('#featureSlideTimer').animate({'width': '100%'}, {duration: parseInt(del), easing: 'linear'});
					}
					else {
						slideDelay = setTimeout(autoCycle, slideSpeed);
						$('#featureSlideTimer').animate({'width': '100%'}, {duration: slideSpeed, easing: 'linear'});
					}
				}
			}, 500);
		}
	}
</script>    </div>
  </div>
</div>
