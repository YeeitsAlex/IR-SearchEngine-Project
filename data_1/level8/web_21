       <script type="text/javascript">
        // toggle a hover class for the photo below the play button overlay when it's hovered
$(document).on('ready', function() {
	$('.play-overlay').hover(function(e) {
 		//$('.play-underlay').trigger(e.type);
    //document.getElementByClass('play-underlay').className += ' play-underlay-hover';
    $(".play-underlay").toggleClass("play-underlay-hover");
	})
});
       </script>
       <!--script type="text/javascript" src="http://ecampus.oregonstate.edu/includes/js/jquery.inview.js"></script-->
       <script type="text/javascript">
        // check if element is visible after scrolling
// https://stackoverflow.com/questions/487073/check-if-element-is-visible-after-scrolling
function isScrolledIntoView(el) {
	var elemTop = el.getBoundingClientRect().top;
	var elemBottom = el.getBoundingClientRect().bottom;
	var isVisible = (elemTop >= 0) && (elemBottom <= window.innerHeight);
	return isVisible;
}
// http://www.hnldesign.nl/work/code/check-if-element-is-visible/
function isVisible(node) {
    // Am I visible?
    // Height and Width are not explicitly necessary in visibility detection, the bottom, right, top and left are the
    // essential checks. If an image is 0x0, it is technically not visible, so it should not be marked as such.
    // That is why either width or height have to be > 0.
    var rect = node.getBoundingClientRect();
    return (
        (rect.height > 0 || rect.width > 0) &&
        rect.bottom >= 0 &&
        rect.right >= 0 &&
        rect.top <= (window.innerHeight || document.documentElement.clientHeight) &&
        rect.left <= (window.innerWidth || document.documentElement.clientWidth)
    );
};
// remove comma from string
function removeComma(str){
  return(str.replace(/,/g,''));
}
// add comma to numeric string in the appropriate location
function commaSeparateNumber(val){
  while (/(\d+)(\d{3})/.test(val.toString())){
    val = val.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,");
  }
  return val;
}
// increment infographic number using animate, with comma, if element is visible
/* COMMENT OUT ANIMATION: start
$(document).on('ready', function() {
	// set flag so the animation only runs once instead of every time the element is in view
	var animationFlag = true;
	// use element 'in view' event plugin
	// https://remysharp.com/2009/01/26/element-in-view-event-plugin
	$('#row-infographic').bind('inview', function (event, visible) {
		if (visible == true) {
			// element is now visible in the viewport
			if (animationFlag) {
				$('.count').each(function () {
    			$(this).prop('Counter',0).animate({
      			Counter: removeComma($(this).text())
    			}, {
      			duration: 2500,
      			easing: 'swing',
      			step: function (now) {
        			$(this).text(commaSeparateNumber(Math.ceil(now)));
      			}
    			});
				});
        animationFlag = false;
			}
		}
		else {
			// element has gone out of viewport
			// do nothing
		}
	});
});
COMMENT OUT ANIMATION: end */
       </script>
