         <script>
          $('.fancylist li').each( function(i) {
		$(this).click(function() {
			window.location.href = $(this).find('a').attr('href');
		});
		$(this).keypress(function(e) {
			if (e.which == 13) {
				window.location.href = $(this).find('a').attr('href');
			}
		});
	});
	$('#inlinegraduate li:last').on('keydown', function (e) { 
		if ($("this:focus") && (e.which == 9)) {
			e.preventDefault();
			$('#inlinegraduate li:first').focus();
		}
	});
         </script>
