         <script>
          $('.fancylist li').each( function(i) {
			$(this).click(function() {
				window.location.href = $(this).find('a').attr('href');
			})
		});
		$('#inlineundergraduate li:last').on('keydown', function (e) { 
			if ($("this:focus") && (e.which == 9)) {
				e.preventDefault();
				$('#inlineundergraduate li:first').focus();
			}
		});
         </script>
