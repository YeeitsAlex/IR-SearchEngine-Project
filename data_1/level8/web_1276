    $( "#librarySearchForm" ).submit(function( event ) {
      primoQueryTemp =  $("#primoQueryTemp").val().replace(/[,]/g, " ");
      var pindex = $("#primoIndex").val();
      $("#primoQuery").val( pindex + ",contains," + primoQueryTemp );
    });

  });
