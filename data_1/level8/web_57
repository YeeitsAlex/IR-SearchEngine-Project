 <script type="text/javascript">
  $('input[type="checkbox"]').change(function (e) {
		    var checked = $(this).prop('checked'), container = $(this).parent(), siblings = container.siblings();
		    container.find('input[type="checkbox"]').prop({
		        indeterminate: false,
		        checked: checked
		    });
		    function checkSiblings(el) {
		        var parent = el.parent().parent(), all = true;
		        el.siblings().each(function () {
		            return all = $(this).children('input[type="checkbox"]').prop('checked') === checked;
		        });
		        if (all && checked) {
		            parent.children('input[type="checkbox"]').prop({
		                indeterminate: false,
		                checked: checked
		            });
		            checkSiblings(parent);
		        } else if (all && !checked) {
		            parent.children('input[type="checkbox"]').prop('checked', checked);
		            parent.children('input[type="checkbox"]').prop('indeterminate', parent.find('input[type="checkbox"]:checked').length > 0);
		            checkSiblings(parent);
		        } else {
		            el.parents('li').children('input[type="checkbox"]').prop({
		                indeterminate: true,
		                checked: false
		            });
		        }
		    }
		    checkSiblings(container);
		});
		//# sourceURL=pen.js
 </script>
 <script>
  $(document).ready(function() {
		    $('input[type=radio][name=searchType]').change(function() {
		        if (this.value == 'activitySearch') {
		            $( "#facultyName").prop( "disabled", true );
		            $( "#keyword :input" ).prop( "disabled", false );
		            $('#country-select').prop('disabled', false).trigger("chosen:updated");
		            $('#dept-select').prop('disabled', false).trigger("chosen:updated");
		            $( ".activity-options :input" ).prop( "disabled", false );
		            $("#activitySearchHeader").addClass("enabled-header");
		            $("#activitySearchHeader").removeClass("disabled-header");
		            $("#profileSearchHeader").addClass("disabled-header");
		            $("#profileSearchHeader").removeClass("enabled-header");
		        }
		        else if (this.value == 'profileSearch') {
		            $( "#facultyName" ).prop( "disabled", false );
		            $( "#keyword :input" ).prop( "disabled", true );
		            $('#dept-select').prop('disabled', true).trigger("chosen:updated");
		            $('#country-select').prop('disabled', true).trigger("chosen:updated");
		            $( ".activity-options :input" ).prop( "disabled", true );
		            $("#activitySearchHeader").addClass("disabled-header");
		            $("#activitySearchHeader").removeClass("enabled-header");
		            $("#profileSearchHeader").addClass("enabled-header");
		            $("#profileSearchHeader").removeClass("disabled-header");
		        }
		        
		    });
	});
 </script>
 <script>
  $("#themeCheck").click(function() {
			if ( $('input[name="themeCheck"]').is(':checked') ) {
			    $('#themeSelect').prop('disabled', false).trigger("chosen:updated");
			} 
			else {
			    $('#themeSelect').prop('disabled', true).trigger("chosen:updated");
			}
		});
 </script>
 <script>
  //$('[id$="sel5"], [id$="sel1"]').multiSelect();
		$(".dept-multi").chosen({
			placeholder_text_multiple: "Select Departments",
			width: "90%",
		});
		$(".country-multi").chosen({
			placeholder_text_multiple: "Select Countries",
			width: "90%",
		});
		$(".themeMulti").chosen({
			placeholder_text_multiple: "Select Themes",
			width: "90%",
		});
 </script>
 <script>
  $("#facultyName").focusin(function() {
		  $("#profileSearch").prop('checked', true);
		    $('#country-select').prop('disabled', true).trigger("chosen:updated");
		    $('#dept-select').prop('disabled', true).trigger("chosen:updated");
		    $( "#facultyName" ).prop( "disabled", false );
		    $("#activitySearchHeader").addClass("disabled-header");
		    $("#activitySearchHeader").removeClass("enabled-header");
		    $("#profileSearchHeader").addClass("enabled-header");
		    $("#profileSearchHeader").removeClass("disabled-header");
		    $( ".activity-options :input" ).prop( "disabled", true );
		});
		$("#keyword").focusin(function() {
			$("#activitySearch").prop('checked', true);
			$("#activitySearchHeader").addClass("enabled-header");
		    $("#activitySearchHeader").removeClass("disabled-header");
		    $("#profileSearchHeader").addClass("disabled-header");
		    $("#profileSearchHeader").removeClass("enabled-header");
		    $('#country-select').prop('disabled', false).trigger("chosen:updated");
		    $('#dept-select').prop('disabled', false).trigger("chosen:updated");
		    $( "#facultyName").prop( "disabled", true );
		});
		$("#country-select").chosen().change(function() { 
			$("#activitySearch").prop('checked', true);
			$("#activitySearchHeader").addClass("enabled-header");
		    $("#activitySearchHeader").removeClass("disabled-header");
		    $("#profileSearchHeader").addClass("disabled-header");
		    $("#profileSearchHeader").removeClass("enabled-header");
		    $('#country-select').prop('disabled', false).trigger("chosen:updated");
		    $('#dept-select').prop('disabled', false).trigger("chosen:updated");
		    $( "#facultyName").prop( "disabled", true );
		});
		$("#dept-select").chosen().change(function() { 
			$("#activitySearch").prop('checked', true);
			$("#activitySearchHeader").addClass("enabled-header");
		    $("#activitySearchHeader").removeClass("disabled-header");
		    $("#profileSearchHeader").addClass("disabled-header");
		    $("#profileSearchHeader").removeClass("enabled-header");
		    $('#country-select').prop('disabled', false).trigger("chosen:updated");
		    $('#dept-select').prop('disabled', false).trigger("chosen:updated");
		    $( "#facultyName").prop( "disabled", true );
		});
 </script>
 <script>
  $(function(){
			   $("#additionalOptionsButton").click(function () {
			      $(this).text(function(i, text){
			          return text === "Hide Additional Search Options" ? "Show Additional Search Options" : "Hide Additional Search Options";
			      })
			   });
			})
 </script>
