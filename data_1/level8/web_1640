    <div class="col" id="main-content" role="main">
     <h1>
      Home
     </h1>
     <!-- node.tpl.php -->
     <article about="/home" class="node node-page " role="article" typeof="sioc:Item foaf:Document">
      <span class="rdf-meta element-hidden" content="Home" property="dc:title">
      </span>
      <span class="rdf-meta element-hidden" content="0" datatype="xsd:integer" property="sioc:num_replies">
      </span>
      <footer class="article-meta">
       <ul class="links">
        <li class="print_html first">
         <a class="print-page" href="/print/716" rel="nofollow" title="Display a printer-friendly version of this page.">
          Print
         </a>
        </li>
        <li class="print_pdf last">
         <a class="print-pdf" href="/printpdf/716" rel="nofollow" title="Display a PDF version of this page.">
          PDF
         </a>
        </li>
