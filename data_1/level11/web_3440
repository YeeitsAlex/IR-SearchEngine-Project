        <script>
         $(document).ready(function(){
// determine which page
var pop_up_page = "";
var page_url = window.location.pathname;
switch(page_url) {
case '/langson':
pop_up_page = "langson";
break;

case '/ayala':
pop_up_page = "ayala";
break;

case '/how-do-i':
pop_up_page = "how-do-i";
break;

default:
pop_up_page = "index";
}
// check if no cookie has been set. if none, pop up the qwidget
if (typeof window.sessionStorage != undefined) {
sessionvar = "UCILibMessage"+pop_up_page;
if (!(sessionStorage.getItem(sessionvar))) {
sessionStorage.setItem(sessionvar, true);
setTimeout(function(){
$('#qp-need-help').css('left','0');
}, 15000);
setTimeout(function(){
$('#qp-need-help-mobile').css('bottom','0');
}, 15000);
}
}
$('#qwidget-button-hide').click(function(event) {
event.preventDefault();
$('#qp-need-help').css('transition','2s');
$('#qp-need-help').css('left','-200');
});

$('#close-need-help-mobile').click(function(event) {
event.preventDefault();
$('#qp-need-help-mobile').css('bottom','-200');
});

$('#close-qwidget').click(function(event) {
	$('#qwidget-button').css('left', '-200px');
	event.preventDefault();
	$('#qwidget-button-mobile').css('bottom', '-200px');
	event.preventDefault();
});

$('#qp-close').click(function() {
	$('#qp').css('left', '-200px');
});

$('#qwidget-button').focus(function() {
$('#qp-need-help').css('left','0');
$('#qp-need-help').css('transition','none');
});

});
        </script>
