<script type="text/javascript">
 jQuery(document).ready(function($) {

    ////
    //  If we're not at the top, toggle menu
    ////

    //if($(document).scrollTop();

        /** detected by Pixel Size **/
    var isMobile = function(){
        console.log($(window).width());
        return $(window).width() < 600 ? true : false;    
    }

    /** detected by User Agent **/
    var isMobileAgent = function() {
        var agents = ['android', 'webos', 'iphone', 'blackberry','Android', 'webos', ,'iPod', 'iPhone', 'iPad', 'Blackberry', 'BlackBerry'];
        var ismobile=false;
        for(var i in agents) {
    
            if (navigator.userAgent.split(agents[i]).length>1) {
                ismobile = true;
    
              }
        }
        return ismobile;            
    }


    $(document).ready(function(){
        if(isMobileAgent() || isMobile()){
            console.log('hiding mobile agent');
            $('.mobile-agent-hide').hide();
            $('.mobile-agent-show').show();
        }
        else{
            console.log('Showing mobile agent');
            $('.mobile-agent-hide').show();
            $('.mobile-agent-show').hide();
        }
    
        if(isMobileAgent() || isMobile()){
            $('#strong-economy .col-70').removeClass('col-70');
        }

    });

    ////
    //  Initialize the Video Carousel
    ////

    $('.testimonials-container').slick({
        infinite: true,
        slidesToShow: 3,
        slidesToScroll: 3,
        centerMode: true,
        centerPadding: '0px',
        nextArrow: '<span class="mouse-pointer next"><i class="material-icons">navigate_next</i></span>',
        prevArrow: '<span class="mouse-pointer prev"><i class="material-icons">navigate_before</i></span>',
        responsive: [
        {
          breakpoint: 1024,
          settings: {
            slidesToShow: 3,
            slidesToScroll: 3,
            infinite: true,
            centerMode: true,
            centerPadding: '0px',            
          }
        },
        {
          breakpoint: 900,
          settings: {
            slidesToShow: 2,
            slidesToScroll: 2,
            infinite: true,
            centerMode: true,
            centerPadding: '0px',            
          }
        },
        {
          breakpoint: 780,
          settings: {
            slidesToShow: 1,
            slidesToScroll: 1,
            infinite: true,
            centerMode: true,
            centerPadding: '0px',            
          }
        },
        {
          breakpoint: 480,
          settings: {
            slidesToShow: 1,
            slidesToScroll: 1,
            infinite: true,
            centerMode: false,
            centerPadding: '0px',            
          }
        }
        // You can unslick at a given breakpoint now by adding:
        // settings: "unslick"
        // instead of a settings object
      ],
    });


    ////
    //  Initialize hover on Testimonials
    ////

    $('.testimonial').hover(function() {
        console.log(this);
        $(this).find('.description-overlay').first().css('display', 'flex');
    }, function() {
        $(this).find('.description-overlay').first().css('display', 'none');
    });
});
</script>

