    <div id="hexagons">
     <h2 class="toggle">
      <span id="toggle_featured">
       Featured
       <span class="iconFont icon-arrow_small_down carrot">
       </span>
      </span>
      <div class="divider">
      </div>
      <span id="toggle_trending">
       Trending
       <span class="iconFont icon-arrow_small_down carrot">
       </span>
      </span>
     </h2>
     <div class="hexagon-grid-container">
      <!-- This needs to find the taxonomies and the percentage of funding -->
      <div class="hex_container health stateAlt " data-track="Homepage Hexagon|click|Depression Grand Challenge">
       <div class="item health" data-percentage="57">
        <div class="hexagon">
         <div class="hexagon-in1">
          <div class="hexagon-in2">
           <div class="backfill">
           </div>
          </div>
         </div>
        </div>
       </div>
       <div class="front-content">
        <div class="icon icon-cause-health-b">
        </div>
        <h3>
         Depression Grand Challenge
        </h3>
        <p>
         UCLA is opening the door to hope for millions, including someone you know.
        </p>
        <div class="info">
        </div>
