			jQuery(document).ready(function($) {
			    $("#nav-logo a, #nav-main-left li a, a.give_now_btn, a.icon-search, a.icon-contact, .causes li a, #college-and-schools a, #campus-areas a, #causes a, #centennial a").each(function() {
				        $(this).click(function(event) {
				           var link_text = $(this).text();
				           var link_class = $(this).attr("class");
				           var parent_div = $(this).closest("div").parent().attr('id');
				           var parent_class = $(this).closest("div").attr("class");
				           var centennial_div = $(this).closest("div").attr('id');

				           //TAKE CARE OF SEARCH AND CONTACT LINKS
				           //link_class RETURNS CLASSES FOR TWO SPECIAL BUTTONS WITH ADDITIONAL SPACE AT THE END
				           if(link_class == "iconFont icon-search "){
					           link_text = "Search";
				           } else if(link_class == "iconFont icon-contact ") {
					           link_text = "Contact Us";
				           }
				           //DETERMINE EVENT CATEGORY
				           if(parent_div == "college-and-schools" || parent_div == "campus-areas"){
					           var event_category = "top_header_schools";
				           } else if(parent_class == "visible_nav causes"){
					           var event_category = "top_header_causes";
					       } else if (centennial_div == "centennial") {
						       var event_category = "secondary_header_centennial";
				           } else {
					           var event_category = "top_header";
				           }
				           //FOR DEBUG
				           //console.log('clicked on '+ link_text + ' with ' + event_category + ' and class ' + parent_class + ' parent div ' + parent_div + ' link class ' + link_class);
				           //event.preventDefault();
						   ga('send', 'event', event_category, 'click', link_text);
						   ga('clientTracker.send', 'event', event_category, 'click', link_text);
				        });
			    });

					//Centennial/Let There Be Tracking
					$('a[href="https://lettherebe.ucla.edu"]').click( function(event) {

						//Analytics Data
						var event_category = 'top_nav';
						var link_text = event_category+"|explore ways to Give To UCLA"; //THIS IS THE LABEL

						//SEND TRACKING INFO
						ga('send', 'event', event_category, 'click', link_text);
						ga('clientTracker.send', 'event', event_category, 'click', link_text);
					});

			    //Parses the Url Query strings and returns an array
				var getUrlParam = function(url) {

					var tmp;
					var tmp_u   = url.split('?');

					if(typeof tmp_u[1] === 'undefined') return null;

					var tmp_p   = tmp_u[1].split('&');
					var param   = {};

					for(var i=0; i<tmp_p.length; i++) {

						tmp = tmp_p[i].split('=');
						param[tmp[0]] = tmp[1];
					}

					return param;
				}

				//send ga user id
				var sendID = function(id) {

					ga('set', 'dimension1', String(id) );
					ga('send', 'event', 'User ID', id);
				}

				//grab the parameters from the url query string
				var params = getUrlParam(document.URL);

				//send the id to google
				if( params !== null && params.uclaid !== null )
					sendID(params.uclaid);
			});
     </script>
