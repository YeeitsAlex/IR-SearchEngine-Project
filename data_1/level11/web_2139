   <script type="text/javascript">
    // <![CDATA[
function Teleport()
	{
		
		var searchArea = document.getElementById("SearchOptions").value;
		var searchString = document.getElementById("qt").value;
		var location = "";
		switch (searchArea)
		{
			case "us":
				location = "http://thoth.library.utah.edu:1701/primo_library/libweb/action/search.do?dscnt=0&scp.scps=scope%3A%28uu%29%2Cprimo_central_multiple_fe&frbg=&tab=default_tab&dstmp=1372199784675&srt=rank&ct=search&mode=Basic&dum=true&indx=1&tb=t&vl(freeText0)=" + searchString + "&fn=search&vid=UUU";
				break;
			case "cr":
				location = "http://thoth.library.utah.edu:1701/primo_library/libweb/action/search.do?dscnt=0&frbg=&scp.scps=scope%3A%28course_reserves%29&tab=course_reserves&dstmp=1372199816895&srt=rank&ct=search&mode=Basic&dum=true&vl(42954513UI0)=any&tb=t&indx=1&vl(freeText0)=" + searchString + "&vid=UUU&fn=search";
				break;
			case "lg":
				location = "http://campusguides.lib.utah.edu/search.php?iid=793&gid=0&c=0&search=" + searchString;
				break;
			case "dl":
				location = "http://content.lib.utah.edu/cdm/search/searchterm/" + searchString + "/field/all/mode/all/conn/and/order/title/ad/asc";
				break;
			case "w":
				location = "http://gsa.search.utah.edu/search?q=" + searchString + "&site=Look-only-in-marriott-library-main-website&client=marriott-library-main_frontend&output=xml_no_dtd&proxystylesheet=marriott-library-main_frontend&numgm=10&btnG.x=-250&btnG.y=-8&btnG=Submit+Search";
				break;
		}
		window.location.href = location;
	}
// ]]>
   </script>
