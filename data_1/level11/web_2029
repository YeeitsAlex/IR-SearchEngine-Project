      <div class="DNNModuleContent ModDNNHTMLC" id="dnn_ctr3541_ModuleContent">
       <div class="Normal" id="dnn_ctr3541_HtmlModule_lblContent">
        <h1>
         <span style="font-size: 48px;">
          Welcome to the Bruin Resource Center!
         </span>
        </h1>
       </div>
      </div>
      <!-- End_Module_3541 -->
