           <script>
            function viewBlogPost(sbnum,postnum,title){
		// load the post via ajax and open modal
		//console.log("View Post initilaized");
		$.get({
			url: '//ucanr.edu/sb3/display/assets/hero_tiles_blogPost_ajax.cfm?sbnum=' + sbnum + '&blogpost=' + postnum + '&title=' + title,
			dataType: 'html',
			success: function(data) {
				var e = $('#blog-dialog');
				//console.log("Post Data: ", data);
				OpenDialog(data,title);
			}
		});		
	}

	function OpenDialog(data,title) {
		// all data must ba added to modal display before dialog init. Otherwise the dialog is no longer in the center after adding content.
		var e = $("#blog-dialog-container");
		e.html(data);

		var _dialog = $("#blog-dialog").dialog({
            width: 900,
            height: 'auto',
            resizable: false,
            draggable: false,
            modal: true
        });	
        
        _dialog.dialog('open');
        $(".ui-widget-overlay").click(function(){
        	_dialog.dialog('close');
        });
	}
           </script>
           <style>
            /*.ui-dialog-titlebar {
		background-color: #eaab22;
		height: 5px;
	}*/
	.ui-dialog-titlebar-close {
		width: 30px;
        height: 30px;
		float:right;
		display: inline-block;
