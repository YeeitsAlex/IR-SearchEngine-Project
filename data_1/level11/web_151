           <script>
            function viewEvent(sbnum,eventnum){
		// load the post via ajax and open modal
		//console.log("View Post initilaized");
		$.get({
			url: '//ucanr.edu/sb3/display_2018/calendar_event.cfm?sbnum=' + sbnum + '&eventnum=' + eventnum,
			dataType: 'html',
			success: function(data) {
				var e = $('#event-dialog');
				console.log("Post Data: ", data);
				OpenDialog(data);
			}
		});		
	}

	function OpenDialog(data) {
		// all data must ba added to modal display before dialog init. Otherwise the dialog is no longer in the center after adding content.
		var e = $("#event-dialog-container");
		e.html(data);

		var _dialog = $("#event-dialog").dialog({
            width: 900,
            height: 'auto',
            resizable: false,
            draggable: false,
            modal: true
        });	
        
        _dialog.dialog('open');
        $(".ui-widget-overlay").click(function(){
        	_dialog.dialog('close');
        });
	}

	$(() => {
		$('.cal-item-link').on('click',function(e){
			let $e = $(this);
			let eventnum = $e.data('eventnum');
			viewEvent(10760,eventnum);
			return false;
		});
	});
           </script>
