<script>
 (function() { // iffe

// survey elements
var cls_survey = document.getElementById('cls-survey');
var survey_close = document.getElementById("survey-close");


// check localstorage
if (typeof(Storage) !== "undefined") {
	
    // localstorage exists
	//console.log(wellnesscommunity);
	if ( localStorage.getItem("surveyshown_wellnesscommunity") ) {
		// survey already shown on this device, do not show	
	} else {
		
		// show only when on fitrec sub-site
		var curURL = window.location.href;
		
		if ( curURL.indexOf('clstest.ucsf.edu/wellnesscommunity') >= 0 ||
			 curURL.indexOf('campuslifeservices.ucsf.edu/wellnesscommunity') >= 0
	
		 ) {
			
			// show survey and mark as shown
			localStorage.setItem("surveyshown_wellnesscommunity", "true");
			cls_survey.style.display = "block";
		}
	}
} 

// When the user clicks on <span> (x), close the modal
survey_close.onclick = function() {
    cls_survey.style.display = "none";
}

// When the user clicks anywhere outside of the modal, close it
window.onclick = function(event) {
    if (event.target == cls_survey) {
        cls_survey.style.display = "none";
    }
}

$('#no-thanks').on('click', function(e) {
	e.preventDefault();
	cls_survey.style.display = "none";
});

})(); // end iffe
</script>
<!-- end survey -->
