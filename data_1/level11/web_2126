        <a id="main-content">
        </a>
        <h1 class="title" id="page-title">
         Welcome to the Library
        </h1>
        <article about="/welcome-library" class="node-25 node node-landing-page view-mode-full clearfix" typeof="sioc:Item foaf:Document">
         <header>
          <span class="rdf-meta element-hidden" content="Welcome to the Library" property="dc:title">
          </span>
          <span class="rdf-meta element-hidden" content="0" datatype="xsd:integer" property="sioc:num_replies">
          </span>
         </header>
        </article>
        <!-- /.node -->
        <div class="block block-bean even" id="block-bean-onesearch">
         <div about="/block/onesearch" class="entity entity-bean bean-simple-block clearfix" typeof="">
          <div class="content">
           <div class="field field-name-field-body field-type-text-with-summary field-label-hidden">
            <div class="field-items">
             <div class="field-item even">
              <!-- Script that converts the query string into valid parameter -->
              <script type="text/javascript">
               function searchPrimo() {
	document.getElementById("primoQuery").value = document.getElementById("primoIndex").value  + document.getElementById("primoQueryTemp").value.replace(/[,]/g, " ");
	document.getElementById("primoFilterQuery").value = document.getElementById("primoFilter").value;
	document.forms["searchForm"].submit();
}
              </script>
              <div class="well well-lg">
               <h2>
                <label for="query">
                 OneSearch
                </label>
               </h2>
               <form action="http://sonoma-primo.hosted.exlibrisgroup.com/primo-explore/search" enctype="application/x-www-form-urlencoded; charset=utf-8" id="simple" method="get" name="searchForm" onsubmit="searchPrimo()" target="_self">
                <!-- Customizable Parameters -->
                <div class="form-group">
                 <input name="institution" type="hidden" value="01CALS_SOL"/>
                 <input name="vid" type="hidden" value="01CALS_SOL"/>
                 <input name="tab" type="hidden" value="everything"/>
                 <input name="search_scope" type="hidden" value="EVERYTHING"/>
                 <input name="mode" type="hidden" value="basic"/>
                 <!-- Fixed parameters -->
                 <input name="displayMode" type="hidden" value="full"/>
                 <input name="bulkSize" type="hidden" value="10"/>
                 <input name="highlight" type="hidden" value="true"/>
                 <input name="dum" type="hidden" value="true"/>
                 <input id="primoQuery" name="query" type="hidden"/>
                 <input id="primoFilterQuery" name="pfilter" type="hidden"/>
                 <input name="displayField" type="hidden" value="all"/>
                 <!-- Enable this if "Expand My Results" is enabled by default in Views Wizard -->
                 <input name="pcAvailabiltyMode" type="hidden" value="true"/>
                 <select class="form-control" id="primoIndex" name="primoIndex">
                  <option value="any,contains,">
                   Keyword
                  </option>
                  <option value="title,contains,">
                   Title
                  </option>
                  <option value="creator,contains,">
                   Author
                  </option>
                 </select>
                 <input class="form-control" id="primoQueryTemp" size="40" type="text" value=""/>
                 <select class="form-control" id="primoFilter" name="primoFilter">
                  <option value="">
                   All Items
                  </option>
                  <option value="pfilter,exact,books,AND">
                   Books
                  </option>
                  <option value="pfilter,exact,ebooks,AND">
                   E-Books
                  </option>
                  <option value="pfilter,exact,articles,AND">
                   Articles
                  </option>
                  <option value="pfilter,exact,dvds,AND">
                   DVDs
                  </option>
                  <option value="pfilter,exact,journals,AND">
                   Journals
                  </option>
                 </select>
                 <!-- Search Button -->
                 <input alt="Search" class="btn btn-default" id="go" onclick="searchPrimo()" title="Search" type="button" value="Search"/>
                </div>
               </form>
               <p>
                <a href="https://sonoma-primo.hosted.exlibrisgroup.com/primo-explore/search?vid=01CALS_SOL&amp;mode=advanced">
                 Advanced Search
                </a>
               </p>
               <h3>
                Focus Your Search
               </h3>
               <p>
                <a href="http://libguides.sonoma.edu/az.php">
                 A-Z List of Article Databases
                </a>
                |
                <a href="http://libguides.sonoma.edu/az.php?t=11959">
                 Newspapers
                </a>
                |
                <a href="http://libguides.sonoma.edu/az.php?t=11957">
                 Encyclopedias and Dictionaries
                </a>
                <br/>
                <a href="/specialcollections/collections">
                 Special Collections
                </a>
                |
                <a href="/research/reserves">
                 Course Reserves
                </a>
               </p>
              </div>
