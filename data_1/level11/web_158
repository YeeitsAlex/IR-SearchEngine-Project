                 <div class="clearfix text-formatted field field--name-field-sc-title field--type-text field--label-hidden field__item">
                  <p>
                   Undergraduate Student Affairs Mission Statement
                  </p>
                 </div>
                 <!-- END OUTPUT from 'core/themes/classy/templates/field/field--text.html.twig' -->
                 <!-- THEME DEBUG -->
                 <!-- THEME HOOK: 'field' -->
                 <!-- FILE NAME SUGGESTIONS:
   * field--paragraph--field-sc-text--content-section.html.twig
   * field--paragraph--field-sc-text.html.twig
