  <script type="text/javascript">
   $(window).on('scroll', function(){
		var position = $(this).scrollTop();
		if(position > 60) {
			$('#mainmenu').addClass('sticky-nav');
		} else {
			$('#mainmenu').removeClass('sticky-nav');
		}		
	});
  </script>
  <!-- End Sticky Menu -->
  <script type="text/javascript">
   $(document).ready(function() {
  $.simpleWeather({
    location: 'Stanford, CA',
    woeid: '',
    unit: 'f',
    success: function(weather) {
      html = '<h2><i class="icon-'+weather.code+'"></i></h2>';
      html += '<ul>';
      html += '<li class="temp"> '+weather.temp+'&deg;'+weather.units.temp+' </li><br>';
      html += '<li class="currently"> '+weather.currently+' </li><br>';
      html += '<li class="winds">Winds '+weather.wind.direction+' '+weather.wind.speed+' '+weather.units.speed+'</li></ul>';
  
      $("#weather").html(html);
    },
    error: function(error) {
      $("#weather").html('<p>'+error+'</p>');
    }
  });
});
  </script>
 </body>
</html>

