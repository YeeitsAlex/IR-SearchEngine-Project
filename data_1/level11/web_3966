	(function ($) {
    	//back to top
    	$(function() {
    		$(window).scroll(function() {
    			if($(this).scrollTop() > 300) {
    				$('#toTop').fadeIn();
    				$('#feedback-div').fadeIn();	
    			} else {
    				$('#toTop').fadeOut();
    				$('#feedback-div').fadeOut();
    			}
    		});
    	 
    		$('#toTop').click(function() {
    			$('body,html').animate({scrollTop:0},1000);
    		});	
    		
    	});
	})(jQuery);
  </script>
