    <div class="so-panel widget widget_sow-editor panel-last-child" data-index="16" id="panel-5543-3-0-1">
     <div class="panel-widget-style panel-widget-style-for-5543-3-0-1">
      <div class="so-widget-sow-editor so-widget-sow-editor-base">
       <div class="siteorigin-widget-tinymce textwidget">
        <p>
         <a href="https://bae.engineering.ucdavis.edu/news/" style="text-decoration: none">
         </a>
        </p>
        <div style="background: #E5E9EF;width: 20%;min-width: 150px;margin: auto">
         <h2 style="text-align: center;padding: 20px 0px">
          More News
