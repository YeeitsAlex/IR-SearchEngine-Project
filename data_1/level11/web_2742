 if (navigator.userAgent.indexOf('iPhone') != -1) 
	{         
		/*addEventListener("load", function() */
		$(window).on('load', function() 
		{                 
			setTimeout(function() 
			{ 
				window.scrollTo(0, 1); 
			}, 0);         
		}, false);

		function updateOrientation()
		{
			window.scrollTo(0, 1); 
		}
	}
</script>
<!-- jQuery toggle -->
<script type="text/javascript">
 $('.collapse').on('click',function () {
		$(this).find('.chevron').toggleClass('up down');
		$(this).parent('.more').slideToggle('600');
		$('.expand.more').slideToggle('600');
		return false;
  	});

  	$('button.toggleButton').on('click',function () {
		$(this).parent().parent().toggleClass('flipped');
   		return false;
  	});

  	function toggleMore(className) {
		var name = '.'+className
		$(name).find('.chevron').toggleClass('up down');
		$(name).siblings('.more').slideToggle('600');
		$(name).parent('.more').slideToggle('600');
		$(name).parent().siblings('.more').slideToggle('600');
	}

	// Showcase home pages
	function toggleShowCase(className) {
		var name = '.'+className
		$(name).parent('.btn').toggleClass('current');
		$(name).parent().siblings('.current').toggleClass('current');
		$(name).parent().parent().parent().parent().siblings('.current').toggleClass('current');
		$(name).parent().parent().parent().parent().siblings(name).toggleClass('current');
	}

	// Featured tabs home page
	function toggleTabs(className) {
		var name = '.'+className
		$(name).find('.btn').toggleClass('current');
		$(name).siblings().children('.current').toggleClass('current');
		$(name).parent().parent().siblings().find('.show').toggleClass('show hide');
		$(name).parent().parent().siblings().find(name).toggleClass('show hide');
	}


	function toggleShow(className) {
		var name = '.'+className
		$(name).next('p').toggleClass('hide');

		var text = $(name).text();
    	$(name).text(
        text == 'Show more' ? 'Show less' : 'Show more' );
	}
	function toggleCompact(className) {
		var name = '.'+className
		$(name).next('.card-list-width').children('.card-list').toggleClass('compact');

		var text = $(name).text();
    	$(name).text(
        text == 'Compact list view' ? 'Expand list view' : 'Compact list view' );
	}
	function toggleSwitch(className) {
		var name = '.'+className
		$('#searchbox.search-header').slideToggle('200');
		$('.icon-search').toggleClass('on off');
	}

	
	function toggleMenu(className) {
		var name = '.'+className
		$('.mmenu-text').find('.chevron').toggleClass('up down');
		$('.mmenu-text').find('.chevron > .switch').toggleClass('show hide');
		
		if($("#main_nav").is(":visible")) {
			$('#main_nav').slideToggle('400');
			$('#showcase').slideToggle('200');
		} else {
			$('#main_nav').slideToggle('400');
			
			if($("#showcase").is(":visible")) {
				$('#showcase').slideToggle('200');
			}
		}
	}

	$('.expand').on('click',function () {
		$(this).find('.chevron').toggleClass('up down');
		$(this).siblings('.expand').find('.chevron').toggleClass('up down');
		$(this).siblings('.expand.more').find('.chevron').toggleClass('up down');
		$(this).siblings('.more').slideToggle('600');

		if($("#showcase").is(":visible")) {
			$('#showcase').slideToggle('200');
		}

		if($("#main_nav").is(":visible")) {
			$('#main_nav').slideToggle('400');
		}
		return false;
  	});
</script>
<!-- filter list show/hide  -->
<script type="text/javascript">
 filterSelection("all") // Execute the function and show all columns
function filterSelection(c) {
  var x, i;
  x = document.getElementsByClassName("list-filter");
  if (c == "all") c = "";
  // Add the "show" class (display:block) to the filtered elements, and remove the "show" class from the elements that are not selected
  for (i = 0; i < x.length; i++) {
    w3RemoveClass(x[i], "show");
    if (x[i].className.indexOf(c) > -1) w3AddClass(x[i], "show");
  }
}

// Show filtered elements
function w3AddClass(element, name) {
  var i, arr1, arr2;
  arr1 = element.className.split(" ");
  arr2 = name.split(" ");
  for (i = 0; i < arr2.length; i++) {
    if (arr1.indexOf(arr2[i]) == -1) {
      element.className += " " + arr2[i];
    }
  }
}

// Hide elements that are not selected
function w3RemoveClass(element, name) {
  var i, arr1, arr2;
  arr1 = element.className.split(" ");
  arr2 = name.split(" ");
  for (i = 0; i < arr2.length; i++) {
    while (arr1.indexOf(arr2[i]) > -1) {
      arr1.splice(arr1.indexOf(arr2[i]), 1); 
    }
  }
  element.className = arr1.join(" ");
}

// Add active class to the current button (highlight it)
var btnContainer = document.getElementById("ql_filterlist");
var btns = btnContainer.getElementsByClassName("qlbtn");
for (var i = 0; i < btns.length; i++) {
  btns[i].addEventListener("click", function(){
    var current = document.getElementsByClassName(" active");
    current[0].className = current[0].className.replace(" active", "");
    this.className += " active";
  });
}
</script>

