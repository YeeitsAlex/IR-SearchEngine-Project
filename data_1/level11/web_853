       <div id="progress-bar">
       </div>
      </div>
      <div class="load-item" id="controls-wrapper">
       <div id="controls">
        <a id="play-button">
         <img alt="pauseplay" id="pauseplay" src="http://mlm.sci.utah.edu/modules/mod_backgroundgallery_pro/images/pause.png"/>
        </a>
        <div id="slidecounter">
         <span class="slidenumber">
         </span>
