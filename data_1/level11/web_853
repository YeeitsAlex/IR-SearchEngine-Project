        <a id="display-bar">
         <img alt="show/hide" id="display-arrow" src="http://mlm.sci.utah.edu/modules/mod_backgroundgallery_pro/images/button-tray-down.png"/>
        </a>
        <a id="tray-button">
         <img alt="show/hide" id="tray-arrow" src="http://mlm.sci.utah.edu/modules/mod_backgroundgallery_pro/images/button-tray-up.png"/>
        </a>
        <ul id="slide-list">
         <li>
         </li>
        </ul>
       </div>
