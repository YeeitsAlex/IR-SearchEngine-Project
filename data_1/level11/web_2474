            // invokes the FACEBOOK Share Dialog...
            function callFacebookShareDialog()
            {
                FB.ui( {
                           method           : 'share_open_graph',
                           action_type      : 'og.shares',
                           action_properties: JSON.stringify( {
                                                                  object: {
                                                                      'og:url'        : "http://alumniday.ucla.edu",
                                                                      'og:title'      : "UCLA ALUMNI DAY",
                                                                      'og:description': "Save the date for UCLA Alumni Day on Saturday, May 18. Return to campus with family and friends to celebrate the launch of the UCLA Centennial.",
                                                                      'og:image'      : "https://assets.alumniday.ucla.edu/vadstyle/1809-VAD19-SocialMedia-1200x630.jpg"
                                                                  }

                                                              } )
                       },

                       // Facebook callback...
                       function ( response ) {
                           if ( response && !response.error_message )
                           {
                               // then get post content
                               //alert( 'successfully posted!' );
                           }
                           else
                           {
                               //alert( 'failed to post' );
                           }
                       } )
            }
