   <script type="text/javascript">
    $('.carousel').carousel();
    $(document).ready(DoThis);
    $(window).resize(DoThis);

    function DoThis() {
        var wheight = $(window).innerHeight();
        var bheight = $('#box').innerHeight();

        if (wheight > bheight) {
            var hdiff = ((wheight - bheight) / 2);
            $('#box').css('margin-top', hdiff + 'px');
        }
        else {
            $('#box').css('margin-top', '0px');
        }
    }
   </script>
