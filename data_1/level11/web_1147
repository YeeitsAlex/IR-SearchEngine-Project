          <script>
           var loaded_items2066642 = 3;
            var totalreleases2066642 = 50;

            function morenewsRows2066642() {

                if (!pp_jquery(".button_more-2066642").hasClass("pp-headlineblock-nav--active"))

                return true;

                show_headlines2066642(loaded_items2066642,3);
                loaded_items2066642 = parseInt(loaded_items2066642) + 3;

                if (loaded_items2066642 >= totalreleases2066642) {

                    pp_jquery(".button_more-2066642").removeClass("pp-headlineblock-nav--active").attr('tabindex', '-1');
                    pp_jquery('.button_more-2066642').focus();

                }

            }

            function morenews2066642() {

                if (!pp_jquery(".button_more-2066642").hasClass("pp-headlineblock-nav--active"))

                return true;

                //Activate Previous button

                pp_jquery(".button_less-2066642").addClass("pp-headlineblock-nav--active").attr('tabindex', '0');

                //Headline block div variables, general and visible

                var moduleBlock = pp_jquery('.pp_blockheadlines_items-2066642 .pp-headline-block');
                var visibleBlock = pp_jquery('.pp_blockheadlines_items-2066642 .pp-headline-block:visible');
                var visibleBlockSiblings = visibleBlock.next(moduleBlock).length;

                //check if next items are already in the DOM

                if (visibleBlockSiblings > 0 ) {

                    visibleBlock.next().prevAll().hide().css("opacity","0");
                    visibleBlock.nextAll(':lt(1)').show().animate({opacity:"1"}, 300);

                    // check again if there are siblings, check current visibleBlock so button can be disabled if no next elements

                    visibleBlock = pp_jquery('.pp_blockheadlines_items-2066642 .pp-headline-block:visible');
                    visibleBlockSiblings = visibleBlock.next(moduleBlock).length;

                // else show_headlines

                } else {

                    show_headlines2066642(loaded_items2066642,3);

                    loaded_items2066642 = parseInt(loaded_items2066642) + 3;

                }

                // deactivate next button if no siblings and when max show_headlines is reached.

                if ((loaded_items2066642 >= totalreleases2066642)
                    && (visibleBlockSiblings == 0 )){

                    pp_jquery(".button_more-2066642").removeClass("pp-headlineblock-nav--active").attr('tabindex', '-1');
                    pp_jquery('.button_more-2066642').focus();

                }


            }

            function backnews2066642(){


                if (!pp_jquery(".button_less-2066642").hasClass("pp-headlineblock-nav--active"))

                return true;

                pp_jquery(".button_more-2066642").addClass("pp-headlineblock-nav--active").attr('tabindex', '0');

                var moduleBlock = pp_jquery('.pp_blockheadlines_items-2066642 .pp-headline-block');
                var visibleBlock = pp_jquery('.pp_blockheadlines_items-2066642 .pp-headline-block:visible');
                var visibleBlockPrevSiblings = visibleBlock.prev(moduleBlock).length;

                if (visibleBlockPrevSiblings > 0 ) {

                    visibleBlock.prev().nextAll().hide().css("opacity","0");
                    visibleBlock.prevAll(':lt(1)').show().animate({opacity:"1"}, 300);

                    // check again if there are previous siblings after the click

                    var currentVisibleBlock = pp_jquery('.pp_blockheadlines_items-2066642 .pp-headline-block:visible');
                    var currentVisibleBlockPrevSiblings = currentVisibleBlock.prev(moduleBlock).length;

                    if (currentVisibleBlockPrevSiblings == 0 ) {

                        pp_jquery(".button_less-2066642").removeClass("pp-headlineblock-nav--active").attr('tabindex', '-1');
                        pp_jquery('.button_less-2066642').focus();

                    }

                }
            }

        function show_headlines2066642(limitstart, limitend){
            var tags_addon = '';
            var multimedia_count = '';
            var sharerelease = '';
                        tags_addon = '&headline_tags=TopStory';
                        
            if (1 == 1) {
                initialOpacity = 'style="opacity:0"';
            } else {
                initialOpacity = "";
            }

            pp_jquery.getJSON('//news.csusm.edu/services/getheadlines.php?releases=["336460","340639","338733","338732","339792","339092","336461","338689","338220","336679","338388","337995","336193","335984","336454","335849","332507","335850","333622","332050","332037","332214","331607","330612","330958","329601","329759","329373","329602","328891","326279","326278","325646","325510","325055","324246","325051","317604","323888","322759","323732","323907","322887","322760","322758","320450","322245","321025","321433","321415"]' + tags_addon + multimedia_count + '&preview=&subtitle=&substring=&video=&limitstart=' + limitstart + '&limitend=' + limitend, function(data) {

                var items = "<div class=\"pp_grid pp-headline-block\" " + initialOpacity + ">";
                pp_jquery.each(data, function(key, val) {
                    if (key != 'error'){
                            var date = val['date'];
                            var date_split = date.split(" ");
                            var date_day = date_split[0];
                            var date_month = date_split[1];
                            var date_year = date_split[2];
                            var tags_items = '';
                            var multimedia_count = '';
                            var sharerelease = '';

                                                        if(val['tags']){
                                tags_items += '<div class=\"pp-block-item-tags-container pp-mt10\">';
                                tags_items += '<div class=\"pp-block-item-tags-title\">Tags</div>';
                                for ( var i in val['tags']) {
                                    tags_items += "<div class=\"pp-ib pp-block-item-tags pp-block-item-tags_" + val['tags'][i] + " pp-block-item-tags_" + i + "\">";
                                    tags_items += "<a href=\"//news.csusm.edu/?h=1&t=" + val['tags'][i] + "\">" + val['tags'][i] + "</a>";
                                    tags_items += "</div>";
                                    if (i == 4){break;}
                                }
                                tags_items += '</div>';
                            }
                            
                            
                            
                            
                            items += "<div class=\"pp_unit pp_one-of-three pp-block-item pp-block-item-" + val['releaseid'] + " pp-equal-js\">";
                            items += "<a class='pp-block-item-container' href=\"" + val['caseurl'] + "\" title=\"" + val['title'] + "\">";
                                                        items += "<div class=\"pp_blockheadlines_thumb pp-block-item-media image-res pcover aspect-2-3\" style=\"" + val['featuredimage'] + "\" aria-hidden=\"true\" role=\"presentation\" tabindex=\"-1\">" + val['videothumbnail'] + "</div>";
                            
                            items += "<div class=\"pp-block-item-content\">";
                            items += "<div class=\"pp-block-item-title pp-mb10 pp-mt10\">";
                            items += "<h3 class=\"pp-block-item-heading\">" + val['title'] + "</h3>";
                            if(val['subtitle']){
                                items += "<h4 class=\"pp-block-item-subheading\">" + val['subtitle'] + "</h4>";
                            }
                            items += "</div>";
                            items += "<div class=\"pp-block-item-date pp-mb10\">";
                                                        items += "<div class=\"pp-block-item-date-month pp-ib\">" + date_month + "</div><span class=\"pp-block-item-date-month-divider pp-ib\"></span>";
                            items += "<div class=\"pp-block-item-date-day pp-ib\">" + date_day + "</div><span class=\"pp_date_day_comma\">,</span>";
                            items += "<div class=\"pp-block-item-date-year pp-ib\">" + date_year + "</div>";
                                                        items += "</div>";
                            items += "<div class=\"pp-block-item-intro\">" + val['message'] + "</div>";
                            items += "<div class=\"pp-block-item-readmore\" title=\"" + val['title'] + "\">Read more</div>";
                            items += "</div>";
                            items += "</a>";
                            items += tags_items;
                            items += sharerelease;
                            items += multimedia_count;
                            items += "</div>";

                    }

                });
                    items +="</div>";

                    if (1 > 1) {

                        pp_jquery(".pp_blockheadlines_items-2066642").append(items);

                    } else {

                        pp_jquery(".pp_blockheadlines_items-2066642 .pp-headline-block").hide().css("opacity","0");
                        pp_jquery(".pp_blockheadlines_items-2066642").append(items);
                        pp_jquery(".pp-headline-block:visible").animate({"opacity":"1"},300);

                    }

                    
            });
        }

        pp_jquery(window).load(function() {
            /**
             * targets calcEqualHeights function (modules_v2h.js)
             * calculates consistent height for all visible items per block module
             */
            pp_jquery('.pp_blockheadlines_items-2066642 .pp-headline-block').each( function() {
                PP_MODULES.calcEqualHeights(this);
            });
        });

        pp_jquery(document).ajaxComplete(function() {
            /**
             * when using block item nav button calcEqualHeights
             * function (modules_v2h.js) will be executed
             */
            pp_jquery('.pp_blockheadlines_items-2066642 .pp-headline-block').each( function() {
                PP_MODULES.calcEqualHeights(this);
            });
        });
          </script>
