<script language="JavaScript">
 <!--
// To be safe, put this code just before closing the 
// BODY tag. There was an issue in NS 3 or 4, where 
// selection could not occur until the form field had 
// rendered on the screen.

// Check the accuracy of form and field names.
var theFormName = "wfform";
var theUserFieldName = "wf_term1";

if (document.forms[theFormName]) {
	f = document.forms[theFormName];
	if (f.elements[theUserFieldName] && f.elements[theUserFieldName].value == "") {
		formInit(f.elements[theUserFieldName]);
	} else if (f.elements[thePasswordFieldName]) {
		formInit(f.elements[thePasswordFieldName]);
	}
}
//-->
</script>
