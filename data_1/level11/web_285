      <section class="block block-views even last block-count-5 block-region-tertiary-content block-news-block-2" id="block-views-news-block-2">
       <div class="block-inner clearfix">
        <h2 class="block-title">
         News &amp; Announcements
        </h2>
        <div class="block-content content">
         <div class="view view-News view-id-News view-display-id-block_2 view-dom-id-b5ad61c27833541e231b05f3221c8f3e">
          <div class="view-content">
           <div class="views-row views-row-1 views-row-odd views-row-first">
            <div class="views-field views-field-field-dt">
             <div class="field-content">
              <a href="https://www.math.uci.eduhttps://www.math.uci.edu/node/36257">
               <span class="date-display-single">
                2/7/2019
               </span>
              </a>
             </div>
