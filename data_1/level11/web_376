   <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js" type="text/javascript">
   </script>
   <script language="javascript" type="text/javascript">
    // var uri = 'https://my.dev.sa.ucsb.edu/Bulletins/api/egradeslogin/active';
                var uri = 'https://my.sa.ucsb.edu/Bulletins/api/egradeslogin/active'

                 $(document).ready(function () {
                    // Send an AJAX request
                    $.getJSON(uri)
                        .done(function (data) {
                            // On success, 'data' contains a list of products.
                            $.each(data, function (key, item) {
                                // Add a list item for the product.
                                $('<li />', { text: formatItem(item) }).appendTo($('#bulletins'));
                            });

                        });

                    function formatItem(item) {
                        return item;
                        alert(item);
                    }
                });
   </script>
   <script language="JavaScript" type="text/javascript">
    function openExternalHelp()
		{
			window.open("html/CookiesHelp.htm","CookiesHelp","menubar=no,resizable=no,toolbar=no,scrollbars=yes");
		}
   </script>
