  <script type="text/javascript">
   jQuery(document).ready(function($){
    $('#myCarousel').on('slide.bs.carousel', function (evt) {
      $('#myCarousel .nav li.active').removeClass('active');
      $('#myCarousel .nav li:eq('+$(evt.relatedTarget).index()+')').addClass('active');
    });
	
	$('#myCarousel .nav a').click(function () {$('#myCarousel').carousel('pause');}); //tlk added to pause slides on click, moved here on 050416 from function above because wasn't working
	$('#myCarousel .item').removeClass('active').eq(Math.floor((Math.random() * $('#myCarousel .item').length))).addClass("active"); //TLK randomize slides
	
	if ( $("#myCarousel .item.slide1").hasClass("active") ) {
		$("#myCarousel .nav li.tab1").addClass("active");
	}
	if ( $("#myCarousel .item.slide2").hasClass("active") ) {
		$("#myCarousel .nav li.tab2").addClass("active");
	}
	if ( $("#myCarousel .item.slide3").hasClass("active") ) {
		$("#myCarousel .nav li.tab3").addClass("active");
	}
	if ( $("#myCarousel .item.slide4").hasClass("active") ) {
		$("#myCarousel .nav li.tab4").addClass("active");
	}
	
	
	//TLK - jquery swipe
	$("#myCarousel.carousel").swiperight(function() {
		$(this).carousel('prev');
	});
	$("#myCarousel.carousel").swipeleft(function() {  
		$(this).carousel('next');
	});
	
	
	//TLK - Launched at Lassonde - Carousel Auto-Cycle
        $("#launched .carousel").carousel({
        interval: 6000
        });
  
	//TLK - Launched at Lassonde - jquery swipe
	$("#launched .carousel").swiperight(function() {
		$(this).carousel('prev');
	});
	$("#launched .carousel").swipeleft(function() {  
		$(this).carousel('next');
	});

});
  </script>
