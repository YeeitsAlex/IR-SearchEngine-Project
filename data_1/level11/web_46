          <section id="main-content">
           <div id="content">
            <div class="block block-system no-title odd first last block-count-4 block-region-content block-main" id="block-system-main">
             <article about="/welcome" class="node node-panel node-promoted article odd node-full ia-r clearfix" id="node-86" role="article" typeof="sioc:Item foaf:Document">
              <div class="node-inner clearfix">
               <header class="node-header">
                <h1 class="node-title">
                 Welcome
                </h1>
               </header>
               <div class="node-content clearfix">
                <div class="two-50 at-panel panel-display clearfix">
                 <div class="region region-two-50-top region-conditional-stack">
                  <div class="region-inner clearfix">
                   <div class="panel-pane pane-views pane-front-slideshow no-title block">
                    <div class="block-inner clearfix">
                     <div class="block-content">
                      <div class="view view-front-slideshow view-id-front_slideshow view-display-id-block view-dom-id-47665cdc6fecf5691c20a6ae29c7affd">
                       <div class="view-content">
                        <div class="skin-default">
                         <div class="views_slideshow_cycle_main views_slideshow_main" id="views_slideshow_cycle_main_front_slideshow-block_1">
                          <div class="views-slideshow-cycle-main-frame views_slideshow_cycle_teaser_section" id="views_slideshow_cycle_teaser_section_front_slideshow-block_1">
                           <div class="views-slideshow-cycle-main-frame-row views_slideshow_cycle_slide views_slideshow_slide views-row-1 views-row-first views-row-odd" id="views_slideshow_cycle_div_front_slideshow-block_1_0">
                            <div class="views-slideshow-cycle-main-frame-row-item views-row views-row-0 views-row-odd views-row-first">
                             <div class="views-field views-field-field-image">
                              <div class="field-content">
                               <img alt="" class="image-style-front-slideshow" height="200" src="https://research-it.berkeley.edu/sites/default/files/styles/front_slideshow/public/research-it-slide1.png?itok=zbl4XAva" typeof="foaf:Image" width="800"/>
                              </div>
                             </div>
                            </div>
                           </div>
                           <div class="views-slideshow-cycle-main-frame-row views_slideshow_cycle_slide views_slideshow_slide views-row-2 views_slideshow_cycle_hidden views-row-even" id="views_slideshow_cycle_div_front_slideshow-block_1_1">
                            <div class="views-slideshow-cycle-main-frame-row-item views-row views-row-0 views-row-odd">
                             <div class="views-field views-field-field-image">
                              <div class="field-content">
                               <img alt="" class="image-style-front-slideshow" height="200" src="https://research-it.berkeley.edu/sites/default/files/styles/front_slideshow/public/research-it-slide2.png?itok=YWSR02x6" typeof="foaf:Image" width="800"/>
                              </div>
                             </div>
                            </div>
                           </div>
                           <div class="views-slideshow-cycle-main-frame-row views_slideshow_cycle_slide views_slideshow_slide views-row-3 views_slideshow_cycle_hidden views-row-last views-row-odd" id="views_slideshow_cycle_div_front_slideshow-block_1_2">
                            <div class="views-slideshow-cycle-main-frame-row-item views-row views-row-0 views-row-odd">
                             <div class="views-field views-field-field-image">
                              <div class="field-content">
                               <img alt="" class="image-style-front-slideshow" height="200" src="https://research-it.berkeley.edu/sites/default/files/styles/front_slideshow/public/research-it-slide3.png?itok=hHRsOOD5" typeof="foaf:Image" width="800"/>
