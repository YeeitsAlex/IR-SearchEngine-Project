  <script>
   $(document).ready(function() {
	setupRotator();
	var chk_path_prefix = "/texis/search";
	if (chk_path_prefix === window.location.pathname.substring(0,chk_path_prefix.length)) {
		trimThunderstoneSearchResultTitles();
	}
});

function setupRotator() {
     if($('.textItem').length > 1)
     {
         $('.textItem:first').addClass('current').fadeIn(1000);
         setInterval('textRotate()', 4000);
     }
}

function textRotate() {
	var current = $('#quotes > .current');
	if(current.next().length == 0) {
	    current.removeClass('current').fadeOut(1000);
	    $('.textItem:first').addClass('current').fadeIn(1000);
	} else {
	    current.removeClass('current').fadeOut(1000);
	    current.next().addClass('current').fadeIn(1000);
	}
}

function trimThunderstoneSearchResultTitles () {
	$('.result-title').each(function (ndx, obj) {
		obj.innerHTML = obj.innerHTML.replace(
			/^UCLA CTSI \| Accelerating Discoveries Toward Better Health \| /,
			"UCLA CTSI: "
		);
	});
}
  </script>
