      <script language="Javascript" type="text/javascript">
       if ($('.slickCarousel').hasClass('slickArrow')) {
     $('.slickCarousel').slick({
	 arrows:true,
	 autoplay: true,
	 autoplaySpeed: 7000,
	 fade: true,
	 speed: 600
     }); } 
 else {
     $('.slickCarousel').slick({
	 arrows:false,
	 autoplay: true,
	 autoplaySpeed: 7000,
	 fade: true,
	 speed: 600
     });
}
 var videoSrcQ = $('.slick-video iframe').attr('src');
     //Check if embed src is youtube and run code if true
     if (videoSrcQ.indexOf("youtube") >= 1) {
         //YTAPI needs id of video
         vidRegEx = /(?:youtube(?:-nocookie)?\.com\/(?:[^\/\n\s]+\/\S+\/|(?:v|e(?:mbed)?)\/|\S*?[?&]v=)|youtu\.be\/)([a-zA-Z0-9_-]{11})/
         vidId = videoSrcQ.match(vidRegEx)[1]
         $('.slick-video iframe').each(function(i) {
             $(this).attr('id', vidId + i);
         });
         //check if embed already has code to manipulate youtube video
         if (videoSrcQ.indexOf("enablejsap") <= 0) {
             if (videoSrcQ.indexOf("?") <= 0) {
                 // append src with google manipulation
                 $('.slick-video iframe').attr('src', function() {
                     return this.src + ('?enablejsapi=1&rel=0&orgin=http://ctsi.ucla.edu');
                 });
             } else {
                 $('.slick-video iframe').attr('src', function() {
                     return this.src + '&enablejsapi=1&rel=0&origin=http://ctsi.ucla.edu';
                 });
             }
         }
     }
 // App to prep Google Videos for tracking
 var players = {};
 var tag = document.createElement('script');
 tag.src = "https://www.youtube.com/iframe_api";
 var firstScriptTag = document.getElementsByTagName('script')[0];
 firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
 function onYouTubeIframeAPIReady(e){
     jQuery('.slick-video iframe').each(function(i){
         var youtubeiframeClass = jQuery(this).attr('id');
         players[youtubeiframeClass] = new YT.Player(youtubeiframeClass, {
             videoId: youtubeiframeClass,
             events: {
                 'onReady': onPlayerReady,
                 'onStateChange': onPlayerStateChange
             }

         });
     });
 }
 var pauseFlag = false;
 function onPlayerReady(e){
     //Player ready functions would go here
 }
 var isVideoOn = "0"
 function onPlayerStateChange(e){
     var videoUrl = (e.target.getVideoUrl() + '?origin=http://ctsi.ucla.edu');
     if ( e.data == YT.PlayerState.PLAYING ){
	 //         gtag('event', 'Play', {
	 //             'event_category' : 'Videos',
	 //             'event_label' : videoUrl
	 //         });
	 //         _gaq.push(['_trackEvent', 'Videos', 'Play', videoTitle]);
         pauseFlag = true;
         isVideoOn = "1";
	 $('.slickCarousel').slick('slickPause');
	 //console.log('playing');
     }
     if ( e.data == YT.PlayerState.ENDED ){
	 //         gtag('event', 'Finished', {
	 //             'event_category' : 'Videos',
	 //             'event_label' : videoUrl
	 //         });
         //         _gaq.push(['_trackEvent', 'Videos', 'Finished', videoTitle]);
         isVideoOn = "2"
	 $('.slickCarousel').slick('slickPlay');
	 //console.log('ended');
     } else if ( e.data == YT.PlayerState.PAUSED && pauseFlag ){
	 //         gtag('event', 'Pause', {
	 //             'event_category' : 'Videos',
	 //             'event_label' : videoUrl
	 //         });
         //            _gaq.push(['_trackEvent', 'Videos', 'Pause', videoTitle]);
         pauseFlag = false;
         isVideoOn = "0"
	 $('.slickCarousel').delay(10000).slick('slickPlay');
     }
 }
      </script>
