  <!-- interface sadness cleanup -->
  <script>
   if (typeof $ !== 'undefined') {
  var suggestionData = $('.yourEntryWouldBeHereData[colspan]');
  var contents = suggestionData.contents();
  var replaced = contents.filter(function(index, node) {
    var value = node.nodeValue;
    if (typeof value !== 'string')
      return true;
    if  (value.match(/--/))
      return false;
    return true;
  });
  suggestionData.empty();
  suggestionData.append(replaced);

  suggestionData.append('<p class="suggest"><strong>UCSF affiliates</strong> can <a class="secondary-button inline-button" href="https://www.library.ucsf.edu/suggest/">Suggest A Title</a></p>');

  $('a:contains("Search as Words")').text('a KEYWORD Search').each(function(index, element) { element.href = element.href.replace(/\/X\?/, '/X?SEARCH='); });
}
  </script>
