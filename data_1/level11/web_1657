   <script>
    var text = $('#hero_text').text();

var doAnimate = function() {
    $('#hero_text span').each(function() {
        var that = $(this);
        setTimeout(function() {
            that.animate({ fontSize: "2.18em" }, 200 )
                .animate({ fontSize: "2.2em" }, 200 );
        },that.index()*100);
    });
}

$('#hero_text').html('');

for(i=0; i<text.length; i++) {
  $('#hero_text').append('<span>'+text[i]+'</span>');
    if(i==text.length-1) doAnimate();
}
   </script>
