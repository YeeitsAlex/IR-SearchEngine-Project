    var resizeEndTimeout;
    $(window).resize(function(){
    clearTimeout(resizeEndTimeout);
    resizeEndTimeout = setTimeout(resizeEnd, 200);
    });
    function resizeEnd(){
    }
    //
    $('#container-navigation .navigation li:nth-child(n+6)').find('.dropdown-menu').addClass('dropdown-menu-right');
    // Top Navigation (Custom Two-Step Navigation)
    $('#container-navigation .navigation ul.nav.navbar-nav li.dropdown a.dropdown-toggle').bind('click',function(e){
    e.preventDefault();
    if($(this).parent().hasClass('open') || $(this).parent().hasClass('noChild')){
    if($(this).data('href')){
    window.location = $(this).data('href');
    }
    }
    });
    $('#container-navigation .navigation ul.nav.navbar-nav li.dropdown').bind('mouseenter',function(){
    $(this).addClass('open').find('a.dropdown-toggle').data('aria-expanded','true');
    }).bind('mouseleave',function(){
    $(this).removeClass('open').find('a.dropdown-toggle').data('aria-expanded','true');
    });
   </script>
