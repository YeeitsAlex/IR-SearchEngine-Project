     <h1 class="page-header">
      Oviatt Library
     </h1>
     <div class="region region-content">
      <section class="block block-system clearfix" id="block-system-main">
       <article class="node node-page clearfix" id="node-6660">
        <div class="field field-name-body field-type-text-with-summary field-label-hidden">
         <div class="field-items ">
          <div class="field-item even ">
           <!-- Script that converts the query string into valid parameter for OneSearch @todo move into an external js -->
           <script type="text/javascript">
            function searchPrimo() {
    document.getElementById("primoQuery").value = "any,contains," + document.getElementById("primoQueryTemp").value.replace(/[,]/g, " ");
    document.forms["searchForm"].submit();
  }
  function primoScopeSwitch(scope) {
    switch(scope) {
      case 'everything':
        document.getElementById('primoTab').value = "everything";
        document.getElementById('primoScope').value = "Everything";
        document.getElementById('primoQueryTemp').placeholder = 'Articles, media, books, and more';
        document.getElementById('filter-option').innerHTML = 'CSUN Collections&nbsp;';
        break;
      case 'reserves':
        document.getElementById('primoTab').value = "course_reserves";
        document.getElementById('primoScope').value = "01CALS_UNO_CR";
        document.getElementById('primoQueryTemp').placeholder = 'Course reserves';
        document.getElementById('filter-option').innerHTML = 'Course Reserves&nbsp;';
        break;
      case 'csu':
        document.getElementById('primoTab').value = "all_libraries";
        document.getElementById('primoScope').value = "CSU_PLUS_NO_FULLTEXT";
        document.getElementById('primoQueryTemp').placeholder = 'Items from all CSU libraries';
        document.getElementById('filter-option').innerHTML = 'All CSU Libraries&nbsp;';
        break;
    }
  }

  (function ($) {
    Drupal.behaviors.primo = {
      attach: function(context, settings) {
        var $scopes = $('#onesearch_scopes li a');
        //console.info($scopes);
        $scopes.on("click", function(e) {
          e.preventDefault();
          primoScopeSwitch($(this).attr('data-value'));
          $scopes.parent().removeClass('selected');
          $(this).parent().addClass('selected');
        });
        $('#onesearch_scopes > li:first-child > a').keydown(function(e) {
          if (e.which == 9 && e.shiftKey) {
            $('#scopes_menu_container').removeClass('open');
          }
        });
        $('#onesearch_scopes > li:last-child > a').keydown(function(e) {
          if (e.which == 9 && !e.shiftKey) {
            $('#scopes_menu_container').removeClass('open');
          }
        });
        $('#scopes_menu_container').on('shown.bs.dropdown', function() {
          $('#onesearch_scopes li.selected a').focus();
        });
        $('#scopes_menu_container').on('hidden.bs.dropdown', function() {
          $('#onesearch_scopes li.selected a').blur();
        });
      }
    };
  }(jQuery));
           </script>
