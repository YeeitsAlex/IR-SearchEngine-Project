  <script src="http://cs.ucsb.edu/sites/cs.ucsb.edu/files/js/js_uTpGZRbRZm_lrt5640lI88hN-6jGIe3E3hxZcagIuss.js">
  </script>
  <script>
   jQuery( document ).ready(function($) {

	original_student_type = null;
	target_field_group = $("fieldset.group-for-alumni");
	original_acad_level_radio_status = new Array();
	
	$('.form-item-field-acad-level-und input[type=radio]').each(function(index){
		original_acad_level_radio_status[index] = $(this).attr('checked');
		if($(this).attr('checked')){
			original_student_type = $(this).val();
		}
	});
	original_degree_earned_checkbox_status = new Array();
	$('.form-item-field-degree-earned-und input[type=checkbox]').each(function(index){
		original_degree_earned_checkbox_status[index] = $(this).attr('checked');
	});
	original_employer = $('#edit-field-employer-und-0-value').val();
	original_office = $('#edit-field-office-und-0-value').val();
	
	//Possibly add more things to show and hide
	
	if(original_student_type == 'alum'){
		target_field_group.removeClass('collapsed');
	}
	$('.form-item-field-acad-level-und input[type=radio]').each(function(index){
		$(this).change(function() {
			if($(this).val() == 'alum'){
				target_field_group.removeClass('collapsed');
				
				//restore the degrees earned
				$('.form-item-field-degree-earned-und input[type=checkbox]').each(function(index){
					$(this).attr('checked',original_degree_earned_checkbox_status[index]);
				});
				
				//restore employer
				$('#edit-field-employer-und-0-value').val(original_employer);
				//remove office
				$('#edit-field-office-und-0-value').val('');
			} else {
				target_field_group.addClass('collapsed');
				//remove the degrees earned
				$('.form-item-field-degree-earned-und input[type=checkbox]').each(function(index){
					$(this).removeAttr('checked');
				});
				//remove employer
				$('#edit-field-employer-und-0-value').val('');
				//restore office
				$('#edit-field-office-und-0-value').val(original_office);
			}
		});
	});
});
  </script>
 </body>
