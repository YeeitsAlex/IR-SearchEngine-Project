<script type="text/javascript">
 $(window).on('scroll', function(){
		var position = $(this).scrollTop();
		if(position > 60) {
			$('#mainmenu').addClass('sticky-nav');
		} else {
			$('#mainmenu').removeClass('sticky-nav');
		}		
	});
</script>
