without having focus on it -->
  <script src="/C21642_tsa/web/js/skip_navigation.js" type="text/javascript">
  </script>
  <script>
   $( function()
    {
      $( "a.payment-date-skip-navigator" ).skip_navigation( "#paymentDatePicker" );
      $( "a.additional-items-skip-navigator" ).skip_navigation( "#submitBtn" );

      var skipFocus = function( navOrContent )
      {
        var contentSelectorWrapper = ".container, .container-fluid";
        var navSelectorWrapper = "nav";
        var focusSelectors = [ "*[tabindex]:visible", "input:visible", "select:visible",
                               "textarea:visible", "button:visible", "a:visible" ];

        var focusToSelector = "";

        if ( navOrContent === "nav" )
        {
          focusToSelector = navSelectorWrapper;
        }
        else if ( navOrContent === "content" )
        {
          focusToSelector = contentSelectorWrapper;
        }

        $( focusToSelector ).find( focusSelectors.join() ).first().focus();
      };

      var skipToNavClickHandler = function( event )
      {
        event.preventDefault();
        skipFocus( "nav" );
      };

      var skipToContentClickHandler = function( event )
      {
        event.preventDefault();
        skipFocus( "content" );
      };

      $( '#skipToNav' ).bind( "click", skipToNavClickHandler );
      $( '#skipNav' ).bind( "click", skipToContentClickHandler );
    } );
  </script>
