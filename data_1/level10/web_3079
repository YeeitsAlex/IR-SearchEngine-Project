   <div class="pull-right" id="search">
    <form class="form-inline">
     <input class="input-small" id="qt" maxlength="50" type="text"/>
     <select id="SearchOptions">
      <option value="lg">
       Subject Guides
      </option>
      <option value="dl">
       Digital Library
      </option>
      <option value="w">
       Website
      </option>
     </select>
     <button class="btn btn-u-red" onclick="Teleport(); return false;">
      <em class="icon-search">
       <!-- Search Icon -->
      </em>
     </button>
    </form>
   </div>
   <script type="text/javascript">
    // <![CDATA[
//Takes you to the correct search depending on what area you select to search on the dropdown.
function Teleport()
	{
		
		var searchArea = document.getElementById("SearchOptions").value;
		var searchString = document.getElementById("qt").value;
		var location = "";
		switch (searchArea)
		{
			
			case "lg":
				location = "http://campusguides.lib.utah.edu/srch.php?q=" + searchString;
				break;
			case "dl":
				location = "https://collections.lib.utah.edu/search?q=" + searchString;
				break;
			case "w":
				location = "https://gcse.search.utah.edu/index.php?q=" + searchString + "&site=Look-only-in-marriott-library-main-website&gcse_action=site_search";
				
				
				break;
		}
		window.location.href = location;
	}
// ]]>
   </script>
