         <!-- UC Davis Library Google Analytics -->
         <script>
          function getParameterByName(name, url) {
				if (!url) url = window.location.href;
				name = name.replace(/[\[\]]/g, "\\$&");
				var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
						results = regex.exec(url);
				if (!results) return null;
				if (!results[2]) return '';
				return decodeURIComponent(results[2].replace(/\+/g, " "));
			}

			function trackOutboundLink(href,data_nav_tag) {
			  var url = '', tag = '';
			  if( typeof href === 'string' && typeof 'data-nav-tag'==='string') {
		          	url = href;
		          	tag = data_nav_tag;
      			  } else if( this.href && this.getAttribute('data-nav-tag')) {
          			url = this.href;
          			tag = this.getAttribute('data-nav-tag');
        		 }
			 var curl = window.location.href;
        		 if (url && tag ) {
			   ga('send', 'event', 'nav-'+tag, url, curl, {transport : 'beacon'});
		    	 }
		 	 if( url && url.indexOf(window.location.host) == -1 ) {
			    ga('send', 'event', 'outbound', url, curl, {transport : 'beacon'});
        		 }
			}

			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
			(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
			m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','//www.google-analytics.com/analytics.js','ga');

			ga('create', 'UA-65988958-1', 'auto');
			if( getParameterByName('inhouse') === 'true' ) {
				ga('send', 'event', 'inhouse', 'view', window.location.pathname, 1);
			} else {
				ga('send', 'pageview');
			}
         </script>
