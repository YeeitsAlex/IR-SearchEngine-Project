        <header class="component-centered-container section-header is-centered has-dash-under" id="section-header--6">
         <h2 class="drop-cap-title has-dash-under no-emphasis">
          <span class="drop-cap-title__name">
          </span>
          <span aria-hidden="true" class="drop-cap-title__drop-cap">
          </span>
         </h2>
        </header>
       </div>
