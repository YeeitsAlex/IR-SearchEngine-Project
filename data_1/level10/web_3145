  <script type="text/javascript">
   jQuery( window ).load(function() {
		jQuery('.carousel').flickity({
			draggable: false,
			wrapAround: true,
			autoPlay: 4000,
			cellAlign: 'left',
			prevNextButtons: false,
			contain: true,
			pageDots: false,
			on : {
				ready: function(){
					jQuery('.carousel').fadeTo('slow',1);
				}
			}
		});
	});
  </script>
  <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js">
  </script>
  <script type="text/javascript">
   var bgFadeDuration = 2000; //bg image crossfade time
	var heroFadeDuration = 1000; //hero text color fade time
	var bgCycleDelay = 5000; //time between bg image cycles
	
	function cycleImages(){ 
		var active = jQuery('#salk-bg-home .active');
		var next;
		
		if(jQuery('#salk-bg-home .active').next().length > 0){
		  next = jQuery('#salk-bg-home .active').next();
		}
		else{
		  next = jQuery('#salk-bg-home div:first');
		}
		
		var nextColor = next.data('tcolor');
		var nextTOverlay = next.data('toverlay');
		console.log(nextTOverlay);
		next.css('z-index',2);//move the next image up the pile
		jQuery('.hero h1, .hero span, .hero p').animate({ 'color': nextColor }, heroFadeDuration); //fade hero text to next color
		console.log('section.hero div background: '+nextTOverlay);
		jQuery('section.hero div').animate({ 'background-color': nextTOverlay }, heroFadeDuration); //fade text color overlat to next color, opacity
		
		active.fadeOut(bgFadeDuration,function(){//fade out the top image
			active.css('z-index',1).show().removeClass('active');//reset the z-index and unhide the image
			next.css('z-index',3).addClass('active');//make the next image the top one
		});
    }

	//fade in homepage panels on scroll
	function hideShow(){
		 /* Check the location of each desired element */
        jQuery('.hideme').each( function(i){
            
            var bottom_of_object = jQuery(this).offset().top + jQuery(this).outerHeight();
            var bottom_of_window = jQuery(window).scrollTop() + jQuery(window).height();
            
            /* If the object is completely visible in the window, fade it it */
            if( bottom_of_window > (bottom_of_object - 450) ){
                jQuery(this).animate({'opacity':'1'},1500);     
            }
        }); 
	}

	
    jQuery(window).load(function(){
		//fade in background	
		jQuery('#salk-bg-home').fadeIn(bgFadeDuration);
		// run every bgCycleDelay seconds
									//show necessary panels on load
		//hideShow();
		
		//on window scroll
		jQuery(window).scroll( function(){
			//hideShow();
		});
	});
  </script>
