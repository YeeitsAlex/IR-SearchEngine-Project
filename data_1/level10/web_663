    display: inline-block;
    font-size: 16px;
	  border-radius: 6px;
}
.button-green-sb a { 
	color: white;
	text-decoration: none !important;
  }

body.home a.continue {
    display: none;
}

.entry-content ol li {
    margin: 0 0 0 35px;
    padding-bottom: 0;
}

