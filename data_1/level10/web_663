div#gsc-iw-id1, div#gsc-iw-id2{
    border: 1px solid #BBB;
    padding: 3px;
}
/* Adds underline to author link */
span.fl-post-grid-author a:link {
    text-decoration: underline;
}
/* Add underline to content links in archives */
body .fl-post-grid-content a {
    text-decoration: underline;
}

/* We Heard You Table Column Width */
.tablepress-id-144 .column-1 {
	width: 350px;
}

/* Buttons for sidebar */
.button-blue-sb {
    background-color: #0064a4; /* UCI Blue */
    border: none;
    color: white;
    padding: 10px 22px;
    text-align: center;
    text-decoration: none;
    display: inline-block;
    font-size: 16px;
	  border-radius: 6px;
}
.button-green-sb {
