  <script>
   function setActive() {
		aObj = document.getElementById('nav-insert').getElementsByTagName('a');
		console.log(aObj);
		for (i = 0; i < aObj.length; i++) { 
			if (aObj[i].href.indexOf(document.location.href) >= 0) {
				aObj[i].parentNode.className='active';
				if (aObj[i].parentNode.parentNode.parentNode.className != 'secondary-nav') {
					aObj[i].parentNode.parentNode.parentNode.className='active';
				}
			}
		}
	}
  </script>
