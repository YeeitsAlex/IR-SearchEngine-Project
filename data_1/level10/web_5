   <script>
    $('body').OUAlert({
        type: 'active',								// default
        activePath: '/ou-alerts/active-alerts.xml',	// default
        icon: true,
        showClass: "zoomIn",
        emergency: {
             class: 'oualerts-notify-error',
             position: 'modal',					 	// Positions are ‘top’, ‘bottom’, ‘modal’
