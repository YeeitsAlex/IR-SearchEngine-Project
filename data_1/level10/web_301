                        <div class="flickr-container-item" id="flickr-container-8017945-9">
                         <a id="flickr-slide-link-item-8017945-9" target="_blank">
                          <img class="flickr-slide-list-item" id="flickr-slide-8017945-9"/>
                         </a>
                        </div>
                        <div class="flickr-container-item" id="flickr-container-8017945-10">
                         <a id="flickr-slide-link-item-8017945-10" target="_blank">
                          <img class="flickr-slide-list-item" id="flickr-slide-8017945-10"/>
                         </a>
                        </div>
                        <div class="clearer" id="clearer-8017945">
                        </div>
                       </div>
                       <div class="flickr-nav" id="flickr-nav-8017945">
                        <div class="flickr-nav-bar">
                        </div>
                        <div class="flickr-nav-button" id="flickr-nav-button-left-8017945">
                         <img id="flickr-nav-previous-8017945" src="/universal/images/slideshow/player-rw.png"/>
                        </div>
                        <div class="flickr-nav-button" id="flickr-nav-button-center-8017945">
                         <img id="flickr-nav-playPause-8017945" src="/universal/images/slideshow/play-button.png"/>
                        </div>
                        <div class="flickr-nav-button" id="flickr-nav-button-right-8017945">
                         <img id="flickr-nav-next-8017945" src="/universal/images/slideshow/player-fw.png"/>
                        </div>
                       </div>
                       <div class="clearer">
                       </div>
                      </div>
                      <div class="social-widget-slug flickr-slug" id="flickr-slug-8017945-1">
                       <div class="social-widget-link flickr-link">
                        <a href="http://www.flickr.com/photos/ucsbgradpost" target="_blank">
                         <img class="avatar" id="flickr-slug-avatar-image-8017945-1" src="/universal/images/social/flickr-link.png"/>
                         <span id="flickr-slug-text-8017945-1">
                         </span>
                        </a>
                       </div>
                      </div>
                     </div>
                     <script type="text/javascript">
                      <!--

Squarespace.Social.Flickr = {

  flickrWidget8017945  : "",
  FADE_FAST_SPEED         : 0.2,
  FADE_SPEED              : 0.5,
  SWIPE_SPEED             : 0.5,   
  slideDelay              : {},
  sidebarWidths           : {},  
  containerXOffsets       : {},
  containerXPositions     : {},  
  activeModule            : "",
  modules                 : new Array(),
  activeSlideIndex        : {},
  nextSlideIndex          : {},
  containerElements       : {},
  navElements             : {},  
  loadCounts              : {},
  displayCounts           : {},  
  timerID                 : {},
  isAutomated             : {},
  isPreloaded             : {},
  isGridPreset            : {},
  isSlidePreset           : {},  
  showSlideTransition     : {},
  isSlideTransitionFade   : {},
  isSlideTransitionSwipe  : {},  
  thumbsPerRow            : {},
  thumbPadding            : {},
  autoCrop                : {},
  autoPlay                : {},
  displayControls         : {},
  userList                : new Array(),
  

    
  onImagesPreloaded : function(mod) {        
    if(this.isSlidePreset[mod]) {
    	if (this.autoPlay[mod]) {
     		// Start slideshow
     		this.startTimer(mod);      		
    		this.isAutomated[mod] = true; 
    		this.swapPausePlayButton(mod);    		
    	} else {
    		this.isAutomated[mod] = false;
    	} 	
  	}
  	this.isPreloaded[mod] = true;
  },
  
  startTimer : function(mod) {
    this.timerID[mod] = YAHOO.lang.later( this.slideDelay[mod], this, 'doAutoPlay', mod, true ); 
  },
  
  stopTimer : function(mod) {
    this.timerID[mod].cancel();
  },
    
  unselectSlide : function() {
    var el = this.getEl();
    D.removeClass(el, 'selected-slide');
  },
		
  selectSlide : function() {
    var el = this.getEl();
    D.addClass(el, 'selected-slide');		
  },
  
  animateNoTransition : function(mod) {

    // unselect active slide
    D.removeClass('flickr-container-' + mod + '-' + this.activeSlideIndex[mod], 'selected-slide');
    
    if (this.nextSlideIndex[mod] == 0) {
    
      // jump to end
      this.nextSlideIndex[mod] = this.loadCounts[mod];
      this.activeSlideIndex[mod] = this.loadCounts[mod] + 1;
    
    } else if (this.nextSlideIndex[mod] == (this.loadCounts[mod] + 1)) {
    
      //jump to beginning
      this.nextSlideIndex[mod] =  1;
      this.activeSlideIndex[mod] = 0;
    }   

    // select next slide
    D.addClass('flickr-container-' + mod + '-' + this.nextSlideIndex[mod], 'selected-slide');
    
  },
  
  animateFade : function(mod) {
  	// fade out active slide  	
  	var slide = D.get('flickr-container-' + mod + '-' + this.activeSlideIndex[mod]);
  	var anim = new YAHOO.util.Anim(slide, { 
  		opacity: { from: 1, to: 0 }
  	}, this.FADE_SPEED);
  	anim.onComplete.subscribe(this.unselectSlide);
  	anim.onStart.subscribe(this.onFadeFinish);
  	anim.animate(); 
  },
  
	onFadeFinish : function() {
	
	  // get current module
    var el = this.getEl();        
    var container = D.getAncestorByClassName(el, 'flickr-container');
    var stringLength = "flickr-container-";
    var mod = container.id.substr(stringLength.length); 
    var that = Squarespace.Social.Flickr;

		if (that.nextSlideIndex[mod] == 0) {
		
			// jump to end
			that.nextSlideIndex[mod] = that.loadCounts[mod];
			that.activeSlideIndex[mod] = that.loadCounts[mod] + 1;
		
		} else if (that.nextSlideIndex[mod] == (that.loadCounts[mod] + 1)) {
			//jump to beginning
			that.nextSlideIndex[mod] =  1;
			that.activeSlideIndex[mod] = 0;
		}		
    
		// fade in next slide
		var slide = D.get('flickr-container-' + mod + '-' + that.nextSlideIndex[mod]);
		D.setStyle(slide, 'opacity', '0');
		var anim = new YAHOO.util.Anim(slide, { 
			opacity: { from: 0, to: 1 }
		}, that.FADE_SPEED);
		anim.onStart.subscribe(that.selectSlide);
		anim.animate();  
	},		
  
	animateSwipe : function(mod) {
	  // swipe out active slide
	 	var anim = new YAHOO.util.Motion(this.containerElements[mod], {
		    points: {
		        to: [this.containerXPositions[mod], D.getY(this.containerElements[mod])]
		    } 
		}, this.SWIPE_SPEED, YAHOO.util.Easing.easeOutStrong);
		anim.onComplete.subscribe(this.onSwipeFinish);
		anim.animate(); 
	},
	 
	onSwipeFinish : function() {
	
	  // get current module
	  var el = this.getEl();	  
	  var stringLength = "flickr-container-";
	  var mod = el.id.substr(stringLength.length);
	  var that = Squarespace.Social.Flickr;
    
    if (that.activeSlideIndex[mod] == 0) {

      // jump to post slide
      that.containerXPositions[mod] = D.getX(that.containerElements[mod]) - ( that.loadCounts[mod] * that.sidebarWidths[mod]);
      D.setX(that.containerElements[mod], that.containerXPositions[mod]);
      that.activeSlideIndex[mod] = that.loadCounts[mod];      
     
    } else if (that.activeSlideIndex[mod] == (that.loadCounts[mod] + 1)) {

      // jump to pre slide    
      D.setX(that.containerElements[mod], that.containerXOffsets[mod] - that.sidebarWidths[mod]);
      that.containerXPositions[mod] = D.getX(that.containerElements[mod]);
      that.activeSlideIndex[mod] = 1;

    }
    
	},
	
	doAutoPlay : function(mod) {

	  if (this.showSlideTransition[mod]) {
	  	  
  		if (this.isSlideTransitionFade[mod]) {
  			this.nextSlideIndex[mod] = this.activeSlideIndex[mod] + 1;
  			this.animateFade(mod);					
  			this.activeSlideIndex[mod] += 1;	
  		
  		} else if (this.isSlideTransitionSwipe[mod]) {
  			this.containerXPositions[mod] = this.containerXPositions[mod] - this.sidebarWidths[mod];
  			this.animateSwipe(mod);					
  			this.activeSlideIndex[mod] += 1;	
  		}  
  		
    } else {
    
        // no animation
        this.nextSlideIndex[mod] = this.activeSlideIndex[mod] + 1;
        this.animateNoTransition(mod);          
        this.activeSlideIndex[mod] += 1;
    }	
 	},
 	 
 	swapPausePlayButton : function(mod) {
		var buttonElement = D.get('flickr-nav-playPause-' + mod);		
		if (this.isAutomated[mod]) {
			buttonElement.src = "/universal/images/slideshow/pause-button.png";
			
		} else {
			buttonElement.src = "/universal/images/slideshow/play-button.png";
		} 	 
 	 }

};	
	
	
  E.onDOMReady( function() {
    
    var flickr = Squarespace.Social.Flickr;
    flickr.modules.push(8017945);
    flickr.activeSlideIndex[8017945] = 1;
    flickr.nextSlideIndex[8017945] = 2;    
    flickr.navElements[8017945] = D.getElementsByClassName('flickr-nav-button', 'div', 'flickr-nav-8017945');
    flickr.loadCounts[8017945] = 10;
    flickr.displayCounts[8017945] = 9;
    flickr.showSlideTransition[8017945] = (true) ? true : false;
    flickr.isSlideTransitionFade[8017945] = (true) ? true : false;
    flickr.isSlideTransitionSwipe[8017945] = (false) ? true : false;
    flickr.isGridPreset[8017945] = (false) ? true : false;
    flickr.isSlidePreset[8017945] = (true) ? true : false;
    flickr.thumbsPerRow[8017945] = 3;
    flickr.thumbPadding[8017945] = 2;
    flickr.autoCrop[8017945] = true;
    flickr.autoPlay[8017945] = true;
    flickr.displayControls[8017945] = (true) ? true : false;
    flickr.slideDelay[8017945] = 6 * 1000;
    flickr.isAutomated[8017945] = false;
    flickr.isPreloaded[8017945] = false;
    
    // get containing sidebar
    var sidebar = D.getAncestorByClassName('moduleContentWrapper8017945', 'verticalNavigationBarWrapper');
    var section = D.getAncestorByClassName('moduleContentWrapper8017945', 'section');
    flickr.sidebarWidths[8017945] = parseInt(D.getStyle(sidebar, "width"));    
    var content = D.get('flickr-content-8017945');
    
    // adjust sidebar for borders
    var borderRightWidth = parseInt(D.getStyle(content, 'borderRightWidth'));
    var borderLeftWidth = parseInt(D.getStyle(content, 'borderLeftWidth'));
    if (borderRightWidth) {
      flickr.sidebarWidths[8017945] -= borderRightWidth;
    }
    if (borderLeftWidth) {
      flickr.sidebarWidths[8017945] -= borderLeftWidth;
    }

    // adjust sidebar for section padding
    var sectionLeftPaddingWidth = parseInt(D.getStyle(section, 'paddingLeft'));
    var sectionRightPaddingWidth = parseInt(D.getStyle(section, 'paddingRight'));
    if (sectionLeftPaddingWidth) {
      flickr.sidebarWidths[8017945] -= sectionLeftPaddingWidth;
    }
    if (sectionRightPaddingWidth) {
      flickr.sidebarWidths[8017945] -= sectionRightPaddingWidth;
    }
    
    // adjust sidebar for section margins
    var sectionLeftMarginWidth = parseInt(D.getStyle(section, 'marginLeft'));
    var sectionRightMarginWidth = parseInt(D.getStyle(section, 'marginRight'));
    if (sectionLeftMarginWidth) {
      flickr.sidebarWidths[8017945] -= sectionLeftMarginWidth;
    }
    if (sectionRightMarginWidth) {
      flickr.sidebarWidths[8017945] -= sectionRightMarginWidth;
    }
    


    //Setup status messages
    if (flickr.isGridPreset[8017945]) {      
      var thumbW = Math.floor(flickr.sidebarWidths[8017945] / flickr.thumbsPerRow[8017945]);
      var estHeight = Math.round( (flickr.displayCounts[8017945] / flickr.thumbsPerRow[8017945])) * thumbW;
      D.setStyle('flickr-wrapper-8017945', 'height', estHeight + 'px');
      D.setStyle('flickr-wrapper-8017945', 'width', flickr.sidebarWidths[8017945] + 'px'); 
      D.setStyle('flickr-content-8017945', 'height', estHeight + 'px'); 
      D.setStyle('flickr-content-8017945', 'width', flickr.sidebarWidths[8017945] + 'px');       
    }
    
          // get user flickr links
      var flickrLinkText = "@ucsbgradpost's stream";
          
              D.get('flickr-slug-text-8017945-1').innerHTML = flickrLinkText.replace("@user", "ucsbgradpost"); 
            
    
    

    flickrWidget8017945 = new Squarespace.Social.FlickrWidget(8017945, {
      preset : 'slide', 
      displayCount : flickr.displayCounts[8017945], 
      loadCount : flickr.loadCounts[8017945],       
      showSlideTransition : flickr.showSlideTransition[8017945],
      slideTransitionFade : flickr.isSlideTransitionFade[8017945],
      slideTransitionSwipe : flickr.isSlideTransitionSwipe[8017945],
      sidebarWidth : flickr.sidebarWidths[8017945],
      autoCrop : flickr.autoCrop[8017945],      
      thumbsPerRow : flickr.thumbsPerRow[8017945],
      thumbPadding : flickr.thumbPadding[8017945]     
    });
    flickrWidget8017945.show();  
    
    if(flickr.isSlidePreset[8017945]) {        
      D.setStyle('flickr-wrapper-8017945', 'width', flickr.sidebarWidths[8017945] + 'px');      
      D.setStyle('flickr-content-8017945', 'height', flickr.sidebarWidths[8017945] + 'px');     
      D.setStyle('flickr-container-8017945', 'height', flickr.sidebarWidths[8017945] + 'px');
      
      // adjust container
      flickr.containerElements[8017945] = new YAHOO.util.Element('flickr-container-8017945');
      flickr.containerElements[8017945].setStyle('opacity', 0);
    }
    
    if (flickr.showSlideTransition[8017945]) {

      if (flickr.isSlideTransitionFade[8017945]) {
      
        // set individual container widths
        for (var i = 1; i <= flickr.loadCounts[8017945]; i++) {      
          D.setStyle('flickr-container-8017945-' + i, 'width', flickr.sidebarWidths[8017945] + 'px');
          D.setStyle('flickr-container-8017945-' + i, 'height', flickr.sidebarWidths[8017945] + 'px');
          D.addClass('flickr-container-8017945-' + i, 'flickr-animation-fade');       
        }    
      
      } else if (flickr.isSlideTransitionSwipe[8017945]) {
        
        // adjust container props
        flickr.containerXOffsets[8017945] = D.getX(flickr.containerElements[8017945]);        
        flickr.containerElements[8017945].setStyle('min-width', ((flickr.sidebarWidths[8017945] * flickr.loadCounts[8017945]) + (flickr.sidebarWidths[8017945] * 2)) + 'px');       
        D.setX(flickr.containerElements[8017945], D.getX(flickr.containerElements[8017945]) - flickr.sidebarWidths[8017945] );
        flickr.containerXPositions[8017945] = D.getX(flickr.containerElements[8017945]); 
    
        // set individual container widths
        for (var i = 1; i <= flickr.loadCounts[8017945]; i++) {      
          D.setStyle('flickr-container-8017945-' + i, 'width', flickr.sidebarWidths[8017945] + 'px');
          D.setStyle('flickr-container-8017945-' + i, 'height', flickr.sidebarWidths[8017945] + 'px');
          D.addClass('flickr-container-8017945-' + i, 'flickr-animation-swipe');
        }
        D.setStyle('flickr-container-8017945-pre', 'width', flickr.sidebarWidths[8017945] + 'px');
        D.setStyle('flickr-container-8017945-post', 'width', flickr.sidebarWidths[8017945] + 'px');
        D.setStyle('flickr-container-8017945-pre', 'height', flickr.sidebarWidths[8017945] + 'px');
        D.setStyle('flickr-container-8017945-post', 'height', flickr.sidebarWidths[8017945] + 'px');
        D.addClass('flickr-container-8017945-pre', 'flickr-animation-swipe');
        D.addClass('flickr-container-8017945-post', 'flickr-animation-swipe');
      }   
      
     } else {
     
        // set individual container widths
        for (var i = 1; i <= flickr.loadCounts[8017945]; i++) {      
          D.setStyle('flickr-container-8017945-' + i, 'width', flickr.sidebarWidths[8017945] + 'px');
          D.setStyle('flickr-container-8017945-' + i, 'height', flickr.sidebarWidths[8017945] + 'px');
          D.addClass('flickr-container-8017945-' + i, 'flickr-animation-none');       
        } 
        
     }
     
     YAHOO.util.Event.on("flickr-content-8017945", "mouseenter", function(e) {      
        if (flickr.isPreloaded[8017945] && flickr.displayControls[8017945]) {        
          var anim = new YAHOO.util.Anim("flickr-nav-8017945", { 
            opacity: { from: 0, to: 1 }
          }, flickr.FADE_FAST_SPEED);
          anim.animate();
          
          var anim2 = new YAHOO.util.Anim(flickr.navElements[8017945], { 
            opacity: { from: 0, to: 0.7 }
          }, flickr.FADE_FAST_SPEED);
          anim2.animate();  
        }    
    });
    
      
    YAHOO.util.Event.on("flickr-content-8017945", "mouseleave", function(e) {
      if (flickr.isPreloaded[8017945] && flickr.displayControls[8017945]) {
        var anim = new YAHOO.util.Anim("flickr-nav-8017945", { 
          opacity: { from: 1, to: 0 }
        }, flickr.FADE_FAST_SPEED);
        anim.animate();  
        
        var anim2 = new YAHOO.util.Anim(flickr.navElements[8017945], { 
          opacity: { from: 0.7, to: 0 }
        }, flickr.FADE_FAST_SPEED);
        anim2.animate();  
      }
    });
    

    YAHOO.util.Event.on(flickr.navElements[8017945], "click", function(e) {
    
      // get current module
      var mod = 8017945;
      
      if (this.id == "flickr-nav-button-center-" + mod) {
      
        // start/stop autoplay
        if (flickr.isAutomated[mod]) {
          flickr.stopTimer(mod);   
          flickr.isAutomated[mod] = false;
        
        } else {
          flickr.startTimer(mod);
          flickr.isAutomated[mod] = true;
        } 
        flickr.swapPausePlayButton(mod);
      
      } else {    
        
        if (flickr.showSlideTransition[mod]) {
        
          if (flickr.isSlideTransitionFade[mod]) { 

              // fade transition 

              if (this.id == "flickr-nav-button-left-" + mod) {                  
                flickr.nextSlideIndex[mod] = flickr.activeSlideIndex[mod] - 1;
                flickr.animateFade(mod);
                flickr.activeSlideIndex[mod] -= 1;  
              
              } else if (this.id == "flickr-nav-button-right-" + mod) {  
                flickr.nextSlideIndex[mod] = flickr.activeSlideIndex[mod] + 1;
                flickr.animateFade(mod);
                flickr.activeSlideIndex[mod] += 1;      
              }   
              
           } else if (flickr.isSlideTransitionSwipe[mod]) {
            
              // swipe transition 
            
              if (this.id == "flickr-nav-button-left-" + mod) {  
                flickr.containerXPositions[mod] = flickr.containerXPositions[mod] + flickr.sidebarWidths[mod];
                flickr.animateSwipe(mod);         
                flickr.activeSlideIndex[mod] -= 1;
              
              } else if (this.id == "flickr-nav-button-right-" + mod) {        
                flickr.containerXPositions[mod] = flickr.containerXPositions[mod] - flickr.sidebarWidths[mod];
                flickr.animateSwipe(mod);         
                flickr.activeSlideIndex[mod] += 1;  
              }
            }
          
          } else {
            
            // no animated transition

            if (this.id == "flickr-nav-button-left-" + mod) {
              flickr.nextSlideIndex[mod] = flickr.activeSlideIndex[mod] - 1;
              flickr.animateNoTransition(mod);
              flickr.activeSlideIndex[mod] -= 1;  
            
            } else if (this.id == "flickr-nav-button-right-" + mod) { 
              flickr.nextSlideIndex[mod] = flickr.activeSlideIndex[mod] + 1;
              flickr.animateNoTransition(mod);
              flickr.activeSlideIndex[mod] += 1;      
            }             
          }
        }
    });
  
  
  });  
  

  

//-->
                     </script>
