  </script>
  <script>
   window.jQuery || document.write('<script src="js/vendor/jquery-1.9.1.min.js"><\/script>')
  </script>
  <script type="text/javascript">
   $(function(){
			$("#d2l-login").css("margin-left", "0").css("height", "0px").css("padding-top", "0px");
			
			$("#loginToggle").click(function(event) {
				event.preventDefault();
				if ($("#d2l-login").css("height") == "0px") {
					$("#d2l-login").animate({height: "230px"}, 100);
					$(".pdx-btn-login-d2l").addClass("pdx-btn-login-expand");
				}
			});
		});

		// test this on the same domain!
		function autoResize(id){
			var newheight;
			if(document.getElementById){
				newheight=document.getElementById(id).contentWindow.document.body.scrollHeight;
			}
			$('#'+id).css('height',(newheight+15)+"px");
		};
  </script>
 </body>
