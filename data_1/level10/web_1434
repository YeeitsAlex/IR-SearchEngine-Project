     <iframe allowtransparency="true" class="welcome" frameborder="0" id="welcome" marginwidth="0" name="welcome" onload="autoResize('welcome');" scrolling="auto" src="/shared/portal/message.html" title="Welcome message">
     </iframe>
    </div>
