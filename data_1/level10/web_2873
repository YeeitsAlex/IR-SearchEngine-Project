  <script>
   $(function() {
        $('#featMed').css('background-color', '#005C8E');
        $('.featCurriculum-block-row').not(':first-child').hide();

        $('.featLink').click(function() {
            $(this).siblings().css('background-color', '#000F41');
            $(this).css('background-color', '#005C8E');

            var id = $(this).attr('id');
            var targetBlock = $('#'+id+'Row');
            var targetContent = targetBlock.children('.featCurriculum-content');
            
            targetBlock.siblings().hide();
            targetBlock.show();
            
            var parentWidth = targetContent.innerWidth();

            targetContent.find('.resize').width(parentWidth);
            
        });
        $('.resize').each(function(){
            var element = $(this);
            var parentWidth = element.parent().width();
            
            element.width(parentWidth);
        });
    });
  </script>
