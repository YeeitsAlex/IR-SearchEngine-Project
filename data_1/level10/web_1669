    <script>
     window.onload = function(){
try {
  // redirect 'Log in' link to ADFS login page
  var loginButton = document.querySelector("#essentialnavbar > div > div > div > div > div.pull-right > div.usermenu > ul > li > a");
  var loginButtonClean = document.querySelector("#page-site-index > header > nav > div > div.usermenu > span > a");
  if(loginButton){
    loginButton.setAttribute("href", "/auth/saml/index.php");
  }
  else if(loginButtonClean){
    loginButtonClean.setAttribute("href", "/auth/saml/index.php");
  }

  // redirect to login page from resource, if not logged in from Kurogo
  var signedIn = document.querySelector('.usermenu .login') === null;
  var onLoginPage = document.URL.indexOf('login') > -1;
  var isKurogo = window.navigator.userAgent.includes('Kurogo');
  if (!signedIn && !onLoginPage && isKurogo) {
    document.querySelector('.continuebutton form input').click();
  }
} catch (e) {
  console.log(e);
}
}
    </script>
