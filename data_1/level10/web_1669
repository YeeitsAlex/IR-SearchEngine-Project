   <script>
    var manualLoginDesc =  document.querySelector("#region-main > div > div > div > div.subcontent.loginsub > div");
            if(manualLoginDesc){
                manualLoginDesc.innerHTML = "<h3><b>SPECIAL CREDENTIALS ONLY - Log in below </b></h3> cctr, program reviewer, sign language interpreter, personal email (Cookies must be enabled in your browser)";
            }
 
try {
            var forgotPass = document.querySelector("#login > div.forgetpass");
            if(forgotPass){
                forgotPass.remove();
            }

            var guestLogin = document.querySelector("#region-main > div > div > div > div.subcontent.guestsub");
            if(guestLogin) {
                guestLogin.remove();
            }

            var loginHeader = document.querySelector(".loginpanel > h2:nth-child(1)");
            if (loginHeader) {
                loginHeader.innerHTML = "ATTENTION: Students, Faculty, Staff ";
            }

            var loginButton = document.querySelector("#region-main > div > div > div > center > a");
            if(loginButton) {
                var nextChild = loginButton.nextSibling;
                var parentElement = loginButton.parentNode;
                var newDiv = document.createElement("form");
                newDiv.setAttribute("action", "index.php");
                newDiv.innerHTML = "<input type='submit' value='Click this button to log in' style='font-size:20px; height:4em;'>";
                
                parentElement.insertBefore(newDiv, nextChild);
                loginButton.remove();
            }
} catch (e) {
   console.log(e);
}
   </script>
