   <script>
    // Google search, JQuery collapses search if clicked outside of the Search bar
		$(document).click(function(event) {
			if(!$(event.target).is('.gsc-input') && !$(event.target).is('.fa-times')) {
				$('.gsc-serach').collapse('hide'); }
		});
   </script>
