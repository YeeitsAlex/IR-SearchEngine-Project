     <div id="omnicontent">
      <!-- com.omniupdate.div label="home-image" group="Everyone" button="727" -->
      <!-- com.omniupdate.editor toolbar="IMG_ONLY" csspath="/z-omniupdate/css/style.css" cssmenu="/z-omniupdate/css/style.txt" width="700" -->
      <p>
       <script type="text/javascript">
        // <![CDATA[
var imgs1 = new Array("/images/rotatingbanner/1.jpg","/images/rotatingbanner/2.jpg","/images/rotatingbanner/3.jpg","/images/rotatingbanner/4.jpg","/images/rotatingbanner/5.jpg","/images/rotatingbanner/6.jpg");
var alt1 = new Array();
var currentAd1 = 0;
var imgCt1 = 6;
function cycle1() {
  if (currentAd1 == imgCt1) {
    currentAd1 = 0;
  }
var banner1 = document.getElementById('adBanner1');
var link1 = document.getElementById('adLink1');
  banner1.src=imgs1[currentAd1]
