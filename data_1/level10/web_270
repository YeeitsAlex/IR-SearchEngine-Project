   <script type="text/javascript">
    jQuery(document).ready(function () {
    ucde_apply_header()
})

function ucde_get_parentsite(p) {

    if (p.indexOf("extension.ucdavis") > -1 || p.indexOf("cpe.ucdavis") > -1) {
        return "extension"
    }
    if (p.indexOf("cie.ucdavis") > -1 || p.indexOf("extension-international-programs") > -1) {
        return "cie"
    }
    if (p.indexOf("humanservices.ucdavis") > -1 || p.indexOf("human-services") > -1) {
        return "hs"
    }

    return "extension"

}


function ucde_apply_header() {
    if (typeof parentSite != "string" || parentSite == "null" || parentSite.length < 8) {
        parentSite = "https://cpe.ucdavis.edu/";
    }
    if (parentSite.substr(-1, 1) != "/") {
        parentSite = parentSite + "/";
    }
    var ucde_settings = {}

    ucde_settings.extension = {}
    ucde_settings.cie = {}
    ucde_settings.hs = {}

    ucde_settings.extension.html = '\
     <header class="ucde_header">\
         <div class="container">\
            <div class="row col-sm-12">\
                <div class="brand navbar-brand">\
                    <a href="parentSite/" title="Home" rel="home" id="logo"><img src="https://ucdavis.box.com/shared/static/fl9g5c59hopylmr3u4ddp1losrw0z9p1.jpg" alt="Home" /></a>\
                </div>\
            </div>\
         </div>\
     </header>'
        // Split and join is to replace parentSite to the parentSite variable
        .split("parentSite/").join(parentSite)


    ucde_settings.cie.html = '\
     <header class="ucde_header">\
        <div class="container">\
            <div class="row col-sm-12">\
                <div class="brand navbar-brand">\
                    <a href="parentSite/" title="Home" rel="home" id="logo"><img src="https://ucdavis.box.com/shared/static/verk86xttekqbhvtonabk6tyeevntrtj.jpg" alt="Home" /></a>\
                 </div>\
            </div>\
         </div>\
     </header>'
        .split("parentSite/").join(parentSite)

    ucde_settings.hs.html = '\
    <header class="ucde_header">\
        <div class="container">\
            <div class="row col-sm-12">\
                <div class="brand navbar-brand">\
                    <a href="parentSite/" title="Home" rel="home" id="logo"><img src="https://ucdavis.box.com/shared/static/tcfg9zof3xrfuxmtfq0p4j9jhyc6rgzm.jpg" alt="Home" /></a>\
                </div>\
     </div>\
     </div>\
     </header>'.split("parentSite/").join(parentSite)


    var site = ucde_get_parentsite(parentSite)

    jQuery("body").prepend(ucde_settings[site]['html'])
}
   </script>
  </link>
