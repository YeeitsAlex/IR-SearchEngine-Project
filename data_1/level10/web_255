          <script language="javascript">
           var _vCalUrl = "/Events/Info/AddToCalendar.aspx?e=" + '2bf6e0b8-a1fd-41a7-8a72-11e81fcd1fd8' + "&cal_type=1&pageId=" + '10';
    var _iCalUrl = "/Events/Info/AddToCalendar.aspx?e=" + '2bf6e0b8-a1fd-41a7-8a72-11e81fcd1fd8' + "&cal_type=2&pageId=" + '10';
            var _CustUrl = '';
            var _CustLocUrl = '';
            var _WeatherUrl = 'http://www.weather.com';
            var _EmailToPlnrUrl = "/Events/Info/EmailToPlanner.aspx?" + 'e=2bf6e0b8-a1fd-41a7-8a72-11e81fcd1fd8';
