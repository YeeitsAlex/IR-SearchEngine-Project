   var qstr = 'e=2bf6e0b8-a1fd-41a7-8a72-11e81fcd1fd8';
        function OpenNavLink(pageUrl) {
            var s;
            if (pageUrl.indexOf("?") != -1) {
                s = utility_GetAppRoot() + pageUrl + "&" + qstr;
            } else {
                s = utility_GetAppRoot() + pageUrl + "?" + qstr;
            }
            document.location = s;
        }
        function OpenNewNavLink(pageUrl) {
            window.open(utility_GetAppRoot() + pageUrl);
        }
        function OpenCustomPageNavLink(pageUrl) {
            var s = utility_GetAppRoot() + pageUrl + "&" + _qstr;
            document.location = s;
        }
        var _Url = "StaffPrinterFriendly.aspx?e=" + "bf1e386a-0063-4b4c-8790-59044d8eee26"
        function showPrinterFriendly() {
            newWindow = window.open(_Url, "popup", "height=500, width=700, menubar, scrollbars, resizable, status");
        }

        // Social Media Follow Bar
        function PopupSocial(_sSocialMediaUrl) {
            newWindow = window.open(_sSocialMediaUrl, "popup", "height=700, width=880, menubar, scrollbars, resizable, status");
        }
