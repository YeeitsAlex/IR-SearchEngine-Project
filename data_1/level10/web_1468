   <div style="float:right;padding:5px 0 7px 0;">
    <form action="/ctsi/institution/personnel-search/" class="margin-form" id="search_fm" method="GET" name="search_fm">
     <input id="q" name="q" type="text" value=""/>
     <input name="formbutton:ok" style="border:1px solid #666666;font-size:12px;font-family:sans-serif;padding:3px;background-color:#e6f3fd;" type="submit" value="	 SEARCH	 "/>
     <input name="mesh" type="hidden" value=""/>
     <input name="person_id" type="hidden" value=""/>
     <input name="form:mode" type="hidden" value="edit"/>
     <input name="form:id" type="hidden" value="search"/>
     <style type="text/css">
      .inline.portrait {
						min-width:		1em;
						max-height:		1.25em;
						max-width:		1.25em;
						margin-right:	0.25em;
					}
     </style>
     <script type="text/javascript">
      $("#q").autocomplete({
						delay:		300,//milliseconds
						minLength:	4,//characters
						source:		'/ctsi/find-matches',
						select:	function(event, selectedOptionUI) {
							var item = selectedOptionUI.item;
							document.search_fm.q.value			= "";
							document.search_fm.person_id.value	= "";
							document.search_fm.mesh.value		= "";

							if(item.type == "person") {
								document.search_fm.person_id.value = item.id;
								document.location = '/ctsi/institution/personnel?personnel_id='+item.id;
							} else if (item.type == "mesh_term") {
								document.search_fm.mesh.value = item.id;
								document.search_fm.submit();
							} else if (item.type == "continuation") {
								return false;
							} else if (item.type == "separator") {
								return false;
							}
							return true;
						}
					}).data("ui-autocomplete")._renderItem = function(ul, item) {
						if (item.type == "separator") {
							return $("<li class='separator'>").append("<hr />").appendTo(ul);
						} else if (item.type == "continuation") {
							return $("<li class='continuation'>").append("More &hellip;").appendTo(ul);
						} else if (item.type == "person") {
							return $("<li class='"+item.type+"'>")
									.append("<a><img class='inline portrait' src='/ctsi/institution/photo?personnel_id="+item.id+"' />"+item.label+"</a>")
									.appendTo(ul);
						} else if (item.type == "mesh_term") {
							return $("<li class='"+item.type+"'>")
									.append("<a>"+item.label+"</a>")
									.appendTo(ul);
						} else {
							return $("<li style='border-bottom:2px solid #e0e7ef'>").append(item.label).appendTo(ul);
						}
					};
     </script>
    </form>
   </div>
