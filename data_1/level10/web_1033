    <!-- JS SCRIPTS -->
    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js">
    </script>
    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js">
    </script>
    <script>
     $(document).ready(function () {
        $('.expired-job-message').append("<div class='search-all-jobs-btn_wrapper'><a href='/all-jobs' class='yellow-btn'>Search All Jobs</a></div>");
        $(function () {
            $(document).scroll(function () {
                var $nav = $(".nav_nav");
                $nav.toggleClass('scrolled', $(this).scrollTop() > $nav.height());

            });
        });

        var Panel = '.side-nav_section';
        var HamburgerBtn = '.nav_bar--hamburger-box';
        var opportunitiesToggle = '[data-nav="opportunities"]';
        var locationToggle = '[data-nav="location"]';
        var MobileBack = '.mobile__btn-arrow'

        //Navigation//
        function toggleMenu() {
            $(".hamburger--spring").toggleClass("is-active");
        }

        $(HamburgerBtn).on("click", function (e) {
            toggleMenu();
            e.stopPropagation();
            if ($(Panel).hasClass('is-active')) {
                $(Panel).removeClass("is-active").addClass("is-non-active");
                $(Panel).removeClass("is-active-location is-active-opportunities").addClass(
                    "is-non-active");
            } else {
                $(Panel).removeClass("is-non-active").addClass("is-active");
            }
        });

        //subnav
        function opportunitiesMenu() {
            if ($(Panel).hasClass('is-active-location')) {
                $(Panel).removeClass("is-active-location").addClass("is-active-opportunities");
            } else {
                $(Panel).addClass("is-active-opportunities");
            }
        }

        function locationMenu() {
            if ($(Panel).hasClass('is-active-opportunities')) {
                $(Panel).removeClass("is-active-opportunities").addClass("is-active-location");
            } else {
                $(Panel).addClass("is-active-location");
            }
        }

        function hamburgerSwitch() {
            if ($(".is-active-location").is('*') || $(".is-active-opportunities").is('*')) {
                $(HamburgerBtn).hide();
                $(MobileBack).fadeIn();
            }

        }



        $(opportunitiesToggle).on("click", function (e) {
            opportunitiesMenu();
            hamburgerSwitch()
            e.stopPropagation();
        });

        $(locationToggle).on("click", function (e) {
            locationMenu();
            hamburgerSwitch()
            e.stopPropagation();
        });

        $(MobileBack).on('click', function () {
            $(Panel).removeClass("is-active-location is-active-opportunities");
            $(HamburgerBtn).fadeIn();
            $(MobileBack).hide();

        });
    });

    //mobile nav

    (function ($) {
        var $window = $(window),
            $mobileBtnContent = $('.nav_section'),
            $mobilePanel = $('.side-nav_section');

        $window.resize(function resize() {
            if ($window.width() < 992) {
                $mobileBtnContent.addClass('is-mobile');
                $mobilePanel.addClass('is-mobile');

            } else {
                $mobileBtnContent.removeClass('is-mobile');
                $mobilePanel.removeClass('is-mobile');

            }

        }).trigger('resize');
    })(jQuery);

    // opportunities and location interior nav

    var int_btn = '.toggle-level';

    $(int_btn).on('click', function () {
        $(".opportunities_nav-ul--mobile").toggleClass("active");
    });
    </script>
   </div>
