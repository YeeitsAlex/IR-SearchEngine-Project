  <script>
   (function(){
  function ready(fn) {
    if(document.readyState != 'loading') {
      fn();
    } else {
      var listener = function () {
        document.removeEventListener('DOMContentLoaded', listener);
        fn();
      };

      document.addEventListener('DOMContentLoaded', listener);
    }
  }

  ready(function() {
    function setupClickTracking (query, a, b, c) {
        var nodeList = document.querySelectorAll(query);

        for (var i = nodeList.length - 1; i >= 0; i--) {
            var node = nodeList[i];

            node.addEventListener('click', function() {
                a = a.constructor === Function ? a(node) : a;
                b = b.constructor === Function ? b(node) : b;
                c = c.constructor === Function ? c(node) : c;
                // ga('send', 'event', a, b, c);
                ga('b.send', 'event', a, b, c);
                ga('c.send', 'event', a, b, c);
            });
        }
    }

    function fromNode(attribute, suffix, node) {
        return node.getAttribute(attribute) + suffix;
    }

    setupClickTracking('input#submitExistingLogin_top', 'Job Seeker & Resume', 'Login & Signup', 'Header Login');
    setupClickTracking('.seeker-menu .sign-up', 'Job Seeker & Resume', 'Login & Signup', 'Header Sign Up');
    setupClickTracking('.for-employer-links a.employer-login-tracking', 'Employers', 'Employer Login', 'Header Employer Login ');
    setupClickTracking('#employer-products a.employer-products-login', 'Employers', 'Looking to Hire Toggle', 'Employer Login');
    setupClickTracking('#employer-products a.employer-post-jobs', 'Employers', 'Looking to Hire Toggle', 'Post Jobs')
    setupClickTracking('#employer-products a.employer-other-links', 'Employers', 'Looking to Hire Toggle', 'Other Links');
    setupClickTracking('ul.social-share li a', 'Share', fromNode.bind(this, 'title', ' Share'), fromNode.bind(this, 'href', ''));
    setupClickTracking('div#site-footer .tools ul li a', 'Tools', fromNode.bind(this, 'title', ''), fromNode.bind(this, 'href', ''));
    setupClickTracking('.recent-jobs h2 a', 'RSS', 'RSS', 'RSS Company Jobs');

    Array.prototype.concat.apply([], document.querySelectorAll('[data-analytics-action][data-analytics-label]')).forEach(function(node) {
      node.addEventListener('click', function(e) {
        // only send for company GA analytics
        var category = 'Custom Careersite Event';
        var action = node.getAttribute('data-analytics-action')
        var label = node.getAttribute('data-analytics-label');
        ga('c.send', 'event', category, action, label);
      })
    });

  });
})();
  </script>
