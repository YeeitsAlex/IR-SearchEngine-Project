   <div class="pull-right" id="search">
    <form class="form-inline">
     <input class="input-small" id="qt" maxlength="50" placeholder="Search" type="text"/>
     <select id="SearchOptions">
      <option value="lg">
       Subject Guides
      </option>
      <option value="dl">
       Digital Library
      </option>
      <option value="w">
       Website
      </option>
     </select>
     <button class="btn btn-u-red" onclick="Teleport(); return false;">
      <i class="icon-search">
       <!-- Search Icon -->
      </i>
     </button>
    </form>
   </div>
   <script type="text/javascript">
    // <![CDATA[
function Teleport()
	{
		
		var searchArea = document.getElementById("SearchOptions").value;
		var searchString = document.getElementById("qt").value;
		var location = "";
		switch (searchArea)
		{
			
			case "lg":
				location = "http://campusguides.lib.utah.edu/srch.php?q=" + searchString;
				break;
			case "dl":
				location = "https://collections.lib.utah.edu/search?q=" + searchString;
				break;
			case "w":
				location = "https://gcse.search.utah.edu/index.php?q=" + searchString + "s&site=Look-only-in-marriott-library-main-website&gcse_action=site_search";
				break;
		}
		window.location.href = location;
	}
// ]]>
   </script>
