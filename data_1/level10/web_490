  <script>
   window.jQuery || document.write('<script src="//templates.utah.edu/_main-v2/_scripts/vendor/jquery-1.11.1.min.js"><\/script>')
  </script>
  <script src="//templates.utah.edu/_main-v2/_scripts/vendor/bootstrap.min.js">
  </script>
  <script src="//templates.utah.edu/_main-v2/_scripts/main.js">
