   <script>
    (function() {
    var link = document.createElement('link');
    link.type = 'image/x-icon';
    link.rel = 'shortcut icon';
    link.href = 'https://uhealthplan.utah.edu/favicon.ico';
    document.getElementsByTagName('head')[0].appendChild(link);
    }());
   </script>
   <script>
    function getParameterByName(name, url) {
    if (!url) url = window.location.href;
    name = name.replace(/[\[\]]/g, "\\$&");
    var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
        results = regex.exec(url);
    if (!results) return null;
    if (!results[2]) return '';
    return decodeURIComponent(results[2].replace(/\+/g, " "));
    }
   </script>
   <script>
    var meta=document.createElement('meta');
    var mhc = getParameterByName('planId');
    meta.name='uuhpgroupid';
    meta.setAttribute('content', mhc);
    document.getElementsByTagName('head')[0].appendChild(meta);
   </script>
   <script language="javascript" type="text/javascript">
    $(document).ready(function() { 
        toggleMHCNav(); 
        $("#planId").change(function() { toggleMHCNav(); });
		function toggleMHCNav()
	    {
		var sitepath = window.location.href;
		var stringsplit = sitepath.split('?');
		var specificsite = stringsplit[1];
		if ((specificsite == "planId=12") || (specificsite == "planId=13") || (specificsite == "planId=14") || (specificsite == "planId=15") || (specificsite == "planId=16"))
			{
			$('#top-bar-container nav').attr("style", "display:none;");
		} else { 
			$('#top-bar-container nav').attr("style", "display: block;");
			}
	    }
	    })
   </script>
   <script>
    $(document).ready(function() {
    changePlanFilters(); 
    $("#planId").change(function() { changePlanFilters(); });
});

function changePlanFilters()
	{
		if (($("#planId").val() == 12) || ($("#planId").val() == 13) || ($("#planId").val() == 14) || ($("#planId").val() == 15) || ($("#planId").val() == 16))
		{
			$('#planId > option[value=11]' ).wrap( "<span>" ),
			$('#planId > option[value=8]' ).wrap( "<span>" ),
			$('#planId > option[value=10]' ).wrap( "<span>" ),
			$('#planId > option[value=9]' ).wrap( "<span>" ),
			$('#planId > option[value=3]' ).wrap( "<span>" ),
			$('#planId > option[value=1]' ).wrap( "<span>" ),
			$('#planId > option[value=2]' ).wrap( "<span>" );
	}	else
		if (($("#planId").val() == 8) || ($("#planId").val() == 9))
		{
			$('#planId > option[value= 11 ]' ).wrap( "<span>" ),
            $('#planId > option[value= 10 ]' ).wrap( "<span>" ),
            $('#planId > option[value= 3 ]' ).wrap( "<span>" ),
            $('#planId > option[value= 12 ]' ).wrap( "<span>" ),
            $('#planId > option[value= 16 ]' ).wrap( "<span>" ),
            $('#planId > option[value= 15 ]' ).wrap( "<span>" ),
            $('#planId > option[value= 14 ]' ).wrap( "<span>" ),
            $('#planId > option[value= 13 ]' ).wrap( "<span>" ),
            $('#planId > option[value= 2 ]' ).wrap( "<span>" ),
            $('#planId > option[value= 1 ]' ).wrap( "<span>" );

	}	else
	{
			$('#planId > option[value= 11 ]' ).wrap( "<span>" ),
			$('#planId > option[value= 10 ]' ).wrap( "<span>" ),
			$('#planId > option[value= 3 ]' ).wrap( "<span>" ),
			$('#planId > option[value= 12 ]' ).wrap( "<span>" ),
			$('#planId > option[value= 16 ]' ).wrap( "<span>" ),
			$('#planId > option[value= 15 ]' ).wrap( "<span>" ),
			$('#planId > option[value= 14 ]' ).wrap( "<span>" ),
			$('#planId > option[value= 13 ]' ).wrap( "<span>" ),
			$('#planId > option[value= 2 ]' ).wrap( "<span>" ),
			$('#planId > option[value= 1 ]' ).wrap( "<span>" ),
			$('#planId > option[value= 8 ]' ).wrap( "<span>" ),
			$('#planId > option[value= 9 ]' ).wrap( "<span>" );
	}
	}
   </script>
