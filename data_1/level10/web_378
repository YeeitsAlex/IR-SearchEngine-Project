      <script type="text/javascript">
       var submit = function () {
             var form = $('#view_app_form');
             var action = '/APP/Number';

             var selected = $('#app_drop_down').find(':selected').val();
             form.attr('action', action + '/' + selected);
             if (selected != 0) {
                 form.submit();
             }
         };

         $('#app_drop_down').change(submit);
         $('#goButton').click(submit);
      </script>
