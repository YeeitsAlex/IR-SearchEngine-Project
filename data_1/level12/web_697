                <ul>
                 <!-- SLIDE  -->
                 <li data-description="" data-easein="default" data-easeout="default" data-hideafterloop="0" data-hideslideonmobile="off" data-index="rs-78" data-link="http://cores.utah.edu/iron-heme/" data-masterspeed="300" data-param1="" data-param10="" data-param2="" data-param3="" data-param4="" data-param5="" data-param6="" data-param7="" data-param8="" data-param9="" data-rotate="0" data-saveperformance="off" data-slotamount="7" data-thumb="http://cores.utah.edu/wp-content/uploads/2016/12/113016_Iron-and_Heme_collage-100x50.jpg" data-title="Iron &amp; Heme" data-transition="random">
