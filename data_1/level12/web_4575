  <script type="text/javascript">
   /* Attach Google Analytics Custom Event Tracking */
$(document).ready(function(){
  /* add ga slider event to Carousel link click event */
  $(".item a").click(function(){     
    /* get current carousel item */  
    console.log("GA_CET:Carousel onClick fired");    
    var activeItem = document.getElementsByClassName("item active")[0];
    var title = activeItem.getElementsByTagName("h2")[0].innerHTML;
    var category = window.location.pathname;
    var action = $(this).attr("href");  
    var label = $(this).text();
    if (label.trim() === "") {              			
      label = "image";
    }
    console.log("GA_CET:Carousel onClick slide title: " + title);

    /* TODO: what if ga is not present? */
    console.log("GA_CET: ga:" + ga); 

    /* get first ga tracker */ 
    var trackerName = ga.getAll()[0].get('name');
    console.log("GA_CET: ga[0].name:" + label);

    /* send slider click event to ga */
    ga(trackerName + '.send', 'event', {
      eventCategory: 'Carousel Slider ',
      eventAction: action,
      eventLabel: title,
      transport: 'beacon'
    });   
  });
  
  console.log("GA_CET:Google Analytics Custom Event Tracking attached");
  return true;
});
  </script>
