        <div class="panel-display panel-2col-bricks clearfix" id="mini-panel-95visual_homepage">
         <div class="panel-panel panel-col-top">
          <div class="inside">
          </div>
         </div>
         <div class="center-wrapper">
          <div class="panel-panel panel-col-first">
           <div class="inside">
           </div>
          </div>
          <div class="panel-panel panel-col-last">
           <div class="inside">
           </div>
