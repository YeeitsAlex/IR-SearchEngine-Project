         <script>
          <!--//--><![CDATA[// ><!--

var randno;
var spotlightItems;
var odoPagerLinks;
var theEventTimer;
jQuery(document).ready(function($){
	if($(".odo-spotlight > div.content > ul > li").length > 1) {
		//var randno = Math.floor(Math.random() * (4 - 0 + 0)) + 0; // max is how many you want. min is starting index.
		randno = Math.floor(Math.random() * (2 - 0 + 0)) + 0; // max is how many you want. min is starting index.
		spotlightItems = $(".odo-spotlight > div.content > ul > li");
		spotlightItems[randno].classList.add("spotted");
		var odoPager = document.createElement("div");
		odoPager.setAttribute("class","odoPager");
		$(".odo-spotlight > div.content > ul").after(odoPager)
		$(".odo-spotlight > div.content > ul > li").each(function(index) {
			$(this).mouseover(stopAutoSwitchEvent);
			if(index==0) {
				$(".odoPager").html('<a href="javascript:void(0);" onclick="switchEvent(-1)">&lang;&nbsp;&nbsp;&nbsp;&nbsp;');
			}
			var pagerLable= index+1;
			$(".odoPager").html($(".odoPager").html() + '<a class="odoPagerLink" href="javascript:void(0);" onclick="switchEvent(' + index + ')"><span>' + pagerLable + '');
			if(index==$(".odo-spotlight > div.content > ul > li").length-1) {
				var odoPager = document.createElement("p");
				$(".odoPager").html($(".odoPager").html() + '&nbsp;&nbsp;&nbsp;&nbsp;<a href="javascript:void(0);" onclick="switchEvent(1000)">&rang;');
			}
		});
		odoPagerLinks = document.getElementsByClassName("odoPagerLink");
		odoPagerLinks[randno].classList.add("spotting");
		theEventTimer = setInterval(autoSwitchEvent, 20000);
	}
	resizeSpotLite();
	$(window).resize( function() { resizeSpotLite() } );
	function resizeSpotLite() {
		if(window.innerWidth < 1365) {
		$(".node-page > .content > .field-name-field-body > .field-items > .field-item").append($(".spotlite"));
		}
		else {
			$("#sidebar-2 .region-sidebar .block:first").after($(".spotlite"));
		}
	}
});

function switchEvent(theEvent) {
	stopAutoSwitchEvent();
	spotlightItems[randno].classList.remove("spotted");
	odoPagerLinks[randno].classList.remove("spotting");
	if(theEvent==-1) {
		theEvent=randno-1;
		if(theEvent==-1) {
			theEvent=spotlightItems.length-1;
		}
	}
	if(theEvent==1000) {
		theEvent=randno+1;
		if(theEvent==spotlightItems.length) {
			theEvent=0;
		}
	}
	randno=theEvent;
	spotlightItems[theEvent].classList.add("spotted");
	odoPagerLinks[theEvent].classList.add("spotting");
}
function autoSwitchEvent() {
	spotlightItems[randno].classList.remove("spotted");
	odoPagerLinks[randno].classList.remove("spotting");
	if(randno == spotlightItems.length-1) {
		randno=-1;
	}
	spotlightItems[randno+1].classList.add("spotted");
	odoPagerLinks[randno+1].classList.add("spotting");
	randno=randno+1;
}
function stopAutoSwitchEvent() {
    clearInterval(theEventTimer);
}

//--><!]]>
         </script>
        </div>
       </div>
      </div>
     </div>
    </div>
