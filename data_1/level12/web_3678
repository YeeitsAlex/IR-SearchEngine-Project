      <div class="title-desc-inner" data-collection-id="57bdd245e58c6235ea08d5e5" data-edit-main-image="Background">
       <div class="page-title-wrapper">
        <h1 class="page-title" data-content-field="title">
         About
        </h1>
       </div>
       <br/>
       <div class="page-desc" data-content-field="description">
        <p>
         Merced Nanomaterials Center for Energy and Sensing
        </p>
       </div>
      </div>
     </div>
    </div>
    <section id="content-wrapper">
     <div class="content">
      <div class="content-inner" data-content-field="main-content">
       <div class="sqs-layout sqs-grid-12 columns-12" data-type="page" data-updated-on="1542224514993" id="page-57bdd245e58c6235ea08d5e5">
        <div class="row sqs-row">
         <div class="col sqs-col-12 span-12">
          <div class="sqs-block code-block sqs-block-code" data-block-type="23" id="block-yui_3_17_2_2_1503024521981_13577">
           <div class="sqs-block-content">
            <style>
