     </i>
     <h2 class="hours-text">
     </h2>
     <h2>
      View all hours
      <a href="hours-and-holidays/" style="color:#fff; text-decoration:underline;">
       here
      </a>
      .
     </h2>
    </div>
    <!-- <script>
		// Get today's date.
		var today = new Date();

		// Zero hours of date.
		today.setHours(0, 0, 0, 0);

		// Text to be displayed
		var text;

		// Status
		var status;

		// An array of holidays. Months start at 00.
		var holiday = [
		new Date(2016, 00, 18), // MLK Day
		new Date(2016, 02, 31), // Cesar Chavez Birthday
		new Date(2016, 04, 30)]; // Memorial Day

		// We'll use a switch statement for the different days of the
		// week.
		// First, we'll get the day and go from there.
		// 0 == Sunday, 1 == Monday, and so on.
		switch (new Date().getDay()) {
			// Sunday
			case 0:
				status = 1;
				text = "The SCAA is open today.";
				break;

			// Mondays and Thursdays
			case 1:
			case 4:
				status = 1;
				text = "The SCAA is open today."
				break;

			// Tuesdays and Wednesdays
			case 2:
			case 3:
				status = 1;
				text = "The SCAA is open today."
				break;

			// Friday and Saturday
			case 5:
			case 6:
				status = 0;
				text = "The SCAA is closed today.";
				break;
		}

		// Iterate through holiday array.
		for ( var i = 0; i < holiday.length; i++ )
		{
			// If today is a holiday, make status closed
			// and set text to closed message.
			if ( today.getTime() == holiday[i].getTime() ) {
				status = 0;
				text = "The SCAA is closed today.";
				break;
			}
		}

		document.getElementsByClassName("hours-text")[0].innerHTML = text;
		text = document.querySelector(".hours");
		// If we are open, display a green background.
		if (status == 1)
			text.style.backgroundColor = "#1E824C";
		// Otherwise, display a red background.
		else
			text.style.backgroundColor = "#CF000F";
		</script> -->
