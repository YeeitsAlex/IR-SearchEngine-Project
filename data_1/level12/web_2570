  <script>
   $( document ).ready(function() {
    $('.news-grid').click(function() {
      $('.news-grid').removeClass('grid-clicked'); 
      $(this).addClass('grid-clicked'); 
    })
    if (!(Modernizr.touch)) {
      $('.news-grid').hover(
        function() {
          $('.news-grid').removeClass('grid-clicked'); 
          $(this).addClass('grid-clicked'); 
        }, function() {
          $(this).removeClass('grid-clicked'); 
        }
      );
    }
  });
  </script>
