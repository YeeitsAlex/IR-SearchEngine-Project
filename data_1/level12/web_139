    <div class="container homepagecontent" id="content">
     <div class="row">
      <div class="main col-md-12" role="main">
       <div class="entry-content" itemprop="mainContentOfPage">
        <div class="homecontent clearfix home-margin">
         <div class="fl-builder-content fl-builder-content-8300 fl-builder-content-primary fl-builder-global-templates-locked" data-post-id="8300">
          <div class="fl-row fl-row-full-width fl-row-bg-video fl-node-5ae96d0e41e65 fl-row-full-height fl-row-align-center fl-row-bg-overlay" data-node="5ae96d0e41e65" id="home-hero">
           <div class="fl-row-content-wrap">
            <div class="fl-bg-video" data-fallback="https://scet.berkeley.edu/wp-content/uploads/campanile3.jpg" data-height="720" data-mp4="https://scet.berkeley.edu/wp-content/uploads/Copy-of-Updated-Background-Video.mp4" data-mp4-type="video/mp4" data-width="1280">
            </div>
            <div class="fl-row-content fl-row-full-width fl-node-content">
             <div class="fl-col-group fl-node-5ae96d0e4e0c6" data-node="5ae96d0e4e0c6">
              <div class="fl-col fl-node-5ae96d0e4e1f6 fl-col-has-cols" data-node="5ae96d0e4e1f6">
               <div class="fl-col-content fl-node-content">
                <div class="fl-col-group fl-node-5ae970df4afdc fl-col-group-nested" data-node="5ae970df4afdc">
                 <div class="fl-col fl-node-5b204e46825e6 fl-col-small" data-node="5b204e46825e6">
                  <div class="fl-col-content fl-node-content">
                  </div>
                 </div>
                 <div class="fl-col fl-node-5ae97235c6a69 fl-col-small" data-node="5ae97235c6a69">
                  <div class="fl-col-content fl-node-content">
