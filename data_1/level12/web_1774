      <script>
       $(document).ready(function () {
            /* Animation time taken for text to appear */
            var animation_time11 = 750;
            /* Delay time for two texts to appear */
            var delay_time11 = 300;
            carousel_animation11(delay_time11 , animation_time11 );
            $(window).on('resize', function () {
                carousel_animation11(delay_time11 , animation_time11 );
            });
            $("a.youtube-popup.11").YouTubePopUp();
        });
        function carousel_animation11 (delay_time, animation_time) {
            var id = "11";
            if ($(window).width() >= 320 && $(window).width() <= 970) {
                $('#'+id+'.animate-text-custom').show();
                $('#11').unbind().on('slide.bs.carousel', function () {
                    $('.itemrt > img').fadeOut(50);
                });
                $('#11').on('slid.bs.carousel', function () {
                    $('#' + id + ' .item.active >.itemrt > img').fadeIn(1000);
                    $('#' + id + ' .itemft').each(function () {
                        if ($(this).data('slideId') == ($('div.active').index())) {
                            $(this).css('display', 'block').children().each(function () {
                                $(this).addClass('current');
                            });
                        }
                        else {
                            $(this).css('display', 'none').children().each(function () {
                                $(this).removeClass('current');
                            });
                        }
                    });
                })
            }
            else {
                $('#' + id + '  .animate-text-custom').fadeOut();
                $('#' + id + '  .animate-text-custom.current').each(function (counter) {
                    /* Increase or decrease times to adjust animation speeds and time delay */
                    $(this).delay((counter + 1) * delay_time).fadeIn(animation_time);
                });
                /* Carousel slide change event handler to animate text acompanying the slide */
                $('#11').unbind().on('slid.bs.carousel', function () {
                    $('#' + id + '  .animate-text-custom.current').each(function (counter) {
                        /* Increase or decrease times to adjust animation speeds and time delay */
                        $(this).fadeOut(animation_time);
                    });
                    $('#' + id + '  .itemft').each(function () {
                        if ($(this).data('slideId') == ($('div.active').index())) {
                            $(this).css('display', 'block').children().each(function () {
                                $(this).addClass('current');
                            });
                        }
                        else {
                            $(this).css('display', 'none').children().each(function () {
                                $(this).removeClass('current');
                            });
                        }
                    });
                    $('#' + id + ' .animate-text-custom.current').each(function (counter) {
                        /* Increase or decrease times to adjust animation speeds and time delay */
                        $(this).delay((counter + 1) * delay_time).fadeIn(animation_time);
                    });
                })
            }
        }
      </script>
     </div>
