  <div id="content">
   <div class="clearfix">
    <div class="container">
     <h1 id="frontpage_header">
      Surface Analysis and Nano-Scale Imaging
     </h1>
     <div class="contentColumns clearfix" id="contentColumns">
      <div id="leftcol">
      </div>
      <!-- end #leftcol -->
      <div id="maincol">
       <div>
        <table class="plain_table" style="height: 1000px" width="700">
         <tbody valign="top">
          <tr style="font-weight: inherit;font-style: inherit">
