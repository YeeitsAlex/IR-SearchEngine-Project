  <!-- Global Javascript (all pages) -->
  <script>
   // custom javascript for ALL PAGES
	$(document).ready(function () {	
		 		
		var bannerimg = $("#banner img");
		var bpState = whichBreakpoint();
		var inMobile = bpState[1];

		
		function changeToMobile() {
			//console.log("INSIDE");			
			bannerimg.each(function(){
				var src = $(this).attr("src");
				src = src.replace(".jpg","_640x280.jpg");
				$(this).attr({
					"src": src,
					"width": "640",
					"height": "280"
				});				
			});			
		
		} // changeToMobile()
	
		function changeToDesktop() {
			//console.log("ABOVE");
			bannerimg.each(function(){
				var src = $(this).attr("src");
				src = src.replace("_640x280.jpg",".jpg");
				$(this).attr({
					"src": src,
					"width": "1240",
					"height": "374"
					});			
			});	
				
		} // changeToDesktop()
			
		// Do the switch on refresh
		if (inMobile) {
			changeToMobile();
		} else {
			changeToDesktop();
		}
		

	

	function beforeChange() {
		$(".navbar-nav li a").removeClass("active");
		//console.log("beforeChange called");
	} // beforeChange
	function afterAction(){
    //console.log(this.owl.currentItem);
    
    var num = $slick.slick("slickCurrentSlide");
    //var num2 = $slick.slick("slicknextSlide");
    //console.log("num is: " + num);
    if (num == 0) {
	    $("li#atcal a").addClass("active");
    } else if (num == 1) {
	    $("li#gtc a").addClass("active");
    } else if (num == 2) {
	    $("li#caa a").addClass("active");
    } else if (num == 3) {
        $("li#berkeley-150 a").addClass("active");
    }

  } // afterAction


	 function whichBreakpoint(){
		var curWidth = $(window).width();
		//console.log("curWidth: " + curWidth);
		var mobile = 767,
				desktop = 992,
				bigtop = 1200;
		var currentBreakPoint;
		
		if(curWidth < mobile) {
			currentBreakPoint = "mobile";
			bpState = true;
		} else if (curWidth >= mobile && curWidth < desktop) {
			currentBreakPoint = "tablet";
			//curOffset = mobile;
			bpState = false;
		} else if (curWidth >= desktop && curWidth < bigtop) {
			currentBreakPoint = "desktop";
			//curOffset = desktop;
			bpState = false;
		} else if (curWidth >= bigtop) {
			currentBreakPoint = "bigtop";
			//curOffset = bigtop;
			bpState = false;
		}

		return [currentBreakPoint, bpState];
			
	} // whichBreakpoint


/////////////////////////////////////////

		
		var $slick = $('.slick');
		$slick.slick({
	    //setting-name: setting-value
	    autoplay: true,
	    pauseOnHover: false,
	    dots: false,
	    arrows: true,
	    infinite: true,
		  speed: 700,
		  fade: true,
		  cssEase: 'linear',
		  mobileFirst: true,
		  responsive: [{
	      breakpoint: 767,
	      settings: {
	        //setting-name: setting-value
	        
	      }
	    }]

	  }); // slick()
			

		$slick.on('beforeChange', function(event, slick, currentSlide, nextSlide){
		  beforeChange();
		});
		$slick.on('afterChange', function(event, slick, currentSlide){
		  afterAction();
		});
		

  
	$(".navbar-nav li a").mouseover(function(){
		var item = $(this).parent().attr("id");
		//console.log("item is: " + item);
		
		if (item == "atcal") {
			$slick.slick("slickGoTo", 0);
		} else if (item == "gtc") {
			$slick.slick("slickGoTo", 1);
		} else if (item == "caa") {
			$slick.slick("slickGoTo", 2);
        } else if (item == "berkeley-150") {
            $slick.slick("slickGoTo", 3);
        }
		//console.log("in");
	}).mouseout(function(){
		$(this).removeClass("active");
		//console.log("out");
	});
	
	$("#primary-nav a, .slick a, #resources a, .footer-social-media a, .givenow-btn").click(function(){
		// pass the JS object, not jQuery Object;
		recordOutboundLink(this.href);
		// return false;
		// recordOutboundLink(link, category, action) {
		//onClick="recordOutboundLink(this, 'Outbound Links', 'http://cal.berkeley.edu');return false;"
	});
	
		
	  // Init
	  afterAction();
	 
	}); // end document ready
  </script>
  <!-- Analytics -->
  <script>
   (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-2480601-52', 'auto');
  ga('send', 'pageview');
  
  

  
	function recordOutboundLink(url) {
		
		 ga('send', 'event', 'Outbound Links', url, url, {'hitCallback':
	     function () {
		     document.location = url;
	     }
	   });
		
	}
  </script>
