   <button class="bando btn btn-white contacttab" role="button" type="button">
    Close
    <span class="glyphicon glyphicon-chevron-down">
    </span>
   </button>
   <p class="popinfo small text-center" id="popinfo">
    Please fill out the form below and we will get back to you as soon as possible.
   </p>
   <form action="" class="basic popper" id="contact" method="post" name="contact">
    <fieldset>
     <div class="field">
      <label class="contactpop contacttab" for="name">
       Name
      </label>
      <input aria-describedby="popinfo" autocomplete="off" class="contacttab" id="name" name="name" placeholder="Name" required="" type="text"/>
     </div>
     <div class="field">
      <label class="contactpop contacttab" for="email">
       Email
      </label>
      <input autocomplete="off" class="contacttab" id="email" name="email" placeholder="Email" required="" type="text"/>
     </div>
     <div class="field">
      <label class="contactpop" for="message">
       Message:
      </label>
      <textarea autocomplete="off" class="contacttab" cols="19" id="message" name="message" placeholder="Message" rows="3" type="text"></textarea>
     </div>
     <input aria-describedby="success" class="btn btn-primary contacttab" id="submitpop" type="button" value="SEND"/>
     <div id="success">
     </div>
    </fieldset>
   </form>
  </div>
  <script>
   /* -- CONTACT US POP UP FORM -- */

	$('.pop').click(function() {
		$('.popup').toggleClass("popopen");	
	});

	$('.bando').click(function() {
		$('.popup').toggleClass("popopen");	
	});


	$('.pop').click(function() {
		$('.contacttab').toggleClass("contacttabshow");	
	});

	$('.bando').click(function() {
		$('.contacttab').toggleClass("contacttabshow");		
	});
	/* -- POP UP PREVENT REDIRECT -- */		

	$(document).ready(function(){

		$('#submitpop').click(function(){
			$('#success').hide(1);
			$.post("/email_ajax.php", $("#contact").serialize(),  function(response) {
				$('#success').html(response);
				$('#success').show(1000);
			});	
			return false;
		});
		
		
		/* ------ REMOVE CONTACT POP-UP ON PAGES WITH ONBASE FORM ------- */
		$(document).ready(function () {
			if ( $( "#VerificationWorksheet" ).length ) {
				$( "#popupper, #popbutton" ).remove();
			}	
		});
		
	});
			
			
	/* -- FORM VALIDATION -- */
		
	$(function () {
		var showClass = 'show';

		$('input').on('checkval', function () {
			var label = $(this).prev('label');
			if(this.value !== '') {
				label.addClass(showClass);
			} else {
				label.removeClass(showClass);
			}
		}).on('keyup', function () {
			$(this).trigger('checkval');
		}); 
	});

	$(function () {
		var showClass = 'show';

		$('textarea').on('checkval', function () {
			var label = $(this).prev('label');
			if(this.value !== '') {
				label.addClass(showClass);
			} else {
				label.removeClass(showClass);
			}
		}).on('keyup', function () {
			$(this).trigger('checkval');
		}); 
	});
	
	/* -- END CONTACT US POP-UP FORM --*/
  </script>
