   <script type="text/javascript">
    jQuery(document).ready(function($) {
        
          jQuery('#header > .region-header').append('<div class="menu-icon" onclick="javascript: jQuery(\'#navigation\').toggle();"><img src="/sites/all/themes/sph/images/menu_icon.png" width="45" /></div>');        
        
        jQuery('#slideshow .views-field-body').css('cursor', 'pointer');
        jQuery('#slideshow .views-field-body').click( function() { 
            
            window.open('http://sph.berkeley.edu/2016-commencement-features-camara-jones','_blank');
            return;
        });
        
	    // menu icons special icons
	var len = jQuery('#block-menu-block-3 .menu li').length;
        //jQuery('#block-menu-block-3 .menu li:eq('+(len-3)+') a').append('&nbsp;<div class="breezi-font-o">o</div>');
        jQuery('#block-menu-block-3 .menu li:eq('+(len-2)+') a').append('&nbsp;<div class="breezi-font-o">o</div>');
        jQuery('#block-menu-block-3 .menu li:eq('+(len-1)+') a').append('&nbsp;<div class="breezi-font-o">o</div>');
        
        // menu icons ends
        // search starts
        jQuery('#block-search-form #edit-submit').removeAttr('class');
        jQuery('#block-search-form #edit-submit').attr('value', '');

        // search ends
	    jQuery('#megamenu-main-menu li').removeClass('active');
        jQuery(".expand").eq(0).show();
        
       
        jQuery('h2.megamenu-parent-title a').removeAttr('href');
        jQuery('h3.megamenu-slot-title a').removeAttr('href');        
        jQuery('.region-sidebar-first h2.block-title > a').removeAttr('href'); 
        
        jQuery('#megamenu-main-menu h3.megamenu-slot-title a').each(function(index) { 
            
            var megamenuRemove = jQuery(this).text();
            //console.log(megamenuRemove);
            if(megamenuRemove == 'MegamenuRemoveTitle') {
                
               //delarray.push(index);
               //jQuery('h3.megamenu-slot-title a').eq(index).text('');               
               jQuery(this).parent().next().css('margin-top', '-10px');
               jQuery(this).parent().remove();
            }   
            
        });
        // Reordering of events
        jQuery(".events-row").sort(function(a,b){      
            //alert(jQuery(a).children().children().children('.date-attr').attr('date-value'));
            return new Date(jQuery(a).children().children().children('.date-attr').attr('date-value') ) < new Date(jQuery(b).children().children().children('.date-attr').attr('date-value') );
            
        }).each(function(){
            
            jQuery(".view-event-views .view-content").prepend(this);            
        });
      
      
    });
   </script>
