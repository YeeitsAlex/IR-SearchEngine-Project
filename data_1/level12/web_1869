           <script type="text/javascript">
            var pageLogin = {
            buttons: ['ctl00_ctl00_formBody_body_btnOKOrg', 'ctl00_ctl00_formBody_body_btnContinueIncident', 'ctl00_ctl00_formBody_body_btnLogIn'],
            btnCancelIncident: document.getElementById('ctl00_ctl00_formBody_body_btnCancelIncident'),
            txtUsername: document.getElementById('ctl00_ctl00_formBody_body_txtUsername'),
            cancelfocus: false,
            submitted: false, // if focus is on the OK button and the user presses Enter, there is one submission for the automatic button click and one for our compensator

            Load: function () {
                if (this.btnCancelIncident) {
                    this.btnCancelIncident.onfocus = function () { pageLogin.cancelfocus = true; };
                    this.btnCancelIncident.onblur = function () { pageLogin.cancelfocus = false; };
                }
            },
            EnterKeySubmission: function (e) {
                if (!e) var e = window.event;
                var key = 0;
                if (Common.Key.GetCode(e) == 13 && !this.cancelfocus) { // if we're focused on a cancel button, we do not want to click OK
                    for (var i = 0; i < this.buttons.length; i++) {
                        var btn = document.getElementById(this.buttons[i]);
                        if (btn != null && this.txtUsername == null)
                            btn.click();
                    }
                }
            },
            init: function () {
                window.onload = function () { pageLogin.Load(); };
                document.forms[0].onsubmit = function () { if (pageLogin.submitted) return false; pageLogin.submitted = true; }; // make sure this submits only once
                document.body.onkeypress = function (e) { pageLogin.EnterKeySubmission(e); };
            }
        }
        pageLogin.init();
           </script>
