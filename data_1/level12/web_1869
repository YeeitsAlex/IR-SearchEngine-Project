 <script src="js/controlpanel/controlpanel.js" type="text/javascript">
 </script>
 <script>
  var copyrightSection;
    var footsection;
    var footerShowing;

    var footerSection = function () {
        return document.getElementById('ctl00_ctl00_footer');
    }

    footsection = footerSection();
    footerShowing = ControlPanel != "undefined" && ControlPanel != null && footsection != "undefined" && footsection != null;

    if (footerShowing) {
        var self = this;
        self.copyrightSectionLogoImage = document.getElementById('ctl00_ctl00_copyrightSectionLogoImage');

        // if we opened a board in a new window, then the footer's parent should be window.top
        var isStandaloneWindow = window == window.top;

        if (isStandaloneWindow && self.copyrightSectionLogoImage)
            self.copyrightSectionLogoImage.className = 'copyrightSectionLogoImageStandAlone';

        ControlPanel.registerFullScreenNotification(window,
            function(msg) {
                var isFullScreen = msg.fullscreenChange.isFullScreenMode;
                if (isFullScreen || isStandaloneWindow)
                    self.copyrightSectionLogoImage.style.display = "inline";
                else
                    self.copyrightSectionLogoImage.style.display = "none";
            }.bind(this));

        // this is here for support of other items.
        var fullScreenChanged = function(isFullScreen) {
            if (!footerShowing) return;
            copyrightSection = copyRightSection();
            footsection = footerSection();


            copyrightSection.style.display = 'block';

            if (footsection != "undefined" && footsection != null)
                footsection.style.display = 'block';

            var copyrightSectionLogoImage = copyRightSectionLogoImage();
            if (copyrightSectionLogoImage != "undefined" && copyrightSectionLogoImage != null) {
                if (isFullScreen)
                    copyrightSectionLogoImage.style.display = 'inline';
                else
                    copyrightSectionLogoImage.style.display = 'none';
            }
        }
    }

    var copyRightSection = function () {
        return document.getElementById('ctl00_ctl00_copyrightSection');
    };

    var copyRightSectionLogoImage = function() {
        return document.getElementById('ctl00_ctl00_copyrightSectionLogoImage');
    };

    copyrightSection = copyRightSection();
 </script>
