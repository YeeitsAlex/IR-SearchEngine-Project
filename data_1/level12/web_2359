  <script>
   $(document).ready(function() {
				var hash = window.location.hash;
				if (hash == "#search") {
					$("#search").focus();
				}

				// Countdown timer  #application.biggiveLiveEnd#

				//var endOfBigGive = new Date(2019, 03-1, 14, 21); // DAY-OF
				var endOfBigGive = new Date(2019, 3, 15, 20)

				//var currentTime = new Date();
				// console.log(endOfBigGive);


				var hours, minutes, seconds;
				var $hourBox = $('#timerHours');
				var $minuteBox = $('#timerMinutes');
				var $secondBox = $('#timerSeconds');

				$('#cdTimer').countdown({
					until: $.countdown.UTCDate(-8, endOfBigGive),
					onTick: watchCountdown,
					padZeroes: true,

					expiryUrl: 'https://give.berkeley.edu/'
				});

				function watchCountdown(periods) {
					// console.log('PERIODS: ' + periods);
					// console.dir( periods[3] + ' ' + periods[4] + ' ' + periods[5] + ' ' + periods[6]);

					hours = periods[4];
					minutes = periods[5];
					seconds = periods[6];

					if(hours < 10) {
						hours = '0' + hours;
					}
					if(minutes < 10) {
						minutes = '0' + minutes;
					}
					if(seconds < 10) {
						seconds = '0' + seconds;
					}

					$hourBox.text(hours);
					$minuteBox.text(minutes);
					$secondBox.text(seconds);
				} // watchCoundown()


			});	// document ready
  </script>
