  $.each($(".node-full .node-image img"), function(k,v) {
    if ($(v).attr('width') > $(v).attr('height')) { 
      $(v).parent().parent().addClass('wide');
    }
    else {
      $(v).parent().parent().addClass('tall');      
    }
  });
