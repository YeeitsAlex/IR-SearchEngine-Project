         <script type="text/javascript">
          // Search function replacement
function search() {
    var textboxdata = $('#search').val();
    if (isNaN(textboxdata) === true) {
        document.location.href = "/MerchList.aspx?txtSearch=" + textboxdata +

            "&searchtype=Description&drpsearch2=Merchandise Desc";
    } else {
        document.location.href = "/MerchList.aspx?txtSearch=" + textboxdata +

            "&searchtype=ItemNum&drpsearch2=Merchandise Item Num";
    }
}

function runScript(e) {
    if (e.keyCode === 13) {
        search();
        return false;
    }
}

$(document).ready(function () {
    $("#searchSubmit").click(function () {
        search();
        return false;
    });
});
         </script>
