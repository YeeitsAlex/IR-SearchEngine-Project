         <script>
          //Scott's slider fix
    $(document).ready(function () {
        var counter = 1;
        var children = $('#output').get(0).childElementCount;
        var timerObject = setInterval(timer, 8000);

        //iterate through slides
        function timer() {
            $('#output>li').css('display', 'none');
            $('#tabs>li>a').removeClass('current');
            $('#output>li:eq(' + counter + ')').css('display', 'list-item');
            $('#tabs>li>a:eq(' + counter + ')').addClass('current');
            counter++;
            if (counter === children) {
                counter = 0;
            }
        }

        //click event for tabs
        $('#tabs>li>a').click(function () {

            //reset interval
            clearTimeout(timerObject);
            timerObject = setInterval(timer, 8000);

            var index = 0;
            var child = $(this).parent().get(0);
            console.log("child", child);
            //get index of clicked child element by counting backwards
            while ((child.previousElementSibling) != null){
                child = child.previousElementSibling;
                index++;
            }
            console.log("index of child clicked: ", index);

            //highlight clicked tab, display clicked slide
            $('#tabs>li>a').removeClass('current');
            $(this).addClass('current');
            $('#output>li').css('display', 'none');
            $('#output>li:eq(' + index + ')').css('display', 'list-item');
            counter = index;
        });
    });
         </script>
