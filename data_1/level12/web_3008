      <script type="text/javascript">
       $(document).ready(function () {
                    $("#login-email").focus(function () {
                        if ($("#login-email").val() == "") {
                            $("#row-login-email").attr("class", "form-row prompt_email hint");
                        } else {
                            if (!isValidEmail($("#login-email").val())) {
                                $("#row-login-email").attr("class", "form-row prompt_email invalid");
                            } else {
                                $.ajax({
                                    type: "POST",
                                    url: "/AJAX/UserProfile/ValidateEmailApplicant/",
                                    data: "email=" + $("#login-email").val(),
                                    cache: false,
                                    dataType: "json",
                                    success: function (data, textStatus, jqXHR) {
                                        if (data.i == "1") {
                                            $("#row-login-email").attr("class", "form-row prompt_email");
                                        } else if (data.i == "2") {
                                            $("#row-login-email").attr("class", "form-row prompt_email required");
                                        } else if (data.i == "3") {
                                            $("#row-login-email").attr("class", "form-row prompt_email invalid");
                                        } else {
                                            $("#row-login-email").attr("class", "form-row prompt_email invalid");
                                            $("#invalid-login-email").html("Error: There is no account with this e-mail address.");
                                        }
                                    },
                                    error: function (jqXHR, textStatus, errorThrown) {
                                        var sErrorText = "";
                                        if (textStatus == "timeout") {
                                            sErrorText = "We're sorry, your request has been timeout. Please try again later.";
                                        }
                                        else if (textStatus == "abort") {
                                            sErrorText = "We're sorry, your request has been aborted. Please try again later.";
                                        }
                                        else {
                                            sErrorText = "We're sorry, an error occured when trying to process your submission";
                                        }

                                        alert(sErrorText);
                                        return false;
                                    }
                                });
                            }
                        }
                    });

                    $("#login-email").focusout(function () {
                        if ($("#login-email").val() == "") {
                            $("#row-login-email").attr("class", "form-row prompt_email");
                        } 
                    });

                    $("#login-email").change(function () {
                        if ($("#login-email").val() != "") {
                            if (!isValidEmail($("#login-email").val())) {
                                $("#row-login-email").attr("class", "form-row prompt_email invalid");
                            } else {
                                $.ajax({
                                    type: "POST",
                                    url: "/AJAX/UserProfile/ValidateEmailApplicant/",
                                    data: "email=" + $("#login-email").val(),
                                    cache: false,
                                    dataType: "json",
                                    success: function (data, textStatus, jqXHR) {
                                        if (data.i == "1") {
                                            $("#row-login-email").attr("class", "form-row prompt_email");
                                        } else if (data.i == "2") {
                                            $("#row-login-email").attr("class", "form-row prompt_email required");
                                        } else if (data.i == "3") {
                                            $("#row-login-email").attr("class", "form-row prompt_email invalid");
                                        } else {
                                            $("#row-login-email").attr("class", "form-row prompt_email invalid");
                                            $("#invalid-login-email").html("Error: There is no account with this e-mail address.");
                                        }
                                    },
                                    error: function (jqXHR, textStatus, errorThrown) {
                                        var sErrorText = "";
                                        if (textStatus == "timeout") {
                                            sErrorText = "We're sorry, your request has been timeout. Please try again later.";
                                        }
                                        else if (textStatus == "abort") {
                                            sErrorText = "We're sorry, your request has been aborted. Please try again later.";
                                        }
                                        else {
                                            sErrorText = "We're sorry, an error occured when trying to process your submission";
                                        }

                                        alert(sErrorText);
                                        return false;
                                    }
                                });
                            }
                        }
                    });

                    $("#login-password").focus(function () {
                        if ($("#login-password").val() == "") {
                            $("#row-login-password").attr("class", "form-row prompt_password hint");
                        }
                    });

                    $("#login-password").focusout(function () {
                        if (trim($("#login-password").val()) == "") {
                            if ($("#row-login-password").attr('class') != "form-row prompt_password required")
                                $("#row-login-password").attr('class', 'form-row prompt_password');
                        }
                    });

                    $("#login-password").change(function () {
                        if (trim($("#login-password").val()) == "") {
                            $("#row-login-password").attr("class", "form-row prompt_password required");
                        }
                    });

                    $("#login-password").keyup(function (e) {
                        if (e.keyCode == 13) {
                            validateFormLogin();
                        }
                    });
                    $("#btnlogin").click(
                        function () {
                            validateFormLogin();
                        }
                    );
                });

                function validateFormLogin() {
                    var bError = false;
                    if ($("#login-email").val() == "") {
                        bError = true;
                        $("#row-login-email").attr("class", "form-row required");
                    } else {
                        if (!isValidEmail($("#login-email").val())) {
                            bError = true;
                            $("#row-login-email").attr("class", "form-row invalid");
                        } else {
                            bError = false;
                            $.ajax({
                                type: "POST",
                                url: "/AJAX/UserProfile/ValidateEmailApplicant/",
                                data: "email=" + $("#login-email").val(),
                                cache: false,
                                dataType: "json",
                                success: function (data, textStatus, jqXHR) {
                                    if (data.i == "1") {
                                        bError = false;
                                        $("#row-login-email").attr("class", "form-row");
                                    } else if (data.i == "2") {
                                        bError = true;
                                        $("#row-login-email").attr("class", "form-row required");
                                    } else if (data.i == "3") {
                                        bError = true;
                                        $("#row-login-email").attr("class", "form-row invalid");
                                    } else {
                                        bError = true;
                                        $("#row-login-email").attr("class", "form-row invalid");
                                        $("#invalid-login-email").html("Error: There is no account with this e-mail address.");
                                    }
                                }
                            });
                        }
                    }

                    if (trim($("#login-password").val()) == "") {
                        $("#row-login-password").attr("class", "form-row prompt_password required");
                        bError = true;
                    }

                    if (bError) {
                        return false;
                    } else {
                        //$("#login-form").submit();
                        setKey($("#login-form"));
                    }
                }
      </script>
     </div>
