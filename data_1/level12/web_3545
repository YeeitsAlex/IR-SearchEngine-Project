        <!--//--><![CDATA[// ><!--

jQuery(document).ready(function($){

$('#termsAgree').change(function(){
  if(this.checked)
      $('#canvasLogin').fadeIn('fast');
      $('#formCanvasLogin').fadeOut('fast');
});


$('.osu-logo-animate').click(function() {
  $("#animated-osu-logo").addClass("active-anim");
});

$('#fasize-tab a').click(function (e) {
  e.preventDefault();
  $(this).tab('show');
})

$('#faflip-tab a').click(function (e) {
  e.preventDefault();
  $(this).tab('show');
})

$('.tooltip-trigger').tooltip();

$('#container-search').keypress(function(event) {
    if (event.keyCode == 13) {
        event.preventDefault();
    }
});

$('.searchable-container .card').each(function(){
$(this).attr('data-search-term', $(this).text().toLowerCase());
});

$('#container-search').on('keyup', function(){

var searchTerm = $(this).val().toLowerCase();

    $('.searchable-container .card').each(function(){

        if ($(this).filter('[data-search-term *= ' + searchTerm + ']').length > 0 || searchTerm.length < 1) {
            $(this).show();
        } else {
            $(this).hide();
        }

    });
$('#not_matches').toggle(!$(".card:visible").length)


});

$( ".chevron-toggle" ).click(function() {
  $(this).find("i.chevron-toggleable").toggleClass( "fa-chevron-down fa-chevron-up" );
});

$('.collapser').click(function() {
  $(this).toggleClass('active');
  $(this).parent().toggleClass('active')
  $(this).next().toggleClass('active');
  $(this).next().collapse('toggle');
});

$('.collapse-all').click(function() {
  $('.collapse').collapse('toggle');
  $('.sl-columns').toggleClass('active');
  $('.sl-collapse').toggleClass('active');
});

$(function () {
    var parent = $(".card-columns");
    var divs = parent.children();
    while (divs.length) {
        parent.append(divs.splice(Math.floor(Math.random() * divs.length), 1)[0]);
    }
});


$('[data-toggle="popover"]').popover({

	html: true,
	content: function() {
          return $('#popover-content').html();
        }
 });

$('body').on('click', function (e) {
    $('[data-toggle="popover"]').each(function () {
        //the 'is' for buttons that trigger popups
        //the 'has' for icons within a button that triggers a popup
        if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && $('.popover').has(e.target).length === 0) {
            $(this).popover('hide');
        }
    });
});

$( "div.flex-child > .view-content" ).addClass( "flex" );
$( "div.flex-center-child > .view-content" ).addClass( "flex-center" );
$( "div.flex-center-justify-child > .view-content" ).addClass( "flex-center-justify" );
$( "div.flex-start-justify-child > .view-content" ).addClass( "flex-start-justify" );
$( "div.flex-start-align-child > .view-content" ).addClass( "flex-start-align" );
$( "div.flex-end-justify-child > .view-content" ).addClass( "flex-end-justify" );
$( "div.flex-end-align-child > .view-content" ).addClass( "flex-end-align" );
$( "div.flex-wrap-child > .view-content" ).addClass( "flex-wrap" );

});

//--><!]]>
       </script>
