  <!-- Bootstrap -->
  <script src="js/bootstrap.min.js">
  </script>
  <script src="js/twitter-bootstrap-hover-dropdown.js" type="text/javascript">
   //
  </script>
  <script src="js/retina.js" type="text/javascript">
   //
  </script>
  <script src="js/customLib.js" type="text/javascript">
   //
  </script>
  <!-- FastClick -->
  <script src="js/fastclick.js">
  </script>
  <!-- NProgress -->
  <script src="js/nprogress.js">
  </script>
  <!-- iCheck -->
  <script src="js/icheck.min.js">
  </script>
  <!-- Pagination -->
  <!--script src="js/pagination.js"></script -->
  <script src="js/simplepagination.js">
  </script>
  <!-- Custom Theme Scripts -->
  <script src="js/custom.min.js">
  </script>
  <!-- Data Function -->
  <script>
   $(function () {
                        getParameterByName = function (name, url) {
                            if (!url)
                                url = window.location.href;
                            name = name.replace(/[\[\]]/g, "\\$&");
                            var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                                    results = regex.exec(url);
                            if (!results)
                                return null;
                            if (!results[2])
                                return '';
                            return decodeURIComponent(results[2].replace(/\+/g, " "));
                        }

                        var deptID = getParameterByName('deptID');
                        var divID = getParameterByName('divID');
                        //load departments by divisions
                        var url = 'https://staffdirectory.tools.lib.utah.edu/department.php';
                        $.get(url, function (data) {

                            data = JSON.parse(data);
                            for (var i = 0; i < Object.keys(data).length; i++) {
                                var division = {};
                                division.id = data[Object.keys(data)[i]].id;
                                division.name = Object.keys(data)[i];
                                division.departments = data[Object.keys(data)[i]].departments;
                                $('#department_list').append('<optgroup label="' + division.name + '">');
                                $('#department_list').append('<option id="' + division.id + '_all" value = "' + Object.keys(data)[i] + '_all ">All ' + Object.keys(data)[i] + '</option>');
                                for (var j = 0; j < division.departments.length; j++) {
                                    $('#department_list').append('<option id="' + division.departments[j].id + '" value = " ' + division.name + '_' + division.departments[j].name + '">' + division.departments[j].name + '</option>');
                                }
                                $('#department_list').append('</optgroup>');
                            }

                            //search on department if query string is not null
                            if (divID != null || deptID != null)
                            {
                                if (deptID != null)
                                    $('#department_list').find('#' + deptID).prop('selected', true);
                                else if (divID != null)
                                    $('#department_list').find('#' + divID + '_all').prop('selected', true);



                            }

                        });
                        //ajax function getting data
                        var url = 'https://staffdirectory.tools.lib.utah.edu/staff.php';
                        //load staff data
                        $.ajax({
                            url: url,
                            success: function (data) {
                                var defaultfilepath = 'url(' + 'https://staffdirectory.tools.lib.utah.edu/resources/library/default-profile/' + 'default-profile.png' + ')';
                                data = JSON.parse(data);
                                //need to update 
                                for (var i = 0; i < data.length; i++) {
                                    //need to update 
                                    var department = data[i].department != null ? data[i].department : "";
                                    var phone = data[i].phone != null ? data[i].phone : "not available";
                                    var email = data[i].email != null ? data[i].email : "not available";
                                    var job_title = data[i].job_title != '0' ? data[i].job_title : "";
                                    var division = data[i].division != null ? data[i].division : "";
                                    $('#profiles').append('<div class="data"><div class="vc-card boxed hoverable-links mini-card"><a id="' + data[i].uNid + '" href="#" class="set-bg uNid" style="background-image: ' + defaultfilepath + '"><img alt="people" class="set-me" style="display: none;" ></a><div class="vc-card-wrapper"><a href="#" class="title name">' + data[i].lname + ', ' + data[i].fname + '</a><div class="subtitle job_title">' + job_title + '</div><div hidden class="division">' + division + '</div><div class="department">' + department + '</div><div class="links"><a><span>' + phone + '</span></a></div><div class="links"><a>' + email + '</span></a></div></div></div></div>');
                                }

                                load(319, false);
                            },
                            complete: function () {

                                search();
                                $('.data').each(function (e) {

                                      loadimage($(this).find('.uNid').prop('id'));
                                });
                            }
                        });
                        //search on keyup 
                        $('#name_search').keyup(
                                function () {
                                    if ($('#name_search').val().trim() != "" || $('#department_list').html().trim() != "All departments")
                                        search();
                                    else {
                                        $('.filtered').each(function () {
                                            $(this).removeClass("filtered");
                                        })
                                        load(319, false);
                                    }
                                }
                        );
                        // select event on select department
                        $('#department_list').on('change', function () {
                            $('#name_search').val('');
                            search();
                        })
                        //search function
                        search = function () {

                            $('.data').hide();

                            $('.filtered').each(function () {
                                $(this).removeClass("filtered");
                            })
                            //get and show search result
                            var i = 0;
                            var keys = $('#name_search').val().trim().toLowerCase().split(" ");
                            var found = 0;
                            $('.data').each(function () {
                                found = 0;
                                var item = $(this);
                                var name = item.find('.name');
                                var job = item.find('.job_title');
                                var department = item.find('.department');
                                var division = item.find('.division');
                                for (var j = 0; j < keys.length; j++) {
                                    if ((name.html().trim().toLowerCase().search(keys[j]) == -1 && job.html().trim().toLowerCase().search(keys[j]) == -1))
                                        break;
                                    //department search
                                    else if ($('#department_list').find('option:selected').html().trim() != 'All departments') {
                                        if ($('#department_list').find('option:selected').val().trim().split('_')[1] != 'all') {
                                            if (department.html().trim() != $('#department_list').val().trim().split('_')[1] || division.html().trim() != $('#department_list').val().trim().split('_')[0])
                                                break;
                                            else
                                                found++;
                                        } else if ($('#department_list').find('option:selected').val().trim().split('_')[1] == 'all') {

                                            if (division.html().trim() != $('#department_list').val().trim().split('_')[0])
                                                break;
                                            else
                                                found++;
                                        }
                                    } else
                                        found++;
                                }

                                if (found > 0 && found == keys.length) {
                                    item.show();
                                    item.addClass("filtered");
                                    i++;
                                }

                            });
                            //alert($(this).html().trim());
                            //do paging 
                            load(i, true);
                        }

                        //clear conditions
                        clear = function () {
                            $('#name_search').val('');
                            $('#department_list').val('All departments');
                            search();
                        }
  
                        //open org chart
                        view_org_chart = function () {
                            var win = window.open('https://staffdirectory.tools.lib.utah.edu/org_index.php', '_blank');
                            if (win) {
                                //Browser has allowed it to be opened
                                win.focus();
                            }

                        }



                    });
                    loadimage = function (uNid) {
                        //need to update
                        var image_url = '/resources/library/profiles/' + uNid + '.jpg';
                        //need to update
                        $.get("https://staffdirectory.tools.lib.utah.edu/resources/library/file_exist.php", {uNid: uNid}, function (data) {

                            if (data == true)
                                $.ajax({
                                    url: image_url,
                                    cache: false,
                                    success: function (d) {
                                        $('#' + uNid + '').css('background-image', 'url("' + image_url + '")');
                                    }
                                    , error: function (xhr) {

                                    }
                                });
                        });
                    }

                    load = function (itemscount, search) {

                        var adjust = 12;
                        var pagenumbertoshow = 5;
                        //paging based on screen size
                        if ($(window).width() > 992) {
                            adjust = 15;
                            pagenumbertoshow = 5;
                        } else if ($(window).width() <= 992 && $(window).width() >= 768) {
                            adjust = 6;
                            pageunmbertoshow = 4;
                        } else if ($(window).width() < 768 && $(window).width() >= 480) {
                            adjust = 3;
                            pagenumbertoshow = 3;
                        } else if ($(window).width() < 480) {
                            adjust = 4;
                            pagenumbertoshow = 1;
                        }

                        $('#pagination').twbsPagination('destroy');
                        $('#pagination').twbsPagination({
                            visiblePages: pagenumbertoshow,
                            totalPages: Math.ceil(itemscount / adjust) != 0 ? Math.ceil(itemscount / adjust) : 1,
                            hideOnlyOnePage: true,
                            prev: "<<",
                            next: ">>",
                            onPageClick: function (event, page) {
                                //custom paging logic here
                                var start = adjust * (page - 1);
                                end = start + adjust;
                                $rows = search == false ? $('#profiles').find('.data') : $('#profiles').find('.filtered');
                                // alert($rows.length);
                                $rows.hide();
                                for (var i = start; i < end; i++) {
                                    $rows.eq(i).show();
                                }
                            }
                        });
                    }
  </script>
 </body>
</html>
