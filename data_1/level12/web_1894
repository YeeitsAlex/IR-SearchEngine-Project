   <div class="pull-right" id="search">
    <form class="form-inline">
     <input class="input-small" id="qt" maxlength="50" placeholder="Search" type="text"/>
     <select id="SearchOptions">
      <option value="us">
       Usearch/Catalog
      </option>
      <option value="cr">
       Course Reserves
      </option>
      <option value="lg">
       Subject Guides
      </option>
      <option value="dl">
       Digital Library
      </option>
      <option value="w">
       Website
      </option>
     </select>
     <button class="btn btn-u-red" onclick="Teleport();
                    return false;">
      <em class="icon-search">
       <!-- Search Icon -->
      </em>
     </button>
    </form>
   </div>
   <script type="text/javascript">
    // <![CDATA[
        //Takes you to the correct search depending on what area you select to search on the dropdown.
        function Teleport()
        {

            var searchArea = document.getElementById("SearchOptions").value;
            var searchString = document.getElementById("qt").value;
            var location = "";
            switch (searchArea)
            {
                case "us":
                    location = "http://utah-primoprod.hosted.exlibrisgroup.com/primo_library/libweb/action/search.do?fn=search&ct=search&initialSearch=true&mode=Basic&tab=everything&indx=1&dum=true&srt=rank&vid=UTAH&frbg=&tb=t&vl%28freeText0%29=" + searchString + "&scp.scps=scope%3A%28uu%29%2Cscope%3A%28UUU_ALMA%29%2Cprimo_central_multiple_fe";
                    break;
                case "cr":
                    location = "http://utah-primoprod.hosted.exlibrisgroup.com/primo_library/libweb/action/search.do?fn=search&ct=search&initialSearch=true&mode=Basic&tab=uuu_alma_cr_restricted&indx=1&dum=true&srt=rank&vid=UTAH&frbg=&tb=t&vl%28freeText0%29=" + searchString + "&scp.scps=scope%3A%28UUU_ALMA_CR_RESTRICTED%29";
                    break;
                case "lg":
                    location = "http://campusguides.lib.utah.edu/search.php?iid=793&gid=0&c=0&search=" + searchString;
                    break;
                case "dl":
                    location = "http://cdmbuntu.lib.utah.edu/cdm/search/searchterm/" + searchString + "http://lib.utah.edu/field/all/mode/all/conn/and/order/title/ad/asc";
                    break;
                case "w":
                    location = "http://gsa.search.utah.edu/search?q=" + searchString + "&site=Look-only-in-marriott-library-main-website&client=marriott-library-main_frontend&output=xml_no_dtd&proxystylesheet=marriott-library-main_frontend&numgm=10&btnG.x=-250&btnG.y=-8&btnG=Submit+Search";
                    break;
            }
            window.location.href = location;
        }
        // ]]>
   </script>
