                            <script>
                             $(function() {
// variables ftw!
var win = $(window);
// fire it once on document.ready
resizeHandler();
// Fires on every resize
win.resize(resizeHandler);
function resizeHandler() {
if (win.width() <= 990) {
$('.header-section .search-button').slice().appendTo('.main-nav .hs-menu-wrapper');
}
else{
$('.header-section .main-nav .hs-menu-wrapper .search-button').slice().appendTo('.header-section .wrapper>.row-fluid-wrapper>.row-fluid')
}
}
});
