     </div>
     <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.8/angular.min.js">
     </script>
     <script src="//code.angularjs.org/1.5.8/angular-sanitize.min.js">
     </script>
     <script>
      angular.module('NAIApp', ['ngSanitize'])
	.filter('hashify', function() {
		return function(str) {
			return str.replace(/\s+/g, ' ') // convert multi spaces to single spaces
				// remove unacceptable characters
				.replace(/[\'\".,\/#!$%\\^&\*;:{}=\-_`~()\[\]|+@?<>]/g, '')
				.replace(/\s{2,}/g, ' ') // remove extra spaces from punctuation
				.toLowerCase() // convert to lower case
				.trim() // trim any surrounding spaces
				.split(' ').join('-'); // replace spaces with underscore
		};
	})
	.component('naiNews', {
		bindings: {
			posts: '=',
		},
		template: [
			'<main>',
				'<section>',
					'<h3>News</h3>',
					'<a ng-repeat="post in $ctrl.data | orderBy:\'date\':reverse=true | limitTo:4"',
						'style="animation: reveal .3s .3s ease both;"',
						'href="/news.html#{{post.title | hashify}}">',
						'<img ng-if="post.imageSmall" class="img_L news-img" ng-src="{{post.imageSmall}}" width="60" alt="{{post.title}}" />',
						'<img ng-if="!post.imageSmall" class="img_L news-img" src="news/ucr_logo.jpg" width="60" alt="ucr logo" />',
						'<header id="{{post.title | hashify}}">',
							'<time>{{post.date | date: \'MM/dd/y\'}}</time>',
						'</header>',
						'<h4 class="news-h">{{post.title}}</h4>',
					'</a>',
				'</section>',
			'</main>'
		].join(''),
		controller: function($http) {
			var posts = this;
			$http.get('/script/news.json')
				.success(function(data) {
					posts.data = data;
				});
		}
	});
     </script>
    </div>
    <!-- End Content Area -->
    <div class="clear">
    </div>
   </div>
   <!-- End Content -->
  </div>
