   <script type="text/javascript">
    $(document).ready(function() {
	
		var $dialog = $("#fineprint").dialog({
				bgiframe: true,
       		    resizable: false,
				width:350,
                   modal: true,
                   autoOpen: false,
                   overlay: {
                        backgroundColor: '#000000',
                        opacity: 1.0
				    },	
			        show : {
            			effect : 'clip',
			            duration : 500
			        },
					close: function() {
						$(this).dialog('close');
					}
				});

		$('#showfineprint').click(function(e) {
			e.preventDefault();
			$dialog.dialog("widget").find(".ui-dialog-title").hide();
			$dialog.dialog("widget").find(".ui-dialog-titlebar").css({
				"float": "right",
		        border: 0,
	    	    padding: 0
	    	})
			$dialog.dialog("widget").find(".ui-dialog-titlebar-close").css({
        		    top: 0,
		            right: 0,
		            margin: 0,
        		    "z-index": 1001
		        });
			$dialog.dialog('open');
		});
	});
   </script>
