            <article about="/home" class="node node-stanford-page clearfix" id="node-71" typeof="sioc:Item foaf:Document">
             <span class="rdf-meta element-hidden" content="Home" property="dc:title">
             </span>
             <div class="content">
             </div>
            </article>
           </div>
          </div>
         </div>
         <!-- /.region -->
        </div>
        <div class="row-fluid content-row3" id="content-row3">
         <div class="region region-content-row3 clearfix">
          <div class="span4 clearfix block block-bean well column span4" id="block-bean-learn-about-our-programs-home-pa">
           <div class="content">
            <div about="/block/learn-about-our-programs-home-pa" class="entity entity-bean bean-stanford-large-block clearfix" typeof="">
             <div class="content">
              <div class="field field-name-field-s-large-block-body field-type-text-long field-label-hidden">
               <div class="field-items">
                <div class="field-item even">
                 <p>
                 </p>
