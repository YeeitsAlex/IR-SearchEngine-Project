   window.isMobile = 0 == 1;
        if (!theForm) {
                var theForm = document.forms['aspnetForm'];
                if (!theForm) 
                    theForm = document.aspnetForm;
            }
            if (theForm) {
                theForm.action = window.location.href;
            }
  </script>
