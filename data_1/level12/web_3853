      <div class="collapse collapse-group-main" id="phContentBox_pnlVideoMain">
       <div class="panel-group">
        <style>
         iframe{ width:100%; height:450px; }
        </style>
        <script>
         //load youtube IFrame API asynchronously
                    var tag = document.createElement('script');
                    tag.src = "https://www.youtube.com/iframe_api";
                    document.getElementById("phContentBox_pnlVideoMain").firstElementChild.insertBefore(tag, document.getElementById("phContentBox_pnlVideoMain").firstElementChild.firstElementChild);

                    //load iframe videos for each video placeholder, and stores the player objects so they remain in scope
                    var playerArray = [];
                    function onYouTubeIframeAPIReady() {
                        $('.collapse-group-video').each(function () {
                            var placeholder = $(this).find('.videoplaceholder')[0];
                            var player = new YT.Player(placeholder.id, {
                                videoId: placeholder.innerHTML,
                                playerVars: {rel: 0},
                                events: {'onReady' : onPlayerReady}
                            });
                            playerArray.push(player);
                        });
                    }

                    //set events on player ready; setting beforehand breaks the player objects
                    function onPlayerReady(event) {
                        var player = event.target;
                        var index = playerArray.indexOf(player);
                        $('.collapse-group-video').eq(index).on('hidden.bs.collapse', function () {
                            player.pauseVideo();
                        });
                        $('.collapse-group-video').eq(index).on('shown.bs.collapse', function () {
                            player.playVideo();
                        });
                    }
        </script>
        <div class="panel panel-default">
         <div class="panel-heading">
          <a data-toggle="collapse" href="#pnlVideoCollapse_3" onclick="HandleCollapseGroupSelect('collapse-group-video', 'pnlVideoCollapse_3')">
           Phone Call
          </a>
         </div>
