     <div class="marquee">
      <div class="marquee_data">
       <div class="marquee_panel" data-image-full="https://daqy2hvnfszx3.cloudfront.net/wp-content/uploads/sites/11/2017/11/10152233/DeathtoStock_QuietFrontier-03.jpg" data-image-large="https://daqy2hvnfszx3.cloudfront.net/wp-content/uploads/sites/11/2017/11/10152233/DeathtoStock_QuietFrontier-03-1024x689.jpg" data-nav-conditional="" data-vertical-orientation="">
        <div class="panel_caption">
         <h1>
          SWC Research Group
         </h1>
        </div>
        <div class="panel_description">
        </div>
        <div class="panel_description_small">
        </div>
       </div>
      </div>
     </div>
     <div class="marquee_mobile_text_container">
     </div>
    </div>
    <!-- End sticky-inner div -->
    <!-- END: Header Image conditional -->
