   <script type="text/javascript">
    var tag = document.createElement('script');
var ratio = .41667; /*.5625 for 16:9, .41667 for ultrawide*/
var width;
tag.src = "https://www.youtube.com/iframe_api";
var firstScriptTag = document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
var player;
function onYouTubeIframeAPIReady() {
	
	player = new YT.Player('player', {
			height:'300',
			width:'100%',
			videoId : 'QVwFj5EEykU',
			playerVars : {
				'autoplay' : 0,
				'controls' : 1
			},
          playerVars:{'rel' : 0},
			events : {
				'onReady' : onPlayerReady,
				'onStateChange' : onPlayerStateChange
			}
		});
}
function onPlayerReady(event) {
		var mq = window.matchMedia("(min-width: 769px)");
		if (mq.matches){
			}
			else{
				width = document.getElementById("player").offsetWidth;
				document.getElementById("player").style.height=width*ratio+"px";
			}
	
}

var firstPlay = 0;
function onPlayerStateChange(event) {
	var iframe = document.getElementById('player');
	if (event.data == YT.PlayerState.PLAYING) {
		if (firstPlay == 0) {
			document.getElementById("player").style.width="100%";
			width = document.getElementById("player").offsetWidth;
			document.getElementById("player").style.transition = "all .3s";
			document.getElementById("player").style.height = width*ratio+"px";
                  	ga('send', {
				hitType: 'event',
				eventCategory: 'Videos',
				eventAction: 'play',
				eventLabel: 'HomePageVideo'
			})
			firstPlay++;
		}
	}
}
function LoadVideo(){
  //debugger;
  var video = document.getElementById('videoBanner');
var source = document.createElement('source');

source.setAttribute('src', 'http://oregontechstatic.azureedge.net/video/campus-life-banner.mp4 ');
source.setAttribute('type', 'video/mp4');
video.appendChild(source);
  video.load();
video.play();
}

$(document).ready(function () {

  $("#videoBanner").click(function(){
  	$("#videoBanner").hide();
    	$("#player").show();
    var iOS = /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream;
    if(!iOS){
    	player.playVideo();
    }
  });
  if(window.matchMedia("(min-width: 769px)").matches){
    LoadVideo();
	//document.getElementById('videoBanner').play();
  }
});
	$(window).resize(function () {
          if (firstPlay >0 && window.matchMedia("(max-width: 768px)").matches){
            //var p = document.getElementById("player");
			//player.playVideo();
          }
		  else{
			if($('#videoBanner').has('source').length == 0){
                          LoadVideo();
				//document.getElementById('videoBanner').play();	
			}
          }
			width = document.getElementById("player").offsetWidth;
			var newHeight = document.getElementById("player").offsetWidth*ratio;
			document.getElementById("player").style.height=newHeight+"px";
	});

$(document).ready(function () {
	
  	/*$(".carousel-control").attr("href", "/#BodyNavigation_C153")*/
});
   </script>
   <script type="text/javascript">
    window.fbAsyncInit = function() {
    FB.init({
      appId      : "1378714562453516",
      xfbml      : true,
      version    : "v2.5"
    });
  };

  (function(d, s, id){
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) {return;}
