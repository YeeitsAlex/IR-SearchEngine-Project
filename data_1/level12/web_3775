<script type="text/javascript">
 // Bootstrap 3 Affix Implementation + Destroying it on mobile - bwahahahahah
	//------------------------------------------------
	//$('.sticky-row').affix({offset: {top: 107} });
	
	$(function () {

    $(window).bind("resize", function () {
        console.log($(this).width())
        if ($(this).width() > 992) {
			// Add affix class if window is greater than 992px
             $('.sticky-row').affix({offset: {top: 107} });
        } else {
			// Destroy affix if window is less than 992px
			$(window).off('.affix')
			$('.sticky-row').removeData('bs.affix').removeClass('affix affix-top affix-bottom')
		}
    }).trigger('resize');
	})
	
	//------------------------------------------------		

	// Add active class to panel-heading when expanded
	$( document ).ready( function () {
		$( '.panel-collapse' ).on( 'show.bs.collapse', function () {
			$( this ).siblings( '.panel-heading' ).addClass( 'active' );
		} );

		$( '.panel-collapse' ).on( 'hide.bs.collapse', function () {
			$( this ).siblings( '.panel-heading' ).removeClass( 'active' );
		} );
	} );

	// Accordion toggle
	function toggleIcon( e ) {
		$( e.target )
			.prev( '.panel-heading' )
			.find( ".more-less" )
			.toggleClass( 'glyphicon-chevron-right glyphicon-chevron-down' );
	}
	$( '.panel-group' ).on( 'hidden.bs.collapse', toggleIcon );
	$( '.panel-group' ).on( 'shown.bs.collapse', toggleIcon );

	// Read More, Read Less toggle for faculty page
	$( '.readbtn' ).click( function () {
		var $this = $( this );
		$this.toggleClass( 'readbtn' );
		if ( $this.hasClass( 'readbtn' ) ) {
			$this.text( 'Read More' );
		} else {
			$this.text( 'Read Less' );
		}
	} );

	// Begin VWO Clicked to Chat Conversion
	$( '.live_chat' ).click( function () {
		// Code for Custom Goal: Clicked Chat Button
		window._vis_opt_queue = window._vis_opt_queue || [];
		window._vis_opt_queue.push( function () {
			_vis_opt_goal_conversion( 000 );
			// uncomment the following line to introduce a half second pause to ensure goal always registers with the server
			_vis_opt_pause( 500 );
		} );
	} );

	// Begin VWO Chat Conversation Conversion
	olark( 'api.chat.onBeginConversation', function () {
		// Code for Custom Goal: Begin Chat
		window._vis_opt_queue = window._vis_opt_queue || [];
		window._vis_opt_queue.push( function () {
			_vis_opt_goal_conversion( 000 );
			// uncomment the following line to introduce a half second pause to ensure goal always registers with the server
			_vis_opt_pause( 500 );
		} );
	} );
</script>
