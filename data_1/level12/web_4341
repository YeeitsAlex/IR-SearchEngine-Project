              jQuery(function ($) {
    $('.calendar-content div[class$="-time"] span').each(function() { 
        var content = $(this).html();
        $(this).parent().append(content);
        $(this).remove();
    });
    $('.calendar-content li[id^="event-"]').each(function() { 
        var time = $('[class$="-time"]', this);
