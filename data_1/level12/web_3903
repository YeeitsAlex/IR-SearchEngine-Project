        </li>
        <!-- SLIDE  -->
        <li data-saveperformance="off" data-slotamount="7" data-transition="notransition">
         <!-- MAIN IMAGE -->
         <img alt="" data-bgfit="cover" data-bgposition="center top" data-bgrepeat="no-repeat" src="http://design.cap.utah.edu/wp-content/uploads/sites/5/2012/11/IMG_3365-X3_bw-e1415247471838.jpg"/>
