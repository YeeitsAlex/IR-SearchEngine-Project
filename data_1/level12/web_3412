       <div class="Normal" id="dnn_ctr51365_HtmlModule_lblContent">
        <h2>
         COLLEGIATE RECOVERY PROGRAM
        </h2>
       </div>
      </div>
      <!-- End_Module_51365 -->
     </div>
     <div class="clear-fix">
