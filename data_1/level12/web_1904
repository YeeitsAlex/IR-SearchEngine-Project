        // <![CDATA[
function OnSubmitSearchForm() {
			var textEl = document.fauxsearch.QuickLink;
			var radioElementList = document.fauxsearch.Type;
			if (textEl.value == '') {
				alert('It appears that you clicked on the "Search" button but forgot to enter any search text. Please enter a word or phrase and try again.')
			} else if (radioElementList[0].checked) {
				document.googlesearch.q.value = textEl.value;
				document.googlesearch.submit();
			} else if (radioElementList[1].checked) {
				document.ucisearch.QuickLink.value = textEl.value;
				document.ucisearch.Type.value = "Full Search";
				document.ucisearch.submit();
			} else if (radioElementList[2].checked) {
				document.ucisearch.QuickLink.value = textEl.value;
				document.ucisearch.Type.value = "Directory";
				document.ucisearch.submit();
			}
			return false;
		}
// ]]>
       </script>
