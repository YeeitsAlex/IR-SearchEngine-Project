       <article class="post-7 page type-page status-publish hentry" id="post-7">
        <div class="entry-content swappable">
         <div class="col-12 col-lg-8" id="home-start">
          <header class="page-header">
           <h2 class="page-title">
            <div class="wpcf-field-textfield wpcf-field-primary-title" id="wpcf-field-primary-title">
             <span class="wpcf-field-value wpcf-field-textfield-value wpcf-field-primary-title-value">
              Mission
             </span>
            </div>
