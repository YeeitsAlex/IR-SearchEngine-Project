   <script src="Scripts/jquery-1.10.2.min.js">
   </script>
   <script src="Scripts/bootstrap.min.js">
   </script>
   <script src="Scripts/jquery-ui.min.js">
   </script>
   <script>
    /* jQuery code for homepage interactivity */
            $(document).ready(function () {

                // set default state of elements
                $('#AboutUsContent').hide(); // hide ABOUT US content
                $(".FPTextPara").hide(); // hide other slider content

                $("#LearnContent").show(); // show LEARN by default

                // hide/show About Us content
                $("#about-us").click(function () {
                    $("#AboutUsContent").toggle("slow");
                });

                $("#learn").click(function () {
                    $("#FinanceContent").hide();
                    $("#ApplyContent").hide();
                    $("#ContactContent").hide();

                    $("#finance-img").attr('src', '/Content/Images/Finance_over.png');
                    $("#apply-img").attr('src', '/Content/Images/Apply_over.png');
                    $("#contact-img").attr('src', '/Content/Images/Contact_over.png');

                    $("#learn-img").attr('src', '/Content/Images/Learn.png');

                    if ($("#LearnContent").is(':visible')) {
                        $("#learn-img").attr('src', '/Content/Images/Learn_over.png');
                    }
                    else {
                        $("#learn-img").attr('src', '/Content/Images/Learn.png');
                    } 

                    $("#LearnContent").toggle("slow");
                });

                $("#nav-learn").click(function () {
                    $("#FinanceContent").hide();
                    $("#ApplyContent").hide();
                    $("#ContactContent").hide();

                    $("#finance-img").attr('src', '/Content/Images/Finance_over.png');
                    $("#apply-img").attr('src', '/Content/Images/Apply_over.png');
                    $("#contact-img").attr('src', '/Content/Images/Contact_over.png');

                    if (!$("#LearnContent").is(':visible')) {
                        $("#learn-img").attr('src', '/Content/Images/Learn.png');
                        $("#LearnContent").toggle("slow");
                    }                  
                });

                $("#finance").click(function () {
                    $("#LearnContent").hide();
                    $("#ApplyContent").hide();
                    $("#ContactContent").hide();

                    $("#learn-img").attr('src', '/Content/Images/Learn_over.png');
                    $("#apply-img").attr('src', '/Content/Images/Apply_over.png');
                    $("#contact-img").attr('src', '/Content/Images/Contact_over.png');

                    if ($("#FinanceContent").is(':visible')) {
                        $("#finance-img").attr('src', '/Content/Images/Finance_over.png');
                    }
                    else {
                        $("#finance-img").attr('src', '/Content/Images/Finance.png');
                    }

                    $("#FinanceContent").toggle("slow");
                });

                $("#nav-finance").click(function () {
                    $("#LearnContent").hide();
                    $("#ApplyContent").hide();
                    $("#ContactContent").hide();

                    $("#learn-img").attr('src', '/Content/Images/Learn_over.png');
                    $("#apply-img").attr('src', '/Content/Images/Apply_over.png');
                    $("#contact-img").attr('src', '/Content/Images/Contact_over.png');

                    if (!$("#FinanceContent").is(':visible')) {
                        $("#finance-img").attr('src', '/Content/Images/Finance.png');
                        $("#FinanceContent").toggle("slow");
                    }
                });

                $("#apply").click(function () {
                    $("#LearnContent").hide();
                    $("#FinanceContent").hide();
                    $("#ContactContent").hide();

                    $("#finance-img").attr('src', '/Content/Images/Finance_over.png');
                    $("#learn-img").attr('src', '/Content/Images/Learn_over.png');
                    $("#contact-img").attr('src', '/Content/Images/Contact_over.png');

                    if ($("#ApplyContent").is(':visible')) {
                        $("#apply-img").attr('src', '/Content/Images/Apply_over.png');
                    }
                    else {
                        $("#apply-img").attr('src', '/Content/Images/Apply.png');
                    }

                    $("#ApplyContent").toggle("slow");
                });

                $("#nav-apply").click(function () {
                    $("#LearnContent").hide();
                    $("#FinanceContent").hide();
                    $("#ContactContent").hide();

                    $("#learn-img").attr('src', '/Content/Images/Learn_over.png');
                    $("#finance-img").attr('src', '/Content/Images/Finance_over.png');
                    $("#contact-img").attr('src', '/Content/Images/Contact_over.png');

                    if (!$("#ApplyContent").is(':visible')) {
                        $("#apply-img").attr('src', '/Content/Images/Apply.png');
                        $("#ApplyContent").toggle("slow");
                    }
                });

                $("#contact").click(function () {
                    $("#LearnContent").hide();
                    $("#FinanceContent").hide();
                    $("#ApplyContent").hide();

                    $("#finance-img").attr('src', '/Content/Images/Finance_over.png');
                    $("#apply-img").attr('src', '/Content/Images/Apply_over.png');
                    $("#learn-img").attr('src', '/Content/Images/Learn_over.png');

                    if ($("#ContactContent").is(':visible')) {
                        $("#contact-img").attr('src', '/Content/Images/Contact_over.png');
                    }
                    else {
                        $("#contact-img").attr('src', '/Content/Images/Contact.png');
                    }

                    $("#ContactContent").toggle("slow");
                });

                $("#nav-contact").click(function () {
                    $("#LearnContent").hide();
                    $("#FinanceContent").hide();
                    $("#ApplyContent").hide();

                    $("#learn-img").attr('src', '/Content/Images/Learn_over.png');
                    $("#finance-img").attr('src', '/Content/Images/Finance_over.png');
                    $("#apply-img").attr('src', '/Content/Images/Apply_over.png');

                    if (!$("#ContactContent").is(':visible')) {
                        $("#contact-img").attr('src', '/Content/Images/Contact.png');
                        $("#ContactContent").toggle("slow");
                    }
                });

                // content close icons
                $(".learn-close-icon").click(function () {
                    $("#learn-img").attr('src', '/Content/Images/Learn_over.png');
                    $("#LearnContent").toggle("slow");
                });

                $(".about-us-close-icon").click(function () {
                    $("#AboutUsContent").toggle("slow");
                });

                $(".finance-close-icon").click(function () {
                    $("#finance-img").attr('src', '/Content/Images/Finance_over.png');
                    $("#FinanceContent").toggle("slow");
                });

                $(".apply-close-icon").click(function () {
                    $("#apply-img").attr('src', '/Content/Images/Appy_over.png');
                    $("#ApplyContent").toggle("slow");
                });

                $(".contact-close-icon").click(function () {
                    $("#contact-img").attr('src', '/Content/Images/Contact_over.png');
                    $("#ContactContent").toggle("slow");
                });

                // get querystring params for tab triggering from other pages
                var vars = [], hash;
                var q = document.URL.split('?')[1];
                if (q != undefined)
                {
                    q = q.split('&');
                    for (var i = 0; i < q.length; i++)
                    {
                        hash = q[i].split('=');
                        vars.push(hash[1]);
                        vars[hash[0]] = hash[1];
                    }

                    if (vars['t'] != undefined && vars['t'] != null)
                    {
                        var t = vars['t'].split('#');
                        if (t != undefined) 
                        {
                            if (t[0] == 'learn') 
                            {
                                $("#nav-learn").trigger("click");
                            }
                            else if (t[0] == 'finance') {
                                $("#nav-finance").trigger("click");
                            }
                            else if (t[0] == 'contact') {
                                $("#nav-contact").trigger("click");
                            }
                            else if (t[0] == 'apply') {
                                $("#nav-apply").trigger("click");
                            }
                        }
                    }
                }
            });
   </script>
