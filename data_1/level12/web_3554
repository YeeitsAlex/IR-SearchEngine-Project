   <!-- SUBSTITUTE FOR SHAREPOINT SEARCH FORM -->
   <script>
    function GoSearch() {
                var term = document.getElementById('studyabroadsearchbox').value;
                window.location = 'http://eap.ucop.edu/Search/Results.aspx?src=hm-foot-login&k=' + term;
            }

          


            $( "#studyabroadsearchbox" ).keypress(function( searchsubmit ) {
                if ( searchsubmit.which == 13 ) 
                    GoSearch()


            }
				);
   </script>
