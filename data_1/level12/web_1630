<script>
 $(document).ready(function(){
	$('#carousel')
		// when carousel initializes, bold the indicator of the first slide.
		.on('init', function(event, slick){
			var first_slide = 0;
			$('#carousel-indicators ul').children().eq(first_slide).addClass("bold");
		})
		// when the carousel changes slide, bold the indicator of the new slide.
		.on('afterChange', function(event, slick, currentSlide, nextSlide){
			$('#carousel-indicators ul').children().removeClass("bold")
			// gets the number of the current slide.
			var currentSlide = $('#carousel').slick('slickCurrentSlide');
			$('#carousel-indicators ul').children().eq(currentSlide).addClass("bold");
		});
	
	$('#carousel').slick({
		infinite:true,
		accessibility:true,
		autoplay:true,
		autoplaySpeed:8000,
		adaptiveHeight: true,
		pauseOnHover: false,
		prevArrow: $('#spin-left'),
		nextArrow: $('#spin-right'),
		responsive: [{
			breakpoint: 768,
			settings: {
				prevArrow:null,
				nextArrow:null
			  }
		}]
	}); //end .slick function
	
	// Play the carousel if the pause-carousel class is not active.
	function play_carousel(){
		if ($('#pause-carousel').attr('class') != 'carousel-control-active'){
			$('#carousel').slick('slickPlay');
		}
	} // end play_carousel function
	
	// Pauses the carousel and makes the #pause-carousel button active.
	$('#carousel-pause').on('click', function() {
		$('#carousel')
			.slick('slickPause')
			.slick('slickSetOption', 'autoplay', false);
			
		$('#play-carousel').removeClass('carousel-control-active');
		$('#pause-carousel').addClass('carousel-control-active');
	}); // end carousel pause function.
	
	// Pauses the carousel and makes the #pause-carousel button active.
	$('#carousel-play').on('click', function() {
		$('#carousel')
			.slick('slickPlay')
			.slick('slickSetOption', 'autoplay', true);
			
		$('#play-carousel').addClass('carousel-control-active');
		$('#pause-carousel').removeClass('carousel-control-active');
	}); // end carousel play function.
	  
	$('#carousel-indicators ul li').click(function() {
		$('#carousel').slick('slickPause');
		$('#play-carousel').removeClass('carousel-control-active');
		$('#pause-carousel').addClass('carousel-control-active');
		indicator_id = $(this).attr('id');
		indicator_position = parseInt(indicator_id.substring(16));
		//clearInterval(carouselspin);
		//carouselspin = setInterval(function() { spinCarousel('right', indicator_position); }, 5000);
		$('#carousel').slick('slickGoTo', indicator_position - 1, false);
			
			
	}); // end $('#carousel-indicators ul li').click(function()
	
	///// START CAROUSEL FOCUSIN AND FOCUSOUT FUNCTIONS /////
	$('.carousel-image').focusin(function() {
		$('#carousel').slick('slickPause');
	}); // end $('.carousel-image').focusout(function() 
	
	$('.carousel-image').focusout(function() {
		play_carousel();
	}); // end $('.carousel-image').focusout(function() 
	
	$('.carousel-text').focusin(function() {
		$('#carousel').slick('slickPause');
	}); // end $('.carousel-image').focusout(function() 
	
	$('.carousel-text').focusout(function() {
		play_carousel();
	}); // end $('.carousel-image').focusout(function() 
	
	$('#spin-left').focusin(function() {
		$('#carousel').slick('slickPause');
	}); // end $('#spin-left').focusin(function()
	
	$('#spin-left').focusout(function() {
		play_carousel();
	}); // end $('#spin-left').focusout(function()
	
	$('#spin-right').focusin(function() {
		$('#carousel').slick('slickPause');
	}); // end $('#spin-right').focusin(function()
	
	$('#spin-right').focusout(function() {
		play_carousel();
	}); // end $('#spin-right').focusout(function()

	$('#carousel-play').focusin(function() {
		$('#carousel').slick('slickPause');
		$('#carousel-play').css("border", "2px solid #5e9ed6");
	}); // end $('#carousel-play').focusin(function() 

	$('#carousel-play').focusout(function() {
		play_carousel();
		$('#carousel-play').css("border", "none");
	}); // end $('#carousel-play').focusout(function()

	$('#carousel-pause').focusin(function() {
		$('#carousel').slick('slickPause');
		$('#carousel-pause').css("border", "2px solid #5e9ed6");
	}); // end $('#carousel-pause').focusin(function() 

	$('#carousel-pause').focusout(function() {
		play_carousel();
		$('#carousel-pause').css("border", "none");
	}); // end $('#carousel-pause').focusout(function() 

	$('.carousel-indicator-list li button').focusin(function() {
		$('#carousel').slick('slickPause');
		$(this).css("border", "2px solid #5e9ed6");
	}); // end $('.carousel-indicator-list li button').focusin(function() 

	$('.carousel-indicator-list li button').focusout(function() {
		play_carousel();
		$(this).css("border", "none");
	}); // end $('.carousel-indicator-list li button').focusout(function()
	
	$('#event-calendar').focusin(function() {
		$('#carousel').slick('slickPause');
	}); // end $('#event-calender').focusin(function()
	
	$('#event-calendar').focusout(function() {
		play_carousel();
	}); // end $('#event-calender').focusout(function()
	
	$('#more-news').focusin(function() {
		$('#carousel').slick('slickPause');
	}); // end $('#more-news').focusin(function()
	
	$('#more-news').focusout(function() {
		play_carousel();
	}); // end $('#more-news').focusin(function()
	
	///// END CAROUSEL FOCUSIN AND FOCUSOUT FUNCTIONS /////

}); // end document ready function.
</script>
