    <!-- BEGIN SEARCH FORM -->
    <div class="row collapse search-small" id="search-container">
     <script type="text/javascript">
      function redirect() {
    var searchitem = document.getElementById("q");
    var searchurl = "https://uci.primo.exlibrisgroup.com/discovery/search?query=any,contains," + searchitem.value + "&tab=Everything&search_scope=MyInst_and_CI&vid=01CDL_IRV_INST:UCI&offset=0";
    window.open(searchurl,"");
    }
     </script>
     <div class="col-sm-6" id="search-column">
      <!--<form action="//www.lib.uci.edu/site-search" method="POST" id="search-form" name="search-form" role="search">-->
      <form action="javascript:redirect()" id="search-form" method="GET" name="search-form" role="search">
       <input id="tab" name="tab" type="hidden" value="primo"/>
       <div id="search-input-and-button">
        <label class="sr-only" for="q">
         Search
        </label>
        <input autocomplete="off" id="q" name="q" type="text"/>
        <button id="search-button" type="submit">
         <i aria-hidden="" class="fa fa-search" id="search-icon">
         </i>
         <span class="sr-only">
          Submit search
         </span>
        </button>
