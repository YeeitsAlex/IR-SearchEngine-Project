           <div about="/home" class="node node-page clearfix" id="node-2181" typeof="foaf:Document">
            <span class="rdf-meta element-hidden" content="Office of Student Life" property="dc:title">
            </span>
            <div class="content">
             <div class="paragraphs-items paragraphs-items-field-paragraph paragraphs-items-field-paragraph-full paragraphs-items-full">
              <div class="field field-name-field-paragraph field-type-paragraphs field-label-hidden">
               <div class="field-items">
                <div class="field-item even">
                 <div about="" class="entity entity-paragraphs-item paragraphs-item-paragraph-1-col p-right paragraphs-item-406" typeof="">
                  <div class="content">
                   <div class="field field-name-field-p-1-col-main field-type-text-long field-label-hidden">
                    <div class="field-items">
                     <div class="field-item even">
                      <h3>
                       Office of Student Life
                      </h3>
                      <p>
                       The Office of Student Life is dedicated to providing an educational and developmental opportunity for students to make informed decisions that support their success at Oregon State University.
                      </p>
                     </div>
                    </div>
                   </div>
                  </div>
                 </div>
                </div>
