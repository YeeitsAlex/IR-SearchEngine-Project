   <script type="text/javascript">
    $('header nav').keyup(function(e) { 
    if (e.which === 27) { 
	$('header nav').removeClass('active');
	$('body').removeClass('openNav');
	/* return focus to menu button */
	$('#navToggle').focus();
 }  // esc   

    /* make other elements ignore event
    e.preventDefault();
    e.stopPropagation(); */
    
});


/*	Mobile Nav
        ----------------------------------------------*/
        $(function () {
            $('#navToggle, #overlay').on('click', function () {
                $('header nav').toggleClass('active');
                $('body').toggleClass('openNav');
               
            });

            $('header nav a').click(function (event) {
                if ($('.mobileNavHeader').is(':visible')) {
                    if ($(this).siblings().size() && !$(this).hasClass('mobileHome') && !$(this).hasClass('mobileBack')) {
                        event.preventDefault();
                        
                        $('.mobileHome').removeClass('active');
                        $('.mobileBack').addClass('active');
                        if (!$(this).next().children('li:first').hasClass('navLabel')) {
                            $(this).next().prepend('<li class="navLabel">' + $(this).html() + '</li>');
                        }
   

                        $('header nav ul ul').hide();
                        $(this).parents('ul').show();
                        $(this).siblings('ul').show();
                        $('#menu-top-navigation').animate({
                            left: '-=100%'
                        }, 250);
                    } else if ($(this).hasClass('mobileBack')) {
                        $('#menu-top-navigation').animate({
                            left: '+=100%'
                        }, 250, function () {
                            if ($(this).css('left') == '0px') {
                                $('.mobileHome').addClass('active');
                                $('.mobileBack').removeClass('active');
                            }
                        });
                    }
                }
            });

            function resetNav() {
                $('.menu-top-navigation-container *').removeAttr('style');
                $('.mobileHome').addClass('active');
                $('.mobileBack').removeClass('active');
            }

            $(window).resize(function () {
                if ($(this).width() > 767) {
                    resetNav();
                }
            });
        });

        
        /*	Add arrows for sub items 
        ----------------------------------------------*/
        
       $('nav li:has(> ul)').addClass('subarrow');
       $('nav li:not(:has(ul))').addClass('nosubarrow');

$('ul#menu-top-navigation>li:last-child a').on('focusout', function () {
                $('header nav').toggleClass('active', false);
                $('body').toggleClass('openNav', false);
});



$('nav li:has(> ul)').attr("aria-haspopup","true");

  
/* fix for hover double tap bug on ios */
$('a').on('click touchend', function() {
    var linkToAffect = $(this);
    var linkToAffectHref = linkToAffect.attr('href');
    
});

$(".sfBreadcrumbWrp").attr("aria-hidden","true");
