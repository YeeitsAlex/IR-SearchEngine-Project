           <svg aria-hidden="true" class="icon google" focusable="false" role="presentation" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
            <use xlink:href="#icon-youtube">
            </use>
           </svg>
          </a>
         </li>
        </ul>
       </div>
      </div>
      <script type="text/javascript">
       $(document).ready(function(){
    $("#searchTextHeader").parent('div').find('button').click(function (e) {
      e.preventDefault();
      var that = $(this);
      var browseValue = '';
      browseValue = that.parent('div').find('input[type=text]').val();
      window.location.href = checkPage(browseValue);
    });
    $('#searchText').keypress(function (e) {
      var key = e.which;
      var that = $(this);
      if (key == 13) {
        e.preventDefault();
        $('#searchButton').click();
      }
    });
    
    $('#searchButton').on('click', function (e) {
      e.preventDefault();
      var url = window.location.pathname.split("/");
      var param = '';
      var that = $(this);
      var browseValue = '';
      browseValue = that.parent('div').find('input[type=text]').val();
      if (browseValue != '') {
        if (typeof url != 'undefined' && url.pop() == 'instructors') {
          param = '&instructors=1';
        }
        window.location.href = checkPage(browseValue);
      }
    });
  });
  
  function checkPage(browseValue)
  {
    var url = '/site-search?searchtext=' + encodeURIComponent(browseValue);
    switch ('home')
    {
      case "Courses & Programs":
        url += '&course=1&certificate=1&category=1';
        break;
      case "Extension Blog":
        url += '&news=1';
        break;
      case "Student Resources":
        url += '&student=1';
        break;
      case "Instructors":
        url += '&instructor=1';
        break;
      default:
        url += '&all=1';        
    }
    return url;
  }
      </script>
     </div>
    </div>
