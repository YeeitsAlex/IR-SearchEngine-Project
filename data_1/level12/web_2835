        <script src="https://food.uci.edu/wp-content/plugins/slider-images/Scripts/jquery.flexslider-min.js">
        </script>
        <script>
         var SLWIDTHR = jQuery('.SLWIDTHR_6').val();
					var SLHEIGHTR = jQuery('.SLHEIGHTR_6').val();
					jQuery('#RW_Load_Content_Navigation6').css('max-height',jQuery('#RW_Load_Content_Navigation6').width()*SLHEIGHTR/SLWIDTHR);
					jQuery(window).resize(function(){
						jQuery('#RW_Load_Content_Navigation6').css('max-height',jQuery('#RW_Load_Content_Navigation6').width()*SLHEIGHTR/SLWIDTHR);
					})
					var array_nav6=[];
					jQuery(".IMHR6").each(function(){
						if( jQuery(this).attr("src") != "" ) { array_nav6.push(jQuery(this).attr("src")); }
					})

					var y_nav6=0;
					for(i=0;i<array_nav6.length;i++)
					{
						jQuery("<img class='IMHR6' />").attr('src', array_nav6[i]).on("load",function(){
							y_nav6++;
							if(y_nav6 == array_nav6.length)
							{
								jQuery("#rich_webSlider1_6").css("display","block");
								jQuery("#RW_Load_Content_Navigation6").remove();
								jQuery('#rich_webSlider1_6 .flexslider').flexslider({
									slideshow: true,
									slideshowSpeed: 3000,
									pauseOnHover: true,
									start: renderPreview, //render preview on start
									before: renderPreview //render preview before moving to the next slide
								});
								var y6 = true;
								function renderPreview(slider) {
									var sl = jQuery(slider);
									var prevWrapper = sl.find('.flex-prev');
									var nextWrapper = sl.find('.flex-next');
									//calculate the prev and curr slide based on current slide
									var curr = slider.animatingTo;
									var prev = (curr == 0) ? slider.count - 1 : curr - 1;
									var next = (curr == slider.count - 1) ? 0 : curr + 1;
									//add prev and next slide details into the directional nav
									prevWrapper.find('.preview, .arrow').remove();
									nextWrapper.find('.preview, .arrow').remove();
									prevWrapper.append(grabContent(sl.find('li:eq(' + prev + ') img')));
									nextWrapper.append(grabContent(sl.find('li:eq(' + next + ') img')));
									resp();

									// console.log(this);
									

									var delIc6 = document.querySelector(".delIc6");
					
									delIc6.onclick = delVideo6;

									function delVideo6(){
										document.querySelector('.flexslider_6 ol').style.display = "block";
										document.querySelector('.flex-direction-nav').style.display = "block";
										var videos = document.querySelectorAll('.rw_nav_video6');
										for( var j = 0; j < videos.length; j++){
											videos[j].setAttribute('src','');
											videos[j].classList.remove("rw_nav_video6_anim");
										}
										delIc6.style.display = "none";
									}

									prevWrapper.click(function(){
										delVideo6()
									})
									nextWrapper.click(function(){
										delVideo6()
									})

								}

								

								
									
								// grab the data and render in HTML
								function grabContent(img) {
									var tn = img.data('thumbnail');
									var alt = img.prop('alt');
									var html = '';
									//you can edit this markup to your own needs, but make sure you style it up accordingly
									html = '<div class="arrow"></div><div class="preview"><img src="' + tn + '" alt="" /><div class="alt">' + alt + '</div></div>';
									return html;
									resp();
								}
								var count=0;
								resp();
								function resp(){
									var SLWIDTHR = jQuery('.SLWIDTHR_6').val();
									var SLHEIGHTR = jQuery('.SLHEIGHTR_6').val();
									var SLCLWR = jQuery('.SLCLWR_6').val();
									var SlClPrFS = jQuery('.SlClPrFS_6').val();
									var hovEffType = jQuery('.hovEffType_6').val();
									var navWidth = jQuery('.navWidth_6').val();
									var navHeight = jQuery('.navHeight_6').val();
									var navType = jQuery('.navType_6').val();
									jQuery('.flexslider_6,#RW_Load_Content6').css('height',Math.floor(jQuery('.flexslider_6').width()*SLHEIGHTR/SLWIDTHR));
									jQuery('.flexslider_6 .flex-direction-nav .flex-next').css('height',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700));
									jQuery('.flexslider_6 .flex-direction-nav .flex-prev').css('height',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700));
									jQuery('.flexslider_6 .flex-direction-nav .flex-next').css('width',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700));
									jQuery('.flexslider_6 .flex-direction-nav .flex-prev').css('width',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700));
									jQuery('.flexslider_6 .flex-direction-nav').css('height',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700));
									jQuery('.flexslider_6 .flex-direction-nav a .preview').css('width',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700));
									jQuery('.flexslider_6 .flex-direction-nav .arrow').css('width',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700));
									jQuery('.flexslider_6 .flex-direction-nav .flex-next .preview img').css('width',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700));
									jQuery('.flexslider_6 .flex-direction-nav .flex-prev .preview img').css('width',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700));
									jQuery('.flexslider_6 .flex-control-paging li a').css('width',Math.floor(navWidth*jQuery('.flexslider_6').width()/700));
									jQuery('.flexslider_6 .flex-control-paging li a').css('height',Math.floor(navHeight*jQuery('.flexslider_6').width()/700));
									if(navType == 'bottom')
									{
										jQuery('.flexslider_6 .flex-control-nav li').css('margin-top',Math.floor(35/jQuery('.flexslider_6 .flex-control-paging li a').width()));
									}
									else if(navType == 'top')
									{
										jQuery('.flexslider_6 .flex-control-nav li').css('margin-top','14px');
									}
									if(hovEffType == 'slide out')
									{
										jQuery('.flexslider_6 .flex-direction-nav .flex-next .preview img').css('right',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700)+'px');
										jQuery('.flexslider_6 .flex-direction-nav .flex-prev .preview img').css('left',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700)+'px');
										jQuery('.flexslider_6 .flex-direction-nav .flex-next .preview .alt').css('right',Math.floor(2*SLCLWR*jQuery('.flexslider_6').width()/700)-1+'px');
										jQuery('.flexslider_6 .flex-direction-nav .flex-prev .preview .alt').css('left',Math.floor(2*SLCLWR*jQuery('.flexslider_6').width()/700)-2+'px');
									}
									else if(hovEffType == 'flip out' || hovEffType == 'double flip out')
									{
										jQuery('.flexslider_6 .flex-direction-nav .flex-next .preview').css('right',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700));
										jQuery('.flexslider_6 .flex-direction-nav .flex-prev .preview').css('left',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700));
										jQuery('.flexslider_6 .flex-direction-nav .flex-next .preview .alt').css('right',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700));
										jQuery('.flexslider_6 .flex-direction-nav .flex-prev .preview .alt').css('left',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700));
									}
									else if(hovEffType == 'both ways')
									{
										jQuery('.flexslider_6 .flex-direction-nav .flex-next .preview .alt').css('left','-50px');
										jQuery('.flexslider_6 .flex-direction-nav .flex-prev .preview .alt').css('left','50px');
									}
									jQuery('.flexslider_6 .flex-direction-nav .flex-next .preview .alt').css('width',Math.floor(2*SLCLWR*jQuery('.flexslider_6').width()/700)+'px');
									jQuery('.flexslider_6 .flex-direction-nav .flex-prev .preview .alt').css('width',Math.floor(2*SLCLWR*jQuery('.flexslider_6').width()/700)+'px');
									jQuery('.flexslider_6 .flex-direction-nav .flex-next .preview .alt').css('font-size',Math.floor(SlClPrFS*jQuery('.flexslider_6').width()/700)+'px');
									jQuery('.flexslider_6 .flex-direction-nav .flex-prev .preview .alt').css('font-size',Math.floor(SlClPrFS*jQuery('.flexslider_6').width()/700)+'px');
									jQuery('.flexslider_6 .flex-direction-nav .flex-next .preview .alt').css('line-height',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700)+'px');
									jQuery('.flexslider_6 .flex-direction-nav .flex-prev .preview .alt').css('line-height',Math.floor(SLCLWR*jQuery('.flexslider_6').width()/700)+'px');
									if(jQuery('.flexslider_6').width()<=400) { jQuery('.flex-control-nav').css('display','none'); }
									else { jQuery('.flex-control-nav').css('display','block'); }
								}
								jQuery(window).resize(function(){ resp(); })
							}
						})
					}
        </script>
        <script type="text/javascript">
         function rw_nav_video_cl6(src,el){
						el.children[1].setAttribute('src',src+'?rel=0&amp;autoplay=1');
						el.children[1].classList.add("rw_nav_video6_anim");
						document.querySelector('.flexslider_6 ol').style.display = "none";
						document.querySelector('.flex-direction-nav').style.display = "none";
						setTimeout(function(){
							document.querySelector('.delIc6').style.display = "inline-block";
						},1000)
					}

					var delIc6 = document.querySelector(".delIc6");
					
					delIc6.onclick = delVideo6;

					function delVideo6(){
						document.querySelector('.flexslider_6 ol').style.display = "block";
						document.querySelector('.flex-direction-nav').style.display = "block";
						var videos = document.querySelectorAll('.rw_nav_video6');
						for( var j = 0; j < videos.length; j++){
							videos[j].setAttribute('src','');
							videos[j].classList.remove("rw_nav_video6_anim");
						}
						delIc6.style.display = "none";
					}
        </script>
        <p>
        </p>
        <h2 style="text-align: center">
         <strong>
          <span style="color: #ff9900">
           Welcome to UCI Hospitality &amp; Dining Services!
          </span>
         </strong>
        </h2>
        <h3 style="text-align: center">
         Feeling hungry?
         <a href="http://uci.campusdish.com/">
          UCI Hospitality &amp; Dining Services
         </a>
         is here to help satisfy your cravings with information on places to eat on-campus, vending locations, and catering on campus.
        </h3>
       </div>
      </article>
