      <script>
       function getSelectedPage() {
    var deptObj = {};
    
    
   
           deptObj[0] = ["Biological Sciences", "biological-sciences", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[1] = ["Chemistry &amp; Biochemistry", "chemistry-biochemistry", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[2] = ["Computer Science", "computer-science", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[3] = ["Geological Sciences", "geological-sciences", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[4] = ["Kinesiology &amp; Health Promotion", "kinesiology-health-promotion", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[5] = ["Mathematics &amp; Statistics", "mathematics-statistics", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[6] = ["Physics &amp; Astronomy", "physics-astronomy", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[7] = ["College of Science Advising Center", "advising-center", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[8] = ["Biological Training in Education and Research (BioTiER)", "biotier", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[9] = ["Center for Excellence in Mathematics and Science Teaching (CEMaST)", "cemast", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[10] = ["CIRM Stem Cell Bridges", "stem-cell-bridges", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[11] = ["Louis Stokes Alliance for Minority Participation", "lsamp", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[12] = ["MBRS RISE", "rise", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[13] = ["Motor Development Clinic", "motor-development-clinic", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[14] = ["PCC-CPP STEM Pathway in Environmental Sciences", "pccenv", "{http://www.cpp.edu/~sci/}"];
            
            deptObj[15] = ["Science Educational Enhancement Services", "sees", "{http://www.cpp.edu/~sci/}"];
            
        
    
    
    


    var showdept = false;
     var maincontent = "";
     var fullurl ="";
     var matches = 0;
    
    Object.keys(deptObj).forEach(function(key) {
 
    if(window.location.pathname.indexOf(deptObj[key][1]) > -1)  {
        fullurl = deptObj[key][2] + "/" + deptObj[key][1]
        fullurl = fullurl.replace(/[{}]/g, "");
        if(matches == 0) {
            maincontent = "<h2><a href='" + fullurl + "'>" + deptObj[key][0] +  "</a></h2>";
        }
        matches++;
        showdept = true;
         console.log(maincontent); 
    }
 

    
});
    if(showdept == true) { 
        $( "#deptheading" ).html(maincontent);
        $( "#college-header-title" ).addClass("with-dept");
        $( "#college-navigation" ).css("display","none");
    }
    
        
}
      </script>
