<script type="text/javascript">
 jQuery( document ).ajaxComplete( function() {
	
	 	Foundation.reInit('equalizer');

	});
      
	jQuery(document).ready(function($){

		

// 		engages offset for heroArea and menu interaction

	jQuery(window).on('scroll', function() {
	      getOffset();
	      
	});

// 		Adds class 'shortfix' to menu when past heroArea
	
	function getOffset(){
		var menu = $('#masthead');
		var offset1 = menu.offset().top;
	    var offset2 = $('header.heroArea').offset().top;
		var height1 =  menu.height();
		var height2 =  $('header.heroArea').height();
			
		if ((offset2 < (offset1 + height1)) && ((offset2 + height2 - 100) > offset1)){
			menu.removeClass('shortfix');
		} else {
			menu.addClass('shortfix');
		}
	};
	
    
	});

$(document).ready(function () {
    
    //smoothscroll
    $('#heroNav a[href^="#"]').on('click', function () {

        
        $('a').each(function () {
            $(this).removeClass('active');
        })
        $(this).addClass('active');
      
        var target = this.hash,
            menu = target;
        $target = $(target);
        $('html, body').stop().animate({
            'scrollTop': $target.offset().top+2
        }, 500, 'swing', function () {
            window.location.hash = target;
        });
    });
});

jQuery(document).ready(function($) {
    $("#playVideo").foundation("open");
});
</script>

