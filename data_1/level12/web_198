  <script>
   jQuery(function() {
    
    jQuery.ajax({

        url: 'https://api3.libcal.com/api_hours_today.php?iid=3646&lid=4312&format=json&callback=?', 
        dataType: 'jsonp',
        success: function(data){
        
            var output = '<table class="nowrap">';
            
            //Loop over each library/department
            var libs = data.locations.length;
            for(var i=0;i<libs;i++){
            
                var name = data.locations[i].name;
                var times = data.locations[i].rendered;
                output += '<tr><td>'+ name + '</td><td>' + times + '</td></tr>';
                
            }
            
            output += '</table>';
            
            jQuery('.libcal_hours').html(output);
        }
    });
});
  </script>
