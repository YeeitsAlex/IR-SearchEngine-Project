      <!-- Main content and middle sidebar -->
      <div class="span12">
       <!-- Pre-content -->
       <!-- Main Content -->
       <div class="tabs">
       </div>
       <div id="content" role="main">
        <div class="region region-content">
         <div class="block block-system" id="block-system-main">
          <div class="content">
           <div about="/home" class="node node-book clearfix" id="node-394" typeof="sioc:Item foaf:Document">
            <span class="rdf-meta element-hidden" content="Environmental Health and Safety" property="dc:title">
            </span>
            <div class="content">
             <div class="field field-name-body field-type-text-with-summary field-label-hidden">
              <div class="field-items">
               <div class="field-item even" property="content:encoded">
                <p>
                 Environmental Health and Safety assists departments in maintaining a safe and healthy university environment for staff, faculty, students, and visitors.
                </p>
                <p>
                 We provide training, consultation, and various safety and health services. We help ensure Oregon State abides by the regulatory requirements established at the local, state and federal level.
                </p>
               </div>
              </div>
             </div>
            </div>
           </div>
          </div>
         </div>
        </div>
       </div>
       <!-- /content -->
