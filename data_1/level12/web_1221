               <div class="field-collection-container clearfix">
                <div class="field field-name-field-s-image-info field-type-field-collection field-label-hidden">
                 <div class="field-items">
                  <div class="field-item even">
                   <div class="field-collection-view clearfix view-mode-stanford-block field-collection-view-final">
                    <div about="/field-collection/field-s-image-info/1611" class="entity entity-field-collection-item field-collection-item-field-s-image-info clearfix" typeof="">
                     <div class="content">
                      <div class="field field-name-field-s-image-image field-type-image field-label-hidden">
                       <div class="field-items">
                        <div class="field-item even">
                         <img alt="Four female colleagues talking around a wooden table outside" height="210" src="https://files-cardinalatwork-stanford-edu.s3-us-west-2.amazonaws.com/s3fs-public/styles/caw-med-landscape/public/0227_joannecamantigue_colleagues_meeting_740x420.png?itok=DhP-hRmo" typeof="foaf:Image" width="370"/>
                        </div>
                       </div>
                      </div>
                     </div>
                    </div>
                   </div>
