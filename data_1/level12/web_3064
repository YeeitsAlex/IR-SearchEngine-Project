  <script type="text/javascript">
   $.ajaxSetup({
        headers: {'X-CSRF-TOKEN': Config.csrfToken}
    });

    function init_map() {
        $('.gmap').each(function (index) {
            var myOptions = {
                zoom     : 18,
                center   : new google.maps.LatLng(41.7048128, 44.79013859999998),
                mapTypeId: google.maps.MapTypeId.ROADMAP
            };
            map = new google.maps.Map(document.getElementById($(this).attr('id')), myOptions);
            marker = new google.maps.Marker({
                map     : map,
                position: new google.maps.LatLng(41.7048128, 44.79013859999998)
            });
            infowindow = new google.maps.InfoWindow({content: '<strong>San Diego State University Office</strong><br>5 Kostava, Tbilisi, Georgia<br>'});
            google.maps.event.addListener(marker, 'click', function () {
                infowindow.open(map, marker);
            });
            infowindow.open(map, marker);
        });
    }

    google.maps.event.addDomListener(window, 'load', init_map);
  </script>
