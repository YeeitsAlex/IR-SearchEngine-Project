<script>
 $(document).ready(function(){
	$("select#bc-req-area-of-study").change(function(){

		var program_area_id = $("select#bc-req-area-of-study option:selected").attr('value');
		// alert(program_area_id);
		$("#bc-req-concentration-fields").html( "" );
		$("#bc-req-certificates").html( "" );
		if (program_area_id.length > 0 ) {
			$.ajax({
				type: "POST",
				url: "/includes/ajax/fetch_bc_req_program_streams.php",
				data: "program_area_id="+program_area_id,
				cache: false,
				beforeSend: function () {
					$('#blank-req-concentration-fields').css("display", "none"); 
					$('#bc-req-concentration-fields').css("display", "block"); 
					$('#bc-req-concentration-fields').html('<p><img src="/images/header/loading.gif" alt="" width="50" height="50"> Loading</p>');
				},
				success: function(html) {
					$("#bc-req-concentration-fields").html( html );
				}
			});
		}
	});
});
</script>
<script>
 $(document).ready(function () {
	$("#request-info-request").click(function(){
		$("#request-info-request-form").fadeToggle();
	});	
	$("#close-request-info-request-form").click(function(){
		$("#request-info-request-form").fadeToggle();
	});			
    $("#footer-expand").click(function () {
		$("#expanded-footer").fadeToggle(); 
    });
    $(".feedback").click(function () {
		$("#submit-feedback").fadeToggle(); 
		$("#submit-feedback-wrap").fadeToggle();
    });
    $(".close-feedback").click(function () {
		$("#submit-feedback").fadeToggle(); 
		$("#submit-feedback-wrap").fadeToggle();
    });
});
