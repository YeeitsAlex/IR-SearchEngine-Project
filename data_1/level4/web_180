<script>
 var savedCount = ; 
	switch(savedCount) {
		case 1: 
			$(".event").css("min-height", "50px");
			break; 
		case 2: 
			$(".event").css("min-height", "150px");
			break; 
		case 3: 
			$(".event").css("min-height", "250px");
			break; 
		case 4: 
			$(".event").css("min-height", "350px");
			break; 							
	}	

$(document).ready(function(){
	$("select#bc-cat-area-of-study").change(function(){

		var program_area_id = $("select#bc-cat-area-of-study option:selected").attr('value');
		// alert(program_area_id);
		$("#bc-cat-concentration-fields").html( "" );
		$("#bc-cat-certificates").html( "" );
		if (program_area_id.length > 0 ) {
			$.ajax({
				type: "POST",
				url: "/includes/ajax/fetch_bc_cat_program_streams.php",
				data: "program_area_id="+program_area_id,
				cache: false,
				beforeSend: function () {
					$('#blank-cat-concentration-fields').css("display", "none"); 
					$('#bc-cat-concentration-fields').css("display", "block"); 
					$('#bc-cat-concentration-fields').html('<p><img src="/images/header/loading.gif" alt="" width="50" height="50"> Loading</p>');
				},
				success: function(html) {
					$("#bc-cat-concentration-fields").html( html );
				}
			});
		}
	});
});
</script>
<script>
 $(document).ready(function(){
$(function() {
    $('.jcarousel')
		.on('jcarousel:create jcarousel:reload', function() {
			var element = $(this),
				width = element.innerWidth();

			// This shows 1 item at a time.
			// Divide `width` to the number of items you want to display,
			// eg. `width = width / 3` to display 3 items at a time.
			element.jcarousel('items').css('width', width + 'px');
		})
        .jcarousel({
            // Core configuration goes here
        })
        .jcarouselAutoscroll({
            interval: 5000,
            target: '+=1',
            autostart: true
        });
	});
	$("#catalog-request").click(function(){
		$("#catalog-request-form").fadeToggle("slow");
		$("#catalog-form").fadeToggle("slow");
	});	
	$("#close-catalog-request-form").click(function(){
		$("#catalog-request-form").fadeToggle("slow");
		$("catalog-form").fadeToggle("slow");
	});			
	$("#catalog").mouseover(function(){
		$("#catalog-form").css("display", "block");
	});	
	$("#close-catalog-form").click(function(){
		$("#catalog-form").fadeToggle("slow");
	});			
});
</script>
<script>
 var options = {
		url: "/pdf/json/search.json",
		getValue: "name",		
		categories: [
			{   //Category Certificates
				maxNumberOfElements: 1000,
				listLocation: "certificate",
				header: "Certificates"
			}, 
			{   //Category Courses
				maxNumberOfElements: 1000,
				listLocation: "course",
				header: "Courses"
			},
			{   //Category Articles
				maxNumberOfElements: 1000,
				listLocation: "article",
				header: "Articles"
			}, 
			{   //Category PDFs
				maxNumberOfElements: 1000,
				listLocation: "pdf",
				header: "PDFs"
			}		
		],
		list: {
			onChooseEvent: function() {
				$("#auto_complete_search").submit();
			},
			onKeyEnterEvent: function() {
				$("#auto_complete_search").submit();
			},
			onClickEvent: function() {
				$("#auto_complete_search").submit();
			},
 			maxNumberOfElements: 4000,
			match: {
				enabled: true
			}			
		}
	};

	$("#autocomplete-search").easyAutocomplete(options);
</script>
