       </script>
       <script type="text/javascript">
        $('.panel-heading > h4 > a').click(function (e) {
                //Destroy previous instances of both calendar controls
                $("#calAnnouncements").destroyPlugin;
                $("#calUpcomingEvents").destroyPlugin;

                var currentPanel = $(this).text().trim();

                // Check the control and load the calendar
                if (currentPanel == "Upcoming Events") {
                    //alert(currentPanel + ' header clicked');
                    $("#calUpcomingEvents").CalendarEvents({
                        user: "ucrhousingservices@gmail.com"
                           , apiKey: "AIzaSyCrPnmIYNcnG3taiMW49KnIYwb6m-hgtuI"
                           , displayType: "perMonth"
                           , searchCriteria: "#housingevent"
                           , searchCondition: "in"
                           , searchPath: "description"
                           , height: "225"
                           , addLinkType: "link"
                           , setContainer: false
                           , logging: true
                    });
                }
                if (currentPanel == "Announcements") {
                    //alert(currentPanel + ' header clicked');
                    $("#calAnnouncements").CalendarEvents({
                        user: "ucrhousingservices@gmail.com"
                           , apiKey: "AIzaSyCrPnmIYNcnG3taiMW49KnIYwb6m-hgtuI"
                           , displayType: "perMonth"
                           , searchCriteria: "#housingannouncement"
                           , searchCondition: "in"
                           , searchPath: "description"
                           , height: "225"
                           , addLinkType: "link"
                           , setContainer: false
                           , logging: true
                    });
                }
            });

            $(document).ready(function () {
                // Open the Announcement collapse
                $("#calAnnouncements").destroyPlugin;
                $("#calAnnouncements").CalendarEvents({
                    user: "ucrhousingservices@gmail.com"
                          , apiKey: "AIzaSyCrPnmIYNcnG3taiMW49KnIYwb6m-hgtuI"
                          , displayType: "perMonth"
                          , searchCriteria: "#housingannouncement"
                          , searchCondition: "in"
                          , searchPath: "description"
                          , height: "225"
                          , addLinkType: "link"
                          , setContainer: false
                          , logging: true
                });

                //Slide Carousel
                $('#slide-carousel').carousel({
                    interval: 4000
                })

                $('#slide-carousel > .carousel-inner > .item').each(function () {
                    var next = $(this).next();
                    if (!next.length) {
                        next = $(this).siblings(':first');
                    }
                    next.children(':first-child').clone().appendTo($(this));

                    for (var i = 0; i < 1; i++) {
                        next = next.next();
                        if (!next.length) {
                            next = $(this).siblings(':first');
                        }
                        next.children(':first-child').clone().appendTo($(this));
                    }
                });
            });
       </script>
